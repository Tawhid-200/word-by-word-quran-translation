{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Pasha Librin",
    "e qartë."
  ],
  "3": [
    "Vërtet Ne",
    "e bëmë atë",
    "Kur'an",
    "arabisht",
    "ashtu që ju",
    "ta rrokni."
  ],
  "4": [
    "Dhe vërtet ai (është)",
    "në",
    "ajkën",
    "e librit",
    "tek Ne",
    "i lartësuar",
    "i urtë."
  ],
  "5": [
    "Vallë a do ta flakim",
    "prej jush",
    "përkujtimin",
    "plotësisht",
    "se",
    "ju jeni",
    "një popull",
    "teprues?!"
  ],
  "6": [
    "E sa shumë",
    "Ne dërguam",
    "nga",
    "pejgamber",
    "në mesin e",
    "të parëve!"
  ],
  "7": [
    "Dhe nuk",
    "u vinte atyre",
    "asnjë",
    "pejgamber",
    "përveçse",
    "ata ishin",
    "me të",
    "duke u tallur."
  ],
  "8": [
    "Prandaj Ne i shkatërruam",
    "më të ashpër",
    "se ata",
    "(në) fuqi",
    "dhe kaloi tashmë",
    "shembulli",
    "i të parëve."
  ],
  "9": [
    "E nëse",
    "ti i pyet ata",
    "\"Kush",
    "i krijoi",
    "qiejt",
    "dhe tokën?\"",
    "gjithsesi ata do të thonë",
    "\"I krijoi ato",
    "i Pamposhturi",
    "i Gjithëdijshmi!\""
  ],
  "10": [
    "Ai i Cili",
    "bëri",
    "për ju",
    "tokën",
    "shtrojë",
    "dhe bëri",
    "për ju",
    "në të",
    "rrugë",
    "ashtu që ju",
    "të orientoheni."
  ],
  "11": [
    "Dhe Ai i Cili",
    "zbriti",
    "prej",
    "qiellit",
    "ujë",
    "me masë",
    "e Ne e gjallërojmë",
    "me të",
    "një vend",
    "të vdekur.",
    "Kështu",
    "(edhe ju) do të nxirreni."
  ],
  "12": [
    "Dhe Ai i Cili",
    "krijoi",
    "çiftet",
    "të gjitha ato",
    "dhe bëri",
    "për ju",
    "prej",
    "anijeve",
    "dhe bagëtive",
    "çfarë",
    "të hipni."
  ],
  "13": [
    "Që të qëndroni",
    "mbi",
    "shpinat e tyre",
    "pastaj",
    "ta kujtoni",
    "dhuntinë",
    "e Zotit tuaj",
    "kur",
    "të vendoseni",
    "mbi to",
    "e të thoni",
    "\"Lartësuar qoftë",
    "Ai i Cili",
    "e nënshtroi",
    "për ne",
    "këtë",
    "dhe nuk",
    "ne do të ishim",
    "për të",
    "të aftë."
  ],
  "14": [
    "Dhe vërtet ne",
    "tek",
    "Zoti ynë",
    "(jemi) njëmend kthyes."
  ],
  "15": [
    "Dhe ata caktuan",
    "për Të",
    "prej",
    "robërve të Tij",
    "pjesë (fëmijë).",
    "Vërtet",
    "njeriu",
    "(është) njëmend shumë mohues",
    "i qartë."
  ],
  "16": [
    "Vallë a",
    "ai mori",
    "prej asaj çfarë",
    "Ai krijoi",
    "vajza",
    "kurse juve ju veçoi",
    "me djem?!"
  ],
  "17": [
    "E kur",
    "përgëzohet",
    "ndonjëri prej tyre",
    "me atë çfarë",
    "i ka sjell",
    "të Gjithëmëshirshmit",
    "shembull",
    "vazhdon të jetë",
    "fytyra e tij",
    "e nxirë",
    "ndërkohë që ai (është)",
    "shumë i mërzitur."
  ],
  "18": [
    "Vallë a ai i cili",
    "rritet",
    "me",
    "stoli",
    "dhe ai është",
    "në",
    "polemikë",
    "jo",
    "i qartë?"
  ],
  "19": [
    "Dhe ata i bënë",
    "engjëjt",
    "të cilët",
    "ata (janë)",
    "robërit",
    "e të Gjithëmëshirshmit",
    "vajza.",
    "A dëshmuan ata",
    "krijimin e tyre?",
    "Do të shkruhet",
    "dëshmia e tyre",
    "dhe ata do të pyeten."
  ],
  "20": [
    "Dhe ata thanë",
    "\"Sikur",
    "të dëshironte",
    "i Gjirhëmëshirshmi",
    "nuk",
    "ne do t'i adhuronim ata!\"",
    "Nuk",
    "ata kanë",
    "për të",
    "kurrfarë",
    "dije.",
    "Nuk (bëjnë)",
    "ata",
    "veçse",
    "trillojnë."
  ],
  "21": [
    "Vallë a",
    "Ne u dhamë atyre",
    "libër",
    "nga",
    "para tij",
    "e ata (janë)",
    "me të",
    "shumë të kapur?!"
  ],
  "22": [
    "Ama",
    "ata thanë",
    "\"Vërtet ne",
    "i gjetëm",
    "baballarët tanë",
    "në",
    "fe",
    "dhe vërtet ne",
    "në",
    "gjurmët e tyre",
    "(jemi) të udhëzuar\"."
  ],
  "23": [
    "Dhe kështu",
    "nuk",
    "Ne dërguam",
    "nga",
    "para teje",
    "në",
    "(ndonjë) vendbanim",
    "asnjë",
    "paralajmërues",
    "përveçse",
    "thanë",
    "zengjinët e tij",
    "\"Vërtet ne",
    "i gjetëm",
    "baballarët tanë",
    "në",
    "fe",
    "dhe vërtet ne",
    "në",
    "gjurmët e tyre",
    "(jemi) pasues\"."
  ],
  "24": [
    "Ai tha",
    "\"Vallë a nuk",
    "jua solla",
    "më të saktën",
    "se ajo çfarë",
    "ju i gjetët",
    "në të",
    "baballarët tuaj?!\"",
    "Ata thanë",
    "\"Vërtet ne",
    "në atë çfarë",
    "ju u dërguat",
    "me të",
    "(jemi) mohues\"."
  ],
  "25": [
    "Sakaq Ne u hakmorrëm",
    "ndaj tyre",
    "prandaj shiko",
    "si",
    "ishte",
    "përfundimi",
    "i përgënjeshtruesve!"
  ],
  "26": [
    "Dhe kur",
    "(i) tha",
    "Ibrahimi",
    "babait të tij",
    "dhe popullit të tij",
    "\"Vërtet unë (jam)",
    "i larguar",
    "nga ajo çfarë",
    "ju adhuroni."
  ],
  "27": [
    "Përveç",
    "Atij i Cili",
    "(më) krijoi mua",
    "se vërtet Ai",
    "do t'më udhëzojë mua\"."
  ],
  "28": [
    "Dhe ai e bëri atë",
    "një frazë",
    "të përjetshme",
    "ndër",
    "pasardhësit e tij",
    "ashtu që ata",
    "të kthehen."
  ],
  "29": [
    "Por",
    "Unë i kënaqa",
    "këta",
    "dhe baballarët e tyre",
    "derisa",
    "u erdhi atyre",
    "e vërteta",
    "dhe një i dërguar",
    "i qartë."
  ],
  "30": [
    "E pasi që",
    "u erdhi atyre",
    "e vërteta",
    "ata thanë",
    "\"Kjo (është)",
    "magji",
    "dhe vërtet ne",
    "në të",
    "(jemi) mohues\"."
  ],
  "31": [
    "Dhe ata thanë",
    "\"Përse të mos",
    "zbritej",
    "ky",
    "Kur'an",
    "tek",
    "një njeri",
    "prej",
    "dy vendbanimeve",
    "i madh?\""
  ],
  "32": [
    "A ata",
    "e ndajnë",
    "mëshirën",
    "e Zotit tënd?",
    "Ne",
    "e ndamë",
    "mes tyre",
    "jetesën e tyre",
    "në",
    "jetën",
    "e kësaj bote",
    "dhe i ngritëm",
    "disa prej tyre",
    "mbi",
    "disa (të tjerë)",
    "(në) shkalla",
    "që t'i marrin",
    "disa prej tyre",
    "disa (të tjerë)",
    "për tallje.",
    "E mëshira",
    "e Zotit tënd",
    "(është) më e mirë",
    "se ajo çfarë",
    "ata tubojnë."
  ],
  "33": [
    "E sikur mos",
    "të",
    "ishte (që)",
    "njerëzit (të jenë)",
    "në fe",
    "një",
    "Ne do t'ia bënim",
    "atij që",
    "mohon",
    "të Gjithëmëshirshmin",
    "për shtëpitë e tyre",
    "çati",
    "prej",
    "argjendi",
    "dhe shkallë",
    "mbi to",
    "do të ngriteshin."
  ],
  "34": [
    "Dhe për shtëpitë e tyre",
    "dyer",
    "dhe kolltuqe",
    "mbi to",
    "do të mbështeteshin."
  ],
  "35": [
    "Edhe stoli.",
    "Dhe nuk (është)",
    "e tëra",
    "ajo",
    "veçse",
    "kënaqësia",
    "e jetës",
    "së kësaj bote.",
    "Ndërsa jeta e fundit",
    "tek",
    "Zoti yt",
    "(është) për të druajturit."
  ],
  "36": [
    "E kushdo që",
    "verbohet",
    "nga",
    "përkujtimi",
    "i të Gjithëmëshirshmit",
    "Ne ia lidhim",
    "atij",
    "një shejtan",
    "e ai (është)",
    "me të",
    "përcjellës i pandarë."
  ],
  "37": [
    "Dhe vërtet ata",
    "njëmend i pengojnë ata",
    "nga",
    "rruga",
    "dhe pandehin",
    "se ata (janë)",
    "të udhëzuar."
  ],
  "38": [
    "Deri",
    "kur",
    "ai vjen tek ne",
    "thotë",
    "\"Ah sikur (të ishte)",
    "mes meje",
    "dhe mes teje",
    "largësia",
    "e lindjes dhe perëndimit",
    "po sa i keq (je)",
    "përcjellës!\""
  ],
  "39": [
    "Dhe nuk do t'ju",
    "bëjë dobi ju",
    "sot",
    "pasi",
    "keni bërë zullum",
    "që ju",
    "në",
    "dënim",
    "(jeni) bashkëpjesëmarrës."
  ],
  "40": [
    "Vallë a ti",
    "mund të bësh të dëgjojnë",
    "të shurdhërit",
    "apo",
    "t'i udhëzosh",
    "të verbërit",
    "dhe atë që",
    "është",
    "në",
    "një devijim",
    "të qartë?"
  ],
  "41": [
    "E nëse",
    "Ne të largojmë",
    "ty",
    "atëherë vërtet Ne",
    "ndaj tyre",
    "(jemi) hakmarrës."
  ],
  "42": [
    "Apo",
    "Ne mund ta shfaqim ty",
    "atë çfarë",
    "Ne u premtuam atyre",
    "atëherë vërtet Ne",
    "ndaj tyre",
    "(jemi) të fuqishëm."
  ],
  "43": [
    "Prandaj ti kapu fort",
    "për atë çfarë",
    "është zbritur",
    "tek ti!",
    "Vërtet ti (je)",
    "në",
    "rrugë",
    "të drejtë."
  ],
  "44": [
    "Dhe vërtet ai (është)",
    "njëmend përkujtim",
    "për ty",
    "dhe për popullin tënd",
    "dhe do të",
    "pyeteni."
  ],
  "45": [
    "Dhe pyete",
    "këdo që",
    "Ne dërguam",
    "nga",
    "para teje",
    "prej",
    "të dërguarve Tanë",
    "a kemi caktuar",
    "nga",
    "pos",
    "të Gjithëmëshirshmit",
    "zota",
    "(që) adhurohen?"
  ],
  "46": [
    "E sigurisht",
    "Ne e dërguam",
    "Musain",
    "me provat Tona",
    "tek",
    "faraoni",
    "dhe paria e tij",
    "e ai tha",
    "\"Vërtet unë (jam)",
    "i dërguari",
    "i Zotit",
    "të botëve!\""
  ],
  "47": [
    "Pasi që",
    "ai u solli atyre",
    "provat Tona",
    "përnjëherë",
    "ata",
    "me to",
    "qeshnin."
  ],
  "48": [
    "Dhe nuk",
    "Ne ua shfaqnim atyre",
    "asnjë",
    "provë",
    "përveçse",
    "ajo (ishte)",
    "më e madhe",
    "se",
    "simotra e saj.",
    "Dhe Ne i mbërthyem ata",
    "me dënim",
    "ashtu që ata",
    "të kthehen."
  ],
  "49": [
    "Dhe ata thanë",
    "\"O ti",
    "magjistar!",
    "Lute",
    "për ne",
    "Zotin tënd",
    "me atë çfarë",
    "të ka premtuar",
    "ty",
    "(se) vërtet ne",
    "(jemi) njëmend të udhëzuar\"."
  ],
  "50": [
    "Pasi që",
    "Ne ua larguam",
    "atyre",
    "dënimin",
    "përnjëherë",
    "ata",
    "e thyen besën."
  ],
  "51": [
    "Dhe thirri",
    "faraoni",
    "në mesin e",
    "popullit të tij",
    "tha",
    "\"O populli im!",
    "A nuk (është)",
    "imi",
    "sundimi",
    "i Egjiptit",
    "dhe këta",
    "lumenj",
    "(që) rrjedhin",
    "nga",
    "poshtë meje?!",
    "Vallë a nuk",
    "shihni?\""
  ],
  "52": [
    "Apo",
    "unë (jam)",
    "më i mirë",
    "se",
    "ky",
    "i cili",
    "ai (është)",
    "një qyqar",
    "dhe nuk",
    "gati hiç",
    "shprehet qartë."
  ],
  "53": [
    "Po përse nuk",
    "iu dhanë",
    "atij",
    "byzylykë",
    "prej",
    "ari",
    "apo",
    "të vinin",
    "me të",
    "engjëjt",
    "shoqërues?"
  ],
  "54": [
    "Ashtu ai e përçmoi",
    "popullin e tij",
    "dhe ata iu bindën atij.",
    "Vërtet ata",
    "ishin",
    "një popull",
    "të prishur."
  ],
  "55": [
    "Pasi që",
    "ata na zemëruan Ne",
    "Ne u hakmorrëm",
    "ndaj tyre",
    "dhe i përmbytëm ata",
    "të gjithë."
  ],
  "56": [
    "Dhe Ne i bëmë ata",
    "një mbetje",
    "dhe shembull",
    "për brezat e fundit."
  ],
  "57": [
    "E kur",
    "është sjellë",
    "biri",
    "i Merjemes",
    "si shembull",
    "përnjëherë",
    "populli yt",
    "prej tij",
    "bezdiseshin."
  ],
  "58": [
    "Dhe thanë",
    "\"A zotat tanë (janë)",
    "më të mirë",
    "apo",
    "ai?\"",
    "Nuk",
    "e sollën atë",
    "për ty",
    "veçse",
    "si polemikë.",
    "Ama",
    "ata (janë)",
    "një popull",
    "shumë ngatërrestarë."
  ],
  "59": [
    "Nuk (është)",
    "ai",
    "veçse",
    "një rob",
    "Ne e begatuam",
    "atë",
    "dhe e bëmë atë",
    "një shembull",
    "për bijtë",
    "e Israilit."
  ],
  "60": [
    "E sikur",
    "Ne të dëshironim",
    "do të bënim",
    "prej jush",
    "engjëj",
    "në",
    "tokë",
    "(që) do t'ju zëvendësonin."
  ],
  "61": [
    "E vërtet ai (është)",
    "njëmend një shenjë",
    "e Kijametit",
    "prandaj mos",
    "dyshoni assesi",
    "në të",
    "dhe më ndiqni mua!",
    "Kjo (është)",
    "rrugë",
    "e drejtë."
  ],
  "62": [
    "Dhe të mos",
    "ju pengojë assesi ju",
    "shejtani!",
    "Vërtet ai (është)",
    "për ju",
    "një armik",
    "i qartë."
  ],
  "63": [
    "Dhe pasi që",
    "erdhi",
    "Isai",
    "me argumente",
    "tha",
    "\"Sigurisht",
    "unë ju solla",
    "pejgamberllëk",
    "dhe që të sqaroj",
    "për ju",
    "diçka",
    "prej asaj çfarë",
    "ju kundërshtohen mes vete",
    "lidhur me të.",
    "Prandaj druajuni",
    "Allahut",
    "dhe më përfillni mua!"
  ],
  "64": [
    "Vërtet",
    "Allahu",
    "Ai (është)",
    "Zoti im",
    "dhe Zoti juaj",
    "prandaj adhuroni Atë!",
    "Kjo (është)",
    "rrugë",
    "e drejtë."
  ],
  "65": [
    "Rrjedhimisht u përçanë",
    "fraksionet",
    "prej",
    "mes tyre.",
    "Ah mjerë",
    "për ata të cilët",
    "bënë zullum",
    "prej",
    "dënimit",
    "të një dite",
    "të dhembshme!"
  ],
  "66": [
    "A",
    "presin ata",
    "veçse",
    "Kijametin",
    "që",
    "t'u vijë atyre",
    "befasisht",
    "ndërkohë që ata",
    "nuk",
    "ndjejnë."
  ],
  "67": [
    "Jaranët",
    "atë ditë",
    "disa prej tyre",
    "me disa (të tjerë)",
    "(do të jenë) armiq",
    "përveç",
    "të druajturve."
  ],
  "68": [
    "\"O robërit e Mi!",
    "Nuk (ka)",
    "frikë",
    "për ju",
    "sot",
    "dhe as",
    "ju",
    "(do të) mërziteni\"."
  ],
  "69": [
    "Ata të cilët",
    "besuan",
    "faktet Tona",
    "dhe ishin",
    "muslimanë."
  ],
  "70": [
    "Hyni",
    "në xhenet",
    "ju",
    "dhe gratë tuaja",
    "duke qenë fytyrëqeshur!"
  ],
  "71": [
    "Ju sillen",
    "atyre",
    "enë",
    "prej",
    "dukati",
    "dhe kupa.",
    "Dhe në të (ka)",
    "ajo çfarë",
    "e pëlqejnë atë",
    "loçkat e shpirtit",
    "dhe lezetohen",
    "sytë",
    "dhe ju",
    "në të",
    "(do të jeni) të gjithmonshëm."
  ],
  "72": [
    "Pra ai (është)",
    "xheneti",
    "i cili",
    "ju është lënë në trashëgim",
    "për atë çfarë",
    "ju ishit",
    "duke punuar."
  ],
  "73": [
    "Ju keni",
    "në të",
    "pemë",
    "të shumta",
    "prej tyre",
    "ju hani."
  ],
  "74": [
    "Vërtet",
    "kriminelët",
    "(janë) në",
    "dënimin",
    "e xhehenemit",
    "të gjithmonshëm."
  ],
  "75": [
    "Nuk",
    "u lehtësohet",
    "atyre",
    "dhe ata (janë)",
    "në të",
    "të zhgënjyer."
  ],
  "76": [
    "Dhe nuk",
    "Ne u bëmë zullum atyre",
    "por",
    "ishin",
    "ata",
    "zullumqarë."
  ],
  "77": [
    "Dhe ata thërrasin",
    "\"O Malik!",
    "Le t'na shfaros",
    "ne",
    "Zoti yt!\"",
    "Ai thotë",
    "\"Vërtet ju (jeni)",
    "qëndrues (të gjithmonshëm)\"."
  ],
  "78": [
    "Sigurisht",
    "Ne jua sollëm ju",
    "të vërtetën",
    "mirëpo",
    "shumica prej jush",
    "ndaj së vërtetës",
    "(jeni) mohues."
  ],
  "79": [
    "A mos",
    "përcaktuan ata",
    "ndonjë çështje",
    "por vërtet Ne",
    "(jemi) përcaktues."
  ],
  "80": [
    "A mos",
    "ata pandehin",
    "se Ne",
    "nuk",
    "dëgjojmë",
    "sekretin e tyre",
    "dhe pëshpëritjen e tyre?",
    "Sigurisht po",
    "edhe të dërguarit Tanë",
    "pranë tyre",
    "shkruajnë."
  ],
  "81": [
    "Thuaj",
    "\"Po të",
    "ishte",
    "për të Gjithëmëshirshmin",
    "fëmijë",
    "atëherë unë (do të isha)",
    "i pari",
    "i adhuruesve\"."
  ],
  "82": [
    "I Patëmetë (është)",
    "Zoti",
    "i qiejve",
    "dhe tokës",
    "Zoti",
    "i Fronit",
    "nga ajo çfarë",
    "ata përshkruajnë!"
  ],
  "83": [
    "Po ti lëri ata",
    "të zhyten",
    "dhe të luajnë",
    "derisa",
    "të puqen",
    "me ditën e tyre",
    "e cila",
    "ju premtohet."
  ],
  "84": [
    "Dhe Ai",
    "i Cili",
    "në",
    "qiell",
    "(është) Zot",
    "edhe në",
    "tokë",
    "(është) Zot",
    "dhe Ai (është)",
    "i Urti",
    "i Gjithëdijshmi."
  ],
  "85": [
    "Dhe bekuar qoftë",
    "Ai i Cili",
    "Atij (i përket)",
    "sundimi",
    "i qiejve",
    "dhe tokës",
    "dhe çfarë (ka)",
    "mes atyre dyjave",
    "dhe tek Ai (është)",
    "dija",
    "e Kijametit",
    "dhe tek Ai",
    "do të ktheheni."
  ],
  "86": [
    "Dhe nuk",
    "posedojnë",
    "ata të cilët",
    "lusin",
    "nga",
    "pos Tij",
    "ndërmjetësimin",
    "përveç",
    "kush",
    "dëshmoi",
    "të vërtetën",
    "dhe ata",
    "e dinë."
  ],
  "87": [
    "E po t'i",
    "pyesësh ata",
    "\"Kush",
    "i krijoi ata?\"",
    "ata do të thonin sigurisht",
    "\"Allahu!\"",
    "Ore, si",
    "bishtnojnë!"
  ],
  "88": [
    "Edhe frazën e tij",
    "\"O Zoti im!",
    "Vërtet",
    "këta (janë)",
    "një popull",
    "(që) nuk",
    "besojnë\"."
  ],
  "89": [
    "E ti largohu",
    "prej tyre",
    "dhe thuaj",
    "\"Paqe!\"",
    "Ama do të",
    "dinë!\""
  ]
}