{
  "1": [
    "Jo!",
    "Unë betohem",
    "në Ditën",
    "e Kijametit."
  ],
  "2": [
    "Dhe jo!",
    "Unë betohem",
    "në shpirtin",
    "vetëqortues."
  ],
  "3": [
    "A mendon",
    "njeriu",
    "që nuk do të",
    "tubojmë",
    "eshtrat e tij?"
  ],
  "4": [
    "Gjithsesi!",
    "(Ne jemi) të aftë",
    "për (atë)",
    "që të",
    "trajtësojmë",
    "majat e gishtërinjve të tij."
  ],
  "5": [
    "Por",
    "dëshiron",
    "njeriu",
    "që të mëkatojë",
    "përpara tij."
  ],
  "6": [
    "Ai pyet",
    "\"Kur",
    "(është) Dita",
    "e Kijametit?\""
  ],
  "7": [
    "E kur",
    "(të) verbohet",
    "shikimi."
  ],
  "8": [
    "Dhe (të) errësohet",
    "hëna."
  ],
  "9": [
    "Dhe (të) bashkohet",
    "dielli",
    "dhe hëna."
  ],
  "10": [
    "Thotë",
    "njeriu",
    "atë Ditë",
    "\"Ku",
    "(është) vendi për ikje?\""
  ],
  "11": [
    "Assesi!",
    "Nuk (ka)",
    "strehim."
  ],
  "12": [
    "Tek",
    "Zoti yt",
    "atë Ditë",
    "(është) vendi i përhershëm."
  ],
  "13": [
    "(Do të) njoftohet",
    "njeriu",
    "atë Ditë",
    "për atë çfarë",
    "çoi përpara",
    "dhe la mbrapa."
  ],
  "14": [
    "Por",
    "njeriu",
    "kundër",
    "vetes së tij",
    "(do të jetë) dëshmitar."
  ],
  "15": [
    "Edhe sikur",
    "ai paraqet",
    "arsyetimet e tij."
  ],
  "16": [
    "Mos",
    "e lëviz",
    "me të",
    "gjuhën tënde",
    "që të shpejtosh",
    "me të."
  ],
  "17": [
    "Vërtet",
    "mbi Ne",
    "(është) tubimi i tij",
    "dhe recitimi i tij."
  ],
  "18": [
    "E kur",
    "Ne e recitojmë atë",
    "atëherë ndiqe",
    "recitimin e tij."
  ],
  "19": [
    "Pastaj",
    "vërtet",
    "mbi Ne",
    "(është) sqarimi i tij."
  ],
  "20": [
    "Assesi!",
    "por",
    "ju e doni",
    "të menjëhershmen."
  ],
  "21": [
    "Dhe e lëni",
    "(atë) të fundit."
  ],
  "22": [
    "(Disa) fytyra",
    "atë Ditë",
    "(do të jenë) të ndriçuara."
  ],
  "23": [
    "Tek",
    "Zoti i tyre",
    "të shikuara."
  ],
  "24": [
    "Dhe (disa) fytyra",
    "atë Ditë",
    "(do të jenë) të shtrembëruara."
  ],
  "25": [
    "Duke menduar",
    "që",
    "(t'u) bëhet",
    "atyre",
    "thyerje kurrizi."
  ],
  "26": [
    "Assesi!",
    "Kur",
    "arrin (shpirti)",
    "klavikulat."
  ],
  "27": [
    "Dhe thuhet",
    "\"Kush",
    "(është) shërues?\""
  ],
  "28": [
    "Dhe ai bindet",
    "që vërtet ajo",
    "(është) ndarja."
  ],
  "29": [
    "Dhe puqet",
    "kofsha",
    "me kofshën."
  ],
  "30": [
    "Tek",
    "Zoti yt",
    "atë Ditë",
    "(është) shkuarja."
  ],
  "31": [
    "Dhe nuk",
    "ai pranoi të vërtetën",
    "dhe nuk",
    "ka falë (namaz)."
  ],
  "32": [
    "Mirëpo",
    "ka përgënjeshtruar",
    "dhe ka kthyer shpinën."
  ],
  "33": [
    "Pastaj",
    "ka shkuar",
    "tek",
    "familja e tij",
    "duke u kapardisur."
  ],
  "34": [
    "Mjerë",
    "për ty",
    "dhe mjerë!"
  ],
  "35": [
    "Pastaj",
    "mjerë",
    "për ty",
    "dhe mjerë!"
  ],
  "36": [
    "A mendon",
    "njeriu",
    "që",
    "(do të) lihet",
    "(në) shpërfillje?"
  ],
  "37": [
    "A nuk",
    "ishte",
    "një farë",
    "nga",
    "një spermë",
    "(që) derdhet."
  ],
  "38": [
    "Pastaj",
    "ishte",
    "një substancë e ngjizur",
    "dhe Ai e krijoi",
    "dhe e balancoi?"
  ],
  "39": [
    "Dhe Ai bëri",
    "prej tij",
    "dy lloje",
    "mashkullin",
    "dhe femrën."
  ],
  "40": [
    "A nuk është",
    "Ai",
    "i aftë",
    "mbi (atë)",
    "që të",
    "ngjallë",
    "të vdekurit?"
  ]
}