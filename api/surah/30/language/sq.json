    "deri në",
    "ditën",
    "e ringjalljes.",
    "Ja kjo (është)",
    "dita",
    "e ringjalljes",
    "mirëpo ju",
    "ishit",
    "nuk",
    "e dinit\"."
  ],
  "57": [
    "E atë ditë",
    "nuk",
    "(u) bën dobi",
    "atyre të cilët",
    "bënë zullum",
    "arsyetimi i tyre",
    "dhe nuk",
    "ata",
    "do të justifikohen."
  ],
  "58": [
    "Dhe sigurisht",
    "Ne sollëm",
    "për njerëzit",
    "në",
    "këtë",
    "Kur'an",
    "prej",
    "çdo",
    "shembulli",
    "dhe po t'u",
    "sillje atyre",
    "ndonjë provë",
    "do të thonin gjithsesi",
    "ata të cilët",
    "mohuan",
    "\"Nuk",
    "ju (jeni)",
    "veçse",
    "dallaveraxhinj\"."
  ],
  "59": [
    "Kështu",
    "vulos",
    "Allahu",
    "mbi",
    "zemrat",
    "e atyre të cilët",
    "nuk",
    "dinë."
  ],
  "60": [
    "Po ti duro",
    "(se) vërtet",
    "premtimi",
    "i Allahut",
    "(është) hak",
    "e mos",
    "të të luhasin kurrsesi ty",
    "ata të cilët",
    "nuk",
    "janë të bindur."
  ]
}