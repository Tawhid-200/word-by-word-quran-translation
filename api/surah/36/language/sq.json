{
  "1": [
    "Ja Sin."
  ],
  "2": [
    "Pasha Kur'anin",
    "e urtë!"
  ],
  "3": [
    "Vërtet ti (je)",
    "njëmend prej",
    "të dërguarve."
  ],
  "4": [
    "Në",
    "një rrugë",
    "të drejtë."
  ],
  "5": [
    "Zbritje",
    "e të Pamposhturit",
    "Mëshiruesit."
  ],
  "6": [
    "Që ti të paralajmërosh",
    "një popull",
    "nuk",
    "u paralajmëruan",
    "baballarët e tyre",
    "prandaj ata (janë)",
    "të shkujdesur."
  ],
  "7": [
    "Sigurisht",
    "është vërtetuar",
    "fjala",
    "ndaj",
    "shumicës së tyre",
    "prandaj ata",
    "nuk",
    "besojnë."
  ],
  "8": [
    "Vërtet Ne",
    "vendosëm",
    "në",
    "qafat e tyre",
    "pranga",
    "e ato (janë)",
    "deri tek",
    "nofullat",
    "prandaj ata (janë)",
    "të lidhur me kokën lart."
  ],
  "9": [
    "Dhe Ne vendosëm",
    "nga",
    "para",
    "tyre",
    "një digë",
    "dhe nga",
    "pas tyre",
    "një digë",
    "ashtu që Ne i mbuluam ata",
    "prandaj ata",
    "nuk",
    "shohin."
  ],
  "10": [
    "Edhe njësoj (është)",
    "për ta",
    "a i paralajmërove ata",
    "apo",
    "nuk",
    "i paralajmërove ata",
    "nuk",
    "besojnë."
  ],
  "11": [
    "Vetëm",
    "ti e paralajmëron",
    "atë që",
    "e ndjek",
    "Përkujtimin",
    "dhe i frikësohet",
    "të Gjithëmëshirshmit",
    "në fshehtësi",
    "prandaj përgëzoje atë",
    "me falje",
    "dhe një shpërblim",
    "fisnik."
  ],
  "12": [
    "Vërtet Ne",
    "Ne",
    "i ngjallim",
    "të vdekurit",
    "dhe shkruajmë",
    "atë çfarë",
    "kanë vënë përpara",
    "edhe gjurmët e tyre.",
    "Dhe çdo",
    "gjë",
    "Ne e kemi evidentuar atë",
    "në",
    "një regjistër",
    "të qartë."
  ],
  "13": [
    "Dhe sill",
    "për ta",
    "një shembull",
    "të banorëve",
    "të vendbanimit",
    "kur",
    "erdhën tek ai",
    "të dërguarit."
  ],
  "14": [
    "Kur",
    "Ne dërguam",
    "tek ta",
    "dy (të dërguar)",
    "e ata i përgënjeshtruan ata dy",
    "ashtu që Ne i forcuam",
    "me një të tretë",
    "e ata thanë",
    "\"Vërtet ne",
    "tek ju",
    "(jemi) të dërguar\"."
  ],
  "15": [
    "Ata thanë",
    "\"Nuk",
    "ju (jeni)",
    "përveçse",
    "njerëz",
    "sikurse ne",
    "dhe nuk",
    "dërgoi",
    "i Gjithëmëshirshmi",
    "asnjë",
    "gjë.",
    "Nuk",
    "ju (jeni)",
    "përveçse",
    "gënjeni\"."
  ],
  "16": [
    "Ata thanë",
    "\"Zoti ynë",
    "e di",
    "(se) vërtet ne",
    "tek ju",
    "(jemi) njëmend të dërguar."
  ],
  "17": [
    "Dhe nuk",
    "(është) mbi ne",
    "përveçse",
    "kumtimi",
    "i qartë\"."
  ],
  "18": [
    "Ata thanë",
    "\"Vërtet ne",
    "parandjejmë një kob",
    "prej jush.",
    "Nëse",
    "nuk",
    "hiqni dorë",
    "gjithsesi ne do t'ju gurëzojmë ju",
    "dhe gjithsesi do t'ju prek ju",
    "prej nesh",
    "një dënim",
    "i dhembshëm\"."
  ],
  "19": [
    "Ata thanë",
    "\"Kobi juaj",
    "(është) me ju.",
    "A veç pse",
    "ju u përkujtuat?",
    "Jo por",
    "ju (jeni)",
    "një popull",
    "të shfrenuar\"."
  ],
  "20": [
    "Dhe erdhi",
    "prej",
    "skajit",
    "të qytetit",
    "një burrë",
    "duke vrapuar.",
    "Ai tha",
    "\"O populli im!",
    "Ndiqni",
    "të dërguarit!"
  ],
  "21": [
    "Ndiqni",
    "atë i cili",
    "nuk",
    "kërkon nga ju",
    "asnjë shpërblim",
    "dhe ata",
    "(janë) të udhëzuar."
  ],
  "22": [
    "E çfarë",
    "(është) për mua",
    "të mos",
    "adhuroj",
    "Atë i Cili",
    "më krijoi mua",
    "dhe tek Ai",
    "(do të) ktheheni."
  ],
  "23": [
    "A të marr",
    "prej",
    "pos Tij",
    "zota?",
    "Nëse",
    "dëshiron për mua",
    "i Gjithëmëshirshmi",
    "ndonjë zarar",
    "nuk",
    "do t'më vlejë",
    "mua",
    "ndërmjetësimi i tyre",
    "asgjë",
    "dhe as",
    "ata do t'më shpëtojnë mua."
  ],
  "24": [
    "Vërtet unë",
    "atëherë",
    "(do të jem) njëmend në",
    "një humbje",
    "të qartë."
  ],
  "25": [
    "Vërtet unë",
    "i besova",
    "Zotit tuaj",
    "prandaj më dëgjoni mua!\""
  ],
  "26": [
    "(Atij do t'i) thuhet",
    "\"Hyr",
    "në xhenet!\"",
    "Ai do të thotë",
    "\"Ah sikur",
    "populli im",
    "të dinin!"
  ],
  "27": [
    "Atë çfarë",
    "më fali",
    "mua",
    "Zoti im",
    "dhe më bëri mua",
    "prej",
    "të nderuarve\"."
  ],
  "28": [
    "Dhe nuk",
    "Ne zbritëm",
    "mbi",
    "popullin e tij",
    "nga",
    "pas tij",
    "asnjë",
    "ushtri",
    "prej",
    "qiellit",
    "dhe nuk",
    "Ne ishim",
    "Zbritës."
  ],
  "29": [
    "Nuk",
    "ajo ishte",
    "veçse",
    "britmë",
    "një",
    "e sakaq",
    "ata (u bënë)",
    "të shuar."
  ],
  "30": [
    "Oh mërzi",
    "për",
    "robërit!",
    "Nuk",
    "u erdhi atyre",
    "asnjë",
    "i dërguar",
    "përveçse",
    "ata ishin",
    "me të",
    "duke u tallur."
  ],
  "31": [
    "A nuk",
    "ata shohin",
    "sa shumë",
    "Ne shkatërruam",
    "para tyre",
    "prej",
    "brezave?",
    "Vërtet ata",
    "tek ta",
    "nuk",
    "do të kthehen."
  ],
  "32": [
    "Dhe sigurisht",
    "të gjithë",
    "atëherë",
    "bashkërisht",
    "tek Ne",
    "(do të jenë) të sjellur."
  ],
  "33": [
    "Dhe një provë",
    "(është) për ta",
    "toka",
    "e vdekur.",
    "Ne e ngjallëm atë",
    "dhe nxorëm",
    "prej saj",
    "kokrra gruri",
    "e prej tij",
    "ata hanë."
  ],
  "34": [
    "Dhe Ne vendosëm",
    "në të",
    "kopshte",
    "nga",
    "hurma",
    "e rrushi",
    "dhe Ne shpërthyem",
    "në të",
    "prej",
    "krojeve."
  ],
  "35": [
    "Që ata të hanë",
    "prej",
    "frutave të tij",
    "dhe asaj çfarë",
    "punuan",
    "duart e tyre.",
    "A hiç nuk",
    "falënderojnë?!"
  ],
  "36": [
    "I Patëmetë (është)",
    "Ai i Cili",
    "krijoi",
    "çiftet",
    "të gjitha ato",
    "prej asaj çfarë",
    "mbin",
    "toka",
    "dhe prej",
    "vetes së tyre",
    "dhe prej asaj çfarë",
    "nuk",
    "ata dinë."
  ],
  "37": [
    "Dhe një provë",
    "(është) për ta",
    "nata.",
    "Ne shqisim",
    "prej saj",
    "ditën",
    "e sakaq",
    "ata",
    "(janë) të errësuar."
  ],
  "38": [
    "Edhe dielli",
    "vrapon",
    "drejt një caku",
    "për të.",
    "Ai (është)",
    "përcaktimi",
    "i të Pamposhturit",
    "të Gjithëdijshmit."
  ],
  "39": [
    "Edhe hënës",
    "Ne ia përcaktuam asaj",
    "pozicione",
    "derisa",
    "të kthehet",
    "si kërcelli",
    "i vjetruar."
  ],
  "40": [
    "As",
    "dielli",
    "i lejohet",
    "atij",
    "ta",
    "arrijë",
    "hënën",
    "dhe as",
    "nata",
    "(është) paraprijëse",
    "e ditës.",
    "Dhe të gjithë",
    "në",
    "orbitë",
    "notojnë."
  ],
  "41": [
    "Dhe një provë",
    "(është) për ta",
    "se vërtet Ne",
    "i bartëm",
    "pasardhësit e tyre",
    "në",
    "anijen",
    "e mbushur."
  ],
  "42": [
    "Dhe Ne krijuam",
    "për ta",
    "nga",
    "e ngjashmja me të",
    "çfarë",
    "ata hipin."
  ],
  "43": [
    "E po të",
    "dëshironim Ne",
    "do t'i fundosnim ata",
    "e nuk (ka)",
    "ndihmues i britmës",
    "për ta",
    "dhe as",
    "ata",
    "(do të) shpëtohen."
  ],
  "44": [
    "Përveç",
    "me mëshirë",
    "prej Nesh",
    "dhe shfrytëzim e përdorim",
    "deri në",
    "një kohë."
  ],
  "45": [
    "E kur",
    "u thuhet",
    "atyre",
    "\"Druajuni",
    "asaj çfarë",
    "(është) pranë",
    "jush",
    "dhe çfarë (është)",
    "mbrapa jush",
    "ashtu që ju",
    "të mëshiroheni."
  ],
  "46": [
    "Dhe nuk",
    "u vjen atyre",
    "asnjë",
    "provë",
    "prej",
    "provave",
    "të Zotit të tyre",
    "përveçse",
    "ata ishin",
    "prej tyre",
    "të zmbrapsur."
  ],
  "47": [
    "E kur",
    "u thuhet",
    "atyre",
    "\"Shpenzoni",
    "prej asaj çfarë",
    "ju furnizoi ju",
    "Allahu!\"",
    "(u) thonë",
    "ata të cilët",
    "mohuan",
    "atyre të cilët",
    "besuan",
    "\"A ne ta ushqejmë",
    "atë të cilin",
    "sikur",
    "të dëshironte",
    "Allahu",
    "do ta ushqente atë.",
    "Nuk",
    "ju (jeni)",
    "veçse",
    "në",
    "një humbje",
    "të qartë!\""
  ],
  "48": [
    "Dhe ata thonë",
    "\"Kur (do të jetë)",
    "ky",
    "premtim",
    "nëse",
    "ju jeni",
    "të sinqertë\"."
  ],
  "49": [
    "Nuk",
    "ata presin",
    "përveçse",
    "britmë",
    "një",
    "(që) i mbërthen ata",
    "ndërkohë që ata",
    "zihen mes vete."
  ],
  "50": [
    "Prandaj nuk",
    "(do të) kenë mundësi",
    "të lënë porosi",
    "dhe as",
    "tek",
    "familja e tyre",
    "(do të) kthehen."
  ],
  "51": [
    "Dhe (do të) fryhet",
    "në",
    "bori",
    "e sakaq",
    "ata",
    "prej",
    "varreve",
    "tek",
    "Zoti i tyre",
    "(do të) nxitojnë."
  ],
  "52": [
    "Ata (do të) thonë",
    "\"Ah medet ç'na gjeti!",
    "Kush",
    "na ngjalli ne",
    "prej",
    "vendfjetjes sonë?\"",
    "\"Kjo (është)",
    "ajo çfarë",
    "premtoi",
    "i Gjithëmëshirshmi",
    "dhe thanë të vërtetën",
    "të dërguarit\"."
  ],
  "53": [
    "Nuk",
    "ajo është",
    "veçse",
    "britmë",
    "një",
    "e sakaq",
    "ata",
    "tok",
    "tek Ne",
    "(do të jenë) të sjellur."
  ],
  "54": [
    "Prandaj sot",
    "nuk",
    "(do t'i) bëhet zullum",
    "asnjë personi",
    "asgjë",
    "dhe nuk",
    "ata (do të) kompensohen",
    "përveçse",
    "atë çfarë",
    "ata ishin",
    "duke punuar."
  ],
  "55": [
    "Vërtet",
    "banorët",
    "e xhenetit",
    "sot",
    "(janë) në",
    "një angazhim",
    "duke u dëfryer."
  ],
  "56": [
    "Ata",
    "dhe bashkëshortet e tyre",
    "(do të jenë) në",
    "hije",
    "mbi",
    "kolltukë",
    "të mbështetur."
  ],
  "57": [
    "Për ta (do të ketë)",
    "në të",
    "fruta",
    "dhe për ta (do të ketë)",
    "çfarëdo që",
    "ata kërkojnë."
  ],
  "58": [
    "\"Paqe!\"",
    "(është) një fjalë",
    "prej",
    "një Zoti",
    "Mëshirues."
  ],
  "59": [
    "Dhe veçohuni",
    "sot",
    "o ju",
    "kriminelë!"
  ],
  "60": [
    "A nuk",
    "shpalla dekret",
    "tek ju",
    "\"O bijtë",
    "e Ademit",
    "të",
    "mos",
    "adhuroni",
    "shejtanin!\"",
    "Vërtet ai (është)",
    "për ju",
    "një armik",
    "i qartë."
  ],
  "61": [
    "\"Dhe të",
    "më adhuroni Mua!\"",
    "Kjo (është)",
    "rrugë",
    "e drejtë."
  ],
  "62": [
    "Dhe sigurisht",
    "ai ka devijuar",
    "prej jush",
    "krijesa",
    "të shumta.",
    "Vallë a hiç",
    "nuk jeni",
    "duke shqyrtuar?!"
  ],
  "63": [
    "Ky (është)",
    "xhehenemi",
    "i cili",
    "ju ishte",
    "premtuar."
  ],
  "64": [
    "Ndizeni atë",
    "sot",
    "për shkak se",
    "ju ishit",
    "duke mohuar."
  ],
  "65": [
    "Sot",
    "Ne do të vulosim",
    "mbi",
    "gojët e tyre",
    "dhe do t'na flasin",
    "duart e tyre",
    "edhe do të dëshmojnë",
    "këmbët e tyre",
    "për atë çfarë",
    "ata ishin",
    "duke fituar."
  ],
  "66": [
    "E sikur",
    "Ne të dëshironim",
    "njëmend Ne do të fshinim",
    "mbi",
    "sytë e tyre",
    "e ata do ta mësynin",
    "rrugën",
    "por si (mund)",
    "të shikojnë?"
  ],
  "67": [
    "Dhe sikur",
    "Ne të dëshironim",
    "njëmend Ne do t'i gjymtonim ata",
    "në",
    "vendin e tyre",
    "e nuk",
    "ata do të kenë mundësi",
    "të shkonin para",
    "dhe as",
    "të kthehen."
  ],
  "68": [
    "Dhe atij të cilit",
    "Ne i japim jetë të gjatë",
    "Ne e rikthejmë atë",
    "në",
    "krijim.",
    "A hiç nuk",
    "rrokin?!"
  ],
  "69": [
    "Dhe nuk",
    "Ne ia mësuam atij",
    "poezinë",
    "edhe nuk",
    "është e duhur",
    "për të.",
    "Nuk (është)",
    "ai",
    "veçse",
    "përkujtim",
    "dhe Kur'an",
    "i qartë."
  ],
  "70": [
    "Që ai ta paralajmërojë",
    "këdo që",
    "është",
    "i gjallë",
    "dhe të përligjet",
    "fjala",
    "kundër",
    "mohuesve."
  ],
  "71": [
    "Vallë a nuk",
    "ata shohin",
    "se Ne",
    "krijuam",
    "për ta",
    "prej asaj çfarë",
    "prodhuan",
    "duart Tona",
    "bagëti",
    "e ata (janë)",
    "të tyre",
    "pronarë?"
  ],
  "72": [
    "Dhe Ne i zbutëm ato",
    "për ta",
    "e prej tyre (disa)",
    "ata i përdorin për hipje",
    "e prej tyre (disa)",
    "ata i hanë."
  ],
  "73": [
    "Dhe për ta",
    "në to (ka)",
    "dobi",
    "dhe pije.",
    "A hiç nuk",
    "falënderojnë?"
  ],
  "74": [
    "Dhe ata morën",
    "prej",
    "pos",
    "Allahut",
    "zota",
    "ashtu që ata",
    "të ndihmohen."
  ],
  "75": [
    "Nuk",
    "ata kanë mundësi",
    "ndihmesën e tyre",
    "dhe ata",
    "për ta (janë)",
    "ushtarë",
    "të gatshëm."
  ],
  "76": [
    "Prandaj mos",
    "ki gajle",
    "(për) fjalën e tyre!",
    "Vërtet Ne",
    "dimë",
    "çfarë",
    "ata fshehin",
    "dhe çfarë",
    "ata shpalosin."
  ],
  "77": [
    "Vallë a nuk",
    "sheh",
    "njeriu",
    "se Ne",
    "e krijuam atë",
    "prej",
    "një fare",
    "e përnjëherë",
    "ai (bëhet)",
    "një kundërshtar",
    "i qartë."
  ],
  "78": [
    "Dhe ai solli",
    "për Ne",
    "një shembull",
    "ndërkohë që ai harroi",
    "krijimin e tij.",
    "Tha",
    "\"Kush",
    "i ngjall",
    "eshtrat",
    "kur ato (janë)",
    "të kalbura?\""
  ],
  "79": [
    "Thuaj",
    "\"I ngjall ato",
    "Ai i Cili",
    "i formoi ato",
    "(për të) parën",
    "herë",
    "dhe Ai (është)",
    "për çdo",
    "krijim",
    "i Gjithëdijshëm."
  ],
  "80": [
    "Ai i Cili",
    "bëri",
    "për ju",
    "prej",
    "drurit",
    "të gjelbër",
    "zjarr",
    "e përnjëherë",
    "ju",
    "prej tij",
    "ndizni."
  ],
  "81": [
    "Vallë a nuk (është)",
    "Ai i Cili",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "i Fortë",
    "që",
    "të",
    "krijojë",
    "sikurse ata?",
    "Gjithsesi!",
    "Dhe Ai (është)",
    "Gjithëkrijuesi",
    "i Gjithëdijshmi."
  ],
  "82": [
    "Vetëm",
    "urdhri i Tij (është)",
    "kur",
    "Ai dëshiron",
    "diçka",
    "t'i",
    "thotë",
    "asaj",
    "\"Ji!",
    "e ajo menjëherë është\"."
  ],
  "83": [
