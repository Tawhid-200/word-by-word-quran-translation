{
  "1": [
    "Thuaj",
    "\"Është shpallur",
    "tek unë",
    "se vërtet",
    "ka dëgjuar",
    "një grup",
    "nga",
    "xhinët",
    "e thanë",
    "\"Vërtet ne",
    "dëgjuam",
    "një Kur'an",
    "të mahnitshëm."
  ],
  "2": [
    "Ai udhëzon",
    "tek",
    "udha e drejtë",
    "prandaj ne besuam",
    "në të",
    "dhe nuk do të",
    "shoqërojmë (në adhurim)",
    "me Zotin tonë",
    "askë."
  ],
  "3": [
    "Dhe se vërtet Ai",
    "Lartësuar është",
    "Madhështia",
    "e Zotit tonë",
    "nuk",
    "Ai ka marrë",
    "grua",
    "dhe as",
    "fëmijë."
  ],
  "4": [
    "Dhe se vërtet",
    "ishte",
    "duke thënë",
    "një mendjelehtë nga ne",
    "për",
    "Allahun",
    "fjalë të tepruara."
  ],
  "5": [
    "Dhe vërtet ne",
    "menduam",
    "se",
    "nuk do të",
    "thonë",
    "njerëzit",
    "dhe xhinët",
    "për",
    "Allahun",
    "ndonjë gënjeshtër."
  ],
  "6": [
    "Dhe vërtet",
    "ishin",
    "disa burra",
    "nga",
    "njerëzit",
    "që kërkonin strehim",
    "nga disa burra",
    "nga",
    "xhinët",
    "prandaj ata ua shtuan atyre",
    "një barrë."
  ],
  "7": [
    "Dhe vërtet ata",
    "menduan",
    "siç",
    "ju menduat",
    "se",
    "nuk do të",
    "ngjall",
    "Allahu",
    "askë."
  ],
  "8": [
    "Dhe vërtet ne",
    "(kemi tentuar) ta prekim",
    "qiellin",
    "mirëpo ne e gjetëm atë",
    "është mbushur",
    "me rojtarë",
    "të fortë",
    "dhe shkëndija zjarri."
  ],
  "9": [
    "Dhe vërtet ne",
    "ishim",
    "të ulur",
    "atje",
    "(në) pozita",
    "për dëgjim.",
    "Prandaj kushdo që",
    "dëgjon",
    "tani",
    "(do të) gjejë",
    "për të",
    "një shkëndijë zjarri",
    "në pritë."
  ],
  "10": [
    "Dhe vërtet ne",
    "nuk",
    "dimë",
    "a një e keqe",
    "dëshirohet",
    "për ata të cilët",
    "(janë) në",
    "tokë",
    "apo",
    "dëshiron",
    "për ta",
    "Zoti i tyre",
    "udhë të drejtë."
  ],
  "11": [
    "Dhe vërtet ne",
    "nga ne",
    "(ka) të mirët",
    "dhe nga ne",
    "(ka) tjerë përveç",
    "kësaj.",
    "Ne jemi",
    "(në) drejtime",
    "të ndryshme."
  ],
  "12": [
    "Dhe vërtet ne",
    "ishim të bindur",
    "se",
    "nuk do të",
    "shmangemi",
    "(nga) Allahu",
    "në",
    "tokë",
    "dhe nuk do të",
    "shpëtojmë prej Tij",
    "duke ikur."
  ],
  "13": [
    "Dhe vërtet ne",
    "pasi",
    "dëgjuam",
    "udhëzimin",
    "besuam",
    "në të.",
    "E kushdo që",
    "beson",
    "në Zotin e tij",
    "atëherë nuk",
    "frikësohet",
    "(për) ndonjë humbje",
    "dhe as",
    "barrë."
  ],
  "14": [
    "Dhe vërtet ne",
    "nga ne",
    "(ka) muslimanë",
    "dhe nga ne",
    "(ka) të padrejtë.",
    "E kushdo që",
    "bëhet musliman",
    "atëherë të tillët",
    "kanë kërkuar",
    "udhë të drejtë."
  ],
  "15": [
    "E sa i përket",
    "të padrejtëve",
    "ata do të jenë",
    "për xhehenem",
    "dru zjarri\"."
  ],
  "16": [
    "E sikur",
    "ata të qëndronin",
    "në",
    "rrugë",
    "sigurisht Ne do t'ua jepnim atyre të pinë",
    "ujë",
    "me bollëk."
  ],
  "17": [
    "Për t'i testuar ata",
    "në të.",
    "E kushdo që",
    "kthen shpinën",
    "nga",
    "përkujtimi",
    "i Zotit të tij",
    "Ai e shpie atë",
    "(në) një dënim",
    "të ashpër."
  ],
  "18": [
    "Dhe vërtet",
    "xhamitë",
    "(janë) për Allahun",
    "prandaj mos",
    "e lutni",
    "me",
    "Allahun",
    "askë."
  ],
  "19": [
    "Dhe vërtet",
    "pasi",
    "u ngrit",
    "robi",
    "i Allahut",
    "duke e lutur Atë",
    "ata gati",
    "ishin",
    "rreth tij",
    "masë e ngjeshur."
  ],
  "20": [
    "Thuaj",
    "\"Vetëm",
    "unë i lutem",
    "Zotit tim",
    "dhe nuk",
    "i shoqëroj (në adhurim)",
    "me Të",
    "askë\"."
  ],
  "21": [
    "Thuaj",
    "\"Vërtet unë",
    "nuk",
    "posedoj",
    "për ju",
    "ndonjë dëm",
    "dhe as",
    "udhë të drejtë\"."
  ],
  "22": [
    "Thuaj",
    "\"Vërtet unë",
    "nuk do të",
    "(mund) t'më mbrojë",
    "nga",
    "Allahu",
    "askush",
    "dhe nuk do të",
    "(mund) të gjej",
    "nga",
    "përveç Tij",
    "ndonjë mbështetje."
  ],
  "23": [
    "Përveç",
    "një kumtim",
    "nga",
    "Allahu",
    "dhe mesazhet e Tij\".",
    "E kushdo që",
    "kundërshton",
    "Allahun",
    "dhe të dërguarin e Tij",
    "atëherë vërtet",
    "për të",
    "(është) zjarri",
    "i xhehenemit",
    "të gjithmonshëm",
    "në të",
    "pambarimisht."
  ],
  "24": [
    "Derisa",
    "kur",
    "ata shohin",
    "atë çfarë",
    "u është premtuar atyre",
    "atëherë ata do të dinë",
    "kush",
    "(është) më i dobët",
    "(në) ndihmës",
    "dhe më i paktë",
    "(në) numër."
  ],
  "25": [
    "Thuaj",
    "\"Nuk",
    "unë di",
    "a (është) e afërt",
    "ajo çfarë",
    "ju premtohet ju",
    "apo",
    "(do të) bëjë",
    "për të",
    "Zoti im",
    "një afat\"."
  ],
  "26": [
    "Njohësi",
    "i së fshehtës",
    "prandaj nuk",
    "Ai shfaq",
    "nga",
    "e fshehta e Tij",
    "askujt."
  ],
  "27": [
    "Përveç",
    "atij që",
    "Ai pëlqen",
    "nga",
    "një i dërguar",
    "atëherë vërtet Ai",
    "vë roje",
    "nga",
    "përpara",
    "tij",
    "dhe nga",
    "mbrapa tij",
    "(si) vëzhgim."
  ],
  "28": [
    "Që të mbajë Ai evidencë",
    "se",
    "sigurisht",
    "ata kumtuan",
    "mesazhet",
    "e Zotit të tyre",
    "dhe Ai ka përfshirë",
    "atë çfarë",
    "(është) tek ta",
    "dhe Ai ka llogaritur saktësisht",
    "çdo",
    "gjë",
    "(në) numër."
  ]
}