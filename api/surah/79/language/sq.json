{
  "1": [
    "Për ata që nxjerrin",
    "me ashpërsi"
  ],
  "2": [
    "Për ata që marrin",
    "me lehtësi"
  ],
  "3": [
    "Për ata që rrëshqasin",
    "duke notuar"
  ],
  "4": [
    "Për ata që garojnë",
    "(si në) garë"
  ],
  "5": [
    "Dhe për ata që rregullojnë",
    "çështje"
  ],
  "6": [
    "Ditën kur",
    "(do të) dridh",
    "dridhja"
  ],
  "7": [
    "E pason atë",
    "tjetra"
  ],
  "8": [
    "Zemrat",
    "atë ditë",
    "(do të jenë) rrëqethëse"
  ],
  "9": [
    "Shikimet e tyre",
    "(do të jenë) të përulura"
  ],
  "10": [
    "Ata thonë",
    "\"A vërtet ne",
    "sigurisht do të kthehemi",
    "në",
    "gjendjen e mëparshme?"
  ],
  "11": [
    "A kur",
    "(të) jemi",
    "eshtra",
    "të kalbur?\""
  ],
  "12": [
    "Ata thonë",
    "\"Ai",
    "atëherë",
    "(do të jetë) një kthim",
    "dështues\""
  ],
  "13": [
    "Por vetëm",
    "ajo",
    "(do të jetë) klithje",
    "një"
  ],
  "14": [
    "E përnjëherë",
    "ata",
    "(do të) kthjellen"
  ],
  "15": [
    "A",
    "ka ardhur tek ti",
    "rrëfimi",
    "i Musait?"
  ],
  "16": [
    "Kur",
    "e thirri atë",
    "Zoti i tij",
    "në luginën",
    "e shenjtë",
    "Tuva"
  ],
  "17": [
    "Shko",
    "tek",
    "Faraoni",
    "vërtet",
    "ai e teproi"
  ],
  "18": [
    "Dhe thuaj",
    "\"A",
    "për ty",
    "deri",
    "që të",
    "pastrohesh?"
  ],
  "19": [
    "Dhe të të udhëzoj ty",
    "drejt",
    "Zotit tënd",
    "e t'i frikësohesh\""
  ],
  "20": [
    "Dhe ai ia shfaqi atij",
    "shenjën",
    "më të madhe"
  ],
  "21": [
    "Por ai përgënjeshtroi",
    "dhe kundërshtoi"
  ],
  "22": [
    "Pastaj",
    "e ktheu shpinën",
    "duke nxituar"
  ],
  "23": [
    "Dhe i tuboi (njerëzit)",
    "dhe thirri"
  ],
  "24": [
    "E tha",
    "\"Unë",
    "(jam) zoti juaj",
    "më i larti\""
  ],
  "25": [
    "Dhe e kapi atë",
    "Allahu",
    "(me) ndëshkimin e tmerrshëm",
    "të jetës së fundit",
    "dhe të parës"
  ],
  "26": [
    "Vërtet",
    "në",
    "atë",
    "sigurisht (ka) mësim",
    "për atë që",
    "frikësohet"
  ],
  "27": [
    "A ju",
    "(keni) më të fuqishëm",
    "krijim",
    "apo",
    "qielli?",
    "Ai e ndërtoi atë"
  ],
  "28": [
    "E ngriti",
    "kulmin e tij",
    "dhe e balancoi atë"
  ],
  "29": [
    "Dhe e errësoi",
    "natën e tij",
    "dhe e nxori",
    "ndriçimin e tij"
  ],
  "30": [
    "Dhe tokën",
    "pas",
    "asaj",
    "e sheshoi"
  ],
  "31": [
    "Nxori",
    "prej saj",
    "ujin e saj",
    "dhe kullosat e saj"
  ],
  "32": [
    "Dhe kodrat",
    "i përforcoi ato"
  ],
  "33": [
    "Furnizim",
    "për ju",
    "dhe për bagëtinë tuaj"
  ],
  "34": [
    "E kur",
    "(të) vijë",
    "gjëma",
    "më e madhe (kijameti)"
  ],
  "35": [
    "Ditën kur",
    "përkujton",
    "njeriu",
    "atë çfarë",
    "është përpjekur"
  ],
  "36": [
    "Dhe shfaqet",
    "zjarri i xhehenemit",
    "për atë që",
    "sheh"
  ],
  "37": [
    "E sa i përket",
    "atij që",
    "ka tepruar"
  ],
  "38": [
    "Dhe i dha përparësi",
    "jetës",
    "së kësaj bote"
  ],
  "39": [
    "Atëherë vërtet",
    "zjarri i xhehenemit",
    "ai (është)",
    "vendstrehimi"
  ],
  "40": [
    "E sa i përket",
    "atij që",
    "iu frikësua",
    "qëndrimit",
    "(para) Zotit të tij",
    "dhe e ndaloi",
    "veten",
    "nga",
    "pasioni"
  ],
  "41": [
    "Atëherë vërtet",
    "xheneti",
    "ai (është)",
    "vendstrehimi"
  ],
  "42": [
    "Ata të pyesin ty",
    "për",
    "Orën (kijametin)",
    "kur",
    "(është) arritja e saj?"
  ],
  "43": [
    "Në çfarë",
    "(je) ti",
    "nga",
    "përmendja e saj?"
  ],
  "44": [
    "Te",
    "Zoti yt",
    "(është) përfundimi i saj"
  ],
  "45": [
    "Vetëm",
    "ti (je)",
    "paralajmërues",
    "i atij që",
    "i frikësohet asaj"
  ],
  "46": [
    "(Do të duket) sikur ata",
    "ditën kur",
    "e shohin atë",
    "nuk",
    "qëndruan",
    "përveç",
    "një mbrëmje",
    "apo",
    "mëngjesin e saj"
  ]
}