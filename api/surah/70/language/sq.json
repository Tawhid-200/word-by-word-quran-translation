{
  "1": [
    "Kërkoi",
    "një lutës",
    "një dënim",
    "të pashmangshëm."
  ],
  "2": [
    "Për mohuesit",
    "nuk (ka)",
    "për të",
    "pengues."
  ],
  "3": [
    "nga",
    "Allahu",
    "Poseduesi",
    "i shkallëve."
  ],
  "4": [
    "Ngjiten",
    "engjëjt",
    "dhe shpirti (Xhibrili)",
    "tek Ai",
    "në",
    "një ditë",
    "është",
    "sasia e saj",
    "pesëdhjetë",
    "mijë",
    "vjet."
  ],
  "5": [
    "Prandaj, duro",
    "me një durim",
    "të bukur."
  ],
  "6": [
    "Vërtet ata",
    "e shohin atë",
    "të largët."
  ],
  "7": [
    "E Ne e shohim atë",
    "të afërt."
  ],
  "8": [
    "Ditën kur",
    "do të jetë",
    "qielli",
    "si kallaji i shkrirë."
  ],
  "9": [
    "Dhe do të jenë",
    "kodrat",
    "si shtëllunga leshi."
  ],
  "10": [
    "Dhe nuk",
    "pyet",
    "(asnjë) jaran",
    "jaranin (e vet)."
  ],
  "11": [
    "Do të vihen duke shikuar njëri-tjetrin.",
    "Dëshiron",
    "krimineli",
    "sikur",
    "(të) flijojë",
    "nga",
    "dënimi",
    "i asaj dite",
    "bijtë e tij."
  ],
  "12": [
    "Dhe gruan e tij",
    "dhe vëllain e tij."
  ],
  "13": [
    "Dhe farefisin e tij",
    "i cili",
    "e strehon atë."
  ],
  "14": [
    "Dhe ata që (janë)",
    "në",
    "tokë",
    "të gjithë",
    "pastaj",
    "(ta) shpëtojnë atë."
  ],
  "15": [
    "Kurrsesi!",
    "Vërtet ajo (është)",
    "një flakë."
  ],
  "16": [
    "Ajo e cila heq",
    "lëkurën e kokës."
  ],
  "17": [
    "Ajo thërret",
    "atë që",
    "është larguar",
    "dhe ka kthyer shpinën."
  ],
  "18": [
    "Dhe ka tubuar",
    "dhe e ka ruajtur."
  ],
  "19": [
    "Vërtet",
    "njeriu",
    "është krijuar",
    "i padurueshëm."
  ],
  "20": [
    "Kur",
    "e prek atë",
    "e keqja",
    "(është) shumë i dëshpëruar."
  ],
  "21": [
    "E kur",
    "e prek atë",
    "e mira",
    "(është) shumë dorështrënguar."
  ],
  "22": [
    "Përveç",
    "namazfalësve."
  ],
  "23": [
    "Të cilët",
    "ata (janë)",
    "në",
    "namazet e tyre",
    "të vazhdueshëm."
  ],
  "24": [
    "Dhe ata të cilët",
    "në",
    "pasuritë e tyre",
    "(është ndarë) një e drejtë",
    "e caktuar."
  ],
  "25": [
    "Për lypësin",
    "dhe të privuarin."
  ],
  "26": [
    "Dhe ata të cilët",
    "besojnë bindshëm",
    "ditën",
    "e kompensimit."
  ],
  "27": [
    "Dhe ata të cilët",
    "ata (janë)",
    "nga",
    "dënimi",
    "i Zotit të tyre",
    "të frikësuar."
  ],
  "28": [
    "Vërtet",
    "dënimi",
    "i Zotit të tyre",
    "(është) i pa",
    "siguruar."
  ],
  "29": [
    "Dhe ata të cilët",
    "ata (janë)",
    "të pjesëve të turpshme të tyre",
    "ruajtës."
  ],
  "30": [
    "Përveç",
    "ndaj",
    "grave të tyre",
    "apo",
    "atë çfarë",
    "posedojnë",
    "të djathtat (duart) e tyre",
    "vërtet ata (janë)",
    "të pa-",
    "qortuar."
  ],
  "31": [
    "E kushdo",
    "kërkon",
    "përtej",
    "asaj",
    "atëherë të tillët",
    "ata (janë)",
    "tepruesit."
  ],
  "32": [
    "Dhe ata të cilët",
    "ata",
    "ndaj amaneteve të tyre",
    "dhe marrêveshjes së tyre",
    "(janë) kujdestarë."
  ],
  "33": [
    "Dhe ata të cilët",
    "ata",
    "ndaj dëshmive të tyre",
    "(janë) zbatues."
  ],
  "34": [
    "Dhe ata të cilët",
    "ata",
    "ndaj",
    "namazeve të tyre",
    "i ruajnë."
  ],
  "35": [
    "Të tillët",
    "në",
    "xhenete",
    "(janë) të nderuar."
  ],
  "36": [
    "E ç'është",
    "(me) ata të cilët",
    "mohuan",
    "përballë teje",
    "me qafë të zgjatur?"
  ],
  "37": [
    "Në",
    "të djathtë",
    "dhe në të",
    "majtë",
    "të ulur në grupe."
  ],
  "38": [
    "A lakmon",
    "secili",
    "njeri",
    "prej tyre",
    "që",
    "të hyjë",
    "(në) xhenetin",
    "e begatisë?!"
  ],
  "39": [
    "Kurrsesi!",
    "Vërtet Ne",
    "i krijuam ata",
    "nga ajo çfarë",
    "ata dinë."
  ],
  "40": [
    "Gjithsesi",
    "betohem",
    "në Zotin",
    "e lindjeve",
    "dhe perëndimeve",
    "(se) vërtet Ne",
    "jemi të fuqishëm."
  ],
  "41": [
    "Për",
    "që të",
    "zëvendësojmë",
    "më të mirë",
    "se ata",
    "dhe nuk",
    "Ne",
    "(jemi) të mposhtur."
  ],
  "42": [
    "Po lëri ata",
    "të zhyten",
    "e të luajnë",
    "derisa",
    "të përballen",
    "me ditën e tyre",
    "e cila",
    "u premtohet."
  ],
  "43": [
    "Ditën kur",
    "ata dalin",
    "nga",
    "varret",
    "duke nxituar",
    "sikur të jenë",
    "drejt",
    "idhujve",
    "duke u ngutur."
  ],
  "44": [
    "Të përulur",
    "shikimet e tyre",
    "i mbërthen ata",
    "poshtërimi.",
    "Ajo (është)",
    "dita",
    "e cila",
    "atyre (u) ishte",
    "premtuar."
  ]
}