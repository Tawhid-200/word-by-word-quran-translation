  ],
  "132": [
    "Vërtet ai (ishte)",
    "prej",
    "robërve Tanë",
    "besimtarë."
  ],
  "133": [
    "Dhe vërtet",
    "Luti",
    "(ishte) njëmend prej",
    "të dërguarve."
  ],
  "134": [
    "Kur",
    "Ne e shpëtuam atë",
    "dhe familjen e tij",
    "të gjithë."
  ],
  "135": [
    "Përveç",
    "një plake",
    "në mesin e",
    "të mbeturve."
  ],
  "136": [
    "Pastaj",
    "Ne i shkatërruam",
    "të tjerët."
  ],
  "137": [
    "Dhe vërtet ju",
    "kaloni",
    "pranë tyre",
    "në mëngjes."
  ],
  "138": [
    "Edhe natën.",
    "A hiç nuk",
    "po e rrokni?!"
  ],
  "139": [
    "Dhe vërtet",
    "Junusi",
    "(ishte) njëmend prej",
    "të dërguarve."
  ],
  "140": [
    "Kur",
    "ai iku",
    "tek",
    "anija",
    "e mbushur."
  ],
  "141": [
    "Atëherë ai hodhi short",
    "e ishte",
    "prej",
    "humbësve."
  ],
  "142": [
    "Sakaq e gëlltiti atë",
    "peshku",
    "ndërkohë që ai (ishte)",
    "i denjë për qortim."
  ],
  "143": [
    "E sikur të mos",
    "që ai",
    "të ishte",
    "prej",
    "lartësuesve."
  ],
  "144": [
    "Njëmend ai do të mbetej",
    "brenda",
    "barkut të tij",
    "deri në",
    "ditën kur",
    "ata (do të) ringjallen."
  ],
  "145": [
    "E Ne e hodhëm atë",
    "në lëndinë",
    "ndërkohë që ai (ishte)",
    "i sëmurë."
  ],
  "146": [
    "Dhe Ne bëmë që të mbijë",
    "mbi të",
    "një pemë",
    "prej",
    "kungulli."
  ],
  "147": [
    "Dhe Ne e dërguam atë",
    "tek",
    "njëqind",
    "mijë",
    "apo",
    "janë më shumë."
  ],
  "148": [
    "E ata besuan",
    "ashtu që Ne u dhamë kënaqësi atyre",
    "deri në",
    "një kohë."
  ],
  "149": [
    "Pa pyeti ata!",
    "\"A për Zotin tënd (janë)",
    "vajzat",
    "kurse për ta",
    "djemtë?\""
  ],
  "150": [
    "Apo",
    "Ne i krijuam",
    "engjëjt",
    "femra",
    "ndërkohë që ata (ishin)",
    "dëshmitarë?"
  ],
  "151": [
    "Vini re!",
    "Vërtet ata",
    "prej",
    "shpifjes së tyre",
    "njëmend po thonë."
  ],
  "152": [
    "\"Ka lind",
    "Allahu!\"",
    "Dhe vërtet ata",
    "(janë) njëmend gënjeshtarë."
  ],
  "153": [
    "A i ka zgjedhur",
    "vajzat",
    "ndaj",
    "djemve?"
  ],
  "154": [
    "Ç'(është)",
    "me ju",
    "si",
    "po gjykoni?"
  ],
  "155": [
    "A hiç nuk",
    "po përkujtoni?"
  ],
  "156": [
    "Apo",
    "ju keni",
    "(ndonjë) argument",
    "të qartë?"
  ],
  "157": [
    "Pra sillni",
    "librin tuaj",
    "nëse",
    "jeni",
    "të sinqertë!"
  ],
  "158": [
    "Dhe ata bënë",
    "mes Tij",
    "dhe mes",
    "xhinëve",
    "lidhje farefisnore",
    "por sigurisht",
    "kanë ditur",
    "xhinët",
    "se ata",
    "(do të jenë) njëmend të sjellur."
  ],
  "159": [
    "I Patëmetë (është)",
    "Allahu",
    "nga ajo çfarë",
    "ata përshkruajnë."
  ],
  "160": [
    "Përveç",
    "robërve",
    "të Allahut",
    "të përzgjedhur."
  ],
  "161": [
    "E vërtet ju",
    "dhe ajo çfarë",
    "ju adhuroni."
  ],
  "162": [
    "Nuk",
    "ju (jeni)",
    "ndaj Tij",
    "sprovues."
  ],
  "163": [
    "Përveç",
    "atij që",
    "ai (është)",
    "ndezës",
    "i zjarrit të xhehenemit."
  ],
  "164": [
    "Dhe nuk (ka)",
    "prej nesh",
    "përveçse",
    "për të (është)",
    "një pozitë",
    "e njohur."
  ],
  "165": [
    "Dhe vërtet ne",
    "njëmend ne (jemi)",
    "të rreshtuarit."
  ],
  "166": [
    "Dhe vërtet ne",
    "njëmend ne (jemi)",
    "lartësuesit."
  ],
  "167": [
    "E megjithatë",
    "ata ishin",
    "duke thënë."
  ],
  "168": [
    "\"Sikur",
    "të",
    "(ishte) tek ne",
    "(ndonjë) përkujtim",
    "prej",
    "të parëve."
  ],
  "169": [
    "Njëmend ne do të ishim",
    "robërit",
    "e Allahut",
    "të përzgjedhurit\"."
  ],
  "170": [
    "Por ata mohuan",
    "në të.",
    "Ama do të",
    "dinë!"
  ],
  "171": [
    "Dhe sigurisht",
    "i ka paraprirë",
    "Fjala Jonë",
    "robërve Tanë",
    "të dërguar."
  ],
  "172": [
    "Vërtet ata",
    "njëmend ata (do të jenë)",
    "të ndihmuarit."
  ],
  "173": [
    "Dhe vërtet",
    "ushtria Jonë",
    "njëmend ata",
    "(do të jenë) ngadhënjyesit."
  ],
  "174": [
    "Prandaj zmbrapsu",
    "prej tyre",
    "deri në",
    "një kohë!"
  ],
  "175": [
    "Dhe vështroji ata",
    "e do të",
    "vështrojnë."
  ],
  "176": [
    "Vallë a dënimin Tonë",
    "ata po e shpejtojnë?"
  ],
  "177": [
    "E kur të",
    "zbres",
    "në arenën e tyre",
    "po sa i keq (është)",
    "sabahu",
    "i të paralajmëruarve!"
  ],
  "178": [
    "Dhe zmbrapsu",
    "prej tyre",
    "deri në",
    "një kohë!"
  ],
  "179": [
    "Dhe vështro",
    "e do të",
    "vështrojnë."
  ],
  "180": [
    "Lartësuar qoftë",
    "Zoti yt",
    "Zoti",
    "i krenarisë",
    "nga ajo çfarë",
    "ata përshkruajnë!"
  ],
  "181": [
    "Dhe paqe",
    "për",
    "të dërguarit!"
  ],
  "182": [
    "Dhe falënderimi (i përket)",
    "Allahut",
    "Zotit",
    "të botëve!"
  ]
}