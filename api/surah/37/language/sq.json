  ],
  "114": [
    "Dhe sigurisht",
    "Ne dhamë privilegje",
    "për",
    "Musain",
    "dhe Harunin."
  ],
  "115": [
    "Dhe Ne i shpëtuam ata dy",
    "dhe popullin e atyre dyve",
    "prej",
    "vuajtjes",
    "së madhe."
  ],
  "116": [
    "Dhe Ne i përkrahëm ata",
    "e ishin",
    "ata",
    "ngadhënjyesit."
  ],
  "117": [
    "Dhe Ne u dhamë atyre dyve",
