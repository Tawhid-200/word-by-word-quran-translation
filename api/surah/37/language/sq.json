{
  "1": [
    "Pasha të rreshtuarit",
    "në rreshta!"
  ],
  "2": [
    "E shtytësit",
    "me shtytje."
  ],
  "3": [
    "E recituesit",
    "e përkujtimit."
  ],
  "4": [
    "Vërtet",
    "Zoti juaj",
    "(është) njëmend Një."
  ],
  "5": [
    "Zoti",
    "i qiejve",
    "dhe i tokës",
    "dhe çfarë (ka)",
    "mes atyre dyjave",
    "dhe Zoti",
    "i lindjeve."
  ],
  "6": [
    "Vërtet Ne",
    "e stolisëm",
    "qiellin",
    "e kësaj bote",
    "me stolinë",
    "e yjeve."
  ],
  "7": [
    "Dhe mbrojtje",
    "prej",
    "secilit",
    "djall",
    "rebel."
  ],
  "8": [
    "Nuk",
    "ata përgjojnë",
    "tek",
    "paria",
    "më e lartë",
    "dhe ata gjuhen",
    "prej",
    "çdo",
    "ane."
  ],
  "9": [
    "(Janë) të dëbuar.",
    "Dhe ata kanë",
    "një dënim",
    "të vazhdueshëm."
  ],
  "10": [
    "Përveç",
    "atij që",
    "rrëmben",
    "rrëmbimin",
    "por e ndjek atë",
    "një shkëndijë",
    "ndezëse."
  ],
  "11": [
    "E ti pyeti ata",
    "\"A ata (janë)",
    "më të fortë",
    "në krijim",
    "apo",
    "ata të cilët",
    "Ne i krijuam?\"",
    "Vërtet Ne",
    "i krijuam ata",
    "prej",
    "një balte",
    "ngjitëse."
  ],
  "12": [
    "Ama",
    "ti çuditesh",
    "kurse ata tallen."
  ],
  "13": [
    "E kur",
    "ata përkujtohen",
    "nuk",
    "kujtojnë."
  ],
  "14": [
    "E kur",
    "ata shohin",
    "një provë",
    "ata tallen."
  ],
  "15": [
    "Dhe thonë",
    "\"Nuk (është)",
    "kjo",
    "përveçse",
    "një magji",
    "e kulluar."
  ],
  "16": [
    "A kur",
    "të vdesim",
    "dhe të jemi",
    "dhe",
    "e eshtra",
    "a vërtet ne",
    "(do të jemi) njëmend të ringjallur?"
  ],
  "17": [
    "Vallë edhe baballarët tanë",
    "të parë?!\""
  ],
  "18": [
    "Thuaj",
    "\"Po",
    "edhe ju (do të jeni)",
    "të nënçmuar\"."
  ],
  "19": [
    "Atëherë vetëm",
    "ajo (do të jetë)",
    "britmë",
    "një",
    "e përnjëherë",
    "ata",
    "(do të) shikojnë."
  ],
  "20": [
    "Dhe ata (do të) thonë",
    "\"Ah kuku për ne!",
    "Kjo (është)",
    "dita",
    "e kompensimit\"."
  ],
  "21": [
    "\"Kjo (është)",
    "dita",
    "vendimtare",
    "të cilën",
    "ju ishit",
    "atë",
    "duke e përgënjeshtruar\"."
  ],
  "22": [
    "\"Tuboni",
    "ata të cilët",
    "bënë zullum",
    "edhe çiftet e tyre",
    "dhe atë çfarë",
    "ata ishin",
    "duke adhuruar."
  ],
  "23": [
    "Nga",
    "pos",
    "Allahut",
    "e çoni ata",
    "tek",
    "rruga",
    "e zjarrit të xhehenemit."
  ],
  "24": [
    "Dhe ndalni ata!",
    "Vërtet ata",
    "(do të jenë) të pyetur\"."
  ],
  "25": [
    "\"Çfarë",
    "(është) me ju",
    "nuk",
    "ndihmoheni mes vete?\""
  ],
  "26": [
    "Jo por",
    "ata (janë)",
    "sot",
    "të nënshtruar."
  ],
  "27": [
    "Dhe afrohen",
    "disa prej tyre",
    "nga",
    "disa (të tjerë)",
    "e pyesin njëri-tjetrin."
  ],
  "28": [
    "Ata (do të thonë)",
    "\"Vërtet ju",
    "ishit",
    "(që) vinit tek ne",
    "nga",
    "e djathta\"."
  ],
  "29": [
    "Ata (do të) thonë",
    "\"Përkundrazi",
    "nuk",
    "ju ishit",
    "besimtarë."
  ],
  "30": [
    "Dhe nuk",
    "ishte",
    "për ne",
    "mbi ju",
    "asnjë",
    "autoritet.",
    "Madje",
    "ju ishit",
    "një popull",
    "shkelës."
  ],
  "31": [
    "E u vërtetua",
    "kundër nesh",
    "fjala",
    "e Zotit tonë",
    "vërtet ne",
    "(jemi) njëmend shijues."
  ],
  "32": [
    "Kështu ne u munduam t'ju devijojmë ju",
    "vërtet ne",
    "ishim",
    "devijues\"."
  ],
  "33": [
    "E vërtet ata (do të jenë)",
    "atë ditë",
    "në",
    "dënim",
    "bashkëpjesëmarrës."
  ],
  "34": [
    "Vërtet Ne",
    "kështu",
    "veprojmë",
    "me kriminelët."
  ],
  "35": [
    "Vërtet ata",
    "ishin",
    "kur",
    "u thuhej",
    "atyre",
    "\"Nuk (ka)",
    "zot",
    "veçse",
    "Allahu\"",
    "bëheshin mendjemëdhenj."
  ],
  "36": [
    "Dhe thonin",
    "\"A vërtet ne",
    "(të jemi) njëmend lënës",
    "të zotave tanë",
    "për një poet",
    "të çmendur?\""
  ],
  "37": [
    "Përkundrazi",
    "ai e solli",
    "të vërtetën",
    "dhe vërtetoi",
    "të dërguarit."
  ],
  "38": [
    "Vërtet ju",
    "(do të jeni) njëmend shijues",
    "të dënimit",
    "të dhembshëm."
  ],
  "39": [
    "Dhe nuk",
    "(do të) kompensoheni",
    "veçse",
    "atë çfarë",
    "ju ishit",
    "duke punuar."
  ],
  "40": [
    "Përveç",
    "robërve",
    "të Allahut",
    "të përzgjedhur."
  ],
  "41": [
    "Të tillët",
    "për ta (do të ketë)",
    "furnizim",
    "i njohur."
  ],
  "42": [
    "Fruta",
    "dhe ata (janë)",
    "të nderuar."
  ],
  "43": [
    "Në",
    "xhenetet",
    "e begatisë."
  ],
  "44": [
    "Mbi",
    "shtretër",
    "përballë njëri-tjetrit."
  ],
  "45": [
    "Qarkullohet",
    "mes tyre",
    "me kupë vere",
    "prej",
    "burimi."
  ],
  "46": [
    "Të bardhë",
    "e lezetshme",
    "për pirësit."
  ],
  "47": [
    "Nuk (ka)",
    "në të",
    "marramendje",
    "dhe as",
    "ata",
    "prej saj",
    "(do të) dehen."
  ],
  "48": [
    "Dhe pranë tyre (do të jenë)",
    "gra të cilat e kufizojnë",
    "shikimin",
    "symëdha."
  ],
  "49": [
    "Si të ishin ato",
    "vezë",
    "të fshehura."
  ],
  "50": [
    "Atëherë i qasen",
    "disa prej tyre",
    "ndaj",
    "disa (të tjerëve)",
    "duke pyetur njëri-tjetrin."
  ],
  "51": [
    "Flet",
    "një folës",
    "prej tyre",
    "\"Vërtet",
    "kisha",
    "unë",
    "një jaran."
  ],
  "52": [
    "Ai thoshte",
    "\"A vërtet ti (je)",
    "njëmend prej",
    "besimtarëve?"
  ],
  "53": [
    "A kur",
    "të vdesim",
    "e të jemi",
    "dhe",
    "e eshtra",
    "a vërtet ne",
    "(do të jemi) njëmend të kompensuar?\""
  ],
  "54": [
    "Ai do të thotë",
    "\"A (jeni)",
    "ju",
    "vështrues?\""
  ],
  "55": [
    "Atëherë ai vështron",
    "dhe e shikon atë",
    "në",
    "mesin",
    "e zjarrit të xhehenemit."
  ],
  "56": [
    "Ai thotë",
    "\"Pasha Allahun",
    "vërtet",
    "ishe gati",
    "të më shkatërrosh."
  ],
  "57": [
    "E sikur të mos (ishte)",
    "begatia",
    "e Zotit tim",
    "njëmend do të isha",
    "prej",
    "atyre që janë sjell (në zjarr)."
  ],
  "58": [
    "Vallë a nuk",
    "ne (do të jemi)",
    "të vdekur?"
  ],
  "59": [
    "Përveç",
    "vdekjes sonë",
    "të parë",
    "dhe nuk",
    "ne (do të jemi)",
    "të ndëshkuar\"."
  ],
  "60": [
    "Vërtet",
    "kjo",
    "njëmend është",
    "fitorja",
    "e madhe."
  ],
  "61": [
    "Për të ngjashmen",
    "me këtë",
    "le të punojnë",
    "punëtorët!"
  ],
  "62": [
    "A ajo (është)",
    "më e mirë",
    "mikpritje",
    "apo",
    "pema",
    "e Zekumit?"
  ],
  "63": [
    "Vërtet Ne",
    "e bëmë atë",
    "sprovë",
    "për zullumqarët."
  ],
  "64": [
    "Vërtet ajo (është)",
    "një pemë",
    "(që) del",
    "në",
    "rrënjën",
    "e zjarrit të xhehenemit."
  ],
  "65": [
    "Pema e saj",
    "si të ishte ajo",
    "kokat",
    "e djajve."
  ],
  "66": [
    "E vërtet ata",
    "(do të jenë) njëmend ngrënës",
    "prej saj",
    "e mbushës",
    "prej saj",
    "të barqeve."
  ],
  "67": [
    "Pastaj",
    "vërtet",
    "për ta",
    "në të",
    "(do të ketë) një përzierje",
    "prej",
    "një uji të vluar."
  ],
  "68": [
    "Pastaj",
    "vërtet",
    "kthimi i tyre",
    "(do të jetë) njëmend tek",
    "zjarri i xhehenemit."
  ],
  "69": [
    "Vërtet ata",
    "i gjetën",
    "baballarët e tyre",
    "të devijuar."
  ],
  "70": [
    "Prandaj ata",
    "në",
    "gjurmët e tyre",
    "turren."
  ],
  "71": [
    "Dhe sigurisht",
    "ka devijuar",
    "para tyre",
    "shumica",
    "e popujve të parë."
  ],
  "72": [
    "Dhe sigurisht",
    "Ne dërguam",
    "në mesin e tyre",
    "paralajmërues."
  ],
  "73": [
    "E ti shiko",
    "si",
    "ishte",
    "përfundimi",
    "i të paralajmëruarve!"
  ],
  "74": [
    "Përveç",
    "robërve",
    "të Allahut",
    "të përzgjedhur."
  ],
  "75": [
    "Dhe sigurisht",
    "Ne na thirri",
    "Nuhu",
    "e sa të mirë",
    "Përgjigjës (jemi Ne)!"
  ],
  "76": [
    "Dhe Ne e shpëtuam atë",
    "dhe familjen e tij",
    "prej",
    "vuajtjes",
    "së madhe."
  ],
  "77": [
    "Dhe Ne i bëmë",
    "pasardhësit e tij",
    "ata",
    "të mbeturit."
  ],
  "78": [
    "Dhe Ne lamë",
    "për të (kujtim)",
    "në mesin e",
    "brezave të mëvonshëm."
  ],
  "79": [
    "\"Paqe",
    "për",
    "Nuhun",
    "në",
    "(mbarë) botët!\""
  ],
  "80": [
    "Vërtet Ne",
    "kështu",
    "i kompensojmë",
    "bamirësit."
  ],
  "81": [
    "Vërtet ai (ishte)",
    "prej",
    "robërve Tanë",
    "besimtarë."
  ],
  "82": [
    "Pastaj",
    "Ne i përmbytëm",
    "të tjerët."
  ],
  "83": [
    "Dhe vërtet",
    "prej",
    "grupit të tij",
    "(ishte) njëmend Ibrahimi."
  ],
  "84": [
    "Kur",
    "ai erdhi",
    "tek Zoti i tij",
    "me zemër",
    "të dëlirë."
  ],
  "85": [
    "Kur",
    "ai i tha",
    "babait të tij",
    "dhe popullit të tij",
    "\"Çfarë",
    "po adhuroni?"
  ],
  "86": [
    "A të trilluar",
    "zota",
    "pos",
    "Allahut",
    "dëshironi?"
  ],
  "87": [
    "Po çfarë (është)",
    "mendimi juaj",
    "për Zotin",
    "e botëve?\""
  ],
  "88": [
    "Atëherë ai shikoi",
    "një shikim",
    "tek",
    "yjet."
  ],
  "89": [
    "E tha",
    "\"Vërtet unë (jam)",
    "i sëmurë!\""
  ],
  "90": [
    "E ata u zmbrapsën",
    "prej tij",
    "duke u larguar."
  ],
  "91": [
    "Atëherë ai u kthye",
    "tek",
    "zotat e tyre",
    "e tha",
    "\"A nuk",
    "po hani?"
  ],
  "92": [
    "Ç'(është)",
    "me ju",
    "(që) nuk",
    "po flisni?\""
  ],
  "93": [
    "Atëherë u kthye",
    "tek ta",
    "duke i goditur",
    "me të djathtën."
  ],
  "94": [
    "E ata u afruan",
    "tek ai",
    "duke shpejtuar."
  ],
  "95": [
    "Ai tha",
    "\"A po adhuroni",
    "atë çfarë",
    "po gdhendni?"
  ],
  "96": [
    "E Allahu",
    "ju krijoi ju",
    "dhe atë çfarë",
    "ju punoni?\""
  ],
  "97": [
    "Ata thanë",
    "\"Ndërtoni",
    "për të",
    "një ndërtim",
    "e hidheni atë",
    "në",
    "zjarrin flakërues!\""
  ],
  "98": [
    "E ata synuan",
    "për të",
    "një komplot",
    "por Ne i bëmë ata",
    "më të ultit."
  ],
  "99": [
    "Dhe ai tha",
    "\"Vërtet unë",
    "po shkoj",
    "tek",
    "Zoti im",
    "Ai do të më udhëzojë mua."
  ],
  "100": [
    "Zoti im!",
    "Më dhuro",
    "mua",
    "prej",
    "punëmirëve!\""
  ],
  "101": [
    "Përnjëherë Ne e përgëzuam atë",
    "me një djalë",
    "të pjekur."
  ],
  "102": [
    "E pasi që",
    "ai arriti",
    "me të",
    "angazhimin",
    "tha",
    "\"O vogëlushi im!",
    "Vërtet unë",
    "po shoh",
    "në",
    "ëndërr",
    "se unë",
    "po të ther ty",
    "prandaj shiko",
    "çfarë",
    "mendon?\"",
    "Ai tha",
    "\"O babush!",
    "Bëje",
    "atë çfarë",
    "je urdhëruar!",
    "Do të më gjesh mua",
    "nëse",
    "do",
    "Allahu",
    "prej",
    "durimtarëve\"."
  ],
  "103": [
    "E pasi që",
    "ata dy u dorëzuan",
    "dhe ai e përmbysi atë",
    "në ballë."
  ],
  "104": [
    "Dhe Ne e thirrëm atë",
    "që",
    "\"O Ibrahim!"
  ],
  "105": [
    "Sigurisht",
    "ti e vërtetove",
    "ëndrrën\".",
    "Vërtet Ne",
    "kështu",
    "i kompensojmë",
    "bamirësit."
  ],
  "106": [
    "Vërtet",
    "kjo",
    "njëmend është",
    "sprova",
    "e qartë."
  ],
  "107": [
    "Dhe Ne e shpaguam atë",
    "me një flijim",
    "të madh."
  ],
  "108": [
    "Dhe Ne lamë",
    "për të (përkujtim)",
    "në mesin e",
    "brezave të mëvonshme."
  ],
  "109": [
    "Paqe",
    "për",
    "Ibrahimin!"
  ],
  "110": [
    "Kështu",
    "Ne i kompensojmë",
    "bamirësit."
  ],
  "111": [
    "Vërtet ai (ishte)",
    "prej",
    "robërve Tanë",
    "besimtarë."
  ],
  "112": [
    "Dhe Ne e përgëzuam atë",
    "me Is'hakun",
    "pejgamber",
    "prej",
    "punëmirëve."
  ],
  "113": [
    "Dhe Ne bekuam",
