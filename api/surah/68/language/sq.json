[
  "Nun.",
  "Bestohem në lapsin",
  "dhe atë çfarë",
  "ata shkruajnë.",
  "Nuk",
  "ti (je)",
  "me dhuntinë",
  "e Zotit tënd",
  "i çmendur.",
  "Dhe vërtet",
  "ti do të kesh",
  "shpërblim",
  "të pa-",
  "këputur.",
  "Dhe vërtet ti (je)",
  "mbi",
  "një moral",
  "madhështor.",
  "E ti do të shohësh",
  "edhe ata do të shohin.",
  "(Se) cili prej jush",
  "(është) i sprovuar.",
  "Vërtet",
  "Zoti yt",
  "Ai (është)",
  "më i ditur",
  "se kush",
  "ka devijuar",
  "nga",
  "rruga e Tij",
  "dhe Ai",
  "(është) më i ditur",
  "për të udhëzuarit.",
  "Prandaj mos",
  "iu bind",
  "përgënjeshtruesve!",
  "Ata dëshiruan",
  "sikur",
  "ti të bësh ujdi (me ta)",
  "edhe ata të bëjnë ujdi (me ty).",
  "Dhe mos",
  "iu bind",
  "asnjë",
  "betuesi",
  "të poshtër.",
  "Përgojues",
  "bartës (i fjalëve)",
  "me shpifje.",
  "Pengues i madh",
  "i së mirës",
  "shkelës",
  "mëkatar.",
  "Shumë i vrazhdë",
  "pas",
  "kësaj",
  "kopil.",
  "(Vetëm) që",
  "është",
  "posedues",
  "i pasurisë",
  "dhe djemve.",
  "Kur",
  "i recitohen",
  "atij",
  "ajetet Tona",
  "ai thotë",
  "\"Legjenda",
  "të të parëve!\"",
  "Ne do ta damkosim atë",
  "në",
  "turi.",
  "Vërtet Ne",
  "i sprovuam ata",
  "ashtu siç",
  "i sprovuam",
  "pronarët",
  "e kopshtit",
  "kur",
  "u betuan",
  "(se) sigurisht do ta vjelin atë",
  "herët në agim.",
  "Dhe nuk (thanë)",
  "\"Në dashtë Allahu!\"",
  "Pastaj e rrethoi",
  "atë (kopshtin)",
  "një bela",
  "nga",
  "Zoti yt",
  "ndërkohë që ata",
  "(ishin) të fjetur.",
  "Dhe ai gdhiu",
  "si i vjelë.",
  "Dhe ata e thirrnin njëri-tjetrin",
  "herët në agim.",
  "Që",
  "\"Zgjohuni herët",
  "tek",
  "të mbjellat tuaja",
  "nëse",
  "jeni",
  "vjelës\".",
  "Dhe u nisën",
  "ndërkohë që ata",
  "pëshpërisnin mes vete.",
  "Që",
  "të mos",
  "hyjë në të assesi",
  "sot",
  "tek ju",
  "një i varfër.",
  "Dhe ata shkuan herët",
  "drejt",
  "një synimi",
  "të aftë (të vendosur për t'i penguar të varfrit).",
  "E pasi që",
  "ata e panë atë",
  "thanë",
  "\"Vërtet ne",
  "(jemi) të humbur\".",
  "Por jo",
  "ne (jemi)",
  "të privuar.",
  "Tha",
  "më i maturi i tyre",
  "\"A nuk",
