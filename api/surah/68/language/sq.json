{
  "1": [
    "Nun.",
    "Bestohem në lapsin",
    "dhe atë çfarë",
    "ata shkruajnë."
  ],
  "2": [
    "Nuk",
    "ti (je)",
    "me dhuntinë",
    "e Zotit tënd",
    "i çmendur."
  ],
  "3": [
    "Dhe vërtet",
    "ti do të kesh",
    "shpërblim",
    "të pa-",
    "këputur."
  ],
  "4": [
    "Dhe vërtet ti (je)",
    "mbi",
    "një moral",
    "madhështor."
  ],
  "5": [
    "E ti do të shohësh",
    "edhe ata do të shohin."
  ],
  "6": [
    "(Se) cili prej jush",
    "(është) i sprovuar."
  ],
  "7": [
    "Vërtet",
    "Zoti yt",
    "Ai (është)",
    "më i ditur",
    "se kush",
    "ka devijuar",
    "nga",
    "rruga e Tij",
    "dhe Ai",
    "(është) më i ditur",
    "për të udhëzuarit."
  ],
  "8": [
    "Prandaj mos",
    "iu bind",
    "përgënjeshtruesve!"
  ],
  "9": [
    "Ata dëshiruan",
    "sikur",
    "ti të bësh ujdi (me ta)",
    "edhe ata të bëjnë ujdi (me ty)."
  ],
  "10": [
    "Dhe mos",
    "iu bind",
    "asnjë",
    "betuesi",
    "të poshtër."
  ],
  "11": [
    "Përgojues",
    "bartës (i fjalëve)",
    "me shpifje."
  ],
  "12": [
    "Pengues i madh",
    "i së mirës",
    "shkelës",
    "mëkatar."
  ],
  "13": [
    "Shumë i vrazhdë",
    "pas",
    "kësaj",
    "kopil."
  ],
  "14": [
    "(Vetëm) që",
    "është",
    "posedues",
    "i pasurisë",
    "dhe djemve."
  ],
  "15": [
    "Kur",
    "i recitohen",
    "atij",
    "ajetet Tona",
    "ai thotë",
    "\"Legjenda",
    "të të parëve!\""
  ],
  "16": [
    "Ne do ta damkosim atë",
    "në",
    "turi."
  ],
  "17": [
    "Vërtet Ne",
    "i sprovuam ata",
    "ashtu siç",
    "i sprovuam",
    "pronarët",
    "e kopshtit",
    "kur",
    "u betuan",
    "(se) sigurisht do ta vjelin atë",
    "herët në agim."
  ],
  "18": [
    "Dhe nuk (thanë)",
    "\"Në dashtë Allahu!\""
  ],
  "19": [
    "Pastaj e rrethoi",
    "atë (kopshtin)",
    "një bela",
    "nga",
    "Zoti yt",
    "ndërkohë që ata",
    "(ishin) të fjetur."
  ],
  "20": [
    "Dhe ai gdhiu",
    "si i vjelë."
  ],
  "21": [
    "Dhe ata e thirrnin njëri-tjetrin",
    "herët në agim."
  ],
  "22": [
    "Që",
    "\"Zgjohuni herët",
    "tek",
    "të mbjellat tuaja",
    "nëse",
    "jeni",
    "vjelës\"."
  ],
  "23": [
    "Dhe u nisën",
    "ndërkohë që ata",
    "pëshpërisnin mes vete."
  ],
  "24": [
    "Që",
    "të mos",
    "hyjë në të assesi",
    "sot",
    "tek ju",
    "një i varfër."
  ],
  "25": [
    "Dhe ata shkuan herët",
    "drejt",
    "një synimi",
    "të aftë (të vendosur për t'i penguar të varfrit)."
  ],
  "26": [
    "E pasi që",
    "ata e panë atë",
    "thanë",
    "\"Vërtet ne",
    "(jemi) të humbur\"."
  ],
  "27": [
    "Por jo",
    "ne (jemi)",
    "të privuar."
  ],
  "28": [
    "Tha",
    "më i maturi i tyre",
    "\"A nuk",
    "(ju) thashë",
    "juve",
    "përse nuk",
    "e lartësoni (Allahun)?\""
  ],
  "29": [
    "Ata thanë",
    "\"Lartësuar qoftë",
    "Zoti ynë",
    "vërtet ne",
    "ishim",
    "zullumqarë!\""
  ],
  "30": [
    "Dhe u kthyen",
    "disa prej tyre",
    "karshi",
    "disa (të tjerëve)",
    "duke u qortuar mes vete."
  ],
  "31": [
    "Ata thanë",
    "\"Mjerë për ne",
    "vërtet ne",
    "ishim",
    "të pabindur\"."
  ],
  "32": [
    "Mbase",
    "Zoti ynë",
    "të",
    "zëvendësojë për ne",
    "më të mirë",
    "se ai",
    "vërtet ne",
    "tek",
    "Zoti ynë",
    "(jemi) synues."
  ],
  "33": [
    "Kështu",
    "(është) dënimi",
    "kurse dënimi",
    "i botës tjetër",
    "(është) më i madh",
    "sikur",
    "(të) ishin",
    "(që) dinë."
  ],
  "34": [
    "Vërtet",
    "për të druajturit",
    "tek",
    "Zoti i tyre",
    "(ka) xhenete",
    "të begatisë."
  ],
  "35": [
    "A do t'i bëjmë",
    "muslimanët",
    "sikur kriminelët?"
  ],
  "36": [
    "Ç'(është)",
    "me ju",
    "si",
    "gjykoni?"
  ],
  "37": [
    "Mos vallë",
    "ju keni",
    "ndonjë libër",
    "prej tij",
    "ju mësoni?"
  ],
  "38": [
    "Vërtet",
    "ju do të keni",
    "në të",
    "çfarëdo që",
    "ju dëshironi?!"
  ],
  "39": [
    "Mos vallë",
    "ju keni",
    "zotime",
    "prej Nesh",
    "të mëdha",
    "deri",
    "në",
    "Kijamet?!",
    "Vërtet",
    "ju keni",
    "atë që",
    "ju gjykoni?!"
  ],
  "40": [
    "Pyeti ata",
    "cili prej tyre",
    "për atë",
    "(është) garantues?"
  ],
  "41": [
    "Mos vallë",
    "ata kanë",
    "ortakë",
    "pra, le t'i sjellin",
    "ortakët e tyre",
    "nëse",
    "ata janë",
    "të sinqertë?"
  ],
  "42": [
    "Ditën kur",
    "(do të) zbulohet",
    "nga",
    "kërciri",
    "dhe ata do të thirren",
    "tek",
    "sexhdeja",
    "mirëpo nuk",
    "(do të) munden."
  ],
  "43": [
    "Të përulura (do të jenë)",
    "shikimet e tyre",
    "i kaplon ata",
    "nënçmimi",
    "ndërkohë që",
    "ata ishin",
    "thirreshin",
    "tek",
    "sexhdeja",
    "kur ata (ishin)",
    "të shëndoshë."
  ],
  "44": [
    "Pra, më lër Mua",
    "bashkë me atë që",
    "përgënjeshtron",
    "këtë",
    "bisedë (Kur'an).",
    "Ne do t'i afrojmë ata pak nga pak (nga dënimi)",
    "prej",
    "aty ku",
    "nuk",
    "dinë."
  ],
  "45": [
    "Dhe i afatizoj",
    "ata",
    "vërtet",
    "Plani Im",
    "(është) i përpiktë."
  ],
  "46": [
    "Mos vallë",
    "ti kërkon prej tyre",
    "shpërblim",
    "e ata",
    "nga",
    "borxhi",
    "(janë) të rënduar?"
  ],
  "47": [
    "Mos vallë",
    "tek ta",
    "(është) e fshehta",
    "kështu që ata",
    "shkruajnë?"
  ],
  "48": [
    "Prandaj duro",
    "ndaj gjykimit",
    "të Zotit tënd",
    "dhe mos",
    "ji",
    "sikurse shoqëruesi",
    "i balenës",
    "kur",
    "thirri",
    "duke qenë ai",
    "i mllefosur."
  ],
  "49": [
    "Sikur të mos",
    "të",
    "arrinte tek ai",
    "begatia",
    "nga",
    "Zoti i tij",
    "ai do të hidhej",
    "në shkretëtirë",
    "duke qenë ai",
    "i përbuzur."
  ],
  "50": [
    "Por e zgjodhi atë",
    "Zoti i tij",
    "dhe e bëri atë",
    "prej",
    "të mirëve."
  ],
  "51": [
    "E",
    "gati sa",
    "ata të cilët",
    "mohuan",
    "do të të zhduknin",
    "me shikimet e tyre",
    "pasi që",
    "dëgjuan",
    "përkujtimin (Kur'anin)",
    "dhe thanë",
    "\"Vërtet ai (është)",
    "i çmendur\"."
  ],
  "52": [
    "Dhe nuk (është)",
    "ai (Kur'ani)",
    "përveç",
    "përkujtim",
    "për botët."
  ]
}