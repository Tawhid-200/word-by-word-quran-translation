{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Zbritja",
    "e Librit",
    "(është) prej",
    "Allahut",
    "të Pamposhturit",
    "të Urtit."
  ],
  "3": [
    "Vërtet",
    "në",
    "qiejt",
    "dhe tokë",
    "sigurisht (ka) dëshmi",
    "për besimtarët."
  ],
  "4": [
    "Dhe në",
    "krijimin tuaj",
    "dhe atë çfarë",
    "Ai shpërndan",
    "nga",
    "gjallesa",
    "(ka) prova",
    "për një popull",
    "(që) binden."
  ],
  "5": [
    "Dhe (në) ndërrimin",
    "e natës",
    "dhe ditës",
    "dhe atë çfarë",
    "zbriti",
    "Allahu",
    "nga",
    "qielli",
    "prej",
    "furnizimit",
    "dhe e ngjalli",
    "me të",
    "tokën",
    "pas",
    "vdekjes së saj",
    "dhe (në) lëvizjen",
    "e erërave",
    "(ka) prova",
    "për një popull",
    "(që) rrokin."
  ],
  "6": [
    "Ato (janë)",
    "provat",
    "e Allahut",
    "Ne t'i recitojmë ato",
    "ty",
    "me vërtetësi.",
    "Po në çfarë",
    "bisedë",
    "pas",
    "Allahut",
    "dhe provave të Tij",
    "(do të) besojnë?!"
  ],
  "7": [
    "Mjerë",
    "për çdo",
    "shpifës",
    "gjynahqar!"
  ],
  "8": [
    "Dëgjon",
    "ajetet",
    "e Allahut",
    "(i) recitohen",
    "atij",
    "pastaj",
    "insiston",
    "si arrogant",
    "sikur të",
    "mos",
    "i dëgjonte ato.",
    "Pra përgëzoje atë",
    "me një dënim",
    "të dhembshëm!"
  ],
  "9": [
    "Dhe kur",
    "ai mëson",
    "prej",
    "ajeteve Tona",
    "diçka",
    "i merr ato",
    "për tallje.",
    "Të tillët",
    "kanë",
    "një dënim",
    "përbuzës."
  ],
  "10": [
    "Nga",
    "para tyre (është)",
    "xhehenemi",
    "dhe nuk",
    "(u) bën dobi",
    "atyre",
    "çfarë",
    "ata fituan",
    "asgjë",
    "dhe as",
    "çfarë",
    "morën",
    "nga",
    "pos",
    "Allahut",
    "si ndihmës",
    "dhe ata kanë",
    "një dënim",
    "të madh."
  ],
  "11": [
    "Ky (është)",
    "udhëzim",
    "dhe ata të cilët",
    "mohuan",
    "ajetet",
    "e Zotit të tyre",
    "ata kanë",
    "një dënim",
    "nga",
    "kërcënim",
    "të dhembshëm."
  ],
  "12": [
    "Allahu (është)",
    "Ai i Cili",
    "nënshtroi",
    "për ju",
    "detin",
    "që të lundrojnë",
    "anijet",
    "në të",
    "me urdhrin e Tij",
    "dhe që të kërkoni",
    "prej",
    "mirësisë së Tij",
    "dhe ashtu që ju",
    "të falënderoni."
  ],
  "13": [
    "Dhe Ai nënshtroi",
    "për ju",
    "çfarë (ka)",
    "në",
    "qiej",
    "dhe çfarë (ka)",
    "në",
    "tokë",
    "krejt",
    "prej Tij.",
    "Vërtet",
    "në",
    "të",
    "(ka) prova",
    "për një popull",
    "(që) mendojnë."
  ],
  "14": [
    "Thuaju",
    "atyre të cilët",
    "besuan",
    "t'ua falin",
    "atyre të cilët",
    "nuk",
    "shpresojnë",
    "premtimet",
    "e Allahut",
    "që Ai ta dënojë",
    "një popull",
    "për atë çfarë",
    "ata ishin",
    "duke fituar."
  ],
  "15": [
    "Kushdo që",
    "punon",
    "një vepër të mirë",
    "(është) për të mirën e vetes së tij",
    "e kushdo që",
    "bën keq",
    "i kthehet bumerang atij.",
    "Pastaj",
    "tek",
    "Zoti juaj",
    "(do të) ktheheni."
  ],
  "16": [
    "Dhe sigurisht",
    "Ne (u) dhamë",
    "bijve",
    "të Israilit",
    "librin",
    "dhe gjykimin",
    "dhe pejgamberllëkun",
    "dhe i furnizuam ata",
    "prej",
    "të mirave",
    "dhe i privilegjuam ata",
    "mbi",
    "botët."
  ],
  "17": [
    "Dhe Ne u dhamë atyre",
    "prova të qarta",
    "prej",
    "çështjes",
    "e nuk",
    "ata u përçanë",
    "veçse",
    "nga",
    "pas",
    "që",
    "u erdhi atyre",
    "dija",
    "si shtypje",
    "mes tyre.",
    "Vërtet",
    "Zoti yt",
    "(do të) gjykojë",
    "mes tyre",
    "ditën",
    "e Kijametit",
    "lidhur me atë çfarë",
    "ata ishin",
    "në të",
    "duke kundërshtuar njëri-tjetrin."
  ],
  "18": [
    "Pastaj",
    "Ne të vendosëm ty",
    "në",
    "një rrugë të çiltër",
    "prej",
    "çështjes (fesë)",
    "prandaj ndiqe atë",
    "dhe mos",
    "ndiq",
    "dëshirat",
    "e atyre të cilët",
    "nuk",
    "dinë."
  ],
  "19": [
    "Vërtet ata",
    "nuk do të",
    "largojnë",
    "nga ti",
    "prej",
    "Allahut",
    "asgjë.",
    "Dhe vërtet",
    "zullumqarët",
    "disa prej tyre (janë)",
    "aleatë",
    "të disa (të tjerëve)",
    "kurse Allahu (është)",
    "aleat",
    "i të devotshmëve."
  ],
  "20": [
    "Ky (është)",
    "ndriçime",
    "për njerëzit",
    "dhe udhëzim",
    "dhe mëshirë",
    "për një popull",
    "(që) besojnë bindshëm."
  ],
  "21": [
    "Vallë",
    "menduan",
    "ata të cilët",
    "vepruan",
    "të këqija",
    "se",
    "Ne do t'i bëjmë ata",
    "sikurse ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "njësoj (të jetë)",
    "jeta e tyre",
    "dhe vdekja e tyre?!",
    "Sa keq (është)",
    "ajo çfarë",
    "ata gjykojnë!\""
  ],
  "22": [
    "Dhe krijoi",
    "Allahu",
    "qiejt",
    "dhe tokën",
    "me vërtetësi",
    "dhe që të kompensohet",
    "çdo",
    "person",
    "me atë çfarë",
    "ka fituar",
    "dhe atyre",
    "nuk",
    "u bëhet zullum."
  ],
  "23": [
    "Vallë, a e pe",
    "atë i cili",
    "e mori për",
    "zotin e tij",
    "dëshirën e tij",
    "dhe e la në humbje atë",
    "Allahu",
    "me",
    "dije",
    "dhe vulosi",
    "mbi",
    "dëgjimin e tij",
    "dhe zemrën e tij",
    "dhe vendosi",
    "mbi",
    "shikimin e tij",
    "perde.",
    "Po kush",
    "do ta udhëzojë atë",
    "nga",
    "pas",
    "Allahut?",
    "Hej, a nuk",
    "përkujtoni?!"
  ],
  "24": [
    "Dhe ata thanë",
    "\"Nuk (është)",
    "ajo",
    "përveçse",
    "jeta jonë",
    "e kësaj bote",
    "vdesim",
    "dhe jetojmë",
    "dhe nuk",
    "na shkatërron ne",
    "përveçse",
    "koha.",
    "E nuk",
    "ata kanë",
    "për të",
    "asnjë",
    "dije.",
    "Nuk (bëjnë tjetër)",
    "ata",
    "përveçse",
    "hamendësojnë."
  ],
  "25": [
    "E kur",
    "(u) recitohen",
    "atyre",
    "ajetet Tona",
    "të qarta",
    "nuk",
    "është",
    "argumenti i tyre",
    "veçse",
    "të",
    "thonë",
    "\"Na sillni",
    "baballarët tanë",
    "nëse",
    "jeni",
    "të sinqertë\"."
  ],
  "26": [
    "Thuaj",
    "\"Allahu",
    "(do) t'ju ngjall ju",
    "pastaj",
    "(do) t'ju vdes ju",
    "pastaj",
    "(do) t'ju tuboj",
    "tek",
    "dita",
    "e Kijametit",
    "nuk (ka)",
    "dyshim",
    "në të",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "dinë\"."
  ],
  "27": [
    "Dhe Allahut i përket",
    "sundimi",
    "i qiejve",
    "dhe i tokës.",
    "E ditën kur",
    "(do të) ndodh",
    "Çasti",
    "atë ditë",
    "do të dështojnë",
    "prishësit."
  ],
  "28": [
    "Dhe ti e sheh",
    "secilin",
    "popull",
    "të gjunjëzuar.",
    "Secili",
    "popull",
    "thirret",
    "tek",
    "libri i tij.",
    "Sot",
    "(do të) kompensoheni",
    "(për) atë çfarë",
    "ju ishit",
    "duke punuar."
  ],
  "29": [
    "Ky (është)",
    "libri ynë",
    "(që) flet",
    "kundër jush",
    "me vërtetësi.",
    "Vërtet Ne",
    "ishim",
    "kërkuam të kopjohet",
    "ajo çfarë",
    "ju ishit",
    "duke punuar."
  ],
  "30": [
    "E sa u përket",
    "atyre të cilët",
    "besuan",
    "dhe punuan",
    "veprat e mira",
    "do t'i fusë ata",
    "Zoti i tyre",
    "brenda",
    "mëshirës së Tij.",
    "Ajo",
    "është",
    "fitorja",
    "e qartë."
  ],
  "31": [
    "E sa u përket",
    "atyre të cilët",
    "mohuan",
    "vallë a nuk",
    "ishin",
    "ajetet e Mia",
    "(ju) recitoheshin",
    "juve",
    "e ju shfaqët arrogancë",
    "dhe ishit",
    "një popull",
    "kriminel!"
  ],
  "32": [
    "Dhe kur",
    "thuhet",
    "\"Vërtet",
    "premtimi",
    "i Allahut",
    "(është) i vërtetë",
    "dhe Çasti",
    "nuk (ka)",
    "dyshim",
    "në të\"",
    "ju thoni",
    "\"Nuk",
    "dimë",
    "ç'(është)",
    "Çasti!",
    "Nuk",
    "hamendësojmë",
    "veçse",
    "një hamendje",
    "dhe nuk",
    "ne (jemi)",
    "të bindur plotësisht\"."
  ],
  "33": [
    "Dhe (u) shpalosen",
    "atyre",
    "të këqijat",
    "e asaj çfarë",
    "ata punuan",
    "dhe i mbërtheu",
    "ata",
    "ajo çfarë",
    "ata ishin",
    "me të",
    "duke u tallur."
  ],
  "34": [
    "Dhe thuhet",
    "\"Sot",
    "Ne do t'ju harrojmë ju",
    "ashtu siç",
    "ju harruat",
    "takimin",
    "e ditës suaj",
    "këtë",
    "dhe vendqëndrimi juaj",
    "(është) zjarri",
    "dhe nuk (ka)",
    "për ju",
    "asnjë",
    "ndihmës\"."
  ],
  "35": [
    "Ajo (është)",
    "për shkak se ju",
    "i morët",
    "ajetet",
    "e Allahut",
    "për tallje",
    "dhe ju mashtroi",
    "jeta",
    "e kësaj bote.",
    "Prandaj sot",
    "nuk",
    "do të dalin",
    "prej tij",
    "dhe as",
    "atyre",
    "do t'u jepet mundësia për arsyetim."
  ],
  "36": [
    "Pra Allahut i përket",
    "falënderimi",
    "Zotit",
    "të qiejve",
    "dhe Zotit",
    "të tokës",
    "Zotit",
    "të botëve."
  ],
  "37": [
    "Dhe Atij (i takon)",
    "Madhështia",
    "në",
    "qiej",
    "dhe tokë",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ]
}