{
  "1": [
    "Lexo",
    "në emër",
    "të Zotit tënd",
    "i Cili",
    "krijoi"
  ],
  "2": [
    "E krijoi",
    "njeriun",
    "nga",
    "një substancë e ngjizur"
  ],
  "3": [
    "Lexo",
    "dhe Zoti yt",
    "(është) më Bujari"
  ],
  "4": [
    "I Cili",
    "ua mësoi",
    "me lapsin"
  ],
  "5": [
    "Ia mësoi",
    "njeriut",
    "atë çfarë",
    "nuk",
    "dinte"
  ],
  "6": [
    "Kurrsesi!",
    "vërtet",
    "njeriu",
    "sigurisht e tepron"
  ],
  "7": [
    "Që",
    "e sheh veten",
    "(se mund të) pavarësohet"
  ],
  "8": [
    "Vërtet",
    "tek",
    "Zoti yt",
    "(është) kthimi"
  ],
  "9": [
    "A e pe",
    "atë i cili",
    "ndalon"
  ],
  "10": [
    "një rob",
    "kur",
    "ai falet?"
  ],
  "11": [
    "A e ke parë",
    "nëse",
    "ai është",
    "në",
    "udhëzim"
  ],
  "12": [
    "Apo",
    "urdhëron",
    "në druajtje?"
  ],
  "13": [
    "A e ke parë",
    "nëse",
    "përgënjeshtron",
    "dhe zmbrapset?"
  ],
  "14": [
    "A nuk",
    "e di ai",
    "se vërtet",
    "Allahu",
    "sheh?"
  ],
  "15": [
    "Por, jo!",
    "Sigurisht, nëse",
    "nuk",
    "tërhiqet",
    "gjithsesi do ta kapim",
    "për balluke"
  ],
  "16": [
    "Balluke",
    "gënjeshtare",
    "mëkatare"
  ],
  "17": [
    "Atëherë ai le të thërrasë",
    "shoqëruesin e tij"
  ],
  "18": [
    "Ne do t'i thërrasim",
    "rojtarët e zjarrit"
  ],
  "19": [
    "Kurrsesi!",
    "mos",
