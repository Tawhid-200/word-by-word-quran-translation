{
  "1": [
    "Kur",
    "(të) dridhet",
    "toka",
    "me dridhjen e saj"
  ],
  "2": [
    "Dhe (të) nxjerrë",
    "toka",
    "barrët e saj"
  ],
  "3": [
    "Dhe (të) thotë",
    "njeriu",
    "\"Çfarë",
    "(është) me të?\""
  ],
  "4": [
    "Atë ditë",
    "ajo i rrëfen",
    "lajmet e saj"
  ],
  "5": [
    "Ngase",
    "Zoti yt",
    "e frymëzoi",
    "atë"
  ],
  "6": [
    "Atë ditë",
    "shfaqen",
    "njerëzit",
    "në grupe të shpërndara",
    "për t'u prezantuar",
    "veprat e tyre"
  ],
  "7": [
    "Pra, kushdo që",
    "punon",
    "(sa) grimca",
    "e një atomi",
    "ndonjë të mirë",
    "do ta shohë atë"
  ],
  "8": [
    "Dhe kushdo që",
    "punon",
    "(sa) grimca",
    "e atomit",
    "ndonjë të keqe",
    "do ta shohë atë"
  ]
}