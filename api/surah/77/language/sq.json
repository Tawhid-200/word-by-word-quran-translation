{
  "1": [
    "Për të dërguarit",
    "njëri pas tjetrit"
  ],
  "2": [
    "Dhe për stuhitë që fryjnë",
    "me furtunë"
  ],
  "3": [
    "Dhe për ata që shpërndajnë (retë)",
    "me shpërndarje"
  ],
  "4": [
    "Dhe për ata që e ndajnë",
    "me ndarje (të qartë)"
  ],
  "5": [
    "Dhe për ata që sjellin",
    "përkujtimin"
  ],
  "6": [
    "(Si) arsyetim",
    "apo",
    "paralajmërim"
  ],
  "7": [
    "Vërtet ajo çfarë",
    "ju premtohet",
    "sigurisht do të ndodhë"
  ],
  "8": [
    "E kur",
    "yjet",
    "(të) shfarosen"
  ],
  "9": [
    "Dhe kur",
    "qielli",
    "(të) hapet"
  ],
  "10": [
    "Dhe kur",
    "kodrat",
    "(të) thërrmohen"
  ],
  "11": [
    "Dhe kur",
    "të dërguarve",
    "(u) caktohet koha"
  ],
  "12": [
    "Për cilën",
    "ditë",
    "shtyhet?"
  ],
  "13": [
    "Për ditën",
    "vendimtare"
  ],
  "14": [
    "E çka",
    "di ti",
    "ç'(është)",
    "dita",
    "vendimtare?"
  ],
  "15": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit"
  ],
  "16": [
    "A nuk",
    "i shkatërruam",
    "(popujt) e hershëm"
  ],
  "17": [
    "Pastaj",
    "i pasojmë ata",
    "(me) të vonshmit"
  ],
  "18": [
    "Kështu",
    "Ne bëjmë",
    "me kriminelët"
  ],
  "19": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit"
  ],
  "20": [
    "A nuk",
    "ju krijuam ju",
    "nga",
    "një ujë",
    "i përbuzshëm?"
  ],
  "21": [
    "Pastaj e vendosëm atë",
    "në",
    "një vend",
    "të sigurtë"
  ],
  "22": [
    "Deri",
    "(në) një periudhë",
    "të njohur"
  ],
  "23": [
    "Pra Ne përcaktuam",
    "e sa të mrekullueshëm",
    "Përcaktues (jemi)!"
  ],
  "24": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit"
  ],
  "25": [
    "A nuk",
    "e bëmë",
    "tokën",
    "vendtubim?"
  ],
  "26": [
    "Për të gjallë",
    "dhe të vdekur."
  ],
  "27": [
    "Dhe bëmë",
    "në të",
    "kodra të rrënjosura",
    "të larta",
    "dhe ju dhamë ju të pini",
    "ujë",
    "të pijshëm."
  ],
  "28": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "29": [
    "\"Shkoni",
    "tek",
    "ajo çfarë",
    "ju ishit",
    "me të",
    "duke përgënjeshtruar\"."
  ],
  "30": [
    "\"Shkoni",
    "tek",
    "një hije",
    "që ka",
    "tri",
    "degëzime."
  ],
  "31": [
    "Nuk",
    "(është) hije freskuese",
    "dhe as",
    "mbron",
    "nga",
    "flaka."
  ],
  "32": [
    "Vërtet ai",
    "gjuan",
    "gaca",
    "si kështjella."
  ],
  "33": [
    "Si të ishin ato",
    "deve",
    "të verdha."
  ],
  "34": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "35": [
    "Kjo",
    "(është) një Ditë",
    "nuk",
    "ata flasin."
  ],
  "36": [
    "Dhe nuk",
    "(do të) lejohet",
    "për ta",
    "e të arsyetohen."
  ],
  "37": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "38": [
    "Kjo",
    "(është) dita",
    "vendimtare;",
    "Ne ju tubuam ju",
    "dhe të parët."
  ],
  "39": [
    "E nëse",
    "është",
    "për ju",
    "ndonjë kurth",
    "atëherë bëni kurth kundër Meje."
  ],
  "40": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "41": [
    "Vërtet",
    "të druajturit",
    "(do të jenë) në",
    "hije",
    "dhe burime uji."
  ],
  "42": [
    "Dhe fruta",
    "nga ato çfarë",
    "ata pëlqejnë."
  ],
  "43": [
    "\"Hani",
    "dhe pini",
    "me ëndje",
    "për atë çfarë",
    "ju ishit",
    "duke punuar\"."
  ],
  "44": [
    "Vërtet Ne",
    "ashtu",
    "i shpërblejmë",
    "bamirësit."
  ],
  "45": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "46": [
    "Hani",
    "dhe kënaquni",
    "pak;",
    "vërtet ju",
    "(jeni) kriminelë."
  ],
  "47": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "48": [
    "Dhe kur",
    "(u) thuhet",
    "atyre",
    "\"Përkuluni!\"",
    "nuk",
    "përkulen."
  ],
  "49": [
    "Mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "50": [
    "Pra në cilën",
    "bisedë",
    "pas tij",
    "ata (do të) besojnë?"
  ]
}