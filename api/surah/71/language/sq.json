{
  "1": [
    "Vërtet Ne",
    "e dërguam",
    "Nuhun",
    "tek",
    "populli i tij",
    "që",
    "paralajmëroje",
    "popullin tënd",
    "nga",
    "para",
    "që",
    "t'ju vijë atyre",
    "një dënim",
    "i dhembshëm."
  ],
  "2": [
    "Ai tha",
    "\"O populli im",
    "vërtet unë (jam)",
    "për ju",
    "një paralajmërues",
    "i qartë."
  ],
  "3": [
    "Që",
    "adhuroni",
    "Allahun",
    "dhe druajuni Atij",
    "dhe më dëgjoni mua."
  ],
  "4": [
    "Ai (ju) fal",
    "ju",
    "prej",
    "gjynaheve tuaja",
    "dhe jua shtyn (jetën)",
    "deri në",
    "një afat",
    "të caktuar.",
    "Vërtet",
    "afati",
    "i Allahut",
    "kur",
    "të vijë",
    "nuk",
    "shtyhet",
    "sikur",
    "(të) ishit",
    "(që) dini\"."
  ],
  "5": [
    "Ai tha",
    "\"Zoti im",
    "vërtet unë",
    "e thirra",
    "popullin tim",
    "natën",
    "dhe ditën."
  ],
  "6": [
    "Por nuk",
    "ua shtoi atyre",
    "thirrja ime",
    "(asgjë) përveç",
    "ikjes."
  ],
  "7": [
    "Dhe vërtet unë",
    "sa herë që",
    "i ftoja ata",
    "që Ti t'ua falësh",
    "atyre",
    "ata vendosnin",
    "gishtërinjtë e tyre",
    "në",
    "veshët e tyre",
    "dhe mbulonin kokën",
    "me rrobat e tyre",
    "dhe insistonin",
    "dhe ishin mendjemëdhenj",
    "me arrogancë."
  ],
  "8": [
    "Pastaj",
    "vërtet unë",
    "i thirra ata",
    "haptazi."
  ],
  "9": [
    "Pastaj",
    "vërtet unë",
    "(u) flisja haptazi",
    "atyre",
    "dhe (u) flisja fshehtazi",
    "atyre",
    "në fshehtësi\"."
  ],
  "10": [
    "Dhe u thashë",
    "\"Kërkoni falje",
    "Zotit tuaj",
    "vërtet Ai",
    "është",
    "Falës i madh."
  ],
  "11": [
    "Ai lëshon",
    "(nga) qielli",
    "për ju",
    "shi të bollshëm."
  ],
  "12": [
    "Dhe jua shton ju",
    "pasuri",
    "dhe fëmijë",
    "dhe bën",
    "për ju",
    "kopshte",
    "dhe bën",
    "për ju",
    "lumenj."
  ],
  "13": [
    "Ç'(është)",
    "me ju",
    "(që) nuk",
    "(i) shprehni",
    "Allahut",
    "madhështi?"
  ],
  "14": [
    "Ndërkohë që",
    "Ai ju krijoi ju",
    "në disa faza"
  ],
  "15": [
    "A nuk",
    "keni parë",
    "si",
    "krijoi",
    "Allahu",
    "shtatë",
    "qiej",
    "në shtresa."
  ],
  "16": [
    "Dhe e bëri",
    "hënën",
    "në ta",
    "dritë",
    "dhe e bëri",
    "diellin",
    "ndriçues."
  ],
  "17": [
    "Dhe Allahu",
    "ju nxori ju",
    "nga",
    "toka",
    "si bimë."
  ],
  "18": [
    "Pastaj",
    "ju kthen ju",
    "në të",
    "dhe ju nxjerr ju",
    "me nxjerrje."
  ],
  "19": [
    "Dhe Allahu",
    "bëri",
    "për ju",
    "tokën",
    "si shtrojë."
  ],
  "20": [
    "Që të ecni",
    "në të",
    "rrugëve",
    "të gjera."
  ],
  "21": [
    "Tha",
    "Nuhu",
    "\"Zoti im",
    "vërtet ata",
    "më kundërshtuan mua",
    "dhe e pasuan",
    "atë që",
    "nuk",
    "ia shtoi atij",
    "pasuria e tij",
    "dhe fëmijët e tij",
    "(asgjë) përveç",
    "dështimin\"."
  ],
  "22": [
    "Dhe ata kurdisën",
    "një kurth",
    "shumë të madh."
  ],
  "23": [
    "Dhe ata thanë",
    "\"Mos",
    "(i) braktisni kurrsesi",
    "zotat tuaj",
    "dhe mos",
    "braktisni kurrsesi",
    "Vedda-në",
    "dhe as",
    "Suva-në",
    "dhe as",
    "Jeguth-in",
    "dhe Jeuk-un",
    "dhe Nesr-in\"."
  ],
  "24": [
    "Dhe sigurisht",
    "ata e devijuan",
    "një shumicë",
    "dhe mos",
    "(ua) shto",
    "zullumqarëve",
    "(asgjë) përveç",
    "humbje."
  ],
  "25": [
    "Për shkak të",
    "gjynaheve të tyre",
    "ata u përmbytën",
    "dhe u futën",
    "(në) zjarr",
    "dhe nuk",
    "gjetën",
    "për ta",
    "nga",
    "pos",
    "Allahut",
    "ndihmues."
  ],
  "26": [
    "Dhe tha",
    "Nuhu",
    "\"Zoti im",
    "mos",
    "lër",
    "mbi",
    "tokë",
    "prej",
    "mohuesve",
    "asnjë."
  ],
  "27": [
    "Vërtet Ti",
    "nëse",
    "i lë ata",
    "ata do t'i devijojnë",
    "robërit e Tu",
    "dhe nuk",
    "do të lindin",
    "përveç",
    "i prishur",
    "mohues i madh."
  ],
  "28": [
    "Zoti im",
    "(më) fal",
    "mua",
    "dhe prindërit e mi",
    "dhe atë që",
    "hyn",
    "(në) shtëpinë time",
    "si besimtar",
    "dhe besimtarët",
    "dhe besimtaret",
    "dhe mos",
    "(ua) shto",
    "zullumqarëve",
    "përveç",
    "dëshpërim\"."
  ]
}