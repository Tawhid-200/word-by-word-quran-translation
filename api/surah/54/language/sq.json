{
  "1": [
    "Është afruar",
    "Kijameti",
    "dhe është çarë",
    "hëna."
  ],
  "2": [
    "E nëse",
    "ata shohin",
    "(ndonjë) dëshmi",
    "zmbrapsen",
    "dhe thonë",
    "\"Magji",
    "e vazhdueshme!\""
  ],
  "3": [
    "Dhe ata përgënjeshtruan",
    "dhe ndoqën",
    "dëshirat e tyre",
    "dhe çdo",
    "çështje",
    "(është) e përcaktuar."
  ],
  "4": [
    "Dhe sigurisht",
    "u erdhi atyre",
    "prej",
    "njoftimeve",
    "ajo çfarë",
    "në të (ka)",
    "qortim."
  ],
  "5": [
    "Urtësi",
    "e përsosur!",
    "Por nuk",
    "bëjnë dobi",
    "paralajmërimet."
  ],
  "6": [
    "Prandaj largohu",
    "prej tyre!",
    "Ditën kur",
    "thërret",
    "thirrësi",
    "tek",
    "një gjë",
    "e tmerrshme."
  ],
  "7": [
    "Të përulur (janë)",
    "shikimet e tyre.",
    "Ata dalin",
    "nga",
    "varret",
    "sikur ata (të ishin)",
    "karkalecë",
    "të shpërndarë."
  ],
  "8": [
    "Të nxituar",
    "drejt",
    "thirrësit",
    "thonë",
    "mohuesit",
    "\"Kjo (është)",
    "një ditë",
    "shumë e vështirë\"."
  ],
  "9": [
    "Ka përgënjeshtruar",
    "para tyre",
    "populli",
    "i Nuhut",
    "prandaj e përgënjeshtruan",
    "robin Tonë",
    "dhe thanë",
    "\"I çmendur!\"",
    "dhe ai u kërcënua."
  ],
  "10": [
    "E ai iu lut",
    "Zotit të tij",
    "\"Vërtet unë (jam)",
    "i mposhtur",
    "prandaj më ndihmo!\""
  ],
  "11": [
    "Menjëherë Ne i hapëm",
    "dyert",
    "e qiellit",
    "me ujë",
    "të vrullshëm."
  ],
  "12": [
    "Dhe shpërthyem",
    "tokën",
    "(me) burime",
    "ashtu që u takua",
    "uji",
    "për",
    "një çështje",
    "tashmë",
    "ishte caktuar."
  ],
  "13": [
    "Dhe Ne e bartëm atë",
    "mbi",
    "atë që ka",
    "dërrasa",
    "dhe gozhda."
  ],
  "14": [
    "Ajo lundronte",
    "nën Sytë Tanë",
    "(si) shpërblim",
    "për atë i cili",
    "ishte",
    "ai u mohua."
  ],
  "15": [
    "Dhe sigurisht",
    "Ne e lamë atë",
    "(si) dëshmi",
    "por a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "16": [
    "E si",
    "ishte",
    "dënimi Im",
    "dhe paralajmërimet e Mia?"
  ],
  "17": [
    "Dhe sigurisht",
    "Ne e kemi lehtësuar",
    "Kur'anin",
    "për kujtim",
    "por a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "18": [
    "Ka përgënjeshtruar",
    "Adi",
    "e si",
    "ishte",
    "dënimi Im",
    "dhe paralajmërimet e Mia?"
  ],
  "19": [
    "Vërtet Ne",
    "dërguam",
    "mbi ta",
    "një erë",
    "të stuhishme",
    "në",
    "një ditë",
    "ogurzezë",
    "të vazhdueshme."
  ],
  "20": [
    "Ajo i shkulte",
    "njerëzit",
    "sikur ata (të ishin)",
    "rrënjë",
    "hurmash",
    "të shkulura."
  ],
  "21": [
    "E si",
    "ishte",
    "dënimi Im",
    "dhe paralajmërimet e Mia?"
  ],
  "22": [
    "Dhe sigurisht",
    "Ne e kemi lehtësuar",
    "Kur'anin",
    "për kujtim",
    "por a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "23": [
    "Ka përgënjeshtruar",
    "Themudi",
    "paralajmërimet."
  ],
  "24": [
    "E thanë",
    "\"A një njeri",
    "nga ne",
    "një",
    "ne (ta) ndjekim atë?!\"",
    "Vërtet ne",
    "atëherë",
    "(do të ishim) në",
    "një humbje",
    "dhe çmenduri."
  ],
  "25": [
    "A iu përcoll",
    "mesazhi",
    "atij",
    "nga",
    "mesi ynë?",
    "Jo, por",
    "ai (është)",
    "një gënjeshtar i madh",
    "mendjemadh."
  ],
  "26": [
    "Do të dinë ata",
    "nesër",
    "kush (është)",
    "gënjeshtari i madh",
    "mendjemadhi!"
  ],
  "27": [
    "Vërtet Ne (jemi)",
    "Dërguesit",
    "e deves",
    "(si) sprovë",
    "për ta",
    "prandaj vështroji ata",
    "dhe ji durimmadh!"
  ],
  "28": [
    "Dhe njoftoji ata",
    "se",
    "uji",
    "(është) përcaktim",
    "mes tyre",
    "çdo",
    "pirje",
    "(është) e vendosur."
  ],
  "29": [
    "E ata e thirrën",
    "shokun e tyre",
    "e ai e mori",
    "dhe përnjëherë e theri."
  ],
  "30": [
    "E si",
    "ishte",
    "dënimi Im",
    "dhe paralajmërimet e Mia?"
  ],
  "31": [
    "Vërtet Ne",
    "dërguam",
    "mbi ta",
    "ushtimë",
    "një",
    "dhe përnjëherë ata u bënë",
    "si kallame",
    "(për) vathë."
  ],
  "32": [
    "Dhe sigurisht",
    "Ne e kemi lehtësuar",
    "Kur'anin",
    "për kujtim",
    "por a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "33": [
    "Ka përgënjeshtruar",
    "populli",
    "i Lutit",
    "paralajmërimet."
  ],
  "34": [
    "Vërtet Ne",
    "dërguam",
    "mbi ta",
    "stuhi gurësh",
    "përveç",
    "familjen",
    "e Lutit",
    "Ne i shpëtuam ata",
    "para agimit."
  ],
  "35": [
    "Begati",
    "nga",
    "ana Jonë.",
    "Kështu",
    "Ne e shpërblejmë",
    "këdo që",
    "falënderon."
  ],
  "36": [
    "Dhe sigurisht",
    "ai i paralajmëroi ata",
    "(për) dënimin Tonë të rëndë",
    "por ata dyshuan",
    "në (ato) paralajmërime."
  ],
  "37": [
    "Dhe sigurisht",
    "ata e joshën atë",
    "nga",
    "mysafirët e tij",
    "prandaj Ne i verbuam",
    "sytë e tyre",
    "\"Pra, shijoni",
    "dënimin Tim",
    "dhe paralajmërimet e Mia!\""
  ],
  "38": [
    "Dhe sigurisht",
    "i mbërtheu ata në mëngjes",
    "herët",
    "një dënim",
    "i përqendruar."
  ],
  "39": [
    "Pra, shijoni",
    "dënimin Tim",
    "dhe paralajmërimet e Mia!"
  ],
  "40": [
    "Dhe sigurisht",
    "Ne e kemi lehtësuar",
    "Kur'anin",
    "për kujtim",
    "por a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "41": [
    "Dhe sigurisht",
    "i erdhën",
    "familjes",
    "së faraonit",
    "paralajmërimet."
  ],
  "42": [
    "Ata i përgënjeshtruan",
    "dëshmitë Tona",
    "të gjitha ato",
    "kështu që Ne i mbërthyem ata",
    "(me) mbërthimin",
    "e një të Pamposhturi",
    "Fuqiploti."
  ],
  "43": [
    "A mohuesit tuaj (janë)",
    "më të mirë",
    "se",
    "ata",
    "apo",
    "ju keni",
    "paprekshmëri",
    "në",
    "librat (e shpallur)?"
  ],
  "44": [
    "Vallë a",
    "ata thonë",
    "\"Ne (jemi)",
    "të gjithë",
    "ngadhënjyes\"."
  ],
  "45": [
    "Do të mposhtet",
    "grupi",
    "dhe ata do t'i kthejnë",
    "shpinat."
  ],
  "46": [
    "Por jo!",
    "Kijameti (është)",
    "takimi i tyre",
    "e Kijameti (është)",
    "më i tmerrshëm",
    "dhe më i hidhur."
  ],
  "47": [
    "Vërtet",
    "kriminelët",
    "(janë) në",
    "humbje",
    "dhe zjarr."
  ],
  "48": [
    "Ditën kur",
    "ata (do të) tërhiqen",
    "në",
    "zjarr",
    "me",
    "fytyrat e tyre",
    "\"Shijoni",
    "prekjen",
    "e Sekarit (një emër për xhehenemin)."
  ],
  "49": [
    "Vërtet Ne",
    "çdo",
    "gjë",
    "e krijuam atë",
    "me masë."
  ],
  "50": [
    "Dhe nuk (është)",
    "urdhri Ynë",
    "përveçse",
    "një",
    "si shkrepje",
    "e shikimit."
  ],
  "51": [
    "Dhe sigurisht",
    "Ne i shkatërruam",
    "të ngjashmit me ju",
    "pra a (ka)",
    "ndonjë",
    "përkujtues?"
  ],
  "52": [
    "Dhe çdo",
    "gjë",
    "ata e bënë atë",
    "(është) në",
    "libra."
  ],
  "53": [
    "Dhe çdo",
    "(punë) e vogël",
    "dhe e madhe",
    "(është) e shkruar."
  ],
  "54": [
    "Vërtet",
    "të druajturit",
    "(janë) në",
    "xhenete",
    "dhe lumenj."
  ],
  "55": [
    "Në",
    "kuvendin",
    "e së vërtetës",
    "pranë",
    "Sunduesit",
    "Fuqiplotë."
  ]
}