{
  "1": [
    "Elif, Lam, Ra.",
    "Këto (janë)",
    "ajete",
    "të Librit",
    "të qartë."
  ],
  "2": [
    "Vërtet Ne",
    "e zbritëm atë",
    "Kur'an",
    "arabisht",
    "ashtu që ju",
    "ta rrokni."
  ],
  "3": [
    "Ne",
    "të rrëfejmë",
    "ty",
    "më të bukurin",
    "(ndër) rrëfimet",
    "për atë që",
    "Ne (të) shpallëm",
    "ty",
    "këtë",
    "Kur'an",
    "e sigurisht",
    "ti ishe",
    "nga",
    "para tij",
    "nga",
    "të painformuarit."
  ],
  "4": [
    "Kur",
    "(i) tha",
    "Jusufi",
    "babait të tij",
    "\"O babush",
    "vërtet unë",
    "kam parë",
    "një",
    "dhjetë",
    "yje",
    "edhe diellin",
    "dhe hënën",
    "i kam parë ata",
    "për mua",
    "bënin sexhde\"."
  ],
  "5": [
    "Ai (Jakubi) tha",
    "\"O vogëlush",
    "mos",
    "(rrëfe",
    "ëndrrën tënde",
    "tek",
    "vëllezërit e tu",
    "se ata do të përgatisin",
    "për ty",
    "dredhi.",
    "Vërtet",
    "shejtani",
    "për njeriun",
    "(është) një armik",
    "i qartë\"."
  ],
  "6": [
    "Dhe kështu",
    "të përzgjedh ty",
    "Zoti yt",
    "dhe të mëson ty",
    "nga",
    "shpjegimi",
    "i bisedave (ëndrrave)",
    "dhe e plotëson",
    "dhuntinë e Tij",
    "ndaj teje",
    "dhe ndaj",
    "familjes",
    "së Jakubit",
    "ashtu siç",
    "e plotësoi atë",
    "ndaj",
    "prindërve tu",
    "nga",
    "përpara",
    "Ibrahimit",
    "dhe Is'hakut.",
    "Vërtet",
    "Zoti yt",
    "(është) i Gjithëdijshëm",
    "i Urtë."
  ],
  "7": [
    "Sigurisht",
    "ishte",
    "në",
    "Jusufin",
    "dhe vëllezërit e tij",
    "dëshmi",
    "për pyetësit."
  ],
  "8": [
    "Kur",
    "ata thanë",
    "\"Vërtet Jusufi",
    "dhe vëllai i tij",
    "është më i dashur",
    "tek",
    "babai ynë",
    "se ne",
    "ndërkohë që ne (jemi)",
    "një grup.",
    "Vërtet",
    "babai ynë",
    "(është) në",
    "një gabim",
    "të qartë."
  ],
  "9": [
    "Vriteni",
    "Jusufin",
    "apo",
    "hidheni atë",
    "(në) një tokë",
    "(do të) veçohet",
    "për ju",
    "fytyra",
    "e babait tuaj",
    "dhe do të jeni",
    "nga",
    "pas tij",
    "njerëz",
    "të mirë."
  ],
  "10": [
    "Tha",
    "një folës",
    "prej tyre",
    "\"Mos",
    "e vrisni",
    "Jusufin",
    "por hidheni atë",
    "në",
    "thellësitë",
    "e pusit",
    "do ta marrin atë",
    "disa",
    "udhëtarë",
    "nëse",
    "jeni",
    "veprues\"."
  ],
  "11": [
    "Ata thanë",
    "\"O babai ynë",
    "çka (është)",
    "me ty",
    "nuk",
    "na beson",
    "për",
    "Jusufin",
    "ndërkohë që ne vërtet",
    "për të",
    "(jemi) këshillues\"."
  ],
  "12": [
    "Dërgoje atë",
    "me ne",
    "nesër",
    "të hajë (me bollëk)",
    "dhe të luajë",
    "e ne vërtet",
    "për të",
    "(jemi) ruajtës."
  ],
  "13": [
    "Ai tha",
    "\"Vërtet mua",
    "më pikëllon",
    "që",
    "ju të shkoni",
    "me të",
    "dhe frikësohem",
    "që",
    "ta hajë atë",
    "ujku",
    "ndërkohë që ju",
    "ndaj tij",
    "(jeni) të pakujdesshëm\"."
  ],
  "14": [
    "Ata thanë",
    "\"Vërtet nëse",
    "e ha atë",
    "ujku",
    "ndërkohë që ne (jemi)",
    "një grup",
    "vërtet ne",
    "atëherë",
    "(do të jemi) humbës\"."
  ],
  "15": [
    "Pasi që",
    "ata shkuan",
    "me të",
    "dhe u pajtuan njëzëri",
    "që",
    "ta vendosin atë",
    "në",
    "thellësitë",
    "e pusit",
    "dhe Ne i shpallëm",
    "atij",
    "\"Gjithsesi ti do t'i njoftosh ata",
    "për punën e tyre",
    "këtë",
    "e ata",
    "nuk",
    "(do të) ndjejnë\"."
  ],
  "16": [
    "Dhe ata erdhën",
    "(tek) babai i tyre",
    "në mbrëmje",
    "duke qarë."
  ],
  "17": [
    "Ata thanë",
    "\"O babai ynë",
    "vërtet ne",
    "shkuam",
    "(të) garojmë",
    "dhe e lamë",
    "Jusufin",
    "tek",
    "gjërat tona",
    "dhe e hëngri atë",
    "ujku.",
    "Dhe nuk",
    "ti",
    "(je) besimtar",
    "në ne",
    "ndonëse",
    "ne jemi",
    "të sinqertë\"."
  ],
  "18": [
    "Dhe ata erdhën",
    "me",
    "këmishën e tij",
    "me një gjak",
    "të rrejshëm.",
    "Ai tha",
    "\"Në fakt",
    "e zbukuroi",
    "për ju",
    "vetja juaj",
    "(këtë) çështje.",
    "Por (halli im është) durim",
    "i bukur",
    "e Allahu (është)",
    "Ai të Cilit i kërkohet ndihma",
    "për",
    "atë çfarë",
    "ju përshkruani\"."
  ],
  "19": [
    "Dhe erdhi",
    "një karvan",
    "dhe dërguan",
    "ujëbartësin e tyre",
    "e ai e lëshoi",
    "kovën e tij",
    "tha",
    "\"O përgëzim",
    "ky (qenka)",
    "një djalosh!\"",
    "Dhe ata e fshehën atë",
    "si një mall",
    "e Allahu (është)",
    "i Gjithëdijshëm",
    "për atë çfarë",
    "ata punojnë."
  ],
  "20": [
    "Dhe ata e shitën atë",
    "me një çmim",
    "të ulët",
    "dërhemë",
    "të numëruar",
    "dhe ishin",
    "për të",
    "nga",
    "jolakmitarët."
  ],
  "21": [
    "Dhe tha",
    "ai i cili",
    "e bleu atë",
    "nga",
    "Egjipti",
    "gruas së tij",
    "\"Përgatitja",
    "vendqëndrimin e tij",
    "mbase",
    "(do) të",
    "na bëjë dobi ne",
    "apo",
    "do ta marrim atë",
    "për fëmijë.",
    "Dhe kështu",
    "Ne ia dhuruam një vend",
    "Jusufit",
    "në",
    "tokë",
    "dhe që t'ia mësojmë atij",
    "nga",
    "shpjegimi",
    "i ëndrrave",
    "e Allahu (është)",
    "Ngadhënjyes",
    "mbi",
    "çështjen e Tij",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "dinë."
  ],
  "22": [
    "E kur",
    "ai arriti",
    "pjekurinë e tij",
    "Ne i dhamë atij",
    "pushtet",
    "dhe dituri",
    "dhe kështu",
    "Ne i shpërblejmë",
    "bamirësit."
  ],
  "23": [
    "Dhe e joshi atë",
    "ajo e cila",
    "ai (ishte)",
    "në",
    "shtëpinë e saj",
    "nga",
    "vetja e tij",
    "dhe i mbylli fort",
    "dyert",
    "dhe i tha",
    "\"U përgatita",
    "për ty!\"",
    "Ai tha",
    "\"Ruana",
    "Zot!\"",
    "Vërtet",
    "zotëriu im",
    "e rregulloi",
    "vendqëndrimin tim",
    "vërtet",
    "nuk",
    "shpëtojnë",
    "zullumqarët\"."
  ],
  "24": [
    "Padyshim",
    "ajo mësyu",
    "drejt tij",
    "edhe ai (do të) mësynte",
    "drejt saj",
    "sikur të mos",
    "të",
    "shihte",
    "provën",
    "e Zotit të tij.",
    "Kështu",
    "që Ne ta largojmë",
    "prej tij",
    "të keqen",
    "dhe të ndytën",
    "vërtet ai (është)",
    "nga",
    "robërit Tanë",
    "të përzgjedhur."
  ],
  "25": [
    "Dhe ata dy vrapuan",
    "(drejt) derës",
    "dhe ajo e grisi",
    "këmishën e tij",
    "nga",
    "mbrapa",
    "dhe ata dy e gjetën",
    "zotëriun (burrin) e saj",
    "pranë",
    "derës.",
    "Ajo tha",
    "\"Çfarë (është)",
    "ndëshkimi",
    "i atij që",
    "(i) dëshiron",
    "familjes tënde",
    "një të keqe",
    "përveç",
    "të",
    "burgoset",
    "apo",
    "një dënim",
    "i dhembshëm?\""
  ],
  "26": [
    "Ai tha",
    "\"Ajo",
    "më joshi mua",
    "nga",
    "vetja ime\".",
    "Dhe dëshmoi",
    "një dëshmitar",
    "nga",
    "familja e saj",
    "\"Nëse",
    "është",
    "këmisha e tij",
    "grisur",
    "nga",
    "përpara",
    "atëherë ajo e ka thënë të vërtetën",
    "ndërsa ai (është)",
    "nga",
    "gënjeshtarët."
  ],
  "27": [
    "E nëse",
    "është",
    "këmisha e tij",
    "grisur",
    "nga",
    "mbrapa",
    "atëherë ajo ka gënjyer",
    "ndërsa ai (është)",
    "nga",
    "të sinqertët\"."
  ],
  "28": [
    "E kur",
    "ai pa",
    "këmishën e tij",
    "ishte grisur",
    "nga",
    "mbrapa",
    "tha",
    "\"Vërtet kjo (është)",
    "nga",
    "dredhia juaj",
    "vërtet",
    "dredhia juaj",
    "(është) e madhe\"."
  ],
  "29": [
    "\"(O) Jusuf",
    "hiqu",
    "nga",
    "kjo",
    "dhe kërko falje (oj grua)",
    "për mëkatin tënd",
    "vërtet ti",
    "ishe",
    "nga",
    "mëkatarët\"."
  ],
  "30": [
    "Dhe thanë",
    "disa gra",
    "në",
    "qytet",
    "\"Gruaja",
    "e Azizit (ministrit)",
    "po e josh",
    "shërbëtorin e saj",
    "nga",
    "vetja e tij",
    "sigurisht",
    "e ka mbërthyer fort atë",
    "dashuria",
    "vërtet ne",
    "e shohim atë",
    "në",
    "një devijim",
    "të qartë."
  ],
  "31": [
    "E kur",
    "ajo dëgjoi",
    "kurthin e tyre",
    "ajo dërgoi",
    "tek to",
    "dhe përgatiti",
    "për to",
    "vend për mbështetje",
    "dhe ia dha",
    "çdo",
    "njërës",
    "prej tyre",
    "thikë",
    "dhe tha",
    "\"Dil",
    "para tyre!\"",
    "E kur",
    "ato e panë atë",
    "u mahnitën me të",
    "dhe i prenë",
    "duart e tyre",
    "dhe thanë",
    "\"Ruana",
    "Zot!\"",
    "Nuk (është)",
    "ky",
    "njeri",
    "nuk (është)",
    "ky",
    "përveç",
    "një engjëll",
    "fisnik\"."
  ],
  "32": [
    "Ajo tha:",
    "\"Pra, ai (është)",
    "i cili",
    "ju më qortuat mua",
    "për të\".",
    "Dhe vërtet",
    "unë e josha atë",
    "nga",
    "vetja e tij",
    "por ai u mbrojt fuqishëm",
    "e nëse",
    "nuk",
    "bën",
    "atë çfarë",
    "unë e urdhëroj atë",
    "sigurisht do të burgoset",
    "dhe do të jetë",
    "nga",
    "të nënçmuarit."
  ],
  "33": [
    "Ai tha",
    "\"Zoti im",
    "burgu",
    "(është) më i dashur",
    "për mua",
    "se ajo çfarë",
    "ato më ftojnë mua",
    "tek ajo",
    "dhe nëse nuk",
    "Ti e largon",
    "prej meje",
    "dredhinë e tyre",
    "unë do të anoj",
    "tek to",
    "dhe do të jem",
    "prej",
    "të paditurve."
  ],
  "34": [
    "Menjëherë iu përgjigj",
    "atij",
    "Zoti i tij",
    "dhe e largoi",
    "prej tij",
    "dredhinë e tyre",
    "vërtet Ai",
    "Ai (është)",
    "Gjithëdëgjuesi",
    "i Gjithëdijshmi."
  ],
  "35": [
    "Pastaj",
    "u ra ndërmend",
    "atyre",
    "nga",
    "pasi",
    "që",
    "ata i panë",
    "provat",
    "\"Gjithsesi ai do të burgoset",
    "deri",
    "në një kohë\"."
  ],
  "36": [
    "Dhe hynë",
    "me të",
    "(në) burg",
    "dy djelmosha.",
    "Tha",
    "njëri prej tyre",
    "\"Vërtet unë",
    "e shoh veten",
    "unë shtrydh",
    "verë\".",
    "E tha",
    "tjetri",
    "\"Vërtet unë",
    "e shoh veten",
    "unë bart",
    "mbi",
    "kokën time",
    "bukë",
    "hanë",
    "shpezët",
    "prej saj\".",
    "Na njofto",
    "për komentimin e saj",
    "vërtet ne",
    "të shohim ty",
    "nga",
    "njohësit e mirë."
  ],
  "37": [
    "Ai tha",
    "\"Nuk",
    "ju vjen juve dyve",
    "ushqim",
    "ju dy ushqeheni me të",
    "përveçse",
    "unë ju njoftoj juve dyve",
    "për shpjegimin e saj",
    "para",
    "se",
    "t'ju vijë juve dyve.",
    "Ajo (është)",
    "prej asaj çfarë",
    "më mësoi mua",
    "Zoti im.",
    "Vërtet unë",
    "braktisa",
    "fenë",
    "e një populli",
    "nuk",
    "ata besojnë",
    "në Allahun",
    "dhe ata",
    "në jetën e fundit",
    "ata (janë)",
    "mohues\"."
  ],
  "38": [
    "Dhe ndoqa",
    "fenë",
    "e baballarëve të mi",
    "Ibrahimit",
    "dhe Is'hakut",
    "dhe Jakubit.",
    "Nuk",
    "është",
    "për ne",
    "që të",
    "shoqërojmë (në adhurim)",
    "me Allahun",
    "asnjë",
    "gjë.",
    "Ajo (është)",
    "nga",
    "mirësia",
    "e Allahut",
    "ndaj nesh",
    "dhe ndaj",
    "njerëzve",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "falënderojnë."
  ],
  "39": [
    "O ju dy shokët e mi",
    "të burgut",
    "a zota",
    "të ndryshëm",
    "(janë) më të mirë",
    "apo",
    "Allahu",
    "i Vetmi",
    "Ngadhënjyes?!"
  ],
  "40": [
    "Nuk",
    "ju adhuroni",
    "prej",
    "përveç Tij",
    "vetëm",
    "emra",
    "ju i emërtuat ata",
    "ju",
    "dhe baballarët tuaj",
    "nuk",
    "zbriti",
    "Allahu",
    "për ta",
    "asnjë",
    "argument",
    "nuk (është)",
    "gjykimi",
    "përveç",
    "i Allahut",
    "Ai urdhëroi",
    "që të mos",
    "ju adhuroni",
    "askë përpos",
    "Atë",
    "ajo (është)",
    "fe",
    "e drejtë",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "dinë."
  ],
  "41": [
    "O ju dy shokët e mi",
    "të burgut",
    "sa i përket",
    "njërit prej juve dyve",
    "ai do t'i japë të pijë",
    "zotëriut të tij",
    "verë",
    "ndërkaq",
    "tjetri",
    "do të kryqëzohet",
    "e do të hanë",
    "shpezët",
    "nga",
    "koka e tij.",
    "Përfundoi",
    "çështja",
    "të cilën",
    "për të",
    "ju dy kërkuat shpjegim."
  ],
  "42": [
    "Dhe i tha",
    "atij për të cilin",
    "besonte",
    "se ai (është)",
    "i shpëtuar",
    "prej atyre dyve",
    "\"Më përmend mua",
    "tek",
    "zotëriu yt",
    "mirëpo ia harroi atij",
    "shejtani",
    "përmendjen",
    "tek zotëriu i tij",
    "kështu që ai qëndroi",
    "në",
    "burg",
    "disa",
    "vite."
  ],
  "43": [
    "Dhe tha",
    "mbreti",
    "\"Vërtet unë",
    "pashë",
    "shtatë",
    "lopë",
    "të majme",
    "i hanin ato",
    "shtatë",
    "të dobëta",
    "dhe shtatë",
    "kallinj",
    "të gjelbër",
    "dhe të tjerë",
    "të thatë.",
    "O ju",
    "pari",
    "më jepni shpjegim",
    "për",
    "ëndrrën time",
    "nëse",
    "ju jeni",
    "për ëndrrën",
    "e komentoni (atë)\"."
  ],
  "44": [
    "Ata thanë",
    "\"Grumbuj",
    "ëndrrash",
    "dhe nuk",
    "ne (jemi)",
    "për shpjegimin",
    "e ëndrrave",
    "njohës\"."
  ],
  "45": [
    "Dhe tha",
    "ai i cili",
    "shpëtoi",
    "prej atyre dyve",
    "dhe u kujtua",
    "pas",
    "një periudhe kohore",
    "\"Unë",
    "ju njoftoj ju",
    "për shpjegimin e saj",
    "prandaj më dërgoni mua!\""
  ],
  "46": [
    "Jusuf",
    "O ti",
    "i sinqerti",
    "na jep përgjigje",
    "për",
    "shtatë",
    "lopë",
    "të majme",
    "i hanë ato",
    "shtatë",
    "të dobëta",
    "dhe (për) shtatë",
    "kallinj",
    "të gjelbër",
    "dhe të tjerë",
    "të thatë",
    "ashtu që",
    "unë kthehem",
    "tek",
    "njerëzit",
    "ashtu që ata",
    "dinë (kuptojnë)."
  ],
  "47": [
    "Ai tha",
    "\"Ju (do të) mbillni",
    "shtatë",
    "vjet",
    "vazhdimisht",
    "e atë çka",
    "ju e korrni",
    "lëreni",
    "në",
    "kallirin e tij",
    "përveç",
    "një pakice",
    "prej asaj çfarë",
    "ju (do të) hani\"."
  ],
  "48": [
    "Pastaj",
    "(do të) vijnë",
    "prej",
    "pas",
    "asaj",
    "shtatë (vjet)",
    "të vështira",
    "ato (do të) hanë",
    "atë çfarë",
    "ju përgatitët",
    "për to",
    "përveç",
    "një pakice",
    "prej asaj çfarë",
    "ju (do të) rezervoni."
  ],
  "49": [
    "Pastaj",
    "(do të) vijë",
    "prej",
    "pas",
    "asaj",
    "një vit",
    "në të",
    "do të bie shi",
    "(për) njerëzit",
    "dhe në të",
    "ata do të shtrydhin (pemë)."
  ],
  "50": [
    "Dhe tha",
    "mbreti",
    "\"Ma sillni mua",
    "atë!\"",
    "E kur",
    "erdhi tek ai",
    "i dërguari",
    "tha",
    "\"Kthehu",
    "tek",
    "zotëriu yt",
    "dhe pyete",
    "çka (është)",
    "puna",
    "e grave",
    "të cilat",
    "i prenë",
    "duart e tyre",
    "vërtet",
    "Zoti im",
    "për dredhinë e tyre",
    "(është) i Gjithëdijshëm\"."
  ],
  "51": [
    "Ai tha",
    "\"Çka (është)",
    "çështja juaj",
    "kur",
    "ju e joshët",
    "Jusufin",
    "nga",
    "vetja e tij?\"",
    "Ato thanë",
    "\"Ruana",
    "Zot!",
    "ne",
    "nuk dimë",
    "për të",
    "asnjë",
    "të keqe\".",
    "Tha",
    "gruaja",
    "e Azizit",
    "\"Tani",
    "doli në pah",
    "e vërteta",
    "unë",
    "e josha atë",
    "nga",
    "vetja e tij",
    "dhe vërtet ai (është)",
    "nga",
    "të sinqertët\"."
  ],
  "52": [
    "Kjo",
    "që ai të dijë",
    "se vërtet unë",
    "nuk",
    "e tradhtova atë",
    "në fshehtësi",
    "dhe se vërtet",
    "Allahu",
    "nuk",
    "udhëzon",
    "dredhinë",
    "e mashtruesve."
  ],
  "53": [
    "Dhe nuk",
    "unë e shfajësoj",
    "veten time.",
    "Vërtet",
    "vetja (egoja)",
    "sigurisht është shumë urdhëruese",
    "për të keqen",
    "përveç",
    "atë që",
    "e mëshiroi",
    "Zoti im",
    "vërtet",
    "Zoti im (është)",
    "Falës i madh",
    "Mëshirues."
  ],
  "54": [
    "Dhe tha",
    "mbreti",
    "\"Ma sillni mua",
    "atë",
    "ta veçoj atë",
    "për veten time\".",
    "E kur",
    "i foli atij",
    "ai tha",
    "\"Vërtet ti",
    "sot",
    "tek ne",
    "(je) i autoritetshëm",
    "i besueshëm."
  ],
  "55": [
    "Ai (Jusufi) tha",
    "\"Më cakto mua",
    "në",
    "thesaret",
    "e vendit",
    "vërtet unë (jam)",
    "ruajtës",
    "i dijshëm\"."
  ],
  "56": [
    "Dhe kështu",
    "Ne i dhamë autoritet",
    "Jusufit",
    "në",
    "tokë",
    "të lëvizë",
    "në të",
    "kudo që",
    "ai dëshiron.",
    "Ne e mbulojmë",
    "me mëshirën Tonë",
    "atë që",
    "Ne dëshirojmë",
    "dhe nuk",
    "e humbim",
    "shpërblimin",
    "e bamirësve."
  ],
  "57": [
    "E sigurisht shpërblimi",
    "i vendbanimit të fundit",
    "(është) më i mirë",
    "për ata të cilët",
    "besuan",
    "dhe ishin",
    "duke u druajtur."
  ],
  "58": [
    "Dhe erdhën",
    "vëllezërit",
    "e Jusufit",
    "dhe hynë",
    "tek ai",
    "e ai i njohu ata",
    "ndërsa ata",
    "për të",
    "(ishin) mosnjohës."
  ],
  "59": [
    "Dhe pasi që",
    "ai i furnizoi ata",
    "me furnizimin e tyre",
    "tha",
    "\"Ma sillni mua",
    "një vëlla",
    "tuajin",
    "nga",
    "babai juaj",
    "a nuk",
    "ju shihni",
    "se vërtet unë",
    "përmbush",
    "barrën",
    "dhe unë (jam)",
    "më i miri",
    "i mikpritësve?\""
  ],
  "60": [
    "E nëse",
    "nuk",
    "ju ma sillni mua",
    "atë",
    "atëherë nuk (ka)",
    "barrë",
    "për ju",
    "tek unë",
    "dhe nuk",
    "ju afroheni tek unë."
  ],
  "61": [
    "Ata thanë",
    "\"Do të bëjmë çmos (për ta marrë)",
    "atë",
    "(nga) babai i tij",
    "dhe vërtet ne (jemi)",
    "gjithsesi veprues\"."
  ],
  "62": [
    "Dhe ai (u) tha",
    "shërbëtorëve të tij",
    "\"Vendosni",
    "mallin e tyre",
    "në",
    "ngarkesat e tyre",
    "ashtu që ata",
    "(do t'i) njohin ato",
    "kur",
    "ata kthehen",
    "tek",
    "familja e tyre",
    "mbase ata",
    "(do të) kthehen\"."
  ],
  "63": [
    "E pasi që",
    "ata u kthyen",
    "tek",
    "babai i tyre",
    "ata thanë",
    "\"O babai ynë",
    "u ndalua",
    "për ne",
    "barra",
    "prandaj dërgo",
    "me ne",
    "vëllain tonë",
    "që të furnizohemi",
    "dhe vërtet ne",
    "për të",
    "(do të jemi) ruajtës\"."
  ],
  "64": [
    "Ai tha",
    "\"A",
    "t'ju besoj ju",
    "për të",
    "përveçse",
    "siç",
    "ju besova ju",
    "për",
    "vëllain e tij",
    "nga",
    "përpara?!",
    "E Allahu (është)",
    "më i miri",
    "Mbrojtës",
    "dhe Ai (është)",
    "më i mëshirshmi",
    "i mëshiruesve\"."
  ],
  "65": [
    "E pasi që",
    "ata hapën",
    "gjërat e tyre",
    "ata gjetën",
    "mallin e tyre",
    "ishte kthyer",
    "tek ta.",
    "Ata thanë",
    "\"O babai ynë",
    "çfarë",
    "ne kërkojmë?",
    "Ky (është)",
    "malli ynë",
    "është kthyer",
    "tek ne",
    "dhe (do të) ushqejmë",
    "familjen tonë",
    "dhe (do të) ruajmë",
    "vëllain tonë",
    "dhe (do të) shtojmë",
    "një barrë",
    "deveje",
    "kjo (është)",
    "një sasi",
    "e lehtë\"."
  ],
  "66": [
    "Ai tha",
    "\"Nuk do të",
    "unë e dërgoj atë",
    "me ju",
    "derisa",
    "ju t'më jepni mua",
    "një besë",
    "nga",
    "Allahu",
    "(që) sigurisht do t'ma sillni mua",
    "atë",
    "përveçse",
    "që të",
    "rrethoheni",
    "ju\".",
    "E pasi që",
    "ata i dhanë atij",
    "besën e tyre",
    "ai tha",
    "\"Allahu",
    "për",
    "atë që",
    "ne themi",
    "(është) garantues\"."
  ],
  "67": [
    "Dhe ai tha",
    "\"O djemtë e mi",
    "mos",
    "hyni",
    "nga",
    "një derë",
    "e vetme",
    "por hyni",
    "nga",
    "dyer",
    "të ndryshme",
    "dhe nuk",
    "unë largoj",
    "prej jush",
    "nga",
    "Allahu",
    "asnjë",
    "gjë",
    "nuk (është)",
    "gjykimi",
    "përveç",
    "i Allahut",
    "tek Ai",
    "unë mbështetem",
    "dhe tek Ai",
    "le të mbështeten",
    "mbështetësit\"."
  ],
  "68": [
    "Dhe pasi që",
    "ata hynë",
    "nga",
    "aty ku",
    "i urdhëroi ata",
    "babai i tyre",
    "nuk",
    "ishte",
    "largonte",
    "prej tyre",
    "nga",
    "Allahu",
    "asnjë",
    "gjë",
    "përveç",
    "një nevoje",
    "në",
    "veten",
    "e Jakubit",
    "ai e shprehu atë",
    "e vërtet ai (është)",
    "posedues",
    "i dijes",
    "ngaae",
    "Ne e mësuam atë",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "dinë."
  ],
  "69": [
    "Dhe pasi që",
    "ata hynë",
    "tek",
    "Jusufi",
    "ai e strehoi",
    "tek ai",
    "vëllain e tij",
    "ai tha",
    "\"Vërtet unë",
    "unë (jam)",
    "vëllai yt",
    "prandaj mos",
    "u mërzit",
    "për atë çfarë",
    "ata ishin",
    "vepronin!\""
  ],
  "70": [
    "E pasi që",
    "ai ua përgatiti atyre",
    "furnizimin e tyre",
    "e vuri",
    "gotën",
    "në",
    "barrën",
    "e vëllait të tij",
    "pastaj",
    "thirri",
    "një thirrës",
    "\"O ju",
    "udhëtarë",
    "ju jeni",
    "vjedhës!\""
  ],
  "71": [
    "Ata thanë",
    "duke u kthyer",
    "nga ta",
    "\"Çka",
    "keni humbur?\""
  ],
  "72": [
    "Ata thanë",
    "\"Kemi humbur",
    "tasin",
    "e mbretit",
    "dhe kushdo që",
    "e sjell",
    "atë",
    "(ka) një barrë",
    "deveje",
    "dhe unë (jam)",
    "për këtë",
    "garantues\"."
  ],
  "73": [
    "Ata thanë",
    "\"Pasha Allahun",
    "sigurisht",
    "ju e dini",
    "nuk",
    "ne erdhëm",
    "që të prishim",
    "në",
    "tokë",
    "dhe nuk",
    "ne jemi",
    "vjedhës\"."
  ],
  "74": [
    "Ata thanë",
    "\"Po çfarë (është)",
    "ndëshkimi i tij",
    "nëse",
    "ju jeni",
    "gënjeshtarë?\""
  ],
  "75": [
    "Ata thanë",
    "\"Ndëshkimi i tij (është)",
    "kushdo që",
    "gjendet (tasi)",
    "në",
    "barrën e tij",
    "atëherë ai (është)",
    "ndëshkimi i tij (robërohet).",
    "Kështu",
    "ne i ndëshkojmë",
    "zullumqarët\"."
  ],
  "76": [
    "Atëherë ai filloi (kërkimin)",
    "në barrët e tyre",
    "para",
    "barrës",
    "së vëllait të tij",
    "pastaj",
    "e nxori atë",
    "nga",
    "barra",
    "e vëllait të tij.",
    "Kështu",
    "Ne planifikuam",
    "për Jusufin.",
    "Nuk",
    "ishte",
    "që ai ta marrë",
    "vëllain e tij",
    "në",
    "fenë",
    "e mbretit",
    "përveçse",
    "të",
    "dëshirojë",
    "Allahu.",
    "Ne i ngrisim",
    "(në) shkallë",
    "atë që",
    "Ne dëshirojmë",
    "e mbi",
    "çdo",
    "posedues",
    "të dijes",
    "(është) i Gjithëdijshëm."
  ],
  "77": [
    "Ata thanë",
    "\"Nëse",
    "ai vjedh",
    "atëherë, sigurisht",
    "ka vjedhur",
    "vëllai",
    "i tij",
    "nga",
    "përpara\".",
    "Por e fshehu atë",
    "Jusufi",
    "në",
    "veten e tij",
    "dhe nuk",
    "(ua) shpalosi atë",
    "atyre.",
    "Ai tha",
    "\"Ju (jeni)",
    "(në) më të keqen",
    "pozitë",
    "e Allahu",
    "di më mirë",
    "atë çfarë",
    "ju përshkruani\"."
  ],
  "78": [
    "Ata thanë",
    "\"O ti",
    "ministër",
    "vërtet",
    "ai ka",
    "një baba",
    "plak",
    "të moshuar",
    "prandaj merre",
    "ndonjërin nga ne",
    "në vend të tij",
    "vërtet ne",
    "të shohim ty",
    "nga",
    "punëmirët\"."
  ],
  "79": [
    "Ai tha",
    "\"Ruana",
    "Zot",
    "që",
    "ne të marrim",
    "përveçse",
    "atë që",
    "gjetëm",
    "sendin tonë",
    "tek ai",
    "vërtet ne",
    "atëherë",
    "(do të jemi) të padrejtë\"."
  ],
  "80": [
    "Pasi që",
    "e humbën shpresën plotësisht",
    "prej tij",
    "u veçuan",
    "duke pëshpëritur (mes vete).",
    "Tha",
    "i madhi i tyre",
    "\"A nuk",
    "e dini",
    "se",
    "babai juaj",
    "tashmë",
    "mori",
    "prej jush",
    "besë",
    "nga",
    "Allahu",
    "dhe nga",
    "përpara",
    "çfarë",
    "ju shkaktuat padrejtësi",
    "lidhur me",
    "Jusufin?!\"",
    "Pra, nuk do të",
    "unë lëviz",
    "(nga) vendi",
    "derisa",
    "t'më lejojë",
    "mua",
    "babai im",
    "apo",
    "të gjykojë",
    "Allahu",
    "për mua",
    "e Ai (është)",
    "më i miri",
    "i gjykatësve."
  ],
  "81": [
    "Kthehuni",
    "tek",
    "babai juaj",
    "dhe thuani",
    "\"O babai ynë",
    "vërtet",
    "biri yt",
    "vodhi",
    "dhe nuk",
    "ne dëshmuam",
    "përveçse",
    "për atë çfarë",
    "ne dinim",
    "dhe nuk",
    "ne jemi",
    "për të fshehtën",
    "ruajtës\"."
  ],
  "82": [
    "Dhe pyete",
    "vendbanimin",
    "të cilin",
    "ne ishim",
    "në të",
    "dhe karvanin",
    "të cilin",
    "ne erdhëm",
    "me të",
    "dhe vërtet ne (jemi)",
    "të sinqertë."
  ],
  "83": [
    "Ai tha",
    "\"Jo, por",
    "ua zbukuroi",
    "juve",
    "vetja juaj",
    "(këtë) çështje.",
    "E (unë po bëj) durim",
    "të mirë",
    "me shpresë që",
    "Allahu",
    "të",
    "m'i sjellë mua",
    "ata",
    "të gjithë",
    "vërtet Ai",
    "Ai (është)",
    "i Gjithëdijshmi",
    "i Urti\"."
  ],
  "84": [
    "Dhe ai ktheu shpinën",
    "nga ta",
    "dhe tha",
    "\"O mërzia ime",
    "për",
    "Jusufin",
    "dhe iu zbardhën",
    "dy sytë e tij",
    "nga",
    "pikëllimi",
    "dhe ai (ishte)",
    "i përmbajtur."
  ],
  "85": [
    "Ata thanë",
    "\"Pasha Allahun",
    "ti në vazhdimësi",
    "po e përmend",
    "Jusufin",
    "derisa",
    "do të jesh",
    "i sëmurë rëndë",
    "apo",
    "do të jesh",
    "nga",
    "të shkatërruarit\"."
  ],
  "86": [
    "Ai tha",
    "\"Vetëm",
    "e parashtroj",
    "dertin tim",
    "dhe pikëllimin tim",
    "tek",
    "Allahu",
    "dhe unë di",
    "prej",
    "Allahut",
    "atë çfarë",
    "nuk",
    "ju dini\"."
  ],
  "87": [
    "\"O bijtë e mi",
    "shkoni",
    "dhe hulumtoni",
    "për",
    "Jusufin",
    "dhe vëllain e tij",
    "dhe mos",
    "u zhgënjeni",
    "nga",
    "mëshira",
    "e Allahut",
    "vërtet",
    "nuk",
    "zhgënjehet",
    "nga",
    "mëshira",
    "e Allahut",
    "përveçse",
    "populli",
    "mohues\"."
  ],
  "88": [
    "Pasi që",
    "ata hynë",
    "tek ai",
    "thanë",
    "\"O ti",
    "ministër",
    "na goditi ne",
    "dhe familjen tonë",
    "skamja",
    "dhe ne erdhëm",
    "me një mall",
    "të paktë",
    "prandaj përmbushe",
    "për ne",
    "masën",
    "dhe jep lëmoshë",
    "për ne!",
    "Vërtet",
    "Allahu",
    "i shpërblen",
    "ata që japin lëmoshë\"."
  ],
  "89": [
    "Ai tha",
    "\"A",
    "e dini",
    "çfarë",
    "vepruat",
    "me Jusufin",
    "dhe vëllain e tij",
    "kur",
    "ju (ishit)",
    "të paditur?\""
  ],
  "90": [
    "Ata thanë",
    "\"A vërtet ti",
    "pikërisht ti (je)",
    "Jusufi?\"",
    "Ai tha",
    "\"Unë (jam)",
    "Jusufi",
    "dhe ky (është)",
    "vëllai im.",
    "Sigurisht",
    "dhuroi (mirësi)",
    "Allahu",
    "për ne",
    "vërtet",
    "kushdo që",
    "druhet",
    "dhe duron",
    "atëherë vërtet",
    "Allahu",
    "nuk",
    "e humb",
    "shpërblimin",
    "e bamirësve\"."
  ],
  "91": [
    "Ata thanë",
    "\"Pasha Allahun",
    "sigurisht",
    "të ka dhënë përparësi",
    "Allahu",
    "ndaj nesh",
    "dhe vërtet",
    "ne ishim",
    "mëkatarë\"."
  ],
  "92": [
    "Ai tha",
    "\"Nuk (ka)",
    "qortim",
    "për ju",
    "sot",
    "(jua) fal",
    "Allahu",
    "ju",
    "dhe Ai (është)",
    "më i mëshirshmi",
    "i mëshiruesve."
  ],
  "93": [
    "Shkoni",
    "me këmishën time",
    "këtë",
    "dhe hidheni atë",
    "mbi",
    "fytyrën",
    "e babait tim",
    "do të bëhet",
    "shikues",
    "dhe ejani tek unë",
    "me familjen tuaj",
    "të gjithë\"."
  ],
  "94": [
    "E pasi që",
    "u largua",
    "karvani",
    "tha",
    "babai i tyre",
    "\"Vërtet unë",
    "e ndiej",
    "erën",
    "e Jusufit",
    "sikur të mos",
    "të",
    "më konsideroni mua matuf!\""
  ],
  "95": [
    "Ata thanë",
    "\"Pasha Allahun",
    "vërtet ti (je)",
    "në",
    "gabimin tënd",
    "të vjetër\"."
  ],
  "96": [
    "Pasi",
    "që",
    "erdhi",
    "përgëzuesi",
    "e hodhi atë",
    "mbi",
    "fytyrën e tij",
    "e përnjëherë ai u kthye",
    "shikues.",
    "Ai tha",
    "\"A nuk",
    "(ju) thashë",
    "ju",
    "vërtet unë",
    "di",
    "nga",
    "Allahu",
    "atë çfarë",
    "nuk",
    "ju dini?!\""
  ],
  "97": [
    "Ata thanë",
    "\"O babai ynë",
    "kërko falje",
    "për ne",
    "(për) gjynahet tona",
    "vërtet ne",
    "ishim",
    "mëkatarë\"."
  ],
  "98": [
    "Ai tha",
    "\"Do të",
    "kërkoj falje",
    "për ju",
    "Zotit tim",
    "vërtet Ai",
    "Ai (është)",
    "Falësi i madh",
    "i Mëshirshmi\"."
  ],
  "99": [
    "Pasi që",
    "ata hynë",
    "tek",
    "Jusufi",
    "ai i afroi",
    "tek ai",
    "prindërit e tij",
    "dhe tha",
    "\"Hyni",
    "(në) Egjipt",
    "nëse",
    "dëshiron",
    "Allahu",
    "të sigurt\"."
  ],
  "100": [
    "Dhe ai i ngriti",
    "prindërit e tij",
    "mbi",
    "fron",
    "dhe ata ranë",
    "për të",
    "të përulur (në sexhde)",
    "dhe ai tha",
    "\"O babush",
    "ky (është)",
    "shpjegimi",
    "i ëndrrës sime",
    "nga",
    "përpara",
    "tashmë",
    "e bëri atë",
    "Zoti im",
    "të vërtetë",
    "dhe tashmë",
    "ai (më) dhuroi mirësi",
    "mua",
    "kur",
    "Ai më nxori mua",
    "nga",
    "burgu",
    "dhe (ju) solli",
    "ju",
    "nga",
    "shkretëtira",
    "nga",
    "pasi",
    "që",
    "futi ngatërresa",
    "shejtani",
    "mes meje",
    "dhe mes",
    "vëllezërve të mi.",
    "Vërtet",
    "Zoti im (është)",
    "i Butë",
    "për atë çfarë",
    "Ai dëshiron",
    "vërtet Ai",
    "Ai (është)",
    "i Gjithëdijshmi",
    "i Urti."
  ],
  "101": [
    "Zoti im",
    "tashmë",
    "Ti më dhurove mua",
    "nga",
    "pushteti",
    "dhe më mësove mua",
    "nga",
    "shpjegimi",
    "i ëndrrave",
    "Shpikës",
    "i qiejve",
    "dhe i tokës",
    "Ti (je)",
    "Mbrojtësi im",
    "në",
    "këtë botë",
    "dhe në të fundit",
    "më merr shpirtin mua",
    "si musliman",
    "dhe më bashko mua",
    "me të mirët!\""
  ],
  "102": [
    "Këto (janë)",
    "nga",
    "lajmet",
    "e së fshehtës",
    "Ne e shpallim atë",
    "tek ti",
    "dhe nuk",
    "ti ishe",
    "në mesin e tyre",
    "kur",
    "ata të gjithë vendosën",
    "çështjen e tyre",
    "dhe ata",
    "bënin dredhi."
  ],
  "103": [
    "Dhe nuk (janë)",
    "shumica",
    "e njerëzve",
    "edhe pse",
    "ti lakmon",
    "besimtarë."
  ],
  "104": [
    "Dhe nuk",
    "ti ua kërkon atyre",
    "për të",
    "asnjë",
    "shpërblim.",
    "Nuk (është)",
    "ai (Kur'ani)",
    "përveçse",
    "përkujtim",
    "për botët."
  ],
  "105": [
    "E sa e sa",
    "nga",
    "dëshmi",
    "(ka) në",
    "qiej",
    "dhe tokë",
    "ata kalojnë",
    "pranë tyre",
    "dhe ata",
    "ndaj tyre",
    "(janë) refuzues."
  ],
  "106": [
    "Dhe nuk",
    "beson",
    "shumica e tyre",
    "në Allahun",
    "përveçse",
    "duke qenë ata",
    "idhujtarë."
  ],
  "107": [
    "Vallë, a u siguruan ata",
    "që",
    "(nuk) do t'ju vijë atyre",
    "një mbulim",
    "nga",
    "dënimi",
    "i Allahut",
    "apo",
    "do t'ju vijë atyre",
    "Kijameti",
    "papritur",
    "duke qenë ata",
    "nuk",
    "ndiejnë?!"
  ],
  "108": [
    "Thuaj",
    "\"Kjo (është)",
    "rruga ime",
    "unë ftoj",
    "tek",
    "Allahu",
    "me",
    "vigjilencë",
    "unë",
    "dhe kushdo që",
    "më ndjek mua.",
    "Dhe i pa të metë (është)",
    "Allahu",
    "dhe nuk",
    "unë (jam)",
    "nga",
    "idhujtarët\"."
  ],
  "109": [
    "Dhe nuk",
    "Ne dërguam",
    "nga",
    "para teje",
    "përveçse",
    "burra",
    "Ne u shpallim",
    "atyre",
    "nga",
    "banorët",
    "e vendbanimeve.",
    "Vallë, a nuk",
    "ata udhëtojnë",
    "në",
    "tokë",
    "e të shohin",
    "si",
    "ishte",
    "fundi",
    "i atyre të cilët",
    "nga",
    "para tyre?",
    "E sigurisht shtëpia",
    "e fundit",
    "(është) më e mirë",
    "për ata të cilët",
    "janë druajtur",
    "vallë, a nuk",
    "logjikoni?!"
  ],
  "110": [
    "Deri",
    "kur",
    "e humbën shpresën",
    "të dërguarit",
    "dhe u bindën",
    "se vërtet ata",
    "tashmë",
    "u përgënjeshtruan",
    "(u) erdhi atyre",
    "ndihma Jonë",
    "e u shpëtua",
    "ai që",
    "Ne dëshiruam",
    "dhe nuk",
    "zmbrapset",
    "dënimi Ynë",
    "nga",
    "populli",
    "kriminel."
  ],
  "111": [
    "Sigurisht",
    "ishte",
    "në",
    "rrëfimet e tyre",
    "mësim",
    "për poseduesit",
    "e mendjeve.",
    "Nuk",
    "është ai (Kur'ani)",
    "një bisedë",
    "e trilluar",
    "mirëpo",
    "vërtetim",
