{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Zbritja",
    "e Librit",
    "(është) prej",
    "Allahut",
    "të Pamposhturit",
    "të Urtit."
  ],
  "3": [
    "Nuk",
    "Ne krijuam",
    "qiejt",
    "dhe tokën",
    "dhe çka (është)",
    "mes atyre dyjave",
    "përveçse",
    "me të vërtetën",
    "dhe afat",
    "të përcaktuar.",
    "E ata të cilët",
    "mohuan",
    "ndaj asaj çfarë",
    "u paralajmëruan",
    "(janë) refuzues."
  ],
  "4": [
    "Thuaj",
    "\"A e shihni ju",
    "atë çfarë",
    "ju adhuroni",
    "nga",
    "përveç",
    "Allahut",
    "më tregoni mua",
    "çfarë",
    "ata krijuan",
    "prej",
    "tokës",
    "apo",
    "ata kanë",
    "pjesëmarrje",
    "në",
    "qiejt?",
    "Më sillni",
    "(ndonjë) libër",
    "nga",
    "para",
    "këtij",
    "apo",
    "(ndonjë) gjurmë",
    "nga",
    "(ndonjë) dije",
    "nëse",
    "ju jeni",
    "të sinqertë\"."
  ],
  "5": [
    "E kush (është)",
    "më i devijuar",
    "sesa ai i cili",
    "(i) lutet",
    "nga",
    "përveç",
    "Allahut",
    "atij që",
    "nuk",
    "i përgjigjet",
    "atij",
    "deri në",
    "ditën",
    "e Kijametit",
    "dhe ata",
    "nga",
    "lutja e tyre",
    "(janë) të shkujdesur."
  ],
  "6": [
    "E kur",
    "të tubohen",
    "njerëzit",
    "ata do të jenë",
    "për ta",
    "armiq",
    "dhe ata do të jenë",
    "ndaj adhurimit të tyre",
    "mohues."
  ],
  "7": [
    "E kur",
    "(u) lexohen",
    "atyre",
    "ajetet Tona",
    "të qarta",
    "thonë",
    "ata të cilët",
    "mohuan",
    "për të vërtetën",
    "pasi që",
    "(u) erdhi atyre",
    "\"Kjo (është)",
    "(një) magji",
    "e qartë!\""
  ],
  "8": [
    "Vallë a",
    "ata thonë",
    "\"Ai e trilloi atë\".",
    "Thuaj",
    "\"Nëse",
    "unë e trillova atë",
    "atëherë nuk",
    "ju keni fuqi",
    "për mua",
    "karshi",
    "Allahut",
    "asgjë.",
    "Ai (është)",
    "më i dijshëm",
    "për atë çfarë",
    "ju shtoni",
    "për Të.",
    "Mjafton",
    "Ai",
    "(si) Dëshmues",
    "mes meje",
    "dhe mes jush",
    "dhe Ai (është)",
    "Falësi i madh",
    "Mëshiruesi\"."
  ],
  "9": [
    "Thuaj",
    "\"Nuk",
    "unë jam",
    "ftillues",
    "prej",
    "të dërguarve",
    "dhe nuk",
    "unë di",
    "çfarë",
    "(do të) bëhet",
    "me mua",
    "dhe as",
    "me ju.",
    "Nuk",
    "unë ndjek",
    "përveçse",
    "atë çfarë",
    "(më) shpallet",
    "mua",
    "dhe nuk",
    "unë (jam)",
    "përveçse",
    "paralajmërues",
    "i qartë\"."
  ],
  "10": [
    "Thuaj",
    "\"Si mendoni ju",
    "nëse",
    "ai është (Kur'ani)",
    "nga",
    "ana",
    "e Allahut",
    "dhe ju e mohoni",
    "atë",
    "dhe dëshmon",
    "(një) dëshmues",
    "nga",
    "bijtë",
    "e Israilit",
    "për",
    "të ngjashmin me të",
    "e ai besoi",
    "ndërkohë që ju ishit mendjemëdhenj?\"",
    "Vërtet",
    "Allahu",
    "nuk",
    "udhëzon",
    "popullin",
    "zullumqar."
  ],
  "11": [
    "Dhe (u) thanë",
    "ata të cilët",
    "mohuan",
    "atyre të cilët",
    "besuan",
    "\"Sikur",
    "të ishte (Kur'ani)",
    "një mirësi",
    "nuk",
    "ata do t'na paraprinin ne",
    "tek ai.",
    "E duke qenë se",
    "nuk",
    "ata udhëzohen",
    "me të",
    "ata do të thonë",
    "\"Ky (është)",
    "një trillim",
    "i lashtë!\""
  ],
  "12": [
    "Dhe nga",
    "para tij",
    "(ishte) libri",
    "i Musait",
    "udhëheqës",
    "dhe mëshirë.",
    "Dhe ky (është)",
    "një Libër",
    "vërtetues",
    "(në) gjuhë",
    "arabe",
    "që t'i paralajmërojë",
    "ata të cilët",
    "bënë zullum",
    "dhe përgëzim",
    "për bamirësit."
  ],
  "13": [
    "Vërtet",
    "ata të cilët",
    "thanë",
    "\"Zoti ynë (është)",
    "Allahu\"",
    "pastaj",
    "s'u tundën nga istikami",
    "atëherë nuk (ka)",
    "frikë",
    "për ta",
    "dhe as",
    "ata",
    "(do të) pikëllohen."
  ],
  "14": [
    "Të tillët (janë)",
    "banorët",
    "e xhenetit",
    "të gjithmonshëm",
    "në të",
    "(si) këmbim",
    "për atë çfarë",
    "ata ishin",
    "duke punuar."
  ],
  "15": [
    "Dhe Ne e porositëm",
    "njeriun",
    "me prindërit e tij",
    "(për) bamirësi.",
    "E barti atë",
    "nëna e tij",
    "(me) mundim",
    "dhe e lindi atë",
    "(me) mundim.",
    "E bartja e tij",
    "dhe gjidhënia e tij",
    "(është) tridhjetë",
    "muaj",
    "deri",
    "kur",
    "ai arrin",
    "pjekurinë e tjj",
    "dhe arrin",
    "dyzet",
    "vjet",
    "thotë",
    "\"Zoti im!",
    "Më fuqizo",
    "që të",
    "falënderoj",
    "(për) begatinë Tënde",
    "(me) të cilën",
    "Ti (më) begatove",
    "mua",
    "dhe [mbi]",
    "prindërit e mi",
    "dhe që të",
    "punoj",
    "vepër të mirë",
    "Ti e pëlqen atë",
    "dhe përmirësoji",
    "për mua",
    "mes",
    "pasardhësve të mi.",
    "Vërtet unë",
    "pendohem",
    "tek Ti",
    "dhe vërtet unë (jam)",
    "prej",
    "muslimanëve\"."
  ],
  "16": [
    "Të tillët (janë)",
    "ata të cilët",
    "Ne e pranojmë",
    "prej tyre",
    "më të mirën",
    "e asaj çfarë",
    "ata punuan",
    "dhe kapërcejmë",
    "nga",
    "të këqijat e tyre",
    "në mesin e",
    "banorëve",
    "të xhenetit.",
    "Premtim",
    "i sinqertë",
    "(me) të cilin",
    "ata janë",
    "premtuar."
  ],
  "17": [
    "Kurse ai i cili",
    "(u) tha",
    "prindërve të tij",
    "\"Uh",
    "për ju dy!",
    "A ju dy më premtoni mua",
    "se",
    "unë do të nxirem",
    "ndërkohë që",
    "kanë kaluar",
    "brezat",
    "nga",
    "para meje?!\"",
    "E ata dy",
    "i përgjërohen",
    "Allahut",
    "\"I mjeri ti!",
    "Beso!",
    "Vërtet",
    "premtimi",
    "i Allahut",
    "(është) i vërtetë\"",
    "e ai thotë",
    "\"Nuk (është)",
    "kjo",
    "përveçse",
    "mitet",
    "e të parëve\"."
  ],
  "18": [
    "Të tillët (janë)",
    "ata të cilët",
    "u përligj",
    "kundër tyre",
    "fjala",
    "në mesin e",
    "popujve",
    "tashmë",
    "kanë kaluar",
    "nga",
    "para tyre",
    "prej",
    "xhinëve",
    "dhe njerëzve",
    "vërtet ata",
    "ishin",
    "të humbur."
  ],
  "19": [
    "E për secilin (ka)",
    "shkalla",
    "sipas asaj çfarë",
    "ata punuan",
    "dhe që Ne t'ua përmbushim atyre",
    "veprat e tyre",
    "e atyre",
    "nuk",
    "u bëhet zullum."
  ],
  "20": [
    "E ditën kur",
    "paraqiten",
    "ata të cilët",
    "mohuan",
    "para",
    "zjarrit",
    "\"Ju i harxhuat",
    "të mirat tuaja",
    "në",
    "jetën tuaj",
    "të kësaj bote",
    "dhe u kënaqët",
    "me to",
    "prandaj sot",
    "(do të) kompensoheni",
    "(me) dënimin",
    "e nënçmimit",
    "për shkak të asaj çfarë",
    "ju ishit",
    "shfaqnit mendjemadhësi",
    "në",
    "tokë",
    "pa",
    "të drejtë",
    "dhe për shkak të asaj çfarë",
    "ju ishit",
    "bënit ligësi\"."
  ],
  "21": [
    "Dhe kujtoje",
    "vëllain",
    "e Adit",
    "kur",
    "ai paralajmëroi",
    "popullin e tij",
    "në Ahkaf",
    "ndërkohë që tashmë",
    "kanë kaluar",
    "paralajmërimet",
    "nga",
    "para",
    "vetes së tij",
    "dhe nga",
    "pas tij",
    "\"Që të mos",
    "adhuroni",
    "përveçse",
    "Allahun",
    "vërtet unë",
    "frikësohem",
    "për ju",
    "dënimit",
    "të një dite",
    "të madhe\"."
  ],
  "22": [
    "Ata thanë",
    "\"A ti ke ardhur tek ne",
    "për t'na zmbrapsur ne",
    "nga",
    "zotat tanë?!",
    "Pra, na sjell",
    "atë çfarë",
    "na kërcënon ne",
    "nëse",
    "ti je",
    "nga",
    "të sinqertët\"."
  ],
  "23": [
    "Ai tha",
    "\"Vetëm",
    "dija",
    "(është) tek",
    "Allahu",
    "kurse unë ua kumtoj",
    "atë çfarë",
    "jam dërguar",
    "me të",
    "mirëpo unë",
    "ju shoh",
    "një popull",
    "(që) nuk dini\"."
  ],
  "24": [
    "E pasi që",
    "ata e panë atë",
    "(si) një re",
    "e drejtuar nga",
    "luginat e tyre",
    "thanë",
    "\"Kjo (është)",
    "një re",
    "lëshuese e shiut për ne\".",
    "Jo por",
    "ajo (është)",
    "çfarë",
    "ju nxituat",
    "me të",
    "një erë",
    "në të (është)",
    "një dënim",
    "i dhembshëm."
  ],
  "25": [
    "Ajo shkatërron",
    "çdo",
    "gjë",
    "me urdhrin",
    "e Zotit të saj",
    "e ata u gdhinë",
    "nuk",
    "shihet",
    "përveçse",
    "vendbanimeve të tyre.",
    "Kështu",
    "Ne e dënojmë",
    "popullin",
    "kriminel."
  ],
  "26": [
    "Dhe sigurisht",
    "Ne ua mundësuam atyre",
    "atë çfarë",
    "nuk",
    "ua mundësuam juve",
    "atë",
    "dhe Ne u dhamë",
    "atyre",
    "dëgjim",
    "dhe shikime",
    "dhe zemra",
    "por nuk",
    "u bëri dobi",
    "atyre",
    "dëgjimi i tyre",
    "dhe as",
    "shikimet e tyre",
    "dhe as",
    "zemrat e tyre",
    "asnjë",
    "gjë",
    "kur",
    "ata ishin",
    "i mohonin",
    "argumentet",
    "e Allahut",
    "dhe i mbërtheu",
    "ata",
    "ajo çfarë",
    "ata ishin",
    "me të",
    "talleshin."
  ],
  "27": [
    "Dhe sigurisht",
    "Ne shkatërruam",
    "atë çfarë (ishte)",
    "përreth jush",
    "nga",
    "vendbanimet",
    "dhe Ne i shkoqitëm",
    "argumentet",
    "ashtu që ata",
    "të kthehen."
  ],
  "28": [
    "E përse nuk",
    "i ndihmuan ata",
    "ata të cilët",
    "i morën",
    "nga",
    "pos",
    "Allahut",
    "si afrim",
    "zota?",
    "Jo por",
    "ata devijuan",
    "prej tyre",
    "dhe ajo (është)",
    "shpifja e tyre",
    "dhe ajo çfarë",
    "ata ishin",
    "trillonin."
  ],
  "29": [
    "Dhe kur",
    "Ne i drejtuam",
    "tek ti",
    "një grup",
    "prej",
    "xhinëve",
    "të dëgjojnë",
    "Kur'anin",
    "e pasi që",
    "ata prezantuan tek ai",
    "thanë",
    "\"Heshtni!\"",
    "E pasi që",
    "mbaroi",
    "ata u kthyen",
    "tek",
    "populli i tyre",
    "si paralajmërues."
  ],
  "30": [
    "Ata thanë",
    "\"O populli ynë",
    "vërtet ne",
    "dëgjuam",
    "një Libër",
    "është zbritur",
    "nga",
    "pas",
    "Musait",
    "vërtetues",
    "i asaj çfarë (është)",
    "para",
    "duarve të tij",
    "ai udhëzon",
    "tek",
    "e vërteta",
    "dhe tek",
    "një rrugë",
    "e drejtë\"."
  ],
  "31": [
    "\"O populli ynë",
    "përgjigjuni",
    "thirrësit",
    "të Allahut",
    "dhe besoni",
    "në Të!",
    "Ai do t'ju falë",
    "ju",
    "nga",
    "mëkatet tuaja",
    "dhe do t'ju strehojë",
    "nga",
    "një dënim",
    "i dhembshëm."
  ],
  "32": [
    "E kushdo që",
    "nuk",
    "i përgjigjet",
    "thirrësit",
    "të Allahut",
    "atëherë ai nuk është",
    "i pamposhtur",
    "në",
    "tokë",
    "dhe nuk ka",
    "për të",
    "nga",
    "pos Tij",
    "mbrojtës.",
    "Të tillët (janë)",
    "në",
    "një humbje",
    "të qartë."
  ],
  "33": [
    "Vallë a nuk",
    "ata shohin",
    "se",
    "Allahu",
    "i Cili",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "dhe nuk",
    "është lodhur",
    "me krijimin e tyre",
    "(është) i Aftë",
    "për",
    "t'i",
    "ngjallë",
    "të vdekurit?!",
    "Sigurisht",
    "vërtet Ai",
    "për",
    "çdo",
    "gjë",
    "(është) i Plotfuqishëm."
  ],
  "34": [
    "E ditën kur",
    "(do të) paraqiten",
    "ata të cilët",
    "mohuan",
    "para",
    "zjarrit",
    "\"A nuk është",
    "kjo",
    "e vërteta?!\"",
    "Ata do të thonë",
    "\"Sigurisht",
    "pasha Zotin tonë!\"",
    "Ai do të thotë",
    "\"Pra, shijojeni",
    "dënimin",
    "për shkak të asaj çfarë",
    "ju ishit",
    "mohuat\"."
  ],
  "35": [
    "Prandaj duro",
    "ashtu siç",
    "duruan",
    "poseduesit",
    "e vendosmërisë",
    "nga",
    "të dërguarit",
    "dhe mos",
    "kërko shpejtim (të dënimit)",
    "për ta!",
    "Duket sikur ata",
    "ditën kur",
    "shohin",
    "atë çfarë",
    "atyre u premtohet",
    "nuk",
    "qëndruan",
    "përveçse",
    "një çast",
    "prej",
    "një dite.",
    "Kumtesë!",
    "Vallë, a",
    "shkatërrohet",
    "përveçse",
    "populli",
    "i prishur!"
  ]
}