{
  "1": [
    "Kef, He, Je, Ajn, Sád."
  ],
  "2": [
    "Përmendje",
    "e mëshirës",
    "së Zotit tënd",
    "(për) robin e Vet",
    "Zekerija."
  ],
  "3": [
    "Kur",
    "ai e thirri",
    "Zotin e vet",
    "(me) një thirrje",
    "të fshehtë."
  ],
  "4": [
    "Ai tha",
    "\"“Im Zot",
    "vërtet unë",
    "u dobësuan",
    "eshtrat",
    "prej meje",
    "m’u zbardh",
    "koka",
    "nga thinjat",
    "dhe nuk",
    "kam qenë asnjëherë",
    "me lutjen ndaj Teje",
    "im Zot",
    "i zhgënjyer"
  ],
  "5": [
    "E, vërtet unë",
    "u frikësohem",
    "të afërmve të mi",
    "nga",
    "pas meje",
    "ndërsa ishte",
    "bashkëshortja ime",
    "shterpë",
    "andaj, dhuro",
    "për mua",
    "nga",
    "ana Jote",
    "një pasardhës"
  ],
  "6": [
    "ai më trashëgon mua",
    "dhe trashëgon",
    "nga",
    "familja",
    "e Jakubit",
    "dhe bëje atë",
    "Zoti im",
    "të pëlqyeshëm!"
  ],
  "7": [
    "“O Zekerija",
    "padyshim Ne",
    "të përgëzojmë ty",
    "me një djalë",
    "emri i tij (është)",
    "Jahja",
    "nuk",
    "kemi dhënë",
    "për të",
    "nga",
    "më parë",
    "(këtë) emër."
  ],
  "8": [
    "Ai tha",
    "\"Zoti im",
    "si",
    "(mund) të jetë",
    "për mua",
    "një djalë",
    "ndërsa, ka qenë (gjithmonë)",
    "bashkëshortja ime",
    "shterpë",
    "e tani",
    "unë kam arritur",
    "prej (në)",
    "pleqërinë",
    "e thellë."
  ],
  "9": [
    "Ai tha",
    "\"Ja, kështu",
    "tha",
    "Zoti yt.",
    "Ai (është)",
    "për Mua",
    "e lehtë",
    "e padushim",
    "Unë të krijova ty",
    "prej",
    "më parë",
    "ndërsa nuk",
    "ishe ti",
    "asgjë."
  ],
  "10": [
    "Ai tha",
    "\"Zoti im",
    "bëj",
    "për mua",
    "një shenjë!\"",
    "Ai tha",
    "“Shenja jote (është)",
    "(që) të mos",
    "u flasësh",
    "njerëzve",
    "(për) tri",
    "net",
    "rresht!\""
  ],
  "11": [
    "Atëherë ai doli",
    "para",
    "popullit të tij",
    "nga",
    "vendfalja",
    "dhe u dha sinjal",
    "atyre",
    "që",
    "\"Lartësojeni (Allahun)",
    "(në) mëngjes",
    "dhe (në) mbrëmje!\""
  ],
  "12": [
    "\"O Jahja!",
    "Rroke",
    "Librin",
    "fuqishëm!\"",
    "Dhe Ne i dhamë atij",
    "urtësi",
    "(kur ai ishte) fëmijë."
  ],
  "13": [
    "Dhe dhembshuri",
    "prej",
    "Nesh",
    "dhe dëlirësi",
    "dhe ai ishte",
    "i devotshëm."
  ],
  "14": [
    "Dhe bamirës",
    "ndaj prindërve të tij",
    "dhe nuk",
    "ai ishte",
    "tiran",
    "mosbindës."
  ],
  "15": [
    "Dhe paqe",
    "mbi të",
    "ditën kur",
    "ai u lind",
    "dhe ditën kur",
    "ai vdes",
    "dhe ditën kur",
    "ai do të ringjallet",
    "i gjallë."
  ],
  "16": [
    "Dhe përmende",
    "në",
    "Libër",
    "Merjemin",
    "kur",
    "ajo u tërhoq",
    "nga",
    "familja e saj",
    "(në) një vend",
    "lindor."
  ],
  "17": [
    "Atëherë ajo mori",
    "nga",
    "ndaj tyre",
    "një perde",
    "e Ne dërguam",
    "tek ajo",
    "Shpirtin Tonë (Xhibrilin)",
    "e ai iu pasqyrua",
    "asaj",
    "(si) një njeri",
    "i formuar."
  ],
  "18": [
    "Ajo tha",
    "\"Vërtet unë",
    "(i) mbështetem",
    "të Gjithëmëshirshmit",
    "nga ti",
    "nëse",
    "ti je",
    "i devotshëm\"."
  ],
  "19": [
    "Ai tha",
    "\"Vetëm",
    "unë (jam)",
    "i dërguari",
    "i Zotit tënd",
    "që të të dhuroj",
    "ty",
    "një djalë",
    "të dëlirë\"."
  ],
  "20": [
    "Ajo tha",
    "\"Si (mund)",
    "të jetë",
    "për mua",
    "një djalë",
    "ndërkohë që nuk",
    "më preku mua",
    "(asnjë) njeri",
    "dhe nuk",
    "unë jam",
    "lavire?\""
  ],
  "21": [
    "Ai tha",
    "\"Kështu",
    "tha",
    "Zoti yt",
    "ajo (është)",
    "për mua",
    "e lehtë",
    "dhe që Ne ta bëjmë atë",
    "një shenjë",
    "për njerëzit",
    "dhe mëshirë",
    "nga Ne",
    "dhe (kjo) është",
    "një çështje",
    "e dekretuar'\"."
  ],
  "22": [
    "Kështu ajo mbeti shtatzënë me të",
    "e u tërhoq",
    "me të",
    "(në) një vend",
    "të largët."
  ],
  "23": [
    "Atëherë e çuan atë",
    "dhembjet e lindjes",
    "tek",
    "trungu",
    "i palmës.",
    "Ajo tha",
    "\"Ah sikur",
    "unë të vdisja",
    "para",
    "kësaj",
    "dhe të isha",
    "harresë",
    "e harruar!\""
  ],
  "24": [
    "Atëherë ai e thirri atë",
    "nga",
    "poshtë saj",
    "\"Që të mos",
    "pikëllohesh",
    "sigurisht",
    "ka bërë",
    "Zoti yt",
    "poshtë teje",
    "një rrjedhë."
  ],
  "25": [
    "Dhe tunde",
    "nga ti",
    "trungun",
    "e palmës",
    "do të bien",
    "mbi ty",
    "hurma të freskëta",
    "të pjekura."
  ],
  "26": [
    "Pra, ha",
    "dhe pi",
    "dhe qetësoji",
    "sytë (e tu)!",
    "E nëse",
    "ti sheh",
    "nga",
    "njerëzit",
    "ndonjë",
    "Atëherë thuaj",
    "\"Vërtet unë",
    "(i) kushtova",
    "të Gjithëmëshirshmit",
    "një agjërim",
    "prandaj nuk",
    "do t'i flas",
    "sot",
    "(asnjë) njeriu!\""
  ],
  "27": [
    "Atëherë ajo erdhi",
    "me të",
    "tek populli i saj",
    "duke e mbajtur atë.",
    "Ata thanë",
    "\"Oj Merjem!",
    "vërtet",
    "ti paske sjellë",
    "një gjë",
    "të padëgjuar!"
  ],
  "28": [
    "Oj motër",
    "e Harunit!",
    "Nuk",
    "ishte",
    "babai yt",
    "një burrë",
    "i keq",
    "dhe nuk",
    "ishte",
    "nëna jote",
    "e pandershme\"."
  ],
  "29": [
    "Atëherë ajo bëri me shenjë",
    "nga ai.",
    "Ata thanë",
    "\"Si",
    "t'i flasim",
    "atij që",
    "është",
    "në",
    "djep",
    "një foshnjë?!\""
  ],
  "30": [
    "Ai tha",
    "\"Vërtet unë (jam)",
    "rob",
    "i Allahut",
    "Ai më dha mua",
    "Librin",
    "dhe më bëri mua",
    "pejgamber."
  ],
  "31": [
    "Dhe Ai më bëri mua",
    "të bekuar",
    "kudo",
    "që",
    "unë të jem",
    "dhe Ai më porositi mua",
    "me namaz",
    "dhe zeqat",
    "për aq",
    "sa të jem",
    "gjallë."
  ],
  "32": [
    "Dhe (të jem) i sjellshëm",
    "me nënën time",
    "dhe nuk",
    "Ai më bëri mua",
    "kryelartë",
    "fatzi."
  ],
  "33": [
    "Dhe paqja (qoftë)",
    "mbi mua",
    "ditën kur",
    "unë u linda",
    "dhe ditën kur",
    "unë (do) të vdes",
    "dhe ditën kur",
    "unë (do) të ngritem",
    "i gjallë\"."
  ],
  "34": [
    "Ai (ishte)",
    "Isai",
    "biri",
    "i Merjemes",
    "fjalë",
    "e së vërtetës",
    "(për) të cilën",
    "rreth saj",
    "ata dyshojnë."
  ],
  "35": [
    "Nuk",
    "është",
    "për Allahun",
    "që",
    "Ai të marrë",
    "asnjë",
    "fëmijë.",
    "Lartësuar qoftë Ai!",
    "Kur",
    "Ai dekreton",
    "një çështje",
    "atëherë vetëm",
    "Ai thotë",
    "për të",
    "\"Ji!",
    "e ajo menjëherë është\"."
  ],
  "36": [
    "\"Dhe vërtet",
    "Allahu",
    "(është) Zoti im",
    "dhe Zoti juaj",
    "prandaj adhurojeni Atë!",
    "Kjo (është)",
    "një rrugë",
    "e drejtë\"."
  ],
  "37": [
    "Por u përçanë",
    "palët",
    "nga",
    "mes tyre",
    "prandaj mjerë",
    "për ata të cilët",
    "mohuan",
    "nga",
    "pamja",
    "e një dite",
    "të madhe."
  ],
  "38": [
    "Sa mirë do të dëgjojnë",
    "ata",
    "dhe sa mirë do të shohin",
    "ditën kur",
    "ata do të vijnë tek Ne",
    "mirëpo",
    "zullumqarët",
    "sot",
    "(janë) në",
    "një gabim",
    "të qartë."
  ],
  "39": [
    "Dhe paralajmëroji ata",
    "(për) ditën",
    "e mërzisë",
    "kur",
    "u vendos",
    "çështja",
    "e ata",
    "(janë) në",
    "shkujdesje",
    "dhe ata",
    "nuk",
    "besojnë."
  ],
  "40": [
    "Vërtet Ne",
    "Ne",
    "e trashëgojmë",
    "tokën",
    "dhe këdo që",
    "(është) mbi të",
    "dhe tek Ne",
    "ata do të kthehen."
  ],
  "41": [
    "Dhe përmende",
    "në",
    "Libër",
    "Ibrahimin!",
    "Vërtet ai",
    "ishte",
    "besnik",
    "pejgamber."
  ],
  "42": [
    "Kur",
    "ai (i) tha",
    "babait të tij",
    "\"O babush",
    "Përse",
    "adhuron",
    "atë që",
    "nuk",
    "dëgjon",
    "dhe nuk",
    "shikon",
    "dhe nuk",
    "vlen",
    "për ty",
    "gjë?"
  ],
  "43": [
    "O babush!",
    "Vërtet mua",
    "tashmë",
    "(më) erdhi mua",
    "prej",
    "dijes",
    "çfarë",
    "nuk",
    "të erdhi ty",
    "prandaj më ndiq mua",
    "unë do të të udhëzoj ty",
    "(në) një rrugë",
    "të drejtë."
  ],
  "44": [
    "O babush!",
    "Mos",
    "adhuro",
    "djallin!",
    "Vërtet",
    "djalli",
    "është",
    "ndaj të Gjithëmëshirshmit",
    "rebel."
  ],
  "45": [
    "O babush!",
    "Vërtet unë",
    "frikësohem",
    "që",
    "do të të prek ty",
    "një dënim",
    "nga",
    "i Gjithëmëshirshmi",
    "e ti të jesh",
    "për djallin",
    "një shok\"."
  ],
  "46": [
    "Ai tha",
    "\"A largues",
    "(je) ti",
    "nga",
    "zotat e mi",
    "o Ibrahim?",
    "Sigurisht nëse",
    "nuk",
    "ti ndalesh",
    "gjithsesi unë do të të gurëzoj ty",
    "ik prej meje",
    "(për) një kohë të gjatë\"."
  ],
  "47": [
    "Ai tha",
    "\"Paqe (qoftë)",
    "mbi ty!",
    "Unë do të kërkoj falje",
    "për ty",
    "Zotit tim.",
    "Vërtet Ai",
    "është",
    "me mua",
    "shumë bamirës."
  ],
  "48": [
    "Dhe unë do të largohem nga ju",
    "dhe çfarë",
    "ju luteni",
    "nga",
    "pos",
    "Allahut",
    "kurse unë do t'i lutem",
    "Zotit tim",
    "mbase",
    "të mos",
    "jem",
    "në lutjen",
    "ndaj Zotit tim",
    "i mjerë\"."
  ],
  "49": [
    "E pasi që",
    "Ai u largua prej tyre",
    "dhe çfarë",
    "ata adhuronin",
    "nga",
    "pos",
    "Allahut",
    "Ne i dhuruam",
    "atij",
    "Is'hakun",
    "dhe Jakubin",
    "dhe secilin (prej tyre)",
    "Ne e bëmë",
    "pejgamber."
  ],
  "50": [
    "Dhe Ne u dhuruam",
    "atyre",
    "nga",
    "mëshira Jonë",
    "dhe bëmë",
    "për ta",
    "një përmendje",
    "të sinqertë",
    "të lartë."
  ],
  "51": [
    "Dhe përmende",
    "në",
    "Libër",
    "Musain!",
    "Vërtet ai",
    "ishte",
    "i përzgjedhur",
    "dhe ishte",
    "një i dërguar",
    "pejgamber."
  ],
  "52": [
    "Dhe Ne e thirrëm atë",
    "nga",
    "ana",
    "e malit Tur",
    "e djathtë",
    "dhe e afruam atë",
    "(për) bisedë."
  ],
  "53": [
    "Dhe Ne i dhuruam",
    "atij",
    "nga",
    "mëshira Jonë",
    "vëllain e tij",
    "Harunin",
    "pejgamber."
  ],
  "54": [
    "Dhe përmende",
    "në",
    "Libër",
    "Ismailin!",
    "Vërtet ai",
    "ishte",
    "i sinqertë",
    "(në) premtim",
    "dhe ishte",
    "një i dërguar",
    "pejgamber."
  ],
  "55": [
    "Dhe ai në vazhdimësi",
    "e urdhëronte",
    "familjen e tij",
    "për namaz",
    "dhe zeqat",
    "dhe ai ishte",
    "pranë",
    "Zotit të tij",
    "i kënaqur."
  ],
  "56": [
    "Dhe përmende",
    "në",
    "Libër",
    "Idrisin.",
    "Vërtet ai",
    "ishte",
    "shumë i sinqertë",
    "pejgamber."
  ],
  "57": [
    "Dhe Ne e ngritëm atë",
    "(në) një vend",
    "të lartë."
  ],
  "58": [
    "Ata (janë)",
    "të cilët",
    "i begatoi",
    "Allahu",
    "ata",
    "nga mesi i",
    "pejgamberëve",
    "nga",
    "pasardhësit",
    "e Ademit",
    "dhe nga ata të cilët",
    "Ne i bartëm",
    "me",
    "Nuhun",
    "dhe nga",
    "pasardhësit",
    "e Ibrahimit",
    "dhe Israilit (Jakubit)",
    "dhe nga ata të cilët",
    "Ne i udhëzuam",
    "dhe i përzgjodhëm.",
    "Kur",
    "u recitoheshin",
    "atyre",
    "ajetet",
    "e të Gjithëmëshirshmit",
    "ata binin",
    "të përulur (në sexhde)",
    "duke qarë."
  ],
  "59": [
    "Dhe erdhën",
    "nga",
    "pas tyre",
    "të mëpasshëm",
    "(që) e braktisën",
    "namazin",
    "dhe ndoqën",
    "pasionet",
    "prandaj do të",
    "ndeshen",
    "(me) shkatërrim."
  ],
  "60": [
    "Përveç",
    "atij që",
    "pendohet",
    "dhe beson",
    "dhe punon",
    "të mira",
    "pra të tillët",
    "do të hyjnë",
    "(në) xhenet",
    "dhe nuk",
    "do t'u bëhet zullum",
    "(në) asgjë."
  ],
  "61": [
    "(Në) kopshte",
    "të Adnit",
    "të cilat",
    "(ua) premtoi",
    "i Gjithëmëshirshmi",
    "robërve të Tij",
    "(në) të fshehtën.",
    "Vërtet",
    "është",
    "premtimi i Tij",
    "i ardhur (sigurisht)."
  ],
  "62": [
    "Nuk",
    "ata dëgjojnë",
    "në to",
    "pallavra",
    "por vetëm",
    "paqe.",
    "Dhe për ta (është)",
    "furnizimi i tyre",
    "në to",
    "mëngjes",
    "e mbrëmje."
  ],
  "63": [
    "Ai (është)",
    "xheneti",
    "të cilin",
    "Ne do t'ua lëmë si trashëgimi",
    "nga",
    "robërit Tanë",
    "ai që",
    "është",
    "i devotshëm."
  ],
  "64": [
    "\"Dhe nuk",
    "ne zbresim",
    "veçse",
    "me urdhrin",
    "e Zotit tënd.",
    "Atij (i takon)",
    "çka (është)",
    "para",
    "nesh",
    "dhe çka (është)",
    "pas nesh",
    "dhe çka (është)",
    "mes",
    "asaj.",
    "Dhe nuk",
    "është",
    "Zoti yt",
    "harrestar."
  ],
  "65": [
    "Zoti",
    "i qiejve",
    "dhe i tokës",
    "dhe i asaj çfarë (është)",
    "mes atyre dyve",
    "prandaj adhuroje Atë",
    "dhe ji durimmadh",
    "në adhurim ndaj Tij!",
    "A",
    "di ti",
    "për Të",
    "(ndonjë) emnak?\""
  ],
  "66": [
    "Dhe thotë",
    "njeriu",
    "\"Vallë, a kur",
    "[të]",
    "unë vdes",
    "sigurisht do të",
    "nxirem",
    "i gjallë?!\""
  ],
  "67": [
    "Vallë a nuk",
    "kujton",
    "njeriu",
    "se Ne",
    "e krijuam atë",
    "nga",
    "përpara",
    "kur nuk",
    "ai ishte",
    "gjë?!"
  ],
  "68": [
    "E për Zotin tënd!",
    "Sigurisht Ne do t'i tubojmë ata",
    "bashkë me djajtë",
    "pastaj",
    "sigurisht Ne do t'i sjellim ata",
    "përreth",
    "xhehenemit",
    "të gjunjëzuar."
  ],
  "69": [
    "Pastaj",
    "sigurisht Ne do të tërheqim",
    "nga",
    "çdo",
    "grupim",
    "cili prej tyre",
    "(ishte) më i ashpër",
    "ndaj",
    "të Gjithëmëshirshmit",
    "(në) rebelizëm."
  ],
  "70": [
    "Pastaj",
    "sigurisht Ne",
    "i dimë më mirë",
    "të cilët",
    "ata (janë)",
    "më meritorë",
    "në të",
    "për t'u djegur."
  ],
  "71": [
    "Dhe nuk (ka)",
    "(askush) prej jush",
    "veçse",
    "(do të jetë) kalues mbi të.",
    "(Kjo) është",
    "nga",
    "Zoti yt",
    "një pashmangshmëri",
    "e dekretuar."
  ],
  "72": [
    "Pastaj",
    "Ne do t'i shpëtojmë",
    "ata të cilët",
    "ishin të përkushtuar",
    "dhe Ne do t'i lëmë",
    "zullumqarët",
    "në të",
    "të gjunjëzuar."
  ],
  "73": [
    "E kur",
    "u recitohen",
    "atyre",
    "ajetet Tona",
    "të qarta",
    "(u) thonë",
    "ata të cilët",
    "mohuan",
    "atyre të cilët",
    "besuan",
    "\"Cili",
    "prej dy grupeve",
    "(është) më i mirë",
    "(në) pozitë",
    "dhe me më të hijshmin",
    "kuvend?\""
  ],
  "74": [
    "Dhe sa shumë",
    "Ne shkatërruam",
    "para tyre",
    "nga",
    "breza!",
    "Ata (ishin)",
    "më mirë",
    "(në) mall",
    "dhe salltanet."
  ],
  "75": [
    "Thuaj",
    "\"Kushdo që",
    "është",
    "në",
    "gabim",
    "atëherë do t'ia zgjasë",
    "atij",
    "i Gjithëmëshirshmi",
    "një zgjatje",
    "derisa",
    "kur",
    "ata do të shohin",
    "çfarë",
    "u është premtuar",
    "ose",
    "dënimin",
    "ose",
    "Çastin (Kijametin)",
    "atëherë ata do të dinë",
    "kush",
    "ai (është)",
    "më i keq",
    "(në) pozitë",
    "dhe më i dobët",
    "(me) ushtri\"."
  ],
  "76": [
    "Dhe ua shton",
    "Allahu",
    "atyre të cilët",
    "u udhëzuan",
    "udhëzim.",
    "E të përhershmet",
    "veprat e mira",
    "(janë) më të mira",
    "tek",
    "Zoti yt",
    "(për) shpërblim",
    "dhe më të mira",
    "(për) kthim."
  ],
  "77": [
    "Vallë a e ke parë",
    "atë i cili",
    "mohoi",
    "ajetet Tona",
    "dhe tha",
    "\"Sigurisht mua do t'më jepet",
    "pasuri",
    "dhe fëmijë\"."
  ],
  "78": [
    "A e pa ai",
    "të fshehtën",
    "apo",
    "mori",
    "nga",
    "i Gjithëmëshirshmi",
    "(ndonjë) premtim?"
  ],
  "79": [
    "Kurrsesi!",
    "Ne do të shkruajmë",
    "çfarë",
    "ai thotë",
    "dhe Ne do të zgjasim",
    "për të",
    "nga",
    "dënimi",
    "(me) zgjatje."
  ],
  "80": [
    "Dhe Ne do ta trashëgojmë atë",
    "çfarë",
    "ai thotë",
    "dhe ai do të vijë tek Ne",
    "fillikat."
  ],
  "81": [
    "Dhe ata morën",
    "nga",
    "pos",
    "Allahut",
    "zota",
    "që ata të jenë",
    "për ta",
    "krenari."
  ],
  "82": [
    "Kurrsesi!",
    "Ata do të mohojnë",
    "adhurimin e tyre",
    "dhe ata do të jenë",
    "kundër tyre",
    "kundërshtarë."
  ],
  "83": [
    "A nuk",
    "ti sheh",
    "se Ne",
    "i dërguam",
    "djajtë",
    "kundër",
    "mohuesve",
    "i yshtin ata",
    "me yshtje."
  ],
  "84": [
    "Prandaj mos",
    "nxito",
    "kundër tyre!",
    "Vetëm",
    "Ne numërojmë",
    "për ta",
    "me numërim."
  ],
  "85": [
    "Ditën kur",
    "Ne do t'i tubojmë",
    "të devotshmit",
    "tek",
    "i Gjithëmëshirshmi",
    "(si) delegacion."
  ],
  "86": [
    "E do t'i çojmë",
    "kriminelët",
    "drejt",
    "xhehenemit",
    "të etur."
  ],
  "87": [
    "Nuk",
    "ata posedojnë",
    "ndërmjetësimin",
    "përveç",
    "atij që",
    "mori",
    "nga",
    "I Gjithëmëshirshmi",
    "premtim."
  ],
  "88": [
    "Dhe ata thonë",
    "\"Ka marrë",
    "i Gjithëmëshirshmi",
    "një fëmijë\"."
  ],
  "89": [
    "Vërtet",
    "ju keni sjellë",
    "një gjë",
    "të tmerrshme."
  ],
  "90": [
    "Pothuajse",
    "qiejt",
    "shpërthejnë",
    "nga ajo",
    "dhe çahet",
    "toka",
    "dhe shemben",
    "kodrat",
    "duke u shkatërruar."
  ],
  "91": [
    "Që",
    "ata pretendojnë",
    "për të Gjithëmëshirshmin",
    "një fëmijë."
  ],
  "92": [
    "Dhe nuk",
    "është e duhur",
    "për të Gjithëmëshirshmin",
    "që",
    "Ai të marrë",
    "fëmijë."
  ],
  "93": [
    "Nuk (ka)",
    "secili",
    "kush (është)",
    "në",
    "qiejt",
    "dhe tokë",
    "veçse",
    "(është) ardhës",
    "(tek) i Gjithëmëshirshmi",
    "(si) rob."
  ],
  "94": [
    "Sigurisht",
    "Ai i përfshiu ata",
    "dhe i numëroi ata",
    "me numërim."
  ],
  "95": [
    "Dhe çdonjëri prej tyre",
    "(do të jetë) ardhës tek Ai",
    "ditën",
    "e Kijametit",
    "fillikat."
  ],
  "96": [
    "Vërtet",
    "ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "do t'u dhurojë",
    "atyre",
    "i Gjithëmëshirshmi",
    "dashuri."
  ],
  "97": [
    "Pra vetëm",
    "Ne e lehtësuam atë",
    "në gjuhën tënde",
    "që ti t'i përgëzosh",
    "me të",
    "të devotshmit",
    "dhe ta paralajmërosh",
    "me të",
    "një popull",
    "grindavec."
  ],
  "98": [
    "Dhe sa shumë",
    "Ne kemi shkatërruar",
    "para tyre",
    "nga",
    "breza!",
    "A",
    "e ndjen",
    "prej tyre",
    "qoftë edhe",
    "një",
    "apo",
    "dëgjon",
    "prej tyre",
    "(ndonjë) pëshpëritje?"
  ]
}