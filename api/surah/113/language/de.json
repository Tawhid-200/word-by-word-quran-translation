[
  "Sag:",
  "\"Ich nehme Zuflucht",
  "beim Herrn",
  "des Tagesanbruchs",
  "vor",
  "(dem) Übel (dessen)",
  "was",
  "Er erschaffen hat",
  "und vor",
  "(dem) Übel",
  "(der) Dunkelheit",
  "wenn",
  "sie zunimmt",
  "und vor",
  "(dem) Übel",
  "der Blasenden",
  "auf",
  "die Knoten",
  "und vor",
  "(dem) Übel",
  "eines Neidenden",
  "wenn",
  "er neidet.\""
]