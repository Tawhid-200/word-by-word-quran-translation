{
  "1": [
    "Për natën",
    "kur",
    "mbulohet"
  ],
  "2": [
    "Për ditën",
    "kur",
    "shkrep"
  ],
  "3": [
    "Dhe atë çfarë",
    "Ai krijoi",
    "mashkullin",
    "dhe femrën"
  ],
  "4": [
    "Vërtet",
    "qarkullimi juaj",
    "(është) gjithsesi i llojllojshëm"
  ],
  "5": [
    "E lidhur me",
    "atë i cili",
    "jep",
    "dhe druhet"
  ],
  "6": [
    "dhe konfirmon bindshëm",
    "për më të mirën"
  ],
  "7": [
    "Ne do t'ia lehtësojmë atij",
    "drejt më të lehtës"
  ],
  "8": [
    "E lidhur me",
    "atë i cili",
    "bën koprraci",
    "dhe e ndien veten të pavarur"
  ],
  "9": [
    "Dhe përgënjeshtron",
    "më të mirën"
  ],
  "10": [
    "Dhe do t'ia lehtësojmë",
    "drejt më të vështirës"
  ],
  "11": [
    "Dhe nuk",
    "(do të) bëjë dobi",
    "për të",
    "pasuria e tij",
    "kur",
    "ai (të) përmbyset"
  ],
  "12": [
    "Vërtet",
    "mbi Ne",
    "(është) gjithsesi udhëzimi"
  ],
  "13": [
    "Dhe vërtet",
    "për Ne",
    "(është) gjithsesi jeta e fundit",
    "dhe e para"
  ],
  "14": [
    "E unë ju paralajmërova ju",
    "(për) një zjarr",
    "(që) flakëron"
  ],
  "15": [
    "Nuk",
    "digjet në të",
    "askush (përveç)",
    "më të mjerit"
  ],
  "16": [
    "Ai i cili",
    "përgënjeshtroi",
    "dhe u zmbraps"
  ],
  "17": [
    "E do të largohet prej tij",
    "më i druajturi"
  ],
  "18": [
    "Ai i cili",
    "jep",
    "pasurinë e tij",
    "(që të) pastrojë veten"
  ],
  "19": [
    "Dhe jo",
    "për askë",
    "tek ai",
    "ndonjë",
    "begati",
    "(që të) kompensohet"
  ],
  "20": [
    "Përveç",
    "duke synuar",
    "Fytyrën",
    "e Zotit të tij",
    "më të Lartit"
  ],
  "21": [
    "E sigurisht do të",
    "kënaqet"
  ]
}