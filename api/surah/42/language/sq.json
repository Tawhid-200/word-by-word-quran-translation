{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Ajn Sin Kaf."
  ],
  "3": [
    "Kështu",
    "shpall",
    "tek ti",
    "dhe tek",
    "ata të cilët (ishin)",
    "nga",
    "para teje",
    "Allahu",
    "i Pamposhturi",
    "i Urti."
  ],
  "4": [
    "Atij (i përket)",
    "ajo çfarë (është)",
    "në",
    "qiej",
    "dhe ajo çfarë (është)",
    "në",
    "tokë",
    "dhe Ai (është)",
    "i Lartësuari",
    "Madhështori."
  ],
  "5": [
    "Gati sa",
    "qiejt",
    "çahen",
    "nga",
    "sipër tyre",
    "kurse engjëjt",
    "madhërojnë",
    "me falënderim",
    "të Zotit të tyre",
    "dhe kërkojnë falje",
    "për ata që (janë)",
    "në",
    "tokë.",
    "Vini re!",
    "Vërtet",
    "Allahu",
    "Ai (është)",
    "Falësi",
    "Mëshiruesi."
  ],
  "6": [
    "E ata të cilët",
    "morën",
    "nga",
    "pos Tij",
    "aleatë",
    "Allahu (është)",
    "garantues",
    "mbi ta",
    "e nuk",
    "ti (je)",
    "mbi ta",
    "mbikëqyrës."
  ],
  "7": [
    "Dhe kështu",
    "Ne shpallëm",
    "tek ti",
    "Kur'an",
    "arabisht",
    "që ta paralajmërosh",
    "kryeqendrën",
    "e vendbanimeve",
    "dhe këdo",
    "përreth saj",
    "dhe të paralajmërosh",
    "(për) ditën",
    "e tubimit",
    "nuk (ka)",
    "dyshim",
    "në të.",
    "Një grup",
    "në",
    "xhenet",
    "dhe një grup",
    "në",
    "zjarrin flakërues."
  ],
  "8": [
    "E sikur",
    "të dëshironte",
    "Allahu",
    "do t'i bënte ata",
    "popull",
    "një",
    "mirëpo",
    "Ai fut",
    "këdo që",
    "Ai dëshiron",
    "në",
    "mëshirën e Tij.",
    "E zullumqarët",
    "nuk (ka)",
    "për ta",
    "asnjë",
    "aleat",
    "e as",
    "ndihmës."
  ],
  "9": [
    "Përkundrazi",
    "ata morën",
    "nga",
    "pos Tij",
    "aleatë.",
    "Ama Allahu",
    "Ai (është)",
    "Aleati",
    "dhe Ai",
    "ngjall",
    "të vdekurit",
    "dhe Ai",
    "për",
    "çdo",
    "gjë",
    "(është) i Plotfuqishëm."
  ],
  "10": [
    "Dhe çfarëdo që",
    "ju kundërshtoheni",
    "lidhur me të",
    "nga",
    "ndonjë send",
    "gjykimi i tij (është)",
    "tek",
    "Allahu.",
    "Ai (është)",
    "Allahu",
    "Zoti im.",
    "Tek Ai",
    "unë mbështetem",
    "dhe tek Ai",
    "unë kthehem."
  ],
  "11": [
    "Çarësi",
    "i qiejve",
    "dhe i tokës.",
    "Ai bëri",
    "për ju",
    "nga",
    "vetja juaj",
    "çifte",
    "dhe nga",
    "bagëtitë",
    "çifte.",
    "Ai ju shumon ju",
    "me të.",
    "Nuk është",
    "si Ai",
    "asgjë",
    "dhe Ai (është)",
    "Gjithëdëgjuesi",
    "Gjithëshikuesi."
  ],
  "12": [
    "Atij (i përkasin)",
    "çelësat",
    "e qiejve",
    "dhe tokës.",
    "Ai e shtrin",
    "furnizimin",
    "kujtdo që",
    "Ai dëshiron",
    "dhe e ngushton.",
    "Vërtet Ai",
    "për çdo",
    "gjë",
    "(është) i Gjithëdijshëm."
  ],
  "13": [
    "Ai ligjësoi",
    "për ju",
    "prej",
    "fesë",
    "çfarë",
    "e porositi",
    "me të",
    "Nuhun",
    "dhe atë çfarë",
    "Ne shpallëm",
    "tek ti",
    "dhe çfarë",
    "e porositëm",
    "me të",
    "Ibrahimin",
    "dhe Musain",
    "dhe Isain",
    "që",
    "\"Zbatoni",
    "fenë",
    "dhe mos",
    "u përçani",
    "në të!\"",
    "Është e rëndë",
    "për",
    "idhujtarët",
    "ajo çfarë",
    "ti i fton ata",
    "në të.",
    "Allahu",
    "përzgjedh",
    "tek Ai",
    "këdo që",
    "Ai dëshiron",
    "dhe udhëzon",
    "tek Ai",
    "këdo që",
    "kthehet."
  ],
  "14": [
    "Dhe nuk",
    "ata u përçanë",
    "përveçse",
    "nga",
    "pasi",
    "që",
    "u erdhi atyre",
    "dija",
    "nga rivaliteti",
    "mes tyre.",
    "E sikur të mos (ishte)",
    "një fjalë",
    "ishte paraprirë",
    "nga",
    "Zoti yt",
    "deri në",
    "një afat",
    "të caktuar",
    "do të merrej vendimi",
    "mes tyre.",
    "Dhe vërtet",
    "ata të cilëve",
    "u është lënë në trashëgim",
    "Libri",
    "nga",
    "pas tyre",
    "(janë) njëmend në",
    "një dyshim",
    "ndaj tij",
    "të turbulluar."
  ],
  "15": [
    "Prandaj për këtë (shkak)",
    "ti fto!",
    "Dhe ji i qëndrueshëm",
    "ashtu siç",
    "je urdhëruar!",
    "Dhe mos",
    "i ndiq",
    "tekat e tyre!",
    "Dhe thuaj",
    "\"Unë besova",
    "në atë çfarë",
    "ka zbritur",
    "Allahu",
    "prej",
    "Librit\".",
    "Dhe jam urdhëruar",
    "të vendos drejtësi",
    "mes jush.",
    "Allahu (është)",
    "Zoti ynë",
    "dhe Zoti juaj.",
    "Për ne (janë)",
    "veprat tona",
    "dhe për ju (janë)",
    "veprat tuaja.",
    "Nuk (ka)",
    "polemikë",
    "mes nesh",
    "dhe mes jush.",
    "Allahu",
    "tubon",
    "mes nesh.",
    "Dhe tek Ai (është)",
    "kthimi."
  ],
  "16": [
    "E ata të cilët",
    "polemizojnë",
    "rreth",
    "Allahut",
    "nga",
    "pasi",
    "që",
    "i është përgjigjur",
    "atij",
    "argumenti i tyre (është)",
    "i flakur",
    "tek",
    "Zoti i tyre",
    "e mbi ta (është)",
    "një zemërim",
    "dhe ata kanë",
    "një dënim",
    "të ashpër."
  ],
  "17": [
    "Allahu (është)",
    "Ai i Cili",
    "zbriti",
    "Librin",
    "me vërtetësi",
    "dhe drejtësi.",
    "Dhe nuk",
    "e di ti",
    "mbase",
    "çasti i Kijametit",
    "(është) i afërt."
  ],
  "18": [
    "E shpejtojnë",
    "atë",
    "ata të cilët",
    "nuk",
    "besojnë",
    "në të.",
    "Kurse ata të cilët",
    "besuan",
    "(janë) të ndrojtur",
    "ndaj tij",
    "dhe ata e dinë",
    "se ai (është)",
    "e vërteta.",
    "Vini re!",
    "Vërtet",
    "ata të cilët",
    "polemizojnë",
    "rreth",
    "Kijametit",
    "(janë) njëmend në",
    "një humbje",
    "të largët."
  ],
  "19": [
    "Allahu (është)",
    "i Butë",
    "me robërit e Tij.",
    "Ai furnizon",
    "këdo që",
    "Ai dëshiron",
    "dhe Ai (është)",
    "i Forti",
    "i Pamposhturi."
  ],
  "20": [
    "Kush",
    "është",
    "(që) dëshiron",
    "të mbjellat",
    "e jetës së fundit",
    "Ne do t'ia shtojmë",
    "atij",
    "në",
    "të mbjellat e tij.",
    "E kush",
    "është",
    "(që) dëshiron",
    "të mbjellat",
    "e kësaj bote",
    "Ne do t'i japim atij",
    "prej saj",
    "por nuk",
    "ai ka",
    "në",
    "jetën e fundit",
    "asnjë",
    "hise."
  ],
  "21": [
    "A mos",
    "ata kanë",
    "ortakë",
    "ua ligjësuan",
    "atyre",
    "prej",
    "fesë",
    "atë çfarë",
    "nuk",
    "e lejon",
    "atë",
    "Allahu.",
    "E sikur të mos (ishte)",
    "fjala",
    "e prerë",
    "do të merrej vendimi përfundimtar",
    "mes tyre.",
    "E vërtet",
    "zullumqarët",
    "do të kenë",
    "një dënim",
    "të dhembshëm."
  ],
  "22": [
    "Ti i sheh",
    "zullumqarët",
    "të ndrojtur",
    "ndaj asaj çfarë",
    "ata fituan",
    "ndërkohë që ajo",
    "u ndodh",
    "atyre.",
    "Ndërsa ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "(do të jenë) në",
    "kopshtet",
    "e xheneteve.",
    "Ata do të kenë",
    "çfarëdo që",
    "ata dëshirojnë",
    "tek",
    "Zoti i tyre.",
    "Ajo",
    "pikërisht ajo (është)",
    "mirësia",
    "e madhe."
  ],
  "23": [
    "Ajo (është)",
    "çfarë",
    "(i) përgëzon",
    "Allahu",
    "robërit e Tij",
    "të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira.",
    "Thuaj",
    "\"Nuk",
    "unë ju kërkoj",
    "për të",
    "ndonjë shpërblim",
    "përveç",
    "dashurisë",
    "për hir të",
    "akrabasë\".",
    "E kushdo që",
    "vepron",
    "një të mirë",
    "Ne ia shtojmë",
    "atij",
    "në të",
    "mirësi.",
    "Vërtet",
    "Allahu",
    "(është) Falës",
    "Mirënjohës."
  ],
  "24": [
    "Vallë a",
    "ata thonë",
    "\"Ai ka shpifur",
    "ndaj",
    "Allahut",
    "gënjeshtër?!\"",
    "E nëse",
    "dëshiron",
    "Allahu",
    "vulos",
    "mbi",
    "zemrën tënde.",
    "Dhe shlyen",
    "Allahu",
    "kotësinë",
    "dhe përforcon",
    "të vërtetën",
    "me fjalët e Tij.",
    "Vërtet Ai (është)",
    "i Gjithëdijshëm",
    "për atë çfarë posedojnë",
    "gjokset."
  ],
  "25": [
    "Dhe Ai (është)",
    "i Cili",
    "pranon",
    "pendimin",
    "nga",
    "robërit e Tij",
    "dhe fshin",
    "prej",
    "të këqijave",
    "dhe Ai di",
    "çfarë",
    "ju bëni."
  ],
  "26": [
    "Dhe Ai u përgjigjet",
    "atyre të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "dhe ua shton atyre",
    "prej",
    "mirësisë së Tij.",
    "Kurse mohuesit",
    "për ta (ka)",
    "dënim",
    "të ashpër."
  ],
  "27": [
    "E sikur",
    "ta shtrinte",
    "Allahu",
    "furnizimin",
    "për robërit e Tij",
    "ata do të rebeloheshin",
    "në",
    "tokë",
    "mirëpo",
    "Ai zbret",
    "me masë",
    "çfarë",
    "Ai dëshiron.",
    "Vërtet Ai",
    "me robërit e Tij",
    "(është) i Gjithëinformuar",
    "Gjithëshikues."
  ],
  "28": [
    "Dhe Ai (është)",
    "i Cili",
    "zbret",
    "shiun",
    "nga",
    "pasi",
    "që",
    "ata u dëshpëruan",
    "dhe Ai e përhap",
    "mëshirën e Tij",
    "dhe Ai (është)",
    "Mbrojtësi",
    "i Lavdishmi."
  ],
  "29": [
    "Dhe prej",
    "provave të Tij",
    "(është) krijimi",
    "i qiejve",
    "dhe i tokës",
    "dhe ajo çfarë",
    "Ai shpërndan",
    "në ato dyja",
    "prej",
    "gjallese.",
    "E Ai (është)",
    "për",
    "tubimin e tyre",
    "kur",
    "Ai dëshiron",
    "i Plotfuqishëm."
  ],
  "30": [
    "Dhe çfarëdo që",
    "ju goditi ju",
    "prej",
    "ndonjë goditje",
    "(është) po për shkak të asaj çfarë",
    "kanë fituar",
    "duart tuaja",
    "dhe Ai shlyen",
    "për",
    "shumë (të tjera)."
  ],
  "31": [
    "Dhe nuk",
    "ju (jeni)",
    "shmangësit",
    "në",
    "tokë",
    "dhe nuk",
    "ju keni",
    "nga",
    "pos",
    "Allahut",
    "asnjë",
    "mbrojtës",
    "dhe as",
    "ndihmës."
  ],
  "32": [
    "Dhe prej",
    "provave të Tij (janë)",
    "anijet",
    "në",
    "det",
    "si kodra."
  ],
  "33": [
    "Nëse",
    "Ai dëshiron",
    "e ndal",
    "erën",
    "e ato mbesin",
    "të palëvizshme",
    "mbi",
    "sipërfaqen e tij.",
    "Vërtet",
    "në",
    "të",
    "(ka) njëmend prova",
    "për secilin",
    "durimtar",
    "falënderues."
  ],
  "34": [
    "Ose",
    "Ai i përmbyt ato",
    "për shkak të asaj çfarë",
    "ata fituan",
    "dhe shlyen",
    "për",
    "shumë (të tjera)."
  ],
  "35": [
    "E le ta dinë",
    "ata të cilët",
    "polemizojnë",
    "rreth",
    "ajeteve Tona",
    "nuk (ka)",
    "për ta",
    "asnjë",
    "arrati."
  ],
  "36": [
    "E çkado që",
    "ju është dhënë ju",
    "prej",
    "ndonjë gjëje",
    "(është) plaçka",
    "e jetës",
    "së kësaj bote",
    "e çkado që (është)",
    "tek",
    "Allahu",
    "(është) më e mirë",
    "dhe e përhershme",
    "për ata të cilët",
    "besuan",
    "dhe tek",
    "Zoti i tyre",
    "mbështeten."
  ],
  "37": [
    "E ata të cilët",
    "(i) shmangen",
    "të mëdhave",
    "gjynahe",
    "dhe paturpësive",
    "e kurdo",
    "që",
    "ata zemërohen",
    "ata",
    "falin."
  ],
  "38": [
    "Edhe ata të cilët",
    "i përgjigjen",
    "Zotit të tyre",
    "dhe e përsosin",
    "namazin",
    "dhe çështja e tyre (është)",
    "këshillim",
    "mes tyre",
    "dhe prej asaj çfarë",
    "Ne i furnizuam ata",
    "ata shpenzojnë."
  ],
  "39": [
    "Edhe ata të cilët",
    "kur",
    "i godet ata",
    "dhuna",
    "ata",
    "ngadhënjejnë."
  ],
  "40": [
    "E kompensimi",
    "i një të keqe",
    "(është) një e keqe",
    "sikurse ajo.",
    "E kushdo që",
    "fal",
    "dhe pajton",
    "shpërblimi i tij (është)",
    "tek",
    "Allahu.",
    "Vërtet Ai",
    "nuk",
    "i do",
    "zullumqarët."
  ],
  "41": [
    "E njëmend kushdo që",
    "e mbron veten",
    "pas",
    "zullumit ndaj tij",
    "atëherë (për) të tillët",
    "nuk (ka)",
    "ndaj tyre",
    "asnjë",
    "ndëshkim."
  ],
  "42": [
    "Vetëm",
    "ndëshkimi (është)",
    "ndaj",
    "atyre të cilët",
    "i bëjnë zullum",
    "njerëzve",
    "dhe rebelohen",
    "në",
    "tokë",
    "pa",
    "të drejtë.",
    "Të tillët",
    "për ta (ka)",
    "një dënim",
    "i dhembshëm."
  ],
  "43": [
    "E njëmend kushdo që",
    "duron",
    "dhe fal",
    "vërtet",
    "ajo (është)",
    "njëmend prej",
    "vendosmërisë",
    "së çështjeve."
  ],
  "44": [
    "E këdo që",
    "e lë në devijim",
    "Allahu",
    "atëherë nuk (ka)",
    "për të",
    "asnjë",
    "mbrojtës",
    "nga",
    "pas Tij.",
    "Dhe ti i sheh",
    "zullumqarët",
    "pasi që",
    "ta kenë parë",
    "dënimin",
    "thonë",
    "\"A (ka)",
    "për",
    "kthim",
    "ndonjë",
    "rrugë?\""
  ],
  "45": [
    "Dhe ti i sheh ata",
    "i ekspozohen",
    "atij",
    "të përulur",
    "nga",
    "nënçmimi",
    "shikojnë",
    "nga",
    "një cep",
    "i fshehtë.",
    "Dhe do të thonë",
    "ata të cilët",
    "besuan",
    "\"Vërtet",
    "humbësit",
    "(janë) ata të cilët",
    "humbën",
    "veten e tyre",
    "dhe familjet e tyre",
    "ditën",
    "e Kijametit\".",
    "Vini re!",
    "Vërtet",
    "zullumqarët",
    "(janë) në",
    "një dënim",
    "të vazhdueshëm."
  ],
  "46": [
    "Dhe nuk",
    "do të jetë",
    "për ta",
    "asnjë",
    "mbrojtës",
    "do t'i ndihmojnë ata",
    "nga",
    "pos",
    "Allahut.",
    "E këdo që",
    "e lë në devijim",
    "Allahu",
    "atëherë nuk (ka)",
    "për të",
    "asnjë",
    "rrugë."
  ],
  "47": [
    "Përgjigjuni",
    "Zotit tuaj",
    "nga",
    "para",
    "se",
    "të vijë",
    "një ditë",
    "nuk (ka)",
    "kthim",
    "për të",
    "prej",
    "Allahut.",
    "Nuk (ka)",
    "për ju",
    "asnjë",
    "strehimore",
    "atë ditë",
    "dhe nuk (ka)",
    "për ju",
    "asnjë",
    "mohim."
  ],
  "48": [
    "E nëse",
    "ata e kthejnë shpinën",
    "po nuk",
    "Ne të dërguam ty",
    "mbi ta",
    "ruajtës.",
    "Nuk (është)",
    "mbi ty",
    "veçse",
    "kumtimi.",
    "E vërtet Ne",
    "kur",
    "bëjmë të shijojë",
    "njeriu",
    "prej Nesh",
    "një mëshirë",
    "ai gëzohet",
    "me të",
    "e nëse",
