    "gara për shumim"
  ],
  "2": [
    "Derisa",
    "(t'i) vizitoni",
    "varrezat"
  ],
  "3": [
    "Assesi!",
    "do të",
    "dini ju"
  ],
  "4": [
    "Pastaj",
    "assesi",
    "do të",
    "dini ju"
  ],
  "5": [
