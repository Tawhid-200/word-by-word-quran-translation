{
  "1": [
    "Ai u ngrys",
    "dhe e ktheu shpinën"
  ],
  "2": [
    "Që",
    "erdhi tek ai",
    "i verbëri"
  ],
  "3": [
    "Dhe çka",
    "di ti",
    "mbase ai",
    "(do të) pastrohet"
  ],
  "4": [
    "Apo",
    "(të) përkujtohet",
    "e t'i bëjë dobi atij",
    "përkujtimi?"
  ],
  "5": [
    "Ndërkaq",
    "ai që",
    "konsideron veten të pavarur"
  ],
  "6": [
    "Pra ti",
    "atij",
    "i jep vëmendje"
  ],
  "7": [
    "Dhe nuk",
    "(është) mbi ty",
    "që nuk",
    "ai pastrohet"
  ],
  "8": [
    "Ndërkaq",
    "ai që",
    "vjen tek ti",
    "duke nxituar"
  ],
  "9": [
    "Ndërkohë që ai",
    "i frikësohet (Allahut)"
  ],
  "10": [
    "Pra ti",
    "ndaj tij",
    "je i shpërqendruar"
  ],
  "11": [
    "Assesi",
    "vërtet ajo",
    "(është) një përkujtim"
  ],
  "12": [
    "Pra kushdo që",
    "dëshiron",
    "(do ta) përmend atë"
  ],
  "13": [
    "Në",
    "fletushka",
    "fisnike"
  ],
  "14": [
    "Të ngritura",
    "të pastra"
  ],
  "15": [
    "Në duar",
    "të engjëjve udhëtues"
  ],
  "16": [
    "Fisnikë",
    "të bindur"
  ],
  "17": [
    "Shkatërruar qoftë",
    "njeriu",
    "sa",
    "mohues që është ai!"
  ],
  "18": [
    "Nga",
    "çfarë",
    "gjë",
    "Ai e krijoi atë?"
  ],
  "19": [
    "Nga",
    "një farë",
    "Ai e krijoi atë",
    "dhe e formësoi"
  ],
  "20": [
    "Pastaj",
    "rrugën",
    "Ai atij ia lehtësoi"
  ],
  "21": [
    "Pastaj",
    "vdekjen ia shkaktoi",
    "dhe varrin ia siguroi"
  ],
  "22": [
    "Pastaj",
    "kur",
    "Ai dëshiron",
    "(do ta) ringjall atë"
  ],
  "23": [
    "Assesi",
    "ende nuk",
    "ai zbatoi",
    "atë çfarë",
    "Ai e urdhëroi atë"
  ],
  "24": [
    "Atëherë le të shikojë",
    "njeriu",
    "tek",
    "ushqimi i tij"
  ],
  "25": [
    "Që Ne",
    "derdhëm",
    "ujë",
    "me bollëk"
  ],
  "26": [
    "Pastaj",
    "e çamë",
    "tokën",
    "(në) plasa"
  ],
  "27": [
    "Atëherë bëmë që të mbijë",
    "në të",
    "kokërr (gruri)"
  ],
  "28": [
    "Dhe rrush",
    "dhe tërfil"
  ],
  "29": [
    "Edhe ullinj",
    "dhe dru palmash"
  ],
  "30": [
    "Dhe kopshte",
    "të dendura"
  ],
  "31": [
    "Dhe pemë",
    "e kullosa"
  ],
  "32": [
    "Furnizim",
    "për ju",
    "dhe për bagëtinë tuaj"
  ],
  "33": [
    "E kur",
    "(të) vijë",
    "ushtima (e kijametit)"
  ],
  "34": [
    "Ditën kur",
    "ik",
    "njeriu",
    "nga",
    "vëllai i tij"
  ],
  "35": [
    "Dhe nëna e tij",
    "dhe babai i tij"
  ],
  "36": [
    "Dhe gruaja e tij",
    "dhe fëmijët e tij"
  ],
  "37": [
    "Për çdo",
    "njeri",
    "prej tyre",
    "atë ditë",
    "(do të jetë) një çështje",
    "(që) i mjafton atij"
  ],
  "38": [
    "(Disa) fytyra",
    "atë ditë",
    "(do të jenë) të shndritura"
  ],
  "39": [
    "Të qeshura",
    "të gëzuara"
  ],
  "40": [
    "E (disa) fytyra",
    "atë ditë",
    "mbi to",
    "(do të ketë) pluhur"
  ],
  "41": [
    "I mbulon ato",
    "një errësirë"
  ],
  "42": [
    "Të tillët",
    "ata",
    "(janë) mohuesit",
    "keqbërësit"
  ]
}