    "Dhe i Cili",
    "përcaktoi",
    "dhe udhëzoi"
  ],
  "4": [
    "Dhe i Cili",
    "nxori",
    "kullosën"
  ],
  "5": [
    "Mandej e bëri atë",
    "kallamishte",
    "të errët"
  ],
  "6": [
    "Ne do të lexojmë ty",
    "dhe nuk",
    "(do të) harrosh"
  ],
  "7": [
    "Përveç",
    "asaj çfarë",
    "dëshiron",
