    "duke bërë hesap."
  ],
  "48": [
    "Dhe (do t'u) shfaqen",
    "atyre",
    "të këqijat",
    "e asaj çfarë",
    "ata fituan.",
    "Dhe do t'i mbërthejë",
    "ata",
    "ajo çfarë",
    "ata ishin",
    "me të",
    "duke u tallur."
  ],
  "49": [
    "E kur",
    "e prek",
    "njeriun",
    "një zarar",
    "Ai na thërret ne",
    "pastaj",
    "kur",
    "Ne e mbulojmë.atë",
    "(me) një begati",
    "prej Nesh",
    "ai thotë",
    "\"Vetëm",
    "mua më është dhënë ajo",
    "në bazë të",
    "një dije\".",
    "Përkundrazi",
    "ajo (është)",
    "një sprovë",
    "mirëpo",
    "shumica e tyre",
    "nuk",
    "dinë."
  ],
  "50": [
    "Sigurisht",
    "e kanë thënë atë",
    "ata të cilët (ishin)",
    "nga",
    "para tyre",
    "ama nuk",
    "u bëri dobi",
    "atyre",
    "ajo çfarë",
    "ata ishin",
    "duke fituar."
  ],
  "51": [
    "Atëherë i qëlluan ata",
    "të këqijat",
    "e asaj çfarë",
    "ata fituan.",
    "E ata të cilët",
    "bënë zullum",
    "prej",
    "këtyre",
    "do t'i qëllojnë ata",
    "të këqijat",
    "e asaj çfarë",
    "ata fituan",
    "dhe nuk",
    "ata (do të jenë)",
    "shmangës."
  ],
  "52": [
    "Vallë a nuk",
    "ata dinë",
    "se",
    "Allahu",
    "e shtrin",
    "furnizimin",
    "për këdo që",
    "Ai dëshiron",
    "dhe e ngushton?",
    "Vërtet",
    "në",
    "të",
    "(ka) njëmend prova",
    "për një popull",
    "(që) besojnë."
  ],
  "53": [
    "Thuaj",
    "\"O robërit e Mi",
    "të cilët",
    "tepruan",
    "ndaj",
    "vetes së tyre",
    "mos",
    "u zhgënjeni",
    "prej",
    "mëshirës",
    "së Allahut!",
    "Vërtet",
    "Allahu",
    "i fal",
    "mëkatet",
    "të gjitha.",
    "Vërtet Ai",
    "Ai (është)",
    "Falësi",
    "Mëshiruesi."
  ],
  "54": [
    "Dhe kthehuni plotësisht",
    "tek",
    "Zoti juaj",
    "dhe nënshtrojuni",
    "për Të",
    "nga",
    "para",
    "se",
    "t'ju vijë",
    "dënimi",
    "pastaj",
    "nuk",
    "(do të) ndihmoheni."
  ],
  "55": [
    "Dhe ndiqni",
    "më të mirën",
    "e asaj çfarë",
    "është zbritur",
    "tek ju",
    "prej",
    "Zotit tuaj",
    "nga",
    "para",
    "se",
    "t'ju vijë",
    "dënimi",
    "papritur",
    "ndërkohë që ju",
    "nuk",
    "(do ta) ndjeni."
  ],
  "56": [
    "Të (mos)",
    "thotë",
    "ndokush",
    "\"Vaj halli për mua",
    "për",
    "atë çfarë",
    "kam shpërfillur",
    "në",
    "kujdesin",
    "(ndaj) Allahut",
    "dhe që",
    "unë isha",
    "njëmend prej",
    "tallësve\"."
  ],
  "57": [
    "Apo",
    "të thotë",
    "\"Sikur",
    "të",
    "Allahu",
    "më udhëzonte mua",
    "njëmend unë do të isha",
    "prej",
    "të druajturve!\""
  ],
  "58": [
    "Apo",
    "të thotë",
    "kur",
    "ta shohë",
    "dënimin",
    "\"Sikur",
    "të",
    "kisha unë",
    "një mundësi (tjetër)",
    "e të isha",
    "prej",
    "bamirësve!\""
  ],
  "59": [
    "Kurrsesi!",
    "Sigurisht",
    "të erdhën ty",
    "provat Tona",
    "e ti i përgënjeshtrove",
    "ato",
    "dhe u bëre mendjemadh",
    "dhe ishe",
    "prej",
    "mohuesve."
  ],
  "60": [
    "E ditën",
    "e Kijametit",
    "do t'i shohësh",
    "ata të cilët",
    "gënjyen",
    "për",
    "Allahun",
    "fytyrat e tyre (do të jenë)",
    "të nxira.",
    "A nuk është",
    "në",
    "xhehenem",
    "strehë",
    "për mendjemëdhenjtë?"
  ],
  "61": [
    "Dhe do t'i shpëtojë",
    "Allahu",
    "ata të cilët",
    "janë druajtur",
    "tek vendshpëtimi i tyre.",
    "Nuk",
    "do t'i prek ata",
    "e keqja",
    "dhe as",
    "nuk",
    "ata do të mërziten."
  ],
  "62": [
    "Allahu (është)",
    "Krijuesi",
    "i çdo",
    "gjëje",
    "Dhe Ai (është)",
    "mbi",
    "çdo",
    "gjë",
    "Mbikëqyrës."
  ],
  "63": [
    "Për Të (janë)",
    "çelësat",
    "e qiejve",
    "dhe tokës.",
    "E ata të cilët",
    "kanë mohuar",
    "ajetet",
    "e Allahut",
    "të tillët",
    "ata (janë)",
    "humbësit."
  ],
  "64": [
    "Thuaj",
    "\"Vallë a tjetër pos",
    "Allahut",
    "ju më urdhëroni mua",
    "të adhuroj",
    "o ju",
    "të paditur?\""
  ],
  "65": [
    "Dhe sigurisht",
    "është shpallur",
    "tek ti",
    "dhe tek",
    "ata të cilët (ishin)",
    "nga",
    "para teje",
    "nëse",
    "shoqëron zota (krahas Allahut)",
    "do të asgjësohet gjithsesi",
    "vepra jote",
    "dhe ti do të jesh gjithsesi",
    "prej",
    "humbësve."
  ],
  "66": [
    "Por vetëm",
    "Allahun",
    "adhuroje",
    "dhe ji",
    "prej",
    "falënderuesve!"
  ],
  "67": [
    "Dhe nuk",
    "ata e vlerësuan",
    "Allahun",
    "(me) të duhurin",
    "vlerësimin e Tij.",
    "Ndërsa toka",
    "e tërë",
    "(do të jetë) në grushtin e Tij",
    "ditën",
    "e Kijametit",
    "kurse qiejt",
    "(do të jenë) të mbështjellur",
    "në të Djathtën e Tij.",
    "I Patëmetë (është) Ai",
    "dhe Lartësuar qoftë",
    "ndaj asaj çfarë",
    "ata shoqërojnë zota (krahas Tij)!"
  ],
  "68": [
    "Dhe do të fryhet",
    "në",
    "bori",
    "e do të bie i vdekur",
    "secili",
    "në",
    "qiej",
    "dhe secili",
    "në",
    "tokë",
    "përveç",
    "atij që",
    "dëshiron",
    "Allahu.",
    "Pastaj",
    "do të fryhet",
    "në të",
    "tjetër (herë)",
    "e përnjëherë",
    "ata (do të jenë)",
    "të ngritur",
    "shikojnë."
  ],
  "69": [
    "Dhe do të ndriçojë",
    "toka",
    "me nurin",
    "e Zotit të saj",
    "dhe do të vendoset",
    "Libri",
    "edhe do të sillen",
    "pejgamberët",
    "dhe dëshmitarët",
    "dhe do të gjykohet",
    "mes tyre",
    "me vërtetësi",
    "dhe atyre",
    "nuk",
    "do t'u bëhet zullum."
  ],
  "70": [
    "Dhe do të shpaguhet",
    "secili",
    "person",
    "për atë çfarë",
    "ka punuar",
    "e Ai",
    "(është) më i ditur",
    "për atë çfarë",
    "ata bëjnë."
  ],
  "71": [
    "Dhe do të dërgohen",
    "ata të cilët",
    "mohuan",
    "tek",
    "xhehenemi",
    "(në) grupe.",
    "Deri",
    "kur",
    "vijnë tek ai",
    "hapen",
    "dyert e tij",
    "dhe (u) thonë",
    "atyre",
    "rojtarët e tij",
    "\"A nuk",
    "ju erdhën ju",
    "të dërguar",
    "prej jush",
    "ata recitonin",
    "tek ju",
    "ajetet",
    "e Zotit tuaj",
    "dhe ju paralajmëronin ju",
    "(për) takimin",
    "e ditës suaj",
    "këtë?\"",
    "Ata do të thonë",
    "\"Natyrisht!\"",
    "Mirëpo",
    "u përligj",
    "verdikti",
    "i dënimit",
    "për",
    "mohuesit."
  ],
  "72": [
    "Do të thuhet",
    "\"Hyni",
    "(në) dyert",
    "e xhehenemit",
    "të gjithmonshëm",
    "në të!\"",
    "Po sa e keqe që është",
    "streha",
    "e mendjemëdhenjve!"
  ],
  "73": [
    "Dhe do të dërgohen",
    "ata të cilët",
    "janë druajtur",
    "Zotit të tyre",
    "tek",
    "xheneti",
    "(në) grupe.",
    "Deri",
    "kur",
    "vijnë tek ai",
    "dhe hapen",
    "dyert e tij",
    "dhe do t'u thonë",
    "atyre",
    "rojtarët e tij",
    "\"Paqe",
    "mbi ju!",
    "Ju ishit të mrekullueshëm",
    "prandaj hyni në të",
    "të gjithmonshëm!\""
  ],
  "74": [
    "Dhe ata do të thonë",
    "\"Falënderimi (është)",
    "për Allahun",
    "i Cili",
    "na përmbushi ne",
    "premtimin e Tij",
    "dhe na la në trashëgim",
    "tokën",
    "ne të vendosemi",
    "anembanë",
    "xhenetit",
    "kudo që",
    "ne dëshirojmë.",
    "Po sa i mirë që është",
    "shpërblimi",
    "i punëtorëve!\""
  ],
  "75": [
    "Dhe ti i sheh",
    "engjëjt",
    "të rrethuar",
    "nga",
    "përreth",
    "Fronit",
    "lartësojnë",
    "me falënderim",
    "të Zotit të tyre.",
    "Dhe do të gjykohet",
    "mes tyre",
    "me vërtetësi",
    "dhe do të thuhet",
    "\"Falënderimi (është)",
    "për Allahun",
    "Zotin",
    "e botëve!\""
  ]
}