{
  "1": [
    "E vërteta e pashmangshme!"
  ],
  "2": [
    "Ç'(është)",
    "e vërteta e pashmangshme?"
  ],
  "3": [
    "E çfarë",
    "ti di",
    "ç'(është)",
    "e vërteta e pashmangshme?"
  ],
  "4": [
    "Përgënjeshtroi",
    "Themudi",
    "dhe Adi",
    "llahtarinë (kijametin)."
  ],
  "5": [
    "Sa i përket",
    "Themudit",
    "ata u shkatërruan",
    "me britmë të tmerrshme."
  ],
  "6": [
    "Ndërkaq",
    "Adi",
    "u shkatërruan",
    "me një erë",
    "të fuqishme",
    "të furishme."
  ],
  "7": [
    "Ai e dërgoi atë",
    "mbi ta",
    "shtatë",
    "net",
    "dhe tetë",
    "ditë",
    "pandërprerë",
    "dhe ti e sheh",
    "popullin",
    "në to",
    "të rrëzuar",
    "si të jenë",
    "trungje",
    "hurmash",
    "të zgavëruara."
  ],
  "8": [
    "Pra, a",
    "sheh",
    "prej tyre",
    "ndonjë",
    "mbetje?"
  ],
  "9": [
    "Dhe erdhi",
    "faraoni",
    "dhe ata të cilët",
    "(ishin) para tij",
    "dhe të përmbysurit",
    "me mëkate."
  ],
  "10": [
    "Ata e kundërshtuan",
    "të dërguarin",
    "e Zotit të tyre",
    "prandaj Ai i dënoi ata",
    "me një dënim",
    "të madh."
  ],
  "11": [
    "Vërtet Ne",
    "pasi që",
    "vërshoi",
    "uji",
    "ju bartëm ju",
    "në",
    "anije."
  ],
  "12": [
    "Që ta bëjmë atë",
    "për ju",
    "përkujtim",
    "dhe ta rrokin atë",
    "veshë",
    "të mprehtë."
  ],
  "13": [
    "E kur",
    "të fryhet",
    "në",
    "Sur",
    "me fryrje",
    "një."
  ],
  "14": [
    "Dhe të bartet",
    "toka",
    "dhe kodrat",
    "dhe të goditen",
    "me goditje",
    "një."
  ],
  "15": [
    "Dhe atëherë",
    "do të ndodhë",
    "ndodhia."
  ],
  "16": [
    "Dhe çahet",
    "qielli",
    "e ai",
    "atë ditë",
    "(është) i molisur."
  ],
  "17": [
    "Dhe engjëjt",
    "(janë) në",
    "skajet e tij",
    "dhe e bartin",
    "Fonin",
    "e Zotit tënd",
    "mbi ta",
    "atë ditë",
    "tetë (engjëj)."
  ],
  "18": [
    "Atë ditë",
    "do të ekspozoheni",
    "nuk",
    "fshihet",
    "nga ju",
    "(asnjë) fshehtësi."
  ],
  "19": [
    "Sa i përket",
    "atij që",
    "i jepet",
    "libri i tij",
    "nga e djathta e tij",
    "ai (do të) thotë",
    "\"Ja",
    "lexoni",
    "librin tim!"
  ],
  "20": [
    "Vërtet unë",
    "isha i bindur",
    "se vërtet unë",
    "(jam) pranues",
    "i llogarisë sime\"."
  ],
  "21": [
    "Ai",
    "(do të jetë) në",
    "një jetesë",
    "të kënaqshme."
  ],
  "22": [
    "Në",
    "një xhenet",
    "të lartë."
  ],
  "23": [
    "Pemët e tij",
    "(janë) të afërta."
  ],
  "24": [
    "\"Hani",
    "dhe pini",
    "shijshëm",
    "për atë çfarë",
    "keni përgatitur",
    "në",
    "ditët",
    "e kaluara\"."
  ],
  "25": [
    "E sa i përket",
    "atij që",
    "i jepet",
    "libri i tij",
    "nga e majta e tij",
    "ai (do të) thotë",
    "\"Ah, sikur",
    "të mos",
    "më jepej",
    "libri im!"
  ],
  "26": [
    "Dhe të mos",
    "e dija",
    "çfarë (është)",
    "llogaria ime."
  ],
  "27": [
    "Ah sikur",
    "ajo (vdekja) të ishte",
    "përfundimi."
  ],
  "28": [
    "Nuk",
    "më bëri dobi",
    "mua",
    "pasuria ime."
  ],
  "29": [
    "Mori fund",
    "nga unë",
    "pushteti im\"."
  ],
  "30": [
    "\"Merreni atë",
    "dhe prangoseni atë."
  ],
  "31": [
    "Pastaj",
    "në xhehenem",
    "hidheni atë."
  ],
  "32": [
    "Pastaj",
    "në",
    "zingjir",
    "gjatësia e tij (është)",
    "shtatëdhjetë",
    "kutë",
    "lidheni atë\"."
  ],
  "33": [
    "Vërtet ai",
    "ishte",
    "nuk",
    "besonte",
    "në Allahun",
    "e Madhërishëm."
  ],
  "34": [
    "Dhe nuk",
    "nxiste",
    "për",
    "ushqyerjen",
    "e nevojtarit."
  ],
  "35": [
    "Prandaj nuk ka",
    "për të",
    "sot",
    "këtu",
    "(asnjë) jaran."
  ],
  "36": [
    "Dhe as",
    "ushqim",
    "përveç",
    "nga",
    "pisllëku."
  ],
  "37": [
    "Nuk",
    "e ha atë",
    "(askush) pos",
    "mëkatarët."
  ],
  "38": [
    "Prandaj",
    "betohem",
    "në atë çfarë",
    "ju shihni."
  ],
  "39": [
    "Dhe në atë çfarë",
    "nuk",
    "ju shihni."
  ],
  "40": [
    "Vërtet ai (Kur'ani)",
    "gjithsesi (është) fjalë",
    "(dhënë) një të dërguari",
    "fisnik."
  ],
  "41": [
    "Dhe nuk (është)",
    "ai",
    "fjalë",
    "e ndonjë poeti.",
    "Sa pak",
    "[çfarë]",
    "ju besoni!"
  ],
  "42": [
    "Dhe as",
    "fjalë",
    "e ndonjë falltari.",
    "Sa pak",
    "[çfarë]",
    "ju përkujtoni!"
  ],
  "43": [
    "(Është) zbritje",
    "nga",
    "Zoti",
    "i botëve."
  ],
  "44": [
    "E sikur",
    "ai të trillonte",
    "ndaj Nesh",
    "disa",
    "fjalë!"
  ],
  "45": [
    "Sigurisht Ne do ta kapnim",
    "atë",
    "për dorën e djathtë."
  ],
  "46": [
    "Pastaj",
    "do t'ia këputnim",
    "atij",
    "arterien."
  ],
  "47": [
    "Dhe nuk (ka)",
    "prej jush",
    "as edhe",
    "një",
    "për të",
    "mbrojtës."
  ],
  "48": [
    "Dhe vërtet ai",
    "(është) përkujtim",
    "për të druajturit."
  ],
  "49": [
    "Dhe vërtet Ne",
    "sigurisht e dimë",
    "që vërtet",
    "prej jush",
    "(ka) përgënjeshtrues."
  ],
  "50": [
    "Dhe vërtet ai",
    "(është) zhgënjim",
    "për",
    "mohuesit."
  ],
  "51": [
    "Dhe vërtet ai",
    "(është) një e vërtetë",
    "bindëse."
  ],
  "52": [
    "Prandaj lartësoje",
    "emrin",
    "e Zotit tënd",
    "të madhërishëm!"
  ]
}