{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Zbritje",
    "prej",
    "të Gjithëmëshirshmit",
    "Mëshiruesit."
  ],
  "3": [
    "Libër",
    "janë shkoqitur",
    "ajetet e tij",
    "Kur'an",
    "arabisht",
    "për një popull",
    "(që) dinë."
  ],
  "4": [
    "Përgëzues",
    "dhe paralajmërues",
    "por ia kthyen shpinën",
    "shumica e tyre",
    "prandaj ata",
    "nuk",
    "dëgjojnë."
  ],
  "5": [
    "Dhe ata thonë",
    "\"Zemrat tona (janë)",
    "në",
    "kuti",
    "prej asaj çfarë",
    "ti na fton ne",
    "tek ajo",
    "e brenda",
    "veshëve tanë",
    "(ka) shurdhësi",
    "dhe nga",
    "mes nesh",
    "dhe mes teje",
    "(është) një perde",
    "prandaj ti puno",
    "vërtet ne",
    "(jemi) punëtorë\"."
  ],
  "6": [
    "Thuaj",
    "\"Vetëm",
    "unë (jam)",
    "një njeri",
    "sikurse ju",
    "(më) shpallet",
    "mua",
    "se",
    "Zoti juaj (është)",
    "Zot",
    "Një",
    "prandaj merrni rrugën e drejtë",
    "tek Ai",
    "dhe kërkoni falje Atij!\"",
    "E mjerë për",
    "idhujtarët!"
  ],
  "7": [
    "Ata të cilët",
    "nuk",
    "japin",
    "zeqatin",
    "dhe ata",
    "(në) jetën e fundit",
    "ata (janë)",
    "mohues."
  ],
  "8": [
    "Vërtet",
    "ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "për ta (ka)",
    "shpërblim",
    "i pa",
    "shkëputur."
  ],
  "9": [
    "Thuaj",
    "\"A vërtet ju",
    "njëmend e mohoni",
    "Atë i Cili",
    "krijoi",
    "tokën",
    "për",
    "dy ditë",
    "dhe i caktoni",
    "Atij",
    "rivalë?",
    "Ai (është)",
    "Zoti",
    "i botëve."
  ],
  "10": [
    "Dhe Ai vendosi",
    "në të",
    "kodra të rrënjosura",
    "prej",
    "sipër saj",
    "dhe lëshoi bereqet",
    "në të",
    "dhe përcaktoi",
    "në të",
    "ushqimet e saj",
    "për",
    "katër",
    "ditë",
    "(është) njësoj",
    "për pyetësit."
  ],
  "11": [
    "Pastaj",
    "u drejtua",
    "tek",
    "qielli",
    "ndërkohë që ai (ishte)",
    "tym",
    "e i tha",
    "atij",
    "dhe tokës",
    "\"Ejani të dy",
    "me hir",
    "a",
    "pa hir!\"",
    "Ata dy thanë",
    "\"Po vijmë",
    "me hir\"."
  ],
  "12": [
    "Atëherë Ai i përfundoi ato",
    "(si) shtatë",
    "qiej",
    "për",
    "dy ditë",
    "dhe Ai shpalli",
    "në",
    "çdo",
    "qiell",
    "urdhrin e Tij.",
    "Dhe Ne e stolisëm",
    "qiellin",
    "e kësaj bote",
    "me llamba",
    "dhe si mbrojtje.",
    "Ai (është)",
    "përcaktimi",
    "i të Pamposhturit",
    "të Gjithëdijshmit."
  ],
  "13": [
    "E nëse",
    "ata e kthejnë shpinën",
    "atëherë thuaj",
    "\"Unë ju paralajmërova ju",
    "(për) një rrufe",
    "sikurse",
    "rrufeja",
    "e Adit",
    "dhe Themudit\"."
  ],
  "14": [
    "Kur",
    "u erdhën atyre",
    "të dërguarit",
    "prej",
    "para",
    "tyre",
    "dhe prej",
    "pas tyre",
    "\"Që të mos",
    "adhuroni",
    "përveç",
    "Allahut\".",
    "Ata thanë",
    "\"Sikur",
    "të dëshironte",
    "Zoti ynë",
    "Ai do të zbriste",
    "engjëj",
    "e vërtet ne",
    "në atë çfarë",
    "ju u dërguat",
    "me të",
    "(jemi) mohues\"."
  ],
  "15": [
    "Sa i përket",
    "Adit",
    "ata ishin mendjemëdhenj",
    "në",
    "tokë",
    "pa",
    "të drejtë",
    "dhe thanë",
    "\"Kush (është)",
    "më i fortë",
    "se ne",
    "(në) fuqi?\"",
    "Vallë a nuk",
    "ata shohin",
    "se",
    "Allahu",
    "i Cili",
    "i krijoi ata",
    "Ai (është)",
    "më i fortë",
    "se ata",
    "(në) fuqi?",
    "E ata ishin",
    "provat Tona",
    "duke i mohuar."
  ],
  "16": [
    "Prandaj Ne dërguam",
    "mbi ta",
    "një erë",
    "të tërbuar",
    "gjatë",
    "(disa) ditëve",
    "të kobshme",
    "që Ne t'i bëjmë ata të shijojnë",
    "dënimin",
    "e përbuzjes",
    "në",
    "jetën",
    "e kësaj bote",
    "e njëmend dënimi",
    "i jetës së fundit",
    "(është) më përbuzës",
    "e ata",
    "nuk",
    "(do të) ndihmohen."
  ],
  "17": [
    "E sa i përket",
    "Themudit",
    "Ne i udhëzuam ata",
    "por ata preferuan",
    "verbërinë",
    "ndaj",
    "udhëzimit",
    "prandaj i mbërtheu ata",
    "rrufeja",
    "e dënimit",
    "të poshtëruar",
    "për shkak të asaj çfarë",
    "ata ishin",
    "duke fituar."
  ],
  "18": [
    "Dhe Ne i shpëtuam",
    "ata të cilët",
    "besuan",
    "dhe ishin",
    "duke u druajtur."
  ],
  "19": [
    "E ditën kur",
    "(do të) tubohen",
    "armiqtë",
    "e Allahut",
    "tek",
    "zjarri",
    "ashtu që ata",
    "(do të) rreshtohen."
  ],
  "20": [
    "Deri",
    "kur",
    "të",
    "vijnë tek ai",
    "do të dëshmojnë",
    "kundër tyre",
    "dëgjimi i tyre",
    "dhe shikimet e tyre",
    "edhe lëkurat e tyre",
    "për atë çfarë",
    "ata ishin",
    "duke punuar."
  ],
  "21": [
    "Dhe ata do t'u thonë",
    "lëkurave të tyre",
    "\"Përse",
    "dëshmuat",
    "kundër nesh?\"",
    "Ata do të thonë",
    "\"Ne na bëri të flasim",
    "Allahu",
    "i Cili",
    "bëri të flasë",
    "çdo",
    "gjë",
    "dhe Ai",
    "ju krijoi ju",
    "(për të) parën",
    "herë",
    "dhe tek Ai",
    "(do të) ktheheni\"."
  ],
  "22": [
    "Dhe nuk",
    "ju ishit",
    "duke u fshehur",
    "se (mos)",
    "dëshmon",
    "kundër jush",
    "dëgjimi juaj",
    "e as",
    "shikimet tuaja",
    "dhe as",
    "lëkurat tuaja",
    "mirëpo",
    "ju pandehnit",
    "se",
    "Allahu",
    "nuk",
    "di",
    "shumë",
    "prej asaj çfarë",
    "ju punoni."
  ],
  "23": [
    "E ajo (ishte)",
    "pandehja juaj",
    "të cilën",
    "ju e pandehnit",
    "për Zotin tuaj.",
    "Ai ju shkatërroi ju",
    "ashtu që ju u bëtë",
    "prej",
    "humbësve."
  ],
  "24": [
    "E nëse",
    "ata durojnë",
    "atëherë zjarri (është)",
    "strehë",
    "për ta.",
    "E nëse",
    "ata kërkojnë favor",
    "atëherë nuk",
    "ata (do të jenë)",
    "prej",
    "të favorizuarve."
  ],
  "25": [
    "Dhe Ne lidhëm",
    "për ta",
    "shoqërues",
    "e ata ua stolisën",
    "atyre",
    "çfarë (ishte)",
    "para",
    "tyre",
    "dhe çfarë (ishte)",
    "pas tyre",
    "dhe u vërtetua",
    "kundër tyre",
    "dekreti",
    "në mesin e",
    "popujve",
    "tashmë",
    "paraprinë",
    "nga",
    "para tyre",
    "prej",
    "xhinëve",
    "dhe njerëzve.",
    "Vërtet ata",
    "ishin",
    "humbës."
  ],
  "26": [
    "Dhe thanë",
    "ata të cilët",
    "mohuan",
    "\"Mos",
    "e dëgjoni",
    "këtë",
    "Kur'an",
    "dhe bëni bujë",
    "ndaj tij",
    "ashtu që ju",
    "të ngadhënjeni\"."
  ],
  "27": [
    "Prandaj Ne do të bëjmë që të shijojnë gjithsesi",
    "ata të cilët",
    "mohuan",
    "një dënim",
    "të ashpër",
    "dhe Ne do t'ua kompensojmë gjithsesi atyre",
    "më të keqen",
    "e asaj çfarë",
    "ata ishin",
    "duke punuar."
  ],
  "28": [
    "Ai (është)",
    "kompensimi",
    "i armiqve",
    "të Allahut",
    "zjarri.",
    "Për ta (do të jetë)",
    "në të",
    "shtëpia",
    "e përhershmërisë",
    "si kompensim",
    "për atë çfarë",
    "ata ishin",
    "provat Tona",
    "duke i mohuar."
  ],
  "29": [
    "Dhe do të thonë",
    "ata të cilët",
    "mohuan",
    "\"Zoti ynë!",
    "Na i shfaq ne",
    "ata dy të cilët",
    "na devijuan ne",
    "prej",
    "xhinëve",
    "dhe njerëzve",
    "t'i vendosim ata dy",
    "nën",
    "këmbët tona",
    "që të jenë ata dy",
    "prej",
    "më të ultëve\"."
  ],
  "30": [
    "Vërtet",
    "ata të cilët",
    "thonë",
    "\"Zoti ynë (është)",
    "Allahu",
    "pastaj",
    "qëndrojnë të vendosur",
    "zbresin",
    "tek ta",
    "engjëjt",
    "\"Të mos",
    "frikësoheni",
    "dhe as",
    "pikëlloheni",
    "dhe përgëzohuni",
    "me xhenetin",
    "me të cilin",
    "ju ishit",
    "duke u premtuar."
  ],
  "31": [
    "Ne (jemi)",
    "miqtë tuaj",
    "në",
    "jetën",
    "e kësaj bote",
    "dhe në",
    "të fundit.",
    "Dhe për ju (ka)",
    "në të",
    "çfarëdo që",
    "lakmojnë",
    "loçkat tuaja",
    "dhe për ju (ka)",
    "në të",
    "çfarëdo që",
    "ju kërkoni."
  ],
  "32": [
    "Mirëseardhje",
    "prej",
    "një Falësi",
    "Mëshiruesi\"."
  ],
  "33": [
    "E kush (ka)",
    "më të mirën",
    "fjalë",
    "se ai i cili",
    "thërret",
    "tek",
    "Allahu",
    "dhe punon",
    "(vepër) të mirë",
    "edhe thotë",
    "\"Vërtet unë (jam)",
    "prej",
    "muslimanëve?\""
  ],
  "34": [
    "Dhe nuk",
    "barazohet",
    "e mira",
    "dhe as",
    "e keqja.",
    "Zmbrapse",
    "me atë që",
    "ajo (është)",
    "më e mirë!",
    "E përnjëherë",
    "ai i cili",
    "mes teje",
    "dhe mes tij",
    "(kishte) armiqësi",
    "si të ishte ai",
    "një mik",
    "i ngushtë."
  ],
  "35": [
    "Por nuk",
    "arrihet ajo",
    "përveç",
    "(nga) ata të cilët",
    "duruan",
    "dhe nuk",
    "arrihet ajo",
    "përveç",
    "(nga) poseduesi",
    "i një fati",
    "të madh."
  ],
  "36": [
    "E nëse",
    "të ngacmon ty",
    "prej",
    "shejtanit",
    "(ndonjë) ngacmim",
    "atëherë kërko strehim",
    "me Allahun!",
    "Vërtet Ai",
    "Ai (është)",
    "Gjithëdëgjuesi",
    "i Gjithëdijshmi."
  ],
  "37": [
    "Dhe prej",
    "provave të Tij",
    "(është) nata",
    "dhe dita",
    "edhe dielli",
    "dhe hëna.",
    "Mos",
    "i bëni sexhde",
    "diellit",
    "dhe as",
    "hënës",
    "por bëni sexhde",
    "Allahut",
    "i Cili",
    "i krijoi ato",
    "nëse",
    "ju jeni",
    "Atë",
    "duke e adhuruar."
  ],
  "38": [
    "Por nëse",
    "ata janë mendjemëdhenj",
    "atëherë ata të cilët",
    "(janë) tek",
    "Zoti yt",
    "e lartësojnë",
    "Atë",
    "natën",
    "e ditën",
    "dhe ata",
    "nuk",
    "bezdisen."
  ],
  "39": [
    "Dhe prej",
    "provave të Tij",
    "(është) se ti",
    "e sheh",
    "tokën",
    "shterpe",
    "por kur",
    "Ne zbresim",
    "mbi të",
    "ujin",
    "ajo dridhet",
    "dhe shtohet.",
    "Vërtet",
    "Ai i Cili",
    "e ngjalli atë",
    "(është) njëmend Ngjallësi",
    "i të vdekurve.",
    "Vërtet Ai (është)",
    "mbi",
    "çdo",
    "gjë",
    "i Plotfuqishëm."
  ],
  "40": [
    "Vërtet",
    "ata të cilët",
    "devijojnë",
    "nga",
    "ajetet Tona",
    "nuk",
    "fshihen",
    "nga Ne.",
    "Vallë a ai i cili",
    "hidhet",
    "në",
    "zjarr",
    "(është) më i mirë",
    "apo",
    "ai i cili",
    "vjen",
    "i sigurt",
    "ditën",
    "e Kijametit?",
    "Punoni",
    "çfarë",
    "dëshironi!",
    "Vërtet Ai (është)",
    "për atë çfarë",
    "ju punoni",
    "Gjithëshikues."
  ],
  "41": [
    "Vërtet",
    "ata të cilët",
    "mohuan",
    "përkujtimin",
    "pasi që",
    "u erdhi atyre",
    "dhe vërtet ai (është)",
    "njëmend një Libër",
    "ngadhënjyes."
  ],
  "42": [
    "Nuk",
    "i vjen atij",
    "e pavërteta",
    "nga",
    "para",
    "tij",
    "dhe as",
    "nga",
    "prapa tij.",
    "(Është) zbritje",
    "prej",
    "një të Urti",
    "të Lavdishmi."
  ],
  "43": [
    "Nuk",
    "thuhet",
    "për ty",
    "veçse",
    "ajo çfarë",
    "tashmë",
    "është thënë",
    "për të dërguarit",
    "nga",
    "para teje.",
    "Vërtet",
    "Zoti yt",
    "(është) njëmend Posedues",
    "i një falje",
    "dhe Posedues",
    "i një ndëshkimi",
    "të dhembshëm."
  ],
  "44": [
    "E sikur",
    "Ne ta bënim atë",
    "një Kur'an",
    "në gjuhë të huaj",
    "ata do të thonin",
    "\"Përse nuk",
    "janë shkoqitur",
    "ajetet e tij?\"",
    "A në gjuhë të huaj",
    "e ai (i dërguari) arab?!",
    "Thuaj",
    "\"Ai (është)",
    "për ata të cilët",
    "besuan",
    "udhëzim",
    "dhe shërim.",
    "Kurse ata të cilët",
    "nuk",
    "besojnë",
    "në",
    "veshët e tyre",
    "(ka) shurdhësi",
    "dhe ai (është)",
    "për ta",
    "verbëri.",
    "Të tillët",
    "(do të) thirren",
    "prej",
    "një vendi",
    "të largët."
  ],
  "45": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Musait",
    "Librin",
    "e u kundërshtua",
    "lidhur me të.",
    "E sikur të mos (ishte)",
    "një verdikt",
    "(i cili) është paraprirë",
    "prej",
    "Zotit tënd",
    "do të përfundonte (çështja)",
    "mes tyre.",
    "E vërtet ata (janë)",
    "njëmend në",
    "një dyshim",
    "rreth tij",
    "të thellë."
  ],
  "46": [
    "Kushdo që",
    "punon",
    "një të mirë",
    "(është) për veten e tij",
    "e kushdo që",
    "bën ligësi",
    "atëherë kundër vetes së tij.",
    "Dhe nuk (është)",
    "Zoti yt",
    "zullumqar",
    "ndaj robërve."
  ],
  "47": [
    "Tek Ai",
    "kthehet",
    "dija",
    "e Orës.",
    "Dhe nuk",
    "dalin",
    "kurrfarë",
    "frutash",
    "prej",
    "lëvoreve të tyre",
    "dhe nuk",
    "bart",
    "asnjë",
    "femër",
    "dhe as",
    "lind",
    "përveçse",
    "me dijen e Tij.",
    "E ditën kur",
    "Ai do t'i thërrasë ata",
    "\"Ku (janë)",
    "shoqëruesit e Mi?\"",
    "Ata do të thonë",
    "\"Ne të njoftuam ty",
    "nuk (ka)",
    "prej nesh",
    "asnjë",
    "dëshmitar\"."
  ],
  "48": [
    "Dhe humb",
    "prej tyre",
    "ajo çfarë",
    "ata ishin",
    "duke lutur",
    "prej",
    "përpara",
    "dhe ata do të binden",
    "nuk (ka)",
    "për ta",
    "asnjë",
    "vend për ikje."
  ],
  "49": [
    "Nuk",
    "bezdiset",
    "njeriu",
    "prej",
    "lutjes",
    "së mirësisë",
    "por nëse",
    "e prek atë",
    "e keqja",
    "atëherë (ai është) shumë i zhgënjyer",
    "zemërthyer."
  ],
  "50": [
    "E nëse",
    "Ne bëjmë që ai të shijojë",
    "një mëshirë",
    "prej Nesh",
    "nga",
    "pas",
    "një fatkeqësie",
    "(që) e prek atë",
    "ai do të thotë sigurisht",
    "\"Kjo (është)",
    "për mua",
    "dhe nuk",
    "unë mendoj",
    "(se) Ora",
    "do të ndodhë",
    "e nëse",
    "unë kthehem",
    "tek",
    "Zoti im",
    "vërtet",
    "për mua",
    "Tek Ai",
    "(do të jetë) njëmend më e mira\".",
    "E Ne do t'i informojmë gjithsesi",
    "ata të cilët",
    "mohuan",
    "për atë çfarë",
    "ata punuan",
    "dhe Ne do të bëjmë që ata të shijojnë gjithsesi",
    "prej",
    "një dënimi",
    "të ashpër."
  ],
  "51": [
    "E kur Ne",
    "lëshojmë begati",
    "mbi",
    "njeriun",
    "ai e kthen shpinën",
    "dhe largohet",
    "në anën e tij.",
    "por kur",
