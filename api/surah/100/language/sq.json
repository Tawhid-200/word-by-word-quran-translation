{
  "1": [
    "Për kuajt vrapues",
    "duke hingëlluar"
  ],
  "2": [
    "Dhe që nxjerrin xixa",
    "duke goditur"
  ],
  "3": [
    "Dhe që vërsulen",
    "në kohën e agimit"
  ],
  "4": [
    "Dhe që lënë gjurmë",
    "në këtë mënyrë",
    "pluhur"
  ],
  "5": [
    "Dhe futen në mes",
    "në këtë mënyrë",
    "bashkërisht"
  ],
  "6": [
    "Vërtet",
    "njeriu",
    "ndaj Zotit të tij",
    "(është) sigurisht mosmirënjohës"
  ],
  "7": [
    "Dhe vërtet ai",
    "për",
    "këtë",
    "(është) sigurisht dëshmues"
  ],
  "8": [
    "Dhe vërtet ai",
    "në dashurinë",
    "e të mirave",
    "(është) sigurisht intensiv"
  ],
  "9": [
    "A nuk",
    "di ai",
    "kur",
    "(të) shpërndahet",
    "ajo çfarë",
    "(është) në",
    "varre?"
  ],
  "10": [
    "Dhe (të) tubohet",
    "ajo çfarë",
    "(është) në",
    "gjoksa"
  ],
  "11": [
    "Vërtet",
    "Zoti i tyre",
    "për ta",
    "atë ditë",
    "(është) i Gjithëinformuar"
  ]
