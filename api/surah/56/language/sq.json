{
  "1": [
    "Kur",
    "të ndodhë",
    "Ndodhia."
  ],
  "2": [
    "Nuk (është)",
    "ndodhia e saj",
    "e rrejshme."
  ],
  "3": [
    "Rrëzuese",
    "ngritëse."
  ],
  "4": [
    "Kur",
    "të tundet",
    "toka",
    "me tundje (të fortë)."
  ],
  "5": [
    "E të copëtohen",
    "kodrat",
    "copë-copë."
  ],
  "6": [
    "E të bëhen",
    "pluhur",
    "i shpërndarë."
  ],
  "7": [
    "Dhe ju (të) jeni",
    "çifte",
    "tre (grupe)."
  ],
  "8": [
    "E banorët",
    "të së djathtës",
    "ç'(janë)",
    "banorët",
    "e së djathtës?"
  ],
  "9": [
    "E banorët",
    "e së majtës",
    "ç'(janë)",
    "banorët",
    "e së majtës?"
  ],
  "10": [
    "E të parët",
    "të parët."
  ],
  "11": [
    "Ata (janë)",
    "të afruarit."
  ],
  "12": [
    "Në",
    "xhenetet",
    "e begatisë."
  ],
  "13": [
    "Shumicë",
    "prej",
    "të parëve."
  ],
  "14": [
    "Dhe pakicë",
    "prej",
    "të mbramëve."
  ],
  "15": [
    "Mbi",
    "divane",
    "të qëndisur me ar."
  ],
  "16": [
    "Të mbështetur",
    "mbi to",
    "përballë njëri-tjetrit."
  ],
  "17": [
    "Sillen",
    "rreth tyre",
    "djelmosha",
    "të gjithmonshëm."
  ],
  "18": [
    "Me kupa",
    "dhe ibrikë",
    "dhe gotë të mbushur",
    "nga",
    "burimi."
  ],
  "19": [
    "Nuk",
    "iu dhemb koka (atyre)",
    "prej saj",
    "dhe as nuk",
    "ata dehen."
  ],
  "20": [
    "Dhe pemë",
    "prej asaj çfarë",
    "ata zgjedhin."
  ],
  "21": [
    "Dhe mish",
    "shpendi",
    "prej asaj çfarë",
    "ata pëlqejnë."
  ],
  "22": [
    "Edhe hyri",
    "symëdha."
  ],
  "23": [
    "Si shembulli",
    "i margaritarit",
    "të ruajtur."
  ],
  "24": [
    "Shpërblim",
    "për atë çfarë",
    "ata ishin",
    "punonin."
  ],
  "25": [
    "Nuk",
    "ata dëgjojnë",
    "në të",
    "pallavra",
    "dhe as",
    "(fjalë) gjynahu."
  ],
  "26": [
    "Përveç",
    "frazës",
    "\"Paqe",
    "paqe!\""
  ],
  "27": [
    "E banorët",
    "e së djathtës",
    "ç'(është puna me)",
    "banorët",
    "e së djathtës?"
  ],
  "28": [
    "Nën",
    "lotus",
    "pa gjemba."
  ],
  "29": [
    "Dhe banane",
    "palë-palë."
  ],
  "30": [
    "Dhe hije",
    "të zgjatur."
  ],
  "31": [
    "Dhe ujë",
    "rrjedhës."
  ],
  "32": [
    "Dhe pemë",
    "të shumta."
  ],
  "33": [
    "Jo",
    "të ndërprera",
    "dhe as",
    "të ndaluara."
  ],
  "34": [
    "Dhe shtretër",
    "të ngritur."
  ],
  "35": [
    "Vërtet Ne",
    "i krijuam ato",
    "(me) një krijim."
  ],
  "36": [
    "Dhe Ne i bëmë ato",
    "virgjëresha."
  ],
  "37": [
    "Të dashura",
    "të një moshe."
  ],
  "38": [
    "Për banorët",
    "e së djathtës."
  ],
  "39": [
    "Shumicë",
    "nga",
    "të parët."
  ],
  "40": [
    "Dhe shumicë",
    "nga",
    "të mbramët."
  ],
  "41": [
    "Kurse banorët",
    "e së majtës",
    "ç'(është puna me)",
    "banorët",
    "e së majtës?"
  ],
  "42": [
    "Në",
    "afsh të një zjarri përcëllues",
    "dhe ujë të vluar."
  ],
  "43": [
    "Dhe hije",
    "nga",
    "një tym i zi."
  ],
  "44": [
    "Jo",
    "e ftohtë",
    "dhe as",
    "e këndshme."
  ],
  "45": [
    "Vërtet ata",
    "ishin",
    "përpara",
    "asaj",
    "të dhënë pas luksit."
  ],
  "46": [
    "Dhe ishin",
    "insistonin",
    "në",
    "mëkatin",
    "e madh."
  ],
  "47": [
    "Dhe ishin",
    "thonin",
    "\"A kur",
    "të vdesim",
    "dhe të jemi",
    "dhé",
    "e eshtra",
    "a vërtet ne",
    "(do të jemi) të ringjallur?\""
  ],
  "48": [
    "A edhe baballarët tanë",
    "të parë?"
  ],
  "49": [
    "Thuaj",
    "\"Vërtet",
    "të parët",
    "dhe të mbramët"
  ],
  "50": [
    "(do të jenë) të tubuar",
    "tek",
    "përcaktimi",
    "i një dite",
    "të njohur\"."
  ],
  "51": [
    "Pastaj",
    "vërtet ju",
    "o",
    "të humbur",
    "përgënjeshtrues"
  ],
  "52": [
    "(Do të jeni) ngrënës",
    "nga",
    "pema",
    "prej",
    "Zekumit."
  ],
  "53": [
    "E (do të jeni) mbushës",
    "prej saj",
    "të barqeve."
  ],
  "54": [
    "E (do të jeni) pirës",
    "pas asaj",
    "prej",
    "ujit të vluar."
  ],
  "55": [
    "Dhe pirës",
    "si pirja",
    "e deveve rë etura."
  ],
  "56": [
    "Kjo (është)",
    "mirëseardhja e tyre",
    "ditën",
    "e kompensimit."
  ],
  "57": [
    "Ne",
    "ju krijuam ju",
    "e përse nuk",
    "konfirmoni (ringjalljen)?"
  ],
  "58": [
    "Vallë, a e shihni",
    "atë çfarë",
    "hidhni?"
  ],
  "59": [
    "A ju",
    "e krijoni atë",
    "apo",
    "Ne (jemi)",
    "Krijuesit?!"
  ],
  "60": [
    "Ne",
    "përcaktuam",
    "mes jush",
    "vdekjen",
    "dhe nuk (jemi)",
    "Ne",
    "të paaftë."
  ],
  "61": [
    "[Ashtu]",
    "që",
    "Ne të zëvendësojmë",
    "me të ngjashëm me ju",
    "dhe t'ju krijojmë ju",
    "në",
    "atë çfarë",
    "nuk",
    "ju dini."
  ],
  "62": [
    "Dhe sigurisht",
    "ju e dini",
    "krijimin",
    "e parë",
    "e si nuk",
    "përkujtoni?"
  ],
  "63": [
    "Vallë a e shihni",
    "atë çfarë",
    "ju korrni?"
  ],
  "64": [
    "A ju",
    "e mbillni atë",
    "apo",
    "Ne (jemi)",
    "Mbjellësit?!"
  ],
  "65": [
    "Sikur",
    "Ne të dëshironim",
    "Ne do ta bënim atë",
    "të thërrmuar",
    "e ju do të mbeteshit",
    "duke u habitur."
  ],
  "66": [
    "(Do të thonit) \"Vërtet ne (jemi)",
    "të dëmtuar\"."
  ],
  "67": [
    "Madje",
    "ne (jemi)",
    "të privuar."
  ],
  "68": [
    "Vallë a e shihni",
    "ujin",
    "të cilin",
    "ju pini."
  ],
  "69": [
    "A ju",
    "e zbritët atë",
    "prej",
    "reve",
    "apo",
    "Ne (jemi)",
    "Zbritësit?"
  ],
  "70": [
    "Sikur",
    "Ne të dëshironim",
    "Ne do ta bënim atë",
    "të njelmët",
    "e përse nuk",
    "falënderoni?"
  ],
  "71": [
    "Vallë a e shihni",
    "zjarrin",
    "të cilin",
    "ju e ndezni?"
  ],
  "72": [
    "A ju",
    "e krijuat",
    "drurin e tij",
    "apo",
    "Ne (jemi)",
    "Krijuesit?"
  ],
  "73": [
    "Ne",
    "e bëmë atë",
    "përkujtim",
    "dhe përfitim",
    "për udhëtarët."
  ],
  "74": [
    "Prandaj lartësoje",
    "emrin",
    "e Zotit tënd",
    "të madh!"
  ],
  "75": [
    "Pra, [nuk]",
    "betohem",
    "në rëniet",
    "e yjeve!"
  ],
  "76": [
    "E vërtet ai (është)",
    "një betim",
    "sikur",
    "ta dini",
    "i madh!"
  ],
  "77": [
    "Vërtet ai (është)",
    "Kur'an",
    "fisnik."
  ],
  "78": [
    "Brenda",
    "një libri",
    "të ruajtur."
  ],
  "79": [
    "Nuk",
    "e prek atë",
    "përveçse",
    "të pastërtit."
  ],
  "80": [
    "Zbritje",
    "nga",
    "Zoti",
    "i botëve."
  ],
  "81": [
    "Vallë a në këtë",
    "bisedë (Kur'an)",
    "ju (jeni)",
    "mohues?"
  ],
  "82": [
    "Dhe ju e konsideroni",
    "furnizimin tuaj",
    "që ju",
    "përgënjeshtroni."
  ],
  "83": [
    "Pra, përse nuk (ndërhyni)",
    "kur",
    "arrin (shpirti)",
    "në fyt?"
  ],
  "84": [
    "Ndërkohë që",
    "ju",
    "shikoni."
  ],
  "85": [
    "E Ne (jemi)",
    "më afër",
    "atij",
    "se ju",
    "mirëpo",
    "nuk",
    "ju shihni."
  ],
  "86": [
    "Pra përse nuk",
    "nëse",
    "jeni",
    "jo",
    "llogaridhënës"
  ],
  "87": [
    "e ktheni atë",
    "nëse",
    "jeni",
    "të sinqertë."
  ],
  "88": [
    "Ndërkaq",
    "nëse",
    "ai është",
    "nga",
    "të afërtit."
  ],
  "89": [
    "Atëherë (ai do të ketë) prehje",
    "dhe mirësi",
    "dhe xhenet",
    "të begatisë."
  ],
  "90": [
    "Ndërsa",
    "nëse",
    "ai është",
    "nga",
    "banorët",
    "e së djathtës."
  ],
  "91": [
    "Atëherë paqe",
    "për ty",
    "nga",
    "banorët",
    "e së djathtës."
  ],
  "92": [
    "Kurse",
    "nëse",
    "ai është",
    "nga",
    "përgënjeshtruesit",
    "të humbur."
  ],
  "93": [
    "Atëherë (për të ka) pritje",
    "nga",
    "ujë i vluar."
  ],
  "94": [
    "Dhe djegie",
    "në zjarr flakërues."
  ],
  "95": [
    "Vërtet",
    "kjo (është)",
    "pikërisht ajo",
    "e vërteta",
    "e sigurt."
  ],
  "96": [
    "Prandaj lartësoje",
    "emrin",
    "e Zotit tënd",
    "të madh."
  ]
}