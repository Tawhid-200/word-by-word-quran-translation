{
  "1": [
    "Për diellin",
    "dhe shkëlqimin e tij"
  ],
  "2": [
    "Për hënën",
    "kur",
    "e ndjek atë"
  ],
  "3": [
    "Për ditën",
    "kur",
    "e shfaq atë"
  ],
  "4": [
    "Për natën",
    "kur",
    "e mbulon atë"
  ],
  "5": [
    "Për qiellin",
    "dhe çfarë",
    "Ai e ndërtoi atë"
  ],
  "6": [
    "Për tokën",
    "dhe çfarë",
    "Ai e shtriu atë"
  ],
  "7": [
    "Për shpirtin",
    "dhe çfarë",
    "Ai e drejtoi atë"
  ],
  "8": [
    "E ia frymëzoi",
    "(të dallojë) të ligën",
    "dhe të drejtën"
  ],
  "9": [
    "Vërtet",
    "ka shpëtuar",
    "ai që",
    "e pastron atë"
  ],
  "10": [
    "E vërtet",
    "ka dështuar",
    "ai që",
    "e përdhos atë"
  ],
  "11": [
    "Ka përgënjeshtruar",
    "Themudi",
    "për shkak të teprimit të tij"
  ],
  "12": [
    "Kur",
    "u dërgua në krye",
    "më i mjeri i tyre"
  ],
  "13": [
    "E u tha",
    "atyre",
    "i dërguari",
    "i Allahut",
    "\"(Kujdes ndaj) devesë",
    "së Allahut",
    "dhe pijes së saj\""
  ],
  "14": [
    "E ata e përgënjeshtruan atë",
    "dhe e therën atë",
    "ashtu që i shkatërroi",
    "ata",
    "Zoti i tyre",
    "për mëkatin e tyre",
    "dhe i rrafshoi ata"
  ],
  "15": [
    "Dhe nuk",
    "i frikësohet",
    "pasojave të tij"
  ]
}