    "zemrat tuaja.",
    "Dhe është",
    "Allahu",
    "Falës",
    "Mëshirues."
  ],
  "6": [
    "Pejgamberi (është)",
    "më i denjë",
    "për besimtarët",
    "sesa",
    "vetja e tyre",
    "kurse gratë e tij (janë)",
    "nënat e tyre.",
    "E ata",
    "të afërmit",
    "disa prej tyre (janë)",
    "më të denjë",
    "për disa (të tjerë)",
    "në",
    "Dekretin",
    "e Allahut",
    "sesa",
    "besimtarët",
    "dhe emigrantët",
    "përveçse",
    "të",
    "bëni",
    "për",
    "dashamirët tuaj",
    "ndonjë të mirë.",
    "Është",
    "ajo",
    "në",
    "Libër",
    "e shkruar."
  ],
  "7": [
    "E kur",
    "Ne morëm",
    "prej",
    "pejgamberëve",
    "besëlidhjen e tyre",
    "dhe prej teje",
    "edhe prej",
    "Nuhut",
