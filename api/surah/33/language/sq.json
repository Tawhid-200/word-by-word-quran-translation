    "O ti",
    "pejgamber!",
    "Vërtet Ne",
    "të dërguam ty",
    "dëshmues",
    "dhe përgëzues",
    "edhe paralajmërues."
  ],
  "46": [
    "Edhe thirrës",
    "tek",
    "Allahu",
    "me lejen e Tij",
    "dhe një fener",
    "ndriçues."
  ],
  "47": [
    "Dhe përgëzoji",
    "besimtarët",
    "se",
    "ata do të kenë",
    "prej",
    "Allahut",
    "një dhunti",
    "të madhe."
  ],
  "48": [
    "Edhe mos",
    "iu bind",
    "mohuesve",
    "dhe hipokritëve",
    "dhe shpërfille",
    "fyerjen e tyre",
    "dhe mbështetu",
    "tek",
    "Allahu!",
    "Dhe mjafton",
    "Allahu",
    "si veqil."
  ],
  "49": [
    "O ju",
    "të cilët",
    "besuan!",
    "Kur",
    "ju i martoni",
    "besimtaret",
    "pastaj",
    "i shkurorëzoni ato",
    "nga",
    "para",
    "se",
    "t'i prekni ato",
    "atëherë nuk (ka)",
    "për ju",
    "ndaj tyre",
    "asnjë",
    "afat të pritjes",
    "ju ta llogarisni atë.",
    "Prandaj dhurojini diçka atyre",
    "dhe lëshoni ato",
    "me një lëshim",
    "të hijshëm."
  ],
  "50": [
    "O ti",
    "pejgamber!",
    "Vërtet Ne",
    "bëmë hallall",
    "për ty",
    "gratë tua",
    "të cilave",
    "ti ua dhe",
    "shpërblimet e tyre",
    "dhe atë çfarë",
    "posedon",
    "e djathta jote",
    "nga ajo çfarë",
    "(të) dhuroi",
    "Allahu",
    "ty",
    "dhe bijat",
    "e xhaxhait tënd",
    "dhe bijat",
    "e hallave tua",
    "dhe bijat",
    "e dajës tënd",
    "dhe bijat",
    "e tezeve tua",
    "të cilat",
    "emigruan",
    "me ty",
    "edhe një grua",
    "besimtare",
    "nëse",
    "ajo e dhuron",
    "veten e saj",
    "për pejgamberin",
    "nëse",
    "dëshiron",
    "pejgamberi",
    "që",
    "ta martojë atë",
    "vetëm",
    "për ty",
    "nga",
    "pos",
    "besimtarëve.",
    "Sigurisht",
    "Ne dimë",
    "çfarë",
    "(u) kemi obliguar",
    "atyre",
    "lidhur me",
    "gratë e tyre",
    "dhe çfarë",
    "posedojnë",
    "të djathtat e tyre",
    "që të mos",
    "jetë",
    "për ty",
    "ndonjë telash.",
    "Dhe është",
    "Allahu",
    "Falës",
    "Mëshirues."
  ],
  "51": [
    "Ti mund ta largosh",
    "këdo që",
    "ti do",
    "prej tyre",
    "dhe mund ta afrosh",
    "tek ti",
    "këdo që",
    "ti do",
    "edhe këdo që",
    "ti dëshiron",
    "prej atyre që",
    "ti je ndarë",
    "atëherë nuk (ka)",
    "telash",
    "për ty.",
    "Ajo (është)",
    "më e duhur",
    "që",
    "të prehen",
    "sytë e tyre",
    "dhe të mos",
    "pikëllohen",
    "dhe të kënaqen",
    "me atë çfarë",
    "ti u ke dhënë atyre",
    "të gjithave.",
    "E Allahu",
    "di",
    "atë çfarë (është)",
    "brenda",
    "zemrave tuaja",
    "dhe është",
    "Allahu",
    "i Gjithëdijshëm",
    "i Matur."
  ],
  "52": [
    "Nuk",
    "lejohen",
    "për ty",
    "gratë",
    "nga",
    "pastaj",
    "dhe as",
    "të",
    "të ndërrosh",
    "me to",
    "prej",
    "grave (të tjera)",
    "edhe sikur",
    "të të mahnisë",
    "bukuria e tyre",
    "përveç",
    "asaj që",
    "posedon",
    "e djathta jote.",
    "Dhe është",
    "Allahu",
    "për",
    "çdo",
    "gjë",
    "Mbikëqyrës."
  ],
  "53": [
    "O ju",
    "të cilët",
    "besuan!",
    "Mos",
    "hyni",
    "në shtëpitë",
    "e pejgamberit",
    "përveçse",
    "të",
    "jepet leje",
    "për ju",
    "për",
    "ushqim",
    "pa",
    "shikuar",
    "përgatitjen e tij",
    "mirëpo",
    "kur",
    "ju ftoheni",
    "atëherë hyni",
    "e kur",
    "ushqeheni",
    "atëherë shpërndahuni",
    "duke mos",
    "qëndruar",
    "për bisedë.",
    "Vërtet",
    "ajo",
    "ishte",
    "(që) e lëndonte",
    "pejgamberin",
    "e ai turpërohej",
    "prej jush",
    "kurse Allahu",
    "nuk",
    "turpërohet",
    "nga",
    "e vërteta.",
    "E kur",
    "të kërkoni prej tyre",
    "(ndonjë) gjë",
    "atëherë kërkoni prej tyre",
    "nga",
    "mbrapa",
    "një perde.",
    "Ajo (është)",
    "më e dëlirë",
    "për zemrat tuaja",
    "dhe zemrat e tyre.",
    "Dhe nuk",
    "është",
    "për ju",
    "që",
    "ju ta lëndoni",
    "të dërguarin",
    "e Allahut",
    "dhe as",
    "të",
    "martoheni",
    "me gratë e tij",
    "nga",
    "pas tij",
    "asnjëherë.",
    "Vërtet",
    "ajo",
    "është",
    "tek",
    "Allahu",
    "madhore."
  ],
  "54": [
    "Nëse",
    "ju shfaqni",
    "ndonjë gjë",
    "apo",
    "e fshehni atë",
    "atëherë vërtet",
    "Allahu",
    "është",
    "për çdo",
    "gjë",
    "i Gjithëdijshëm."
  ],
  "55": [
    "Nuk (ka)",
    "gjynah",
    "për to",
    "ndaj",
    "baballarëve të tyre",
    "dhe as",
    "bijve të tyre",
    "dhe as",
    "vëllezërve të tyre",
    "dhe as",
    "bijve",
    "të vëllezërve të tyre",
    "dhe as",
    "bijve",
    "të motrave të tyre",
    "dhe as",
    "grave të tyre",
    "dhe as",
    "asaj çfarë",
    "posedojnë",
    "të djathtat e tyre.",
    "Dhe druajuni",
    "Allahut!",
    "Vërtet",
    "Allahu",
    "është",
    "për",
    "çdo",
    "gjë",
    "Dëshmues."
  ],
  "56": [
    "Vërtet",
    "Allahu",
    "dhe engjëjt e Tij",
    "dërgojnë bekime",
    "mbi",
    "pejgamberin.",
    "O ju",
    "të cilët",
    "besuan!",
    "Dërgoni bekime",
    "mbi të",
    "dhe përshëndeteni atë",
    "me përshëndetje!"
  ],
  "57": [
    "Vërtet",
    "ata të cilët",
    "njollosin",
    "Allahun",
    "dhe të dërguarin e Tij",
    "i mallkoi ata",
    "Allahu",
    "në",
    "këtë botë",
    "dhe në të fundit",
    "dhe u përgatiti",
    "atyre",
    "dënim",
    "poshtërues."
  ],
  "58": [
    "Dhe ata të cilët",
    "njollosin",
    "besimtarët",
    "dhe besimtaret",
    "për diç tjetër",
    "të asaj çfarë",
    "ata fituan",
    "sigurisht",
    "ata u ngarkuan",
    "me shpifje",
    "dhe mëkat",
    "të kulluar."
  ],
  "59": [
    "O ti",
    "pejgamber!",
    "Thuaju",
    "grave tua",
    "dhe bijave tua",
    "dhe grave",
    "të besimtarëve",
    "të lëshojnë",
    "mbi to",
    "prej",
    "petkave të tyre.",
    "Ajo (është)",
    "më e duhur",
    "që",
    "ato të njihen",
    "e të mos",
    "njollosen.",
    "Dhe është",
    "Allahu",
    "Falës",
    "Mëshirues."
  ],
  "60": [
    "Po nëse",
    "nuk",
    "ndalen",
    "hipokritët",
    "dhe ata të cilët",
    "në",
    "zemrat e tyre",
    "(kanë) sëmundje",
    "edhe ngatërrestarët",
    "në",
    "Medine",
    "sigurisht Ne do të të japim ty pushtet",
    "mbi ta",
    "pastaj",
    "nuk",
    "do të qëndrojnë në fqinjësinë tënde",
    "në të",
    "veçse",
    "për pak."
  ],
  "61": [
    "Të mallkuar!",
    "Kudo që",
    "gjenden",
    "kapen",
    "dhe masakrohen",
    "me masakër."
  ],
  "62": [
    "Ligji",
    "i Allahut",
    "për",
    "ata të cilët",
    "kaluan",
    "nga",
    "përpara.",
    "Dhe nuk do të",
    "gjesh",
    "në ligjin",
    "e Allahut",
    "asnjë ndryshim."
  ],
  "63": [
    "Të pyesin ty",
    "njerëzit",
    "për",
    "Kijametin.",
    "Thuaj",
    "\"Vetëm",
    "dija e tij (është)",
    "tek",
    "Allahu",
    "e çka",
    "bën ti të dish",
    "mbase",
    "Kijameti",
    "është",
    "afër\"."
  ],
  "64": [
    "Vërtet",
    "Allahu",
    "i mallkoi",
    "mohuesit",
    "dhe ua përgatiti",
    "atyre",
    "zjarrin flakërues."
  ],
  "65": [
    "Të gjithmonshëm",
    "në të",
    "përherë.",
    "Nuk",
    "ata (do të) gjejnë",
    "mbrojtës",
    "dhe as",
    "ndihmës."
  ],
  "66": [
    "Ditën kur",
    "do të përmbysen",
    "fytyrat e tyre",
    "në",
    "zjarr",
    "ata (do të) thonë",
    "\"Ah sikur ne",
    "t'i bindeshim",
    "Allahut",
    "dhe t'i bindeshim",
    "të dërguarit!\""
  ],
  "67": [
    "Dhe ata thonë",
    "\"Zoti ynë!",
    "Vërtet ne",
    "iu bindëm",
    "shefave tanë",
    "dhe elitës sonë",
    "po ata na devijuan ne",
    "(nga) rruga\"."
  ],
  "68": [
    "Zoti ynë!",
    "Jepu atyre",
    "dyfishin",
    "prej",
    "dënimit",
    "dhe mallkoji ata",
    "me nëmë",
    "të madhe!"
  ],
  "69": [
    "O ju",
    "të cilët",
    "besuan!",
    "Mos",
    "jini",
    "sikur ata të cilët",
    "e lëndonin",
    "Musain",
    "prandaj e dëliri atë",
    "Allahu",
    "prej asaj çfarë",
    "ata thonin",
    "dhe ai ishte",
    "tek",
    "Allahu",
    "i ngritur."
  ],
  "70": [
    "O ju",
    "të cilët",
    "besuan!",
    "Druajuni",
    "Allahut",
    "dhe thuani",
    "fjalë",
    "të drejtë!"
  ],
  "71": [
    "Ai do të përmirësojë",
    "për ju",
    "punët tuaja",
    "dhe do t'ju falë",
    "ju",
    "mëkatet tuaja.",
    "E kushdo që",
    "(i) bindet",
    "Allahut",
    "dhe të dërguarit të Tij",
    "sigurisht",
    "ka arritur",
    "një arritje",
    "të madhe."
  ],
  "72": [
    "Vërtet Ne",
    "e ofruam",
    "amanetin",
    "para",
    "qiejve",
    "dhe tokës",
    "dhe kodrave",
    "por ato refuzuan",
    "ta",
    "bartin atë",
    "dhe u frikësuan",
    "prej tij",
    "dhe e barti atë",
    "njeriu.",
    "Vërtet ai",
    "ishte",
    "zullumqar",
    "injorant."
  ],
  "73": [
    "Që t'i dënojë",
    "Allahu",
    "hipokritët",
    "dhe hipokritet",
    "edhe idhujtarët",
    "dhe idhujtaret",
    "dhe të pranojë pendimin",
    "Allahu",
    "për",
    "besimtarët",
    "dhe besimtaret.",
    "Dhe është",
    "Allahu",
    "Falës",
    "Mëshirues."
  ]
}