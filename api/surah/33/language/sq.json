{
  "1": [
    "O ti",
    "pejgamber!",
    "Druaju",
    "Allahut",
    "dhe mos",
    "iu bind",
    "mohuesve",
    "dhe hipokritëve!",
    "Vërtet",
    "Allahu",
    "është",
    "i Gjithëdijshëm",
    "i Urtë."
  ],
  "2": [
    "Dhe ndiq",
    "atë çfarë",
    "shpallet",
    "tek ti",
    "prej",
    "Zotit tënd!",
    "Vërtet",
    "Allahu",
    "është",
    "për atë çfarë",
    "ju punoni",
    "i Gjithëinformuar."
  ],
  "3": [
    "Dhe mbështetu",
    "tek",
    "Allahu",
    "dhe mjafton",
    "Allahu",
    "si garantues."
  ],
  "4": [
    "Nuk",
    "bëri",
    "Allahu",
    "për asnjë burrë",
    "nga",
    "dy zemra",
    "në",
    "brendinë e tij.",
    "Dhe nuk",
    "i bëri",
