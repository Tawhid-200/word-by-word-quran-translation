[
  "Kur",
  "të vijnë tek ti",
  "hipokritët",
  "thonë",
  "\"Ne dëshmojmë",
  "se vërtet ti (je)",
  "i dërguari",
  "i Allahut\"",
  "e Allahu",
  "e di",
  "se vërtet ti (je)",
  "i dërguari i Tij",
  "dhe Allahu",
  "dëshmon",
  "(se) vërtet",
  "hipokritët",
  "(janë) gënjeshtarë.",
  "Ata morën",
  "premtimet e tyre",
  "si mburojë",
  "prandaj ata penguan",
  "nga",
  "rruga",
  "e Allahut.",
  "Vërtet ata",
  "sa keq (është)",
  "ajo çfarë",
  "ata ishin",
  "duke punuar!",
  "Kjo",
  "ngase ata",
  "besuan",
  "pastaj",
  "mohuan",
  "prandaj u vulos",
  "mbi",
  "zemrat e tyre",
  "kështu që ata",
  "nuk",
  "kuptojnë.",
  "E kur",
  "t'i shohësh ata",
  "të mahnisin ty",
  "trupat e tyre",
  "e nëse",
  "ata flasin",
  "ti dëgjon",
  "fjalën e tyre.",
  "Ata janë sikur",
  "trunguj",
  "të mbështetur",
  "mendojnë se",
  "çdo",
  "thirrje",
  "(është) kundër tyre.",
  "Ata (janë)",
  "armiq",
  "prandaj ruaju prej tyre!",
  "I vraftë",
  "Allahu",
  "si",
  "shmangen (nga e vërteta)!",
  "E kur",
  "u thuhet",
  "atyre",
  "\"Ejani",
  "të kërkojë falje",
  "për ju",
  "i dërguari",
  "i Allahut",
  "shtrembërojnë",
  "kokat e tyre",
  "dhe ti i sheh ata",
  "(si) zmbrapsen",
  "(duke qenë) ata",
  "mendjemëdhenj.",
  "Njësoj (është)",
  "për ta",
  "a kërkove falje",
  "për ta",
  "apo",
  "nuk",
  "kërkove falje",
  "për ta",
  "nuk do t'ua",
  "falë",
  "Allahu",
  "atyre.",
  "Vërtet",
  "Allahu",
  "nuk",
  "e udhëzon",
  "popullin",
  "e prishur.",
  "Ata (janë)",
  "të cilët",
  "thonë",
  "\"Mos",
  "shpenzoni",
  "për",
  "këdo që",
  "(është) te",
  "i dërguari",
  "i Allahut",
  "derisa",
  "të shpërndahen!\"",
  "E Allahut i përkasin",
  "thesaret",
  "e qiejve",
  "dhe të tokës",
  "mirëpo",
  "hipokritët",
  "nuk",
  "kuptojnë",
  "Ata thonë:",
  "\"Nëse",
  "ne kthehemi",
  "në",
  "Medine",
  "gjithsesi do ta nxjerrë",
  "më i forti",
  "nga aty",
  "më të dobëtin!\"",
  "Kurse Allahut i përket",
  "fuqia",
  "dhe të dërguarit të Tij",
  "dhe besimtarëve",
  "mirëpo",
  "hipokritët",
  "nuk",
  "dinë.",
  "O ju",
  "të cilët",
  "besuat",
  "mos",
  "t'ju joshin",
  "pasuritë tuaja",
  "e as",
  "fëmijët tuaj",
  "nga",
  "përmendja",
  "e Allahut.",
  "E kushdo që",
  "bën",
  "atë",
  "atëherë të tillët",
  "ata (janë)",
  "të humburit.",
  "Dhe shpenzoni",
  "nga",
  "ajo çfarë",
  "Ne ju furnizuam ju",
  "nga",
  "përpara",
  "se",
  "t'i vijë",
  "ndonjërit nga ju",
  "vdekja",
  "e ai të thotë",
  "\"Zoti im",
  "sikur",
  "t'më kishe shtyrë",
  "deri",
  "(në) një afat",
  "të afërt",
  "ashtu që të jepja lëmoshë",
  "dhe të bëhesha",
  "nga",
  "punëmirët!\"",
  "E nuk do t'ia",
  "shtyejë (për më vonë)",
  "Allahu",
  "asnjë personi",
  "kur",
  "t'i vijë",
  "afati i caktuar.",
  "E Allahu (është)",
  "i Mirëinformuar",
  "për atë çfarë",
  "ju punoni."
]