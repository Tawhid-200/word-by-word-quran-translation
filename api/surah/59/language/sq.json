{
  "1": [
    "E lartëson",
    "Allahun",
    "ajo çfarë (është)",
    "në",
    "qiejt",
    "dhe ajo çfarë (është)",
    "në",
    "tokë",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ],
  "2": [
    "Ai (është)",
    "i Cili",
    "i nxori",
    "ata të cilët",
    "mohuan",
    "nga",
    "pasuesit",
    "e Librit",
    "nga",
    "shtëpitë e tyre",
    "në të parin",
    "tubim.",
    "Nuk",
    "ju menduat",
    "që",
    "ata (do të) dalin",
    "dhe menduan",
    "se ata",
    "(do t'i) mbrojnë ata",
    "fortifikatat e tyre",
    "nga",
    "Allahu",
    "por u erdhi atyre",
    "Allahu",
    "nga",
    "aty ku",
    "nuk",
    "ata llogaritën",
    "dhe Ai hodhi",
    "në",
    "zemrat e tyre",
    "frikën",
    "ata i rrënojnë",
    "shtëpitë e tyre",
    "me duart e tyre",
    "dhe duart",
    "e besimtarëve",
    "prandaj merrni mësim",
    "o posedues",
    "të vëzhgimit."
  ],
  "3": [
    "E sikur të mos",
    "të",
    "obligonte",
    "Allahu",
    "atyre",
    "shpërnguljen",
    "do t'i dënonte ata",
    "në",
    "këtë botë",
    "dhe ata do të kenë",
    "në",
    "jetën e fundit",
    "dënimin",
    "e zjarrit."
  ],
  "4": [
    "Kjo",
    "ngase ata",
    "e kundërshtuan",
    "Allahun",
    "dhe të dërguarin e Tij",
    "e kushdo që",
    "e kundërshton",
    "Allahun",
    "atëherë vërtet",
    "Allahu",
    "(është) i ashpër",
    "(në) ndëshkim."
  ],
  "5": [
    "Nuk",
    "ju këputët",
    "asnjë",
    "pemë palme",
    "apo",
    "ju e latë atë",
    "të ngritur",
    "mbi",
    "rrënjët e saj",
    "pos me lejen",
    "e Allahut",
    "dhe që Ai t'i poshtërojë",
    "të prishurit."
  ],
  "6": [
    "Dhe çfarëdo që",
    "dhuroi",
    "Allahu",
    "për",
    "të dërguarin e Tij",
    "prej tyre",
    "nuk",
    "ju hipët",
    "për të",
    "asnjë",
    "kalë",
    "dhe as",
    "deve",
    "mirëpo",
    "Allahu",
    "i bën mbizotërues",
    "të dërguarit e Tij",
    "mbi",
    "atë që",
    "Ai dëshiron",
    "e Allahu",
    "mbi",
    "çdo",
    "gjë",
    "(është) i Plotfuqishëm."
  ],
  "7": [
    "Ajo çfarë",
    "dhuroi",
    "Allahu",
    "për",
    "të dërguarin e Tij",
    "nga",
    "banorët",
    "e vendeve",
    "atëherë (ajo) i përket Allahut",
    "dhe të dërguarit",
    "dhe për",
    "të afërmit",
    "dhe jetimëve",
    "dhe nevojtarëve",
    "dhe (personit)",
    "(në) rrugë",
    "ashtu që",
    "(të) mos",
    "jetë",
    "qarkullim",
    "mes",
    "të pasurve",
    "nga ju.",
    "Dhe atë çka",
    "ju jep",
    "i dërguari",
    "merreni atë",
    "dhe atë çka",
    "ju ndalon",
    "prej saj",
    "largohuni!",
    "Dhe druajuni",
    "Allahut",
    "vërtet",
    "Allahu",
    "(është) i ashpër",
    "(në) ndëshkim."
  ],
  "8": [
    "Për të varfrit",
    "emigrues",
    "të cilët",
    "u nxorën",
    "nga",
    "shtëpitë e tyre",
    "dhe pasuritë e tyre ë",
    "ata synojnë",
    "mirësi",
    "nga",
    "Allahu",
    "dhe kënaqësi",
    "dhe e ndihmojnë",
    "Allahun",
    "dhe të dërguarin e Tij.",
    "Të tillët",
    "ata (janë)",
    "të sinqertët."
  ],
  "9": [
    "Dhe ata të cilët",
    "përgatitën",
    "vendbanimin",
    "dhe besimin",
    "nga",
    "përpara tyre",
    "ata e duan",
    "këdo që",
    "shpërngulet",
    "tek ta",
    "dhe nuk",
    "gjejnë",
    "në",
    "gjokset e tyre",
    "(ndonjë) nevojë",
    "nga ajo çfarë",
    "u është dhënë atyre",
    "dhe japin përparësi",
    "ndaj",
    "vetes së tyre",
    "edhepse",
    "ishte",
    "për ta",
    "nevojë e madhe.",
    "E kushdo që",
    "(i) ruhet",
    "lakmisë",
    "së vetes së tij",
    "të tillët",
    "ata (janë)",
    "të shpëtuarit."
  ],
  "10": [
    "E ata të cilët",
    "erdhën",
    "nga",
    "pas tyre",
    "thonë",
    "\"Zoti ynë",
    "(na) fal",
    "ne",
    "dhe vëllezërit tanë",
    "të cilët",
    "na paraprinë",
    "me besim",
    "dhe mos",
    "fut",
    "në",
    "zemrat tona",
    "urrejtje",
    "për ata të cilët",
    "besuan",
    "Zoti ynë",
    "vërtet Ti (je)",
    "shumë i Butë",
    "Mëshirues\"."
  ],
  "11": [
    "A nuk",
    "ti sheh",
    "tek",
    "ata të cilët",
    "u bënë hipokritë",
    "(u) thonë",
    "vëllezërve të tyre",
    "të cilët",
    "mohuan",
    "nga",
    "pasuesit",
    "e librit",
    "\"Nëse",
    "ju dëboheni",
    "ne do të dalim gjithsesi",
    "me ju",
    "dhe nuk",
    "(do t'i) bindemi",
    "lidhur me ju",
    "askujt",
    "asnjëherë",
    "e nëse",
    "ju vriteni",
    "ne do t'ju ndihmojmë gjithsesi\".",
    "E Allahu",
    "dëshmon",
    "vërtet ata (janë)",
    "gënjeshtarë."
  ],
  "12": [
    "Nëse",
    "ata dëbohen",
    "nuk",
    "dalin",
    "me ta",
    "e nëse",
    "vriten",
    "nuk",
    "i përkrahin ata",
    "e nëse",
    "i përkrahin ata",
    "do t'i kthejnë gjithsesi",
    "shpinat",
    "pastaj",
    "nuk",
    "përkrahen."
  ],
  "13": [
    "Vërtet ju (jeni)",
    "(me) më të madhe",
    "frikë",
    "në",
    "kraharorët e tyre",
    "sesa",
    "(frika nga) Allahu.",
    "Kjo (është)",
    "ngase ata (janë)",
    "një popull",
    "nuk",
    "kuptojnë."
  ],
  "14": [
    "Nuk",
    "ata ju luftojnë ju",
    "të gjithë",
    "përveç",
    "në",
    "vendbanime",
    "të fortifikuara",
    "apo",
    "nga",
    "mbrapa",
    "mureve.",
    "Armiqësia",
    "mes tyre",
    "(është) e ashpër.",
    "Ti i konsideron ata",
    "të gjithë (të bashkuar)",
    "ndërkohë që zemrat e tyre (janë)",
    "të përçara.",
    "Kjo (është)",
    "ngase ata (janë)",
    "një popull",
    "nuk",
    "mendojnë."
  ],
  "15": [
    "Sikurse shembulli",
    "i atyre të cilët",
    "nga",
    "para tyre",
    "afër",
    "ata shijuan",
    "belanë",
    "e punës së tyre",
    "dhe ata kanë",
    "një dënim",
    "të dhembshëm."
  ],
  "16": [
    "Sikurse shembulli",
    "i shejtanit",
    "kur",
    "i tha",
    "njeriut",
    "\"Moho (Allahun)!\"",
    "E pasi që",
    "ai mohoi",
    "tha",
    "\"Vërtet unë (jam)",
    "i larguar",
    "prej teje",
    "vërtet unë",
    "(i) frikësohem",
    "Allahut",
    "Zotit",
    "të botëve\"."
  ],
  "17": [
    "Prandaj ishte",
    "përfundimi i atyre dyve",
    "se ata dy (do të jenë)",
    "në",
    "zjarr",
    "të gjithmonshëm",
    "në të",
    "dhe ai (është)",
    "ndëshkimi",
    "i zullumqarëve."
  ],
  "18": [
    "O ju",
    "të cilët",
    "besuan",
    "druajuni",
    "Allahut",
    "dhe le të shikojë",
    "(çdo) person",
    "çfarë",
    "përgatiti",
    "për nesër",
    "dhe druajuni",
    "Allahut",
    "vërtet",
    "Allahu",
    "(është) i Gjithëinformuar",
    "për atë çfarë",
    "ju punoni."
  ],
  "19": [
    "Dhe mos",
    "jini",
    "sikurse ata të cilët",
    "harruan",
    "Allahun",
    "prandaj Ai bëri që ata harruan",
    "veten e tyre",
    "të tillët",
    "ata (janë)",
    "të prishurit."
  ],
  "20": [
    "Nuk",
    "barazohen",
    "banorët",
    "e zjarrit",
    "dhe banorët",
    "e xhenetit.",
    "Banorët",
    "e xhenetit",
    "ata (janë)",
    "fituesit."
  ],
  "21": [
    "Sikur",
    "Ne ta zbritnim",
    "këtë",
    "Kur'an",
    "mbi",
    "një kodër",
    "ti do ta shihje atë",
    "të përulur",
    "të thërrmuar",
    "nga",
    "frika",
    "e Allahut.",
    "Dhe këta (janë)",
    "shembujt",
    "Ne ua sjellim ato",
    "njerëzve",
    "ashtu që ata",
    "(të) mendojnë."
  ],
  "22": [
    "Ai (është)",
    "Allahu",
    "i Cili",
    "nuk (ka)",
    "zot",
    "përveç",
    "Tij.",
    "Njohësi",
    "i së padukshmes",
    "dhe i së dukshmes",
    "Ai (është)",
    "i Gjithëmëshirshmi",
    "Mëshiruesi."
  ],
  "23": [
    "Ai (është)",
    "Allahu",
    "i Cili",
    "nuk (ka)",
    "zot",
    "përveç",
    "Tij.",
    "Sunduesi",
    "i Shenjti",
    "Paqedhënësi",
    "Siguruesi",
    "Mbizotëruesi",
    "i Pamposhturi",
    "i Fuqishmi",
    "i Madhërishmi",
    "Lartësuar qoftë",
    "Allahu",
    "nga ajo çfarë",
    "ata i shoqërojnë."
  ],
  "24": [
    "Ai (është)",
    "Allahu",
    "Krijuesi",
    "Shpikësi",
    "Formësuesi",
    "Ai ka",
    "emrat",
    "më të bukur",
    "e lartëson",
    "Atë",
    "çfarë (është)",
    "në",
    "qiejt",
    "dhe tokë",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ]
