{
  "1": [
    "Sigurisht",
    "kanë fituar",
    "besimtarët."
  ],
  "2": [
    "Të cilët",
    "ata (janë)",
    "gjatë",
    "namazit të tyre",
    "të përulur."
  ],
  "3": [
    "Dhe të cilët",
    "ata (janë)",
    "nga",
    "pallavrat",
    "të shmangur."
  ],
  "4": [
    "Dhe të cilët",
    "ata (janë)",
    "për zeqatin",
    "veprues."
  ],
  "5": [
    "Dhe të cilët",
    "ata (janë)",
    "për pjesët e tyre intime",
    "ruajtës."
  ],
  "6": [
    "Përveç",
    "ndaj",
    "grave të tyre",
    "apo",
    "asaj çfarë",
    "posedojnë",
    "të djathtat e tyre",
    "atëherë ata",
    "nuk (janë)",
    "të qortuar."
  ],
  "7": [
    "E kushdo që",
    "kërkon",
    "përtej",
    "asaj",
    "atëherë të tillët",
    "ata (janë)",
    "shkelësit."
  ],
  "8": [
    "Dhe të cilët",
    "ata (janë)",
    "për amanetet e tyre",
    "dhe besën e tyre",
    "kujdestarë."
  ],
  "9": [
    "Dhe të cilët",
    "ata",
    "për",
    "namazet e tyre",
    "përkujdesen."
  ],
  "10": [
    "Të tillët",
    "ata (janë)",
    "trashëguesit."
  ],
  "11": [
    "Të cilët",
    "do të trashëgojnë",
    "Firdeusin.",
    "Ata (janë)",
    "në të",
    "të gjithmonshëm."
  ],
  "12": [
    "Dhe sigurisht",
    "Ne e krijuam",
    "njeriun",
    "nga",
    "një palcë",
    "prej",
    "balte."
  ],
  "13": [
    "Pastaj",
    "Ne e bëmë atë",
    "një farë",
    "në",
    "një vend",
    "të qëndrueshëm."
  ],
  "14": [
    "Pastaj",
    "Ne e sajuam",
    "farën",
    "(në) një droçkë gjaku",
    "pastaj Ne e sajuam",
    "droçkën e gjakut",
    "(në) një kafshatë mishi",
    "pastaj Ne e sajuam",
    "kafshatën e mishit",
    "(në) eshtra",
    "pastaj Ne i veshëm",
    "eshtrat",
    "(me) mish",
    "pastaj",
    "Ne e gatuajmë atë",
    "(si) një krijesë",
    "tjetër.",
    "Pra i bekuar qoftë",
    "Allahu",
    "më i miri",
    "i Sajuesve!"
  ],
  "15": [
    "Pastaj",
    "vërtet ju",
    "pas",
    "asaj",
    "sigurisht (jeni) të vdekshëm."
  ],
  "16": [
    "Pastaj",
    "vërtet ju",
    "ditën",
    "e Kijametit",
    "do të ringjalleni."
  ],
  "17": [
    "Dhe sigurisht",
    "Ne kemi krijuar",
    "mbi ju",
    "shtatë",
    "shtresa qiejsh",
    "dhe nuk",
    "Ne jemi",
    "ndaj",
    "krijimit",
    "të shkujdesur."
  ],
  "18": [
    "Dhe Ne zbritëm",
    "nga",
    "qielli",
    "ujë",
    "me masë",
    "pastaj Ne e depozitojmë atë",
    "në",
    "tokë.",
    "E vërtet Ne",
    "për",
    "largim",
    "të tij",
    "(jemi) sigurisht të Aftë."
  ],
  "19": [
    "Pastaj Ne prodhuam",
    "për ju",
    "me të",
    "kopshte",
    "nga",
    "palma",
    "dhe vreshta",
    "për ju",
    "në to",
    "(ka) fruta",
    "të shumta",
    "dhe prej tyre",
    "ju hani."
  ],
  "20": [
    "Dhe një pemë",
    "(që) del",
    "nga",
    "mali Tur",
    "i Sinait",
    "(e cila) nxjerr",
    "vaj",
    "dhe mëlmesë",
    "për ngrënësit."
  ],
  "21": [
    "Dhe vërtet",
    "për ju",
    "në",
    "bagëti",
    "(ka) sigurisht mësim.",
    "Ne ju japim të pini",
    "prej asaj çfarë",
    "(është) në",
    "barqet e tyre",
    "dhe për ju",
    "në to",
    "(ka) dobi",
    "të shumta",
    "dhe prej tyre",
    "ju hani."
  ],
  "22": [
    "Dhe mbi to",
    "edhe mbi",
    "anije",
    "ju barteni."
  ],
  "23": [
    "Dhe sigurisht",
    "Ne e dërguam",
    "Nuhun",
    "tek",
    "populli i tij",
    "e ai (u) tha",
    "\"O populli im!",
    "Adhuroni",
    "Allahun!",
    "Nuk (ka)",
    "për ju",
    "asnjë",
    "zot",
    "tjetër përveç Tij.",
    "A hiç nuk",
    "tuteni?\""
  ],
  "24": [
    "Por tha",
    "paria",
    "ata të cilët",
    "mohuan",
    "nga",
    "populli i tij",
    "\"Nuk (është)",
    "ky",
    "veçse",
    "një njeri",
    "sikurse ju",
    "ai dëshiron",
    "të",
    "të ngjitet",
    "mbi ju",
    "e sikur",
    "të donte",
    "Allahu",
    "do të zbriste",
    "engjëj.",
    "Nuk",
    "ne kemi dëgjuar",
    "për këtë",
    "nga",
    "baballarët tanë",
    "të parë."
  ],
  "25": [
    "Nuk (është)",
    "ai",
    "veçse",
    "një burrë",
    "në të",
    "(ka) çmendi",
    "prandaj pritni",
    "lidhur me të",
    "deri në",
    "një kohë\"."
  ],
  "26": [
    "Ai tha",
    "\"Zoti im!",
    "Më ndihmo mua",
    "për shkak se",
    "ata më përgënjeshtrojnë mua\"."
  ],
  "27": [
    "Kështu Ne i shpallëm",
    "atij",
    "që",
    "\"Ndërtoje",
    "anijen",
    "para Syve Tanë",
    "dhe frymëzimit Tonë",
    "e kur",
    "të vijë",
    "urdhri Ynë",
    "dhe të gufojë",
    "furra",
    "atëherë fut",
    "në të",
    "nga",
    "çdo (lloj)",
    "çift",
    "nga dy",
    "edhe familjen tënde",
    "përveç",
    "atyre që",
    "është paraprirë",
    "për të",
    "fjala",
    "prej tyre.",
    "Dhe mos",
    "M'u drejto",
    "lidhur me",
    "ata të cilët",
    "bënë zullum",
    "vërtet ata",
    "(janë) të fundosur."
  ],
  "28": [
    "E kur",
    "të hipësh",
    "ti",
    "dhe kushdo që",
    "(është) me ty",
    "në",
    "anije",
    "atëherë thuaj",
    "\"Falënderimi",
    "(është) për Allahun",
    "i Cili",
    "na shpëtoi ne",
    "nga",
    "populli",
    "zullumqar\"."
  ],
  "29": [
    "Dhe thuaj",
    "\"Zoti im!",
    "Më zbarko",
    "(në) një vend",
    "të bekuar",
    "e Ti (je)",
    "më i miri",
    "i zbarkuesve\"."
  ],
  "30": [
    "Vërtet",
    "në",
    "të",
    "(ka) sigurisht dëshmi",
    "e vërtet",
    "Ne jemi",
    "sigurisht Sprovuesit."
  ],
  "31": [
    "Pastaj",
    "Ne krijuam",
    "nga",
    "pas tyre",
    "një brez",
    "të tjerë."
  ],
  "32": [
    "Dhe Ne dërguam",
    "në mesin e tyre",
    "një të dërguar",
    "prej tyre",
    "që",
    "\"Adhuroni",
    "Allahun!",
    "Nuk (ka)",
    "për ju",
    "asnjë",
    "zot",
    "tjetër përveç Tij.",
    "A hiç nuk",
    "tuteni?\""
  ],
  "33": [
    "Dhe tha",
    "paria",
    "nga",
    "populli i tij",
    "të cilët",
    "mohuan",
    "dhe përgënjeshtruan",
    "takimin",
    "e jetës së fundit",
    "ndërkohë që Ne u dhamë atyre luksin",
    "në",
    "jetën",
    "e kësaj bote",
    "\"Nuk (është)",
    "ky",
    "veçse",
    "një njeri",
    "sikurse ju.",
    "Ai ha",
    "nga ajo çfarë",
    "ju hani",
    "prej tij",
    "dhe ai pi",
    "nga ajo çfarë",
    "ju pini."
  ],
  "34": [
    "E nëse",
    "ju i bindeni",
    "një njeriu",
    "sikurse ju",
    "vërtet ju",
    "atëherë",
    "(do të jeni) sigurisht humbës."
  ],
  "35": [
    "A ai ju premton ju",
    "se ju",
    "kur",
    "të vdisni",
    "e të jeni",
    "dhé",
    "e eshtra",
    "se ju",
    "(do të jeni) të nxjerrë?"
  ],
  "36": [
    "Sa larg",
    "sa larg",
    "(është) ajo çfarë",
    "ju premtohet!"
  ],
  "37": [
    "Nuk (është)",
    "ajo",
    "veçse",
    "jeta jonë",
    "e kësaj bote",
    "ne vdesim",
    "dhe ne jetojmë",
    "dhe nuk",
    "ne (do të jemi)",
    "të ringjallur."
  ],
  "38": [
    "Nuk (është)",
    "ai",
    "veçse",
    "një burrë",
    "ka trilluar",
    "për",
    "Allahun",
    "gënjeshtër",
    "dhe nuk",
    "ne",
    "në të",
    "(jemi) besimtarë\"."
  ],
  "39": [
    "Ai tha",
    "\"Zoti im!",
    "Më ndihmo",
    "për shkak se",
    "ata më përgënjeshtruan mua\"."
  ],
  "40": [
    "Ai tha",
    "\"Pas",
    "pak",
    "gjithsesi ata do të bëhen",
    "pishman\"."
  ],
  "41": [
    "Kështu që ata i mbërtheu",
    "britma",
    "me meritë",
    "dhe Ne i bëmë ata",
    "(si) gjethe të fishkura.",
    "Pra larg (qoftë)",
    "populli",
    "zullumqar!"
  ],
  "42": [
    "Pastaj",
    "Ne krijuam",
    "nga",
    "pas tyre",
    "breza",
    "të tjerë."
  ],
  "43": [
    "Nuk (mund)",
    "ta paraprijë",
    "asnjë",
    "popull",
    "afatin e tij",
    "dhe nuk",
    "ata (mund) ta shtyjnë (atë)."
  ],
  "44": [
    "Pastaj",
    "Ne i dërguam",
    "të dërguarit Tanë",
    "në mënyrë të njëpasnjëshme.",
    "Çdo (herë)",
    "që",
    "(i) vinte",
    "një populli",
    "i dërguari i tij",
    "ata e përgënjeshtronin atë",
    "prandaj Ne bëmë që ata të ndjekin",
    "disa prej tyre",
    "disa (të tjerë)",
    "dhe i bëmë ata",
    "rrëfenja.",
    "Pra larg (qoftë)",
    "populli",
    "(që) nuk",
    "besojnë!"
  ],
  "45": [
    "Pastaj",
    "Ne e dërguam",
    "Musain",
    "dhe vëllain e tij",
    "Harunin",
    "me dëshmitë Tona",
    "dhe një autoritet",
    "të qartë."
  ],
  "46": [
    "Tek",
    "faraoni",
    "dhe paria e tij",
    "por ata shfaqën arrogancë",
    "dhe ishin",
    "një popull",
    "kryelartë."
  ],
  "47": [
    "Atëherë ata thanë",
    "\"A ne t'i besojmë",
    "dy njerëzve",
    "sikurse ne",
    "ndërkohë që populli i atyre dyve",
    "ndaj nesh",
    "(janë) të nënshtruar?\""
  ],
  "48": [
    "Kësisoj ata i përgënjeshtruan ata dy",
    "e ata u bënë",
    "nga",
    "të shkatërruarit."
  ],
  "49": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Musait",
    "Librin",
    "ashtu që ata",
    "të udhëzoheshin."
  ],
  "50": [
    "Dhe Ne e bëmë",
    "birin",
    "e Merjemes",
    "dhe nënën e tij",
    "një dëshmi",
    "dhe Ne i strehuam ata dy",
    "rrëzë",
    "një kodre",
    "(që) kishte",
    "qetësi",
    "dhe përrua."
  ],
  "51": [
    "O ju",
    "të dërguar!",
    "Hani",
    "nga",
    "të mirat",
    "dhe punoni",
    "mirësi!",
    "Vërtet Unë (jam)",
    "për atë çfarë",
    "ju punoni",
    "i Gjithëdijshëm."
  ],
  "52": [
    "Dhe vërtet",
    "kjo",
    "feja juaj",
    "(është) fe",
    "një",
    "dhe Unë (jam)",
    "Zoti juaj",
    "prandaj ruajuni nga Unë!"
  ],
  "53": [
    "Por ata e copëtuan",
    "çështjen e tyre",
    "mes tyre",
    "në degëzime.",
    "Secili",
    "grup",
    "me atë çfarë",
    "ata kanë",
    "(janë) të kënaqur."
  ],
  "54": [
    "Prandaj lëri ata",
    "në",
    "turbullirën e tyre",
    "deri në",
    "një kohë."
  ],
  "55": [
    "A mendojnë ata",
    "se ajo çfarë",
    "Ne u shtojmë atyre",
    "me të",
    "nga",
    "pasuria",
    "dhe djemtë"
  ],
  "56": [
    "Ne ua shpejtojmë",
    "atyre",
    "në",
    "të mirat?",
    "Jo por",
    "nuk",
    "ata ndjejnë."
  ],
  "57": [
    "Vërtet",
    "ata të cilët",
    "ata",
    "nga",
    "frika",
    "e Zotit të tyre",
    "(janë) të druajtur."
  ],
  "58": [
    "Dhe ata të cilët",
    "ata",
    "në dëshmitë",
    "e Zotit të tyre",
    "besojnë."
  ],
  "59": [
    "Dhe ata të cilët",
    "ata",
    "me Zotin e tyre",
    "nuk",
    "i shoqërojnë (zota në adhurim)."
  ],
  "60": [
    "Dhe ata të cilët",
    "japin",
    "atë çfarë",
    "ata japin",
    "duke qenë zemrat e tyre",
    "plot frikë",
    "ngase ata",
    "tek",
    "Zoti i tyre",
    "(janë) kthyes."
  ],
  "61": [
    "Të tillët",
    "shpejtojnë",
    "drejt",
    "të mirave",
    "dhe ata",
    "në to",
    "(janë) të parët."
  ],
  "62": [
    "Dhe nuk",
    "Ne e ngarkojmë",
    "(asnjë) person",
    "veçse",
    "(sipas) kapacitetit të tij",
    "dhe tek Ne",
    "(është) një Libër",
    "(që) flet",
    "me vërtetësi",
    "dhe atyre",
    "nuk",
    "do t'u bëhet zullum."
  ],
  "63": [
    "Jo por",
    "zemrat e tyre",
    "(janë) në",
    "një turbullirë",
    "nga",
    "kjo",
    "dhe ata kanë",
    "vepra",
    "nga",
    "poshtë",
    "asaj",
    "ata",
    "për to",
    "(janë) punues."
  ],
  "64": [
    "Deri",
    "kur",
    "Ne i mbërthejmë",
    "të shfrenuarit e tyre",
    "me dënim",
    "papritur",
    "ata",
    "rënkojnë."
  ],
  "65": [
    "\"Mos",
    "rënkoni",
    "sot!",
    "Vërtet ju",
    "prej Nesh",
    "nuk",
    "do të ndihmoheni."
  ],
  "66": [
    "Sigurisht",
    "ishin",
    "ajetet e Mia",
    "recitoheshin",
    "për ju",
    "por ju vazhdimisht",
    "me",
    "thembrat tuaja",
    "zmbrapseshit."
  ],
  "67": [
    "(Duke qenë) arrogantë",
    "ndaj tij (Kur'anit)",
    "gjatë natës",
    "ju flisnit ligësi\"."
  ],
  "68": [
    "Vallë a nuk",
    "ata e bluajnë",
    "Fjalën (Kur'anin)",
    "apo",
    "(u) erdhi atyre",
    "ajo çfarë",
    "nuk",
    "(u) erdhi",
    "baballarëve të tyre",
    "të parë?"
  ],
  "69": [
    "Apo",
    "nuk",
    "ata e njohin",
    "të dërguarin e tyre",
    "kështu që ata",
    "të tij",
    "(janë) mohues?"
  ],
  "70": [
    "Apo",
    "ata thonë",
    "\"Në të (ka)",
    "çmenduri?\"",
    "Jo por",
    "ai (ua) solli atyre",
    "të vërtetën",
    "mirëpo shumica e tyre",
    "për të vërtetën",
    "(janë) urrejtës."
  ],
  "71": [
    "E sikur",
    "t'i ndiqte",
    "e vërteta",
    "dëshirat e tyre",
    "sigurisht do të prisheshin",
    "qiejt",
    "dhe toka",
    "dhe kushdo që",
    "(është) në to.",
    "Jo por",
    "Ne u kemi sjellë atyre",
    "përkujtuesin e tyre",
    "por ata",
    "ndaj",
    "përkujtuesit të tyre",
    "(janë) të shmangur."
  ],
  "72": [
    "Apo",
    "ti (u) kërkon atyre",
    "(ndonjë) pagesë?",
    "Por pagesa",
    "e Zotit tënd",
    "(është) më e mirë",
    "dhe Ai (është)",
    "më i Miri",
    "i Furnizuesve."
  ],
  "73": [
    "Dhe vërtet ti",
    "sigurisht i fton ata",
    "tek",
    "një rrugë",
    "e drejtë."
  ],
  "74": [
    "Dhe vërtet",
    "ata të cilët",
    "nuk",
    "besojnë",
    "në jetën e fundit",
    "nga",
    "rruga",
    "(janë) sigurisht të shmangur."
  ],
  "75": [
    "E sikur",
    "Ne t'i mëshironim ata",
    "dhe të largonim",
    "atë çfarë",
    "(ishte) në ta",
    "nga",
    "vështirësia",
    "sigurisht ata do të këmbëngulnin",
    "në",
    "rebelimin e tyre",
    "duke u endur verbërisht."
  ],
  "76": [
    "Dhe sigurisht",
    "Ne i mbërthyem ata",
    "me dënim",
    "por nuk",
    "ata iu përulen",
    "Zotit të tyre",
    "dhe nuk",
    "ata përgjërohen."
  ],
  "77": [
    "Deri",
    "kur",
    "Ne (ua) hapim",
    "atyre",
    "një derë",
    "(që) ka",
    "dënim",
    "të ashpër",
    "papritmas",
    "ata",
    "në të",
    "(do të jenë) të zhgënjyer."
  ],
  "78": [
    "Dhe Ai (është)",
    "i Cili",
    "prodhoi",
    "për ju",
    "dëgjimin",
    "dhe shikimet",
    "dhe ndjenjat.",
    "Sa pak",
    "(është) ajo çfarë",
    "ju falënderoni!"
  ],
  "79": [
    "Dhe Ai (është)",
    "i Cili",
    "ju shpërndau ju",
    "në",
    "tokë",
    "dhe tek Ai",
    "ju (do të) tuboheni."
  ],
  "80": [
    "Dhe Ai (është)",
    "i Cili",
    "jep jetë",
    "dhe sjell vdekje",
    "Dhe Atij i përket",
    "ndërrimi",
    "i natës",
    "dhe ditës.",
    "Vallë a nuk",
    "ju perceptoni?!"
  ],
  "81": [
    "Jo por",
    "ata thonë",
    "sikurse",
    "ajo çfarë",
    "thanë",
    "të parët."
  ],
  "82": [
    "Ata thanë",
    "\"A pasi",
    "ne të vdesim",
    "e të jemi",
    "dhé",
    "e eshtra",
    "a vërtet ne",
    "(do të jemi) sigurisht të ringjallur?"
  ],
  "83": [
    "Sigurisht",
    "na është premtuar",
    "ne",
    "dhe baballarëve tanë",
    "kjo",
    "nga",
    "përpara.",
    "Nuk (është)",
    "kjo",
    "përveç",
    "gojëdhënat",
    "e të parëve\"."
  ],
  "84": [
    "Thuaj",
    "\"E kujt (është)",
    "toka",
    "dhe kush (është)",
    "në të",
    "nëse",
    "ju jeni",
    "(që) dini?\""
  ],
  "85": [
    "Ata do të thonë",
    "\"E Allahut\".",
    "Thuaj",
    "\"Vallë a nuk",
    "përkujtoni?\""
  ],
  "86": [
    "Thuaj",
    "\"Kush (është)",
    "Zoti",
    "i qiejve",
    "shtatë",
    "dhe Zoti",
    "i Fronit",
    "Madhështor?\""
  ],
  "87": [
    "Ata do të thonë",
    "\"Allahu\".",
    "Thuaj",
    "\"Vallë a nuk",
    "druheni?\""
  ],
  "88": [
    "Thuaj",
    "\"Kush (është)",
    "në Dorën e Tij",
    "(është) mbisundimi",
    "i çdo",
    "gjëje",
    "dhe Ai",
    "mbron",
    "dhe nuk",
    "(mund të) mbrohet",
    "kundër Tij",
    "nëse",
    "ju jeni",
    "(që) dini?\""
  ],
  "89": [
    "Ata do të thonë",
    "\"Allahu\".",
    "Thuaj",
    "\"Oh, si",
    "magjepseni?!\""
  ],
  "90": [
    "Ja!",
    "Ne ua sollëm atyre",
    "të vërtetën",
    "por vërtet ata",
    "(janë) sigurisht gënjeshtarë."
  ],
  "91": [
    "Nuk",
    "mori",
    "Allahu",
    "asnjë",
    "fëmijë",
    "dhe nuk",
    "ështe",
    "me Të",
    "asnjë",
    "zot.",
    "Përndryshe",
    "sigurisht do të merrte",
    "secili",
    "zot",
    "atë çfarë",
    "ka krijuar",
    "dhe sigurisht do të ndërkreheshin",
    "disa prej tyre",
    "mbi",
    "disa (të tjerë).",
    "Lartësuar qoftë",
    "Allahu",
    "nga ajo çfarë",
    "ata përshkruajnë!"
  ],
  "92": [
    "Njohësi",
    "i së fshehtës",
    "dhe i së dukshmes.",
    "Pra, i Lartë është Ai",
    "nga ajo çfarë",
    "ata i shoqërojnë (zota në adhurim)!"
  ],
  "93": [
    "Thuaj",
    "\"Zoti im!",
    "Nëse",
    "Ti do t'më shfaqësh mua",
    "atë çfarë",
    "u premtohet atyre,"
  ],
  "94": [
    "Zoti im!",
    "Atëherë mos",
    "më bëj mua",
    "në mesin e",
    "popullit",
    "zullumqar!\""
  ],
  "95": [
    "Dhe vërtet Ne",
    "për (atë)",
    "që",
    "Ne të shfaqim ty",
    "atë çfarë",
    "Ne u premtojmë atyre",
    "(jemi) sigurisht të Aftë."
  ],
  "96": [
    "Largoje",
    "me atë të cilën",
    "ajo (është)",
    "më e mira",
    "të keqen.",
    "Ne",
    "dimë më mirë",
    "atë çfarë",
    "ata përshkruajnë."
  ],
  "97": [
    "Dhe thuaj",
    "\"Zoti im!",
    "unë mbrohem",
    "me Ty",
    "nga",
    "cytjet",
    "e djajve."
  ],
  "98": [
    "Dhe unë mbrohem",
    "me Ty",
    "Zoti im",
    "që",
    "ata të më afrohen\"."
  ],
  "99": [
    "Deri",
    "kur",
    "(i) vjen",
    "ndonjërit prej tyre",
    "vdekja",
    "ai thotë",
    "\"Zoti im!",
    "Më ktheni mua!"
  ],
  "100": [
    "Ashtu që",
    "unë (të) punoj",
    "mirësi",
    "në atë çfarë",
    "unë kam lënë\".",
    "Kurrsesi!",
    "Vërtet ajo (është)",
    "një fjalë",
    "ai",
    "e flet atë",
    "e nga",
    "përpara tyre",
    "(është) një perde",
    "deri në",
    "ditën kur",
    "ata (do të) ringjallen."
  ],
  "101": [
    "E kur",
    "të fryhet",
    "në",
    "bori",
    "atëherë nuk (ka)",
    "lidhje familjare",
    "mes tyre",
    "atë ditë",
    "dhe nuk",
    "ata e pyesin njëri-tjetrin."
  ],
  "102": [
    "E kujtdo që",
    "(i) rëndohen",
    "peshoret e tij",
    "atëherë të tillët",
    "ata (janë)",
    "të suksesshmit."
  ],
  "103": [
    "Ndërsa kujtdo që",
    "janë të lehta",
    "peshoret e tij",
    "atëherë të tillët",
    "(janë) ata të cilët",
    "humbën",
    "veten e tyre",
    "në",
    "xhehenem",
    "(janë) të gjithmonshëm."
  ],
  "104": [
    "Do t'i përvëlojë",
    "fytyrat e tyre",
    "zjarri",
    "dhe ata",
    "në të",
    "(do të jenë) të ngërdheshur."
  ],
  "105": [
    "A nuk",
    "ishin",
    "ajetet e Mia",
    "recitoheshin",
    "për ju",
    "e ju",
    "ato",
    "i përgënjeshtronit?\""
  ],
  "106": [
    "Ata (do të) thonë",
    "\"Zoti ynë",
    "(na) pushtoi",
    "ne",
    "mjerimi ynë",
    "dhe ne ishim",
    "një popull",
    "të humbur."
  ],
  "107": [
    "Zoti ynë!",
    "Na nxirr",
    "prej tij",
    "e nëse",
    "ne kthehemi",
    "atëherë vërtet ne",
    "(do të jemi) zullumqarë\"."
  ],
  "108": [
    "Ai (do të) thotë:",
    "\"Trusni more",
    "aty",
    "dhe mos",
    "(më) folni hiç!\""
  ],
  "109": [
    "Vërtet",
    "ishte",
    "një grup",
    "prej",
    "robërve të Mi",
    "(që) thonin",
    "\"Zoti ynë!",
    "Ne besuam",
    "prandaj (na) fal",
    "ne",
    "dhe na mëshiro",
    "e Ti (je)",
    "më i miri",
    "i mëshiruesve!\""
  ],
  "110": [
    "Por ju i morët ata",
    "(me) tallje",
    "derisa",
    "ata ju bënë të harroni",
    "përkujtimin Tim",
    "dhe ju ishit",
    "me ta",
    "qeshnit."
  ],
  "111": [
    "Vërtet Unë",
    "i shpërbleva ata",
    "sot",
    "ngase",
    "ata duruan",
    "vërtet ata",
    "ata (janë)",
    "fitimtarët."
  ],
  "112": [
    "Ai (Allahu) do të thotë",
    "\"Sa",
    "qëndruat",
    "në",
    "tokë",
    "(me) numrin",
    "e viteve?\""
  ],
  "113": [
    "Ata do të thonë",
    "\"Ne qëndruam",
    "një ditë",
    "apo",
    "një pjesë",
    "të një dite",
    "por pyeti",
    "numëruesit!\""
  ],
  "114": [
    "Ai (Allahu) do të thotë",
    "\"Nuk",
    "keni qëndruar",
    "veçse",
    "pak",
    "sikur",
    "njëmend ju",
    "(të) ishit",
    "(që) dinit!"
  ],
  "115": [
    "Vallë a menduat ju",
    "se vetëm",
    "Ne ju krijuam ju",
    "kot",
    "dhe se ju",
    "tek Ne",
    "nuk",
    "do të ktheheni?\""
  ],
  "116": [
    "Pra lartësuar qoftë",
    "Allahu",
    "Sunduesi",
    "i Vërteti!",
    "Nuk (ka)",
    "zot",
    "veçse",
    "Ai",
    "Zoti",
    "i Fronit",
    "Fisnik."
  ],
  "117": [
    "E kushdo që",
    "(i) lutet",
    "me",
    "Allahun",
    "një zoti",
    "tjetër",
    "nuk (ka)",
    "provë",
    "për të",
    "në të.",
    "Atëherë vetëm",
    "llogaria e tij",
    "(është) tek",
    "Zoti i tij.",
    "Vërtet",
    "nuk",
    "shpëtojnë",
    "mohuesit."
  ],
  "118": [
    "Dhe thuaj",
    "\"Zoti im!",
    "Fal",
    "dhe mëshiro",
    "dhe Ti (je)",
    "më i Miri",
    "i mëshiruesve\"."
  ]
}