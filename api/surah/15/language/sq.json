{
  "1": [
    "Elif, Lam, Ra.",
    "Këto (janë)",
    "ajetet",
    "e Librit",
    "dhe të një Kur'ani",
    "të qartë."
  ],
  "2": [
    "Shpeshherë",
    "dëshirojnë",
    "ata të cilët",
    "mohuan",
    "sikur",
    "të ishin",
    "muslimanë."
  ],
  "3": [
    "Lëri ata",
    "të hanë",
    "e të kënaqen",
    "e t'i joshë ata",
    "shpresa",
    "por do të",
    "dinë."
  ],
  "4": [
    "Dhe nuk",
    "Ne shkatërruam",
    "asnjë",
    "vendbanim",
    "përveçse",
    "ai kishte",
    "një përcaktim",
    "të ditur."
  ],
  "5": [
    "Nuk (mund)",
    "ta paraprijë",
    "asnjë",
    "popull",
    "afatin e tij",
    "dhe nuk (mund)",
    "ta shtyejnë (për më vonë)."
  ],
  "6": [
    "Dhe ata do të thonë",
    "\"O ti",
    "të cilit",
    "iu zbrit",
    "atij",
    "përkujtimi (Kur'ani)",
    "vërtet ti (je)",
    "i çmendur\"."
  ],
  "7": [
    "Përse",
    "nuk",
    "na sjell ne",
    "engjëjt",
    "nëse",
    "ti je",
    "nga",
    "të sinqertët?"
  ],
  "8": [
    "Nuk",
    "Ne i zbresim",
    "engjëjt",
    "përveçse",
    "me të vërtetën",
    "dhe nuk",
    "ata do të ishin",
    "atëherë",
    "të afatizuar."
  ],
  "9": [
    "Vërtet Ne",
    "Ne",
    "zbritëm",
    "përkujtimin (Kur'anin)",
    "dhe vërtet Ne",
    "për të (jemi)",
    "Ruajtës."
  ],
  "10": [
    "Dhe sigurisht",
    "Ne dërguam",
    "nga",
    "para teje",
    "në",
    "grupet",
    "e brezave të parë."
  ],
  "11": [
    "Dhe nuk",
    "u vjen atyre",
    "asnjë",
    "i dërguar",
    "përveçse",
    "ata ishin",
    "me të",
    "talleshin."
  ],
  "12": [
    "Kështu",
    "Ne e fusim atë (mohimin)",
    "në",
    "zemrat",
    "e kriminelëve."
  ],
  "13": [
    "Nuk",
    "ata besojnë",
    "në të (Kur'anin)",
    "e tashmë",
    "ka mbaruar",
    "ligji (i ndëshkimit)",
    "i brezave të parë."
  ],
  "14": [
    "E sikur",
    "Ne t'ua hapnim",
    "atyre",
    "një portë",
    "nga",
    "qielli",
    "e ata në vazhdimësi",
    "në të",
    "(të) ngjiteshin"
  ],
  "15": [
    "ata do të thonin",
    "\"Vetëm",
    "u shushatën",
    "shikimet tona.",
    "Jo por",
    "ne (jemi)",
    "një popull",
    "të magjepsur\"."
  ],
  "16": [
    "Dhe sigurisht",
    "Ne vendosëm",
    "në",
    "qiell",
    "yjësi",
    "dhe i stolisëm ato",
    "për shikuesit."
  ],
  "17": [
    "Dhe Ne i ruajtëm atë",
    "nga",
    "çdo",
    "shejtan",
    "i mallkuar."
  ],
  "18": [
    "Përveç",
    "atij që",
    "përgjon",
    "dëgjimin",
    "dhe përnjëherë e ndjek atë",
    "një yll i zjarrtë",
    "i dukshëm."
  ],
  "19": [
    "Dhe tokën",
    "Ne e zgjeruam atë",
    "dhe vendosëm",
    "në të",
    "kodra",
    "dhe bëmë që të mbijë",
    "në të",
    "nga",
    "çdo",
    "gjë",
    "e harmonishme."
  ],
  "20": [
    "Dhe Ne bëmë",
    "për ju",
    "në të",
    "mjete jetese",
    "edhe atë",
    "ju nuk jeni",
    "për të",
    "furnizues."
  ],
  "21": [
    "Dhe nuk (ka)",
    "asnjë",
    "gjë",
    "përveçse",
    "tek Ne (janë)",
    "rezervat e tij",
    "dhe nuk",
    "Ne e zbresim atë",
    "përveçse",
    "me një masë",
    "të njohur."
  ],
  "22": [
    "Dhe Ne i dërgojmë",
    "erërat",
    "për pllenim",
    "ashtu që Ne zbresim",
    "nga",
    "qielli",
    "ujë",
    "e Ne ju japim të pini atë",
    "dhe nuk",
    "ju (jeni)",
    "për të",
    "rezervues."
  ],
  "23": [
    "Dhe vërtet Ne",
    "vetëm Ne",
    "japim jetë",
    "dhe sjellim vdekje",
    "dhe Ne (jemi)",
    "Trashëguesit."
  ],
  "24": [
    "Dhe sigurisht",
    "Ne i dimë",
    "ata që ishin përpara",
    "prej jush",
    "dhe sigurisht",
    "Ne i dimë",
    "ata që do të vijnë (pas jush)."
  ],
  "25": [
    "Dhe vërtet",
    "Zoti yt",
    "Ai",
    "do t'i tubojë ata",
    "vërtet Ai (është)",
    "i Urtë",
    "i Gjithëdijshëm."
  ],
  "26": [
    "Dhe sigurisht",
    "Ne e krijuam",
    "njeriun",
    "nga",
    "një argjilë",
    "nga",
    "një baltë e zezë",
    "e lëmuar."
  ],
  "27": [
    "Edhe xhinët",
    "Ne i krijuam ata",
    "nga",
    "përpara",
    "nga",
    "zjarri",
    "pa tym."
  ],
  "28": [
    "Dhe kur",
    "(i) tha",
    "Zoti yt",
    "engjëjve",
    "\"Vërtet Unë (jam)",
    "Krijues",
    "i një njeriu",
    "nga",
    "një argjilë",
    "nga",
    "një baltë e zezë",
    "e lëmuar."
  ],
  "29": [
    "E kur",
    "Unë e formësoj atë",
    "dhe Unë fryj",
    "në të",
    "nga",
    "shpirti Im",
    "atëherë bini",
    "për të",
    "të përulur (në sexhde)."
  ],
  "30": [
    "Përnjëherë u përulën (duke bërë sexhde)",
    "engjëjt",
    "të gjithë ata",
    "së bashku"
  ],
  "31": [
    "Përveç",
    "Iblisit",
    "ai refuzoi",
    "që",
    "të jetë",
    "me",
    "të përulurit."
  ],
  "32": [
    "Ai tha",
    "\"O Iblis",
    "ç'(është)",
    "me ty",
    "që të mosh",
    "jesh",
    "me",
    "të përulurit?\""
  ],
  "33": [
    "Ai tha",
    "\"Nuk",
    "unë jam",
    "që t'i përulem",
    "një njeriu",
    "Ti e krijove atë",
    "nga",
    "një argjilë",
    "nga",
    "një baltë e zezë",
    "e lëmuar\"."
  ],
  "34": [
    "Ai (Allahu) tha",
    "\"More shporru",
    "nga aty",
    "se vërtet ti (je)",
    "i mallkuar!"
  ],
  "35": [
    "Dhe vërtet",
    "mbi ty (do të jetë)",
    "mallkimi",
    "deri",
    "në ditën",
    "e Kompensimit."
  ],
  "36": [
    "Ai (Iblisi) tha",
    "\"Zoti im",
    "atëherë më jep afat",
    "deri",
    "ditën kur",
    "ata (do të) ringjallen\"."
  ],
  "37": [
    "Ai (Allahu) tha",
    "\"Vërtet ti (je)",
    "prej",
    "të afatizuarve"
  ],
  "38": [
    "deri",
    "(në) ditën",
    "e kohës",
    "së njohur\"."
  ],
  "39": [
    "Ai (Iblisi) tha",
    "\"Zoti im",
    "për shkak të asaj që",
    "Ti më çove në humbje mua",
    "gjithsesi unë do t'ua zbukuroj",
    "atyre",
    "në",
    "tokë",
    "dhe do t'i çoj në humbje ata",
    "të gjithë."
  ],
  "40": [
    "Përveç",
    "robërve Tu",
    "prej tyre",
    "të përzgjedhur\"."
  ],
  "41": [
    "Ai (Allahu) tha",
    "\"Kjo (është)",
    "një rrugë",
    "tek Unë",
    "e drejtë\"."
  ],
  "42": [
    "Vërtet",
    "robërit e Mi",
    "nuk (ka)",
    "për ty",
    "mbi ta",
    "asnjë fuqi",
    "përveç",
    "atij që",
    "të pason ty",
    "nga",
    "të humburit."
  ],
  "43": [
    "Dhe vërtet",
    "xhehenemi",
    "(është) sigurisht vendi i premtuar i tyre",
    "të gjithëve."
  ],
  "44": [
    "Ai ka",
    "shtatë",
    "porta",
    "për çdo",
    "portë",
    "(secili) prej tyre (ka)",
    "një hise",
    "të caktuar."
  ],
  "45": [
    "Vërtet",
    "të druajturit",
    "(do të jenë) në",
    "xhenete",
    "dhe burime."
  ],
  "46": [
    "\"Hyni në to",
    "me paqe",
    "të sigurt\"."
  ],
  "47": [
    "Dhe Ne shkulëm",
    "atë çfarë (është)",
    "në",
    "gjokset e tyre",
    "çfarëdo",
    "urrejtje",
    "(ata do të jenë) vëllezër",
    "mbi",
    "frone",
    "duke qëndruar përballë njëri-tjetrit."
  ],
  "48": [
    "Nuk",
    "(do t'i) prek ata",
    "në to",
    "lodhje",
    "dhe nuk (do të jenë)",
    "ata",
    "prej aty",
    "të dëbuar."
  ],
  "49": [
    "Njoftoji",
    "robërit e Mi",
    "se vërtet Unë",
    "vetëm Unë (jam)",
    "Falësi i madh",
    "Mëshiruesi."
  ],
  "50": [
    "Dhe se vërtet",
    "dënimi Im",
    "ai (është)",
    "dënimi",
    "i dhembshëm."
  ],
  "51": [
    "Dhe njoftoji ata",
    "për",
    "mysafirët",
    "e Ibrahimit."
  ],
  "52": [
    "Kur",
    "ata hynë",
    "tek ai",
    "e thanë",
    "\"Paqe!\"",
    "Ai tha",
    "\"Vërtet ne (jemi)",
    "prej jush",
    "të frikësuar\"."
  ],
  "53": [
    "Ata thanë",
    "\"Mos",
    "u frikëso!",
    "Vërtet ne",
    "po të përgëzojmë ty",
    "me një djalosh",
    "të ditur\"."
  ],
  "54": [
    "Ai tha",
    "\"A po më përgëzoni mua",
    "për (atë)",
    "që",
    "më preku mua",
    "pleqëria?",
    "Vallë me çka",
    "ju (më) përgëzoni?\""
  ],
  "55": [
    "Ata thanë",
    "\"Ne po të përgëzojmë ty",
    "me vërtetësi",
    "prandaj mos",
    "ji",
    "nga",
    "shpresëhumburit!\""
  ],
  "56": [
    "Ai tha",
    "\"E kush",
    "dëshpërohet",
    "nga",
    "mëshira",
    "e Zotit të tij",
    "përveç",
    "të humburit\"."
  ],
  "57": [
    "Ai tha",
    "\"E çka (është)",
    "puna juaj",
    "o ju",
    "të dërguar?\""
  ],
  "58": [
    "Ata thanë",
    "\"Vërtet ne",
    "u dërguam",
    "tek",
    "një popull",
    "keqbërës."
  ],
  "59": [
    "Përveç",
    "familjes",
    "së Lutit.",
    "Vërtet ne (jemi)",
    "shpëtuesit e tyre",
    "të gjithëve."
  ],
  "60": [
    "Përveç",
    "gruas së tij",
    "Ne përcaktuam",
    "se ajo (është)",
    "nga",
    "të mbeturit mbrapa (me të dënuarit)\"."
  ],
  "61": [
    "E pasi që",
    "(i) erdhën",
    "familjes",
    "së Lutit",
    "të dërguarit."
  ],
  "62": [
    "Ai tha",
    "\"Vërtet ju (jeni)",
    "një popull",
    "të panjohur\"."
  ],
  "63": [
    "Ata thanë",
    "\"Jo por",
    "ne erdhëm te ti",
    "me atë çfarë",
    "ata ishin",
    "në të",
    "dyshonin."
  ],
  "64": [
    "Dhe ne ta sollëm ty",
    "të vërtetën",
    "dhe vërtet ne (jemi)",
    "të sinqertë\"."
  ],
  "65": [
    "Prandaj nisu ti",
    "me familjen tënde",
    "në një pjesë",
    "prej",
    "natës",
    "dhe shko",
    "pas tyre",
    "dhe të mos",
    "e kthejë fytyrën",
    "prej jush",
    "askush",
    "dhe vazhdoni",
    "atje ku",
    "urdhëroheni."
  ],
  "66": [
    "Dhe Ne ia kumtuam",
    "atij",
    "atë",
    "vendim",
    "se",
    "rrënja",
    "e këtyre",
    "(do të jetë) e këputur",
    "në agim."
  ],
  "67": [
    "Dhe erdhën",
    "banorët",
    "e qytetit",
    "duke e përgëzuar njëri-tjetrin."
  ],
  "68": [
    "Ai tha",
    "\"Vërtet",
    "këta",
    "(janë) mysafirët e mi",
    "prandaj mos",
    "më turpëroni mua!"
  ],
  "69": [
    "Dhe druajuni",
    "Allahut",
    "dhe mos",
    "më poshtëroni mua!\""
  ],
  "70": [
    "Ata thanë",
    "\"Vallë a nuk",
    "ne të ndaluam ty",
    "nga",
    "njerëzit e botës?!\""
  ],
  "71": [
    "Ai tha",
    "\"Këto (janë)",
    "bijat e mia",
    "nëse",
    "jeni",
    "veprues!\""
  ],
  "72": [
    "Pasha jetën tënde (o Muhamed)",
    "vërtet ata",
    "në",
    "dehjen e tyre",
    "enden të përhumbur."
  ],
  "73": [
    "Prandaj i mbërtheu ata",
    "ushtima",
    "gjatë lindjes së diellit."
  ],
  "74": [
    "E Ne e bëmë",
    "pjesën e lartë të tij",
    "pjesën e poshtme të tij",
    "dhe lëshuam shi",
    "mbi ta",
    "gurë",
    "nga",
    "një baltë e pjekur."
  ],
  "75": [
    "Vërtet",
    "në",
    "këtë",
    "(ka) dëshmi",
    "për vigjilentët."
  ],
  "76": [
    "Dhe vërtet ato (vendbanime)",
    "janë pranë një rruge",
    "të dukshme."
  ],
  "77": [
    "Vërtet",
    "në",
    "këtë",
    "(ka) dëshmi",
    "për besimtarët."
  ],
  "78": [
    "Dhe sigurisht",
    "ishin",
    "banorët",
    "e Ejkes",
    "zullumqarë."
  ],
  "79": [
    "Prandaj Ne u hakmorrëm",
    "kundër tyre",
    "dhe që të dy ato (vendbanime)",
    "(janë) pranë një rruge",
    "të qartë."
  ],
  "80": [
    "Dhe sigurisht",
    "i përgënjeshtruan",
    "banorët",
    "e Hixhrit (luginës shkëmbore)",
    "të dërguarit."
  ],
  "81": [
    "Dhe Ne u dhamë atyre",
    "dëshmitë Tona",
    "por ata ishin",
    "ndaj tyre",
    "kundërshtues."
  ],
  "82": [
    "Dhe ata ishin",
    "skalitnin",
    "prej",
    "kodrave",
    "shtëpi",
    "(duke qenë) të sigurt."
  ],
  "83": [
    "E i mbërtheu ata",
    "ushtima",
    "në agim."
  ],
  "84": [
    "Prandaj nuk",
    "(u) bën dobi",
    "atyre",
    "ajo çfarë",
    "ata ishin",
    "fitonin."
  ],
  "85": [
    "Dhe nuk",
    "Ne krijuam",
    "qiejt",
    "dhe tokën",
    "dhe çka (ka)",
    "mes atyre dyve",
    "përveçse",
    "me vërtetësi",
    "dhe vërtet",
    "momenti (Kijameti)",
    "gjithsesi do të vijë",
    "prandaj ji tolerant",
    "(me) tolerancë",
    "të bukur!"
  ],
  "86": [
    "Vërtet",
    "Zoti yt",
    "Ai (është)",
    "Gjithëkrijuesi",
    "i Gjithëdijshmi."
  ],
  "87": [
    "Dhe sigurisht",
    "Ne të dhamë ty",
    "shtatë (ajete)",
    "nga",
    "të përsëriturat",
    "dhe Kur'anin",
    "madhështor."
  ],
  "88": [
    "Mos",
    "i lësho kurrsesi",
    "sytë e tu",
    "tek",
    "ajo çfarë",
    "Ne i begatuam",
    "me të",
    "disa grupe",
    "prej tyre",
    "dhe mos",
    "u pikëllo",
    "për ta",
    "dhe ule",
    "krahun tënd",
    "për besimtarët."
  ],
  "89": [
    "Dhe thuaj",
    "\"Vërtet unë",
    "unë (jam)",
    "paralajmëruesi",
    "i qartë\"."
  ],
  "90": [
    "Sikurse",
    "Ne zbritëm",
    "për",
    "ndarësit (e librave të mëparshëm)."
  ],
  "91": [
    "Ata të cilët",
    "e bënë",
    "Kur'anin",
    "pjesë-pjesë."
  ],
  "92": [
    "E pasha Zotin tënd",
    "Ne do t'i pyesim gjithsesi ata",
    "të gjithë."
  ],
  "93": [
    "Për atë çfarë",
    "ata ishin",
    "punonin."
  ],
  "94": [
    "Po ti shpalle haptazi",
    "atë me çfarë",
    "urdhërohesh",
    "dhe hiqu",
    "prej",
    "idhujtarëve!"
  ],
  "95": [
    "Vërtet Ne",
    "të mjaftojmë ty",
    "(kundër) tallësve."
  ],
  "96": [
    "Atyre të cilët",
    "bëjnë",
    "(bashkë) me",
    "Allahun",
    "një zot",
    "tjetër",
    "por do ta",
    "dinë."
  ],
  "97": [
    "Dhe sigurisht",
    "Ne dimë",
    "se vërtet ti",
    "ngushtohet",
    "gjoksi yt",
    "për atë çfarë",
    "ata flasin."
  ],
  "98": [
    "Po ti lartësoje",
    "me falënderim",
    "Zotin tënd",
    "dhe ji",
    "nga",
    "të përulurit!"
  ],
  "99": [
    "Dhe adhuroje",
    "Zotin tënd",
    "derisa",
    "të të vijë ty",
    "vdekja!"
  ]
}