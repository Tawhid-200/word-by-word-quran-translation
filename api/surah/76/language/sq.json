{
  "1": [
    "A",
    "ka ardhur",
    "për",
    "njeriun",
    "një periudhë",
    "nga",
    "koha",
    "nuk",
    "ishte",
    "një gjë",
    "e përmendur?"
  ],
  "2": [
    "Vërtet Ne",
    "e krijuam",
    "njeriun",
    "nga",
    "një farë",
    "një përzierje",
    "(që) Ne ta testojmë atë",
    "ashtu që Ne e bëmë atë",
    "dëgjues",
    "shikues."
  ],
  "3": [
    "Vërtet Ne",
    "e udhëzuam atë",
    "(në) rrugë",
    "qoftë",
    "(do të jetë) falënderues",
    "qoftë",
    "mosmirënjohës."
  ],
  "4": [
    "Vërtet Ne",
    "kemi përgatitur",
    "për mohuesit",
    "zinxhirë",
    "dhe pranga",
    "dhe zjarr."
  ],
  "5": [
    "Vërtet",
    "punëmirët",
    "(do të) pinë",
    "nga",
    "një gotë",
    "është",
    "përzierja e saj",
    "(nga) kamfuri."
  ],
  "6": [
    "Një burim",
    "(do të) pinë",
    "prej tij",
    "robërit",
    "e Allahut",
    "ata e shpërthejnë atë",
    "vrullshëm."
  ],
  "7": [
    "Ata përmbushin",
    "zotimin",
    "dhe frikësohen",
    "(nga) një Ditë",
    "është",
    "e keqja e saj",
    "e përhapur gjerësisht."
  ],
  "8": [
    "Dhe ata ushqejnë",
    "(me) ushqim",
    "nga",
    "dashuria për të",
    "nevojtarin",
    "dhe jetimin",
    "dhe robin e zënë."
  ],
  "9": [
    "\"Vetëm",
    "ne ju ushqejmë ju",
    "për fytyrën",
    "e Allahut.",
    "Nuk",
    "dëshirojmë",
    "nga ju",
    "shpërblim",
    "dhe as",
    "falënderim."
  ],
  "10": [
    "Vërtet ne",
    "frikësohemi",
    "nga",
    "Zoti ynë",
    "një ditë",
    "e zymtë",
    "shqetësuese\"."
  ],
  "11": [
    "Por i mbrojti ata",
    "Allahu",
    "(nga) e keqja",
    "e asaj",
    "dite",
    "dhe u dhuroi atyre",
    "shkëlqim",
    "dhe gëzim."
  ],
  "12": [
    "Dhe i shpërbleu ata",
    "për atë çfarë",
    "ata duruan",
    "(me) xhenet",
    "dhe mëndafsh."
  ],
  "13": [
    "Të mbështetur",
    "në të",
    "mbi",
    "kolltukë.",
    "Nuk",
    "(do të) shohin",
    "në të",
    "diell",
    "dhe as",
    "acar."
  ],
  "14": [
    "Dhe afër",
    "mbi ta",
    "(janë) hijet e tyre",
    "dhe rrinë",
    "kalaveshët e tyre",
    "të varur poshtë."
  ],
  "15": [
    "Dhe (do të) qarkullohet",
    "mes tyre",
    "me enë",
    "nga",
    "argjendi",
    "dhe gota",
    "(që) janë",
    "të kristalta."
  ],
  "16": [
    "Të kristalta",
    "nga",
    "argjendi.",
    "Ata i përcaktuan ato",
    "me masë."
  ],
  "17": [
    "Dhe u jepet të pinë",
    "në të",
    "një gotë",
    "është",
    "përzierja e saj",
    "xhenxhefil."
  ],
  "18": [
    "Një burim",
    "në të",
    "(që) quhet",
    "Selsebil."
  ],
  "19": [
    "Dhe qarkullojnë",
    "mes tyre",
    "fëmijë",
    "të gjithmonshëm",
    "kur",
    "ti i sheh ata",
    "i konsideron ata",
    "(si) margaritarë",
    "të derdhur."
  ],
  "20": [
    "Dhe kur",
    "ti sheh",
    "atje",
    "do të shohësh",
    "begati",
    "dhe mbretëri",
    "të madhe."
  ],
  "21": [
    "Mbi ta",
    "(do të ketë) rroba",
    "(nga) mëndafshi i butë",
    "të gjelbra",
    "dhe mëndafsh të trashë.",
    "Dhe ata do të stolisen",
    "(me) bylyzykë",
    "nga",
    "argjendi",
    "dhe u jep atyre të pinë",
    "Zoti i tyre",
    "një pije",
    "të pastër."
  ],
  "22": [
    "Vërtet",
    "kjo",
    "është",
    "për ju",
    "(si) shpërblim",
    "dhe është",
    "angazhimi juaj",
    "i falënderuar."
  ],
  "23": [
    "Vërtet Ne",
    "Ne",
    "zbritëm",
    "mbi ty",
    "Kur'anin",
    "gradualisht."
  ],
  "24": [
    "Prandaj duro",
    "për urdhrin",
    "e Zotit tënd",
    "dhe mos",
    "(iu) bind",
    "prej tyre",
    "asnjë mëkatari",
    "apo",
    "mohuesi."
  ],
  "25": [
    "Dhe përmende",
    "emrin",
    "e Zotit tënd",
    "mëngjes",
    "dhe mbrëmje."
  ],
  "26": [
    "Dhe nga",
    "nata",
    "bëj sexhde",
    "për Të",
    "dhe lavdëroje Atë",
    "(gjatë) një nate",
    "të gjatë."
  ],
  "27": [
    "Vërtet",
    "këta",
    "e duan",
    "të menjëhershmen",
    "dhe e lënë",
    "përpara tyre",
    "një Ditë",
    "të rëndë."
  ],
  "28": [
    "Ne",
    "i krijuam ata",
    "dhe i përforcuam",
    "format e tyre",
    "e kur",
    "(të) dëshirojmë",
    "Ne (mund t'i) ndryshojmë",
    "të tjerë si ata",
    "(me) një ndryshim."
  ],
  "29": [
    "Vërtet",
    "ky",
    "(është) një përkujtim",
    "prandaj kushdo që",
    "dëshiron",
    "ai merr",
    "tek",
    "Zoti i tij",
    "(një) rrugë."
  ],
  "30": [
    "Dhe nuk",
    "ju dëshironi",
    "përveç",
    "që",
    "dëshiron",
    "Allahu.",
    "Vërtet",
    "Allahu",
    "është",
    "i Gjithëdijshëm",
    "i Urtë."
  ],
  "31": [
    "Ai fut",
    "këdo që",
    "Ai dëshiron",
    "në",
    "mëshirën e Tij",
    "e zullumqarët",
    "Ai përgatiti",
    "për ta",
    "një dënim",
    "të dhembshëm."
  ]
}