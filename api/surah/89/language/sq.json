{
  "1": [
    "Për agimin"
  ],
  "2": [
    "Për netë",
    "dhjetë"
  ],
  "3": [
    "Për çiftin",
    "dhe tekun"
  ],
  "4": [
    "Për natën",
    "kur",
    "kalon"
  ],
  "5": [
    "A (është)",
    "në",
    "këtë",
    "ndonjë betim",
    "për atë që ka",
    "perceptim?"
  ],
  "6": [
    "A nuk",
    "ke parë",
    "si",
    "bëri",
    "Zoti yt",
    "me Adin?"
  ],
  "7": [
    "Iremin",
    "poseduesin",
    "e shtyllave të larta"
  ],
  "8": [
    "I cili",
    "nuk",
    "u krijua",
    "si ai",
    "në",
    "vende"
  ],
  "9": [
    "Dhe Themudin",
    "të cilët",
    "shpuan",
    "shkëmbinjtë",
    "në luginë"
  ],
  "10": [
    "Dhe Faraonin",
    "poseduesin",
    "e hunjve"
  ],
  "11": [
    "Të cilët",
    "e tepruan",
    "në",
    "vende"
  ],
  "12": [
    "Dhe shtuan shumë",
    "në to",
    "shkatërrimin"
  ],
  "13": [
    "Ashtu që derdhi",
    "mbi ta",
    "Zoti yt",
    "breshëri",
    "dënimesh"
  ],
  "14": [
    "Vërtet",
    "Zoti yt",
    "(është) sigurisht në pritë"
  ],
  "15": [
    "E sa i përket",
    "njeriut",
    "kur",
    "e",
    "teston atë",
    "Zoti i tij",
    "dhe e nderon atë",
    "dhe e begaton atë",
    "e ai thotë",
    "\"Zoti im",
    "më nderoi mua\""
  ],
  "16": [
    "Por",
    "kur",
    "e",
    "teston atë",
    "dhe ia ngushton",
    "atij",
    "furnizimin e tij",
    "e ai thotë",
    "\"Zoti im",
    "më nënçmoi\""
  ],
  "17": [
    "Kurrsesi!",
    "por",
    "nuk",
    "e nderoni",
    "jetimin"
  ],
  "18": [
    "Dhe nuk",
    "e nxitni njëri-tjetrin",
    "për",
    "ushqimin",
    "e të ngratit"
  ],
  "19": [
    "Dhe ju hani",
    "trashëgiminë",
    "duke e kapërdirë",
    "bashkërisht"
  ],
  "20": [
    "Dhe e doni",
    "pasurinë",
    "me dashuri",
    "të tepruar"
  ],
  "21": [
    "Kurrsesi!",
    "kur",
    "(të) dridhet",
    "toka",
    "me goditje",
    "(e) shkatërrim"
  ],
  "22": [
    "Dhe të vijë",
    "Zoti yt",
    "dhe engjëjt",
    "radhë",
    "radhë"
  ],
  "23": [
    "Dhe do të sillet",
    "atë ditë",
    "xhehenemi",
    "atë ditë",
    "(do të) kujtojë",
    "njeriu",
    "por nga ku",
    "(do të jetë) për të",
    "përkujtimi?"
  ],
  "24": [
    "Ai (do të) thotë",
    "\"Ah sikur",
    "të isha parapërgatitur",
    "për jetën time!\""
  ],
  "25": [
    "E atë ditë",
    "nuk",
    "dënon",
    "(si) dënimi i Tij",
    "askush"
  ],
  "26": [
    "Dhe nuk",
    "(do të) farkojë",
    "(si) farkimi i Tij",
    "askush"
  ],
  "27": [
    "O ti",
    "shpirt",
    "i qetësuar!"
  ],
  "28": [
    "Kthehu",
    "tek",
    "Zoti yt",
    "i kënaqur (për vete)",
    "kënaqës (për Zotin)!"
  ],
  "29": [
    "Pra hyr",
    "në mesin",
    "e robërve të Mi!"
  ],
  "30": [
    "Dhe hyr",
    "në xhenetin Tim!"
  ]
}