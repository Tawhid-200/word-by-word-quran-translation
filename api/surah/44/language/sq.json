{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Pasha Librin",
    "e qartë!"
  ],
  "3": [
    "Vërtet Ne",
    "e zbritëm atë",
    "në",
    "një natë",
    "të bekuar.",
    "Vërtet Ne",
    "jemi",
    "paralajmërues."
  ],
  "4": [
    "Në të",
    "veçohet",
    "çdo",
    "çështje",
    "vendimtare."
  ],
  "5": [
    "Çështje",
    "nga",
    "Ana Jonë.",
    "Vërtet Ne",
    "jemi",
    "dërgues."
  ],
  "6": [
    "Mëshirë",
    "nga",
    "Zoti yt.",
    "Vërtet Ai",
    "Ai (është)",
    "Gjithëdëgjuesi",
    "i Gjithëdijshmi."
  ],
  "7": [
    "Zotit",
    "të qiejve",
    "dhe të tokës",
    "dhe çfarë (është)",
    "mes atyre dyve",
    "nëse",
    "jeni",
    "të bindur."
  ],
  "8": [
    "Nuk (ka)",
    "zot",
    "veçse",
    "Ai.",
    "Ai ngjall",
    "dhe i vdes (të tjerët).",
    "Zoti juaj",
    "dhe Zoti",
    "i baballarëve tuaj",
    "të parë."
  ],
  "9": [
    "Ama",
    "ata",
    "në",
    "një dyshim",
    "po luajnë."
  ],
  "10": [
    "Po ti prit",
    "ditën kur",
    "do të sjellë",
    "qielli",
    "një tym",
    "të kthjellët."
  ],
  "11": [
    "Ai do t'i mbulojë",
    "njerëzit.",
    "Ky (është)",
    "një dënim",
    "i dhembshëm."
  ],
  "12": [
    "Zoti ynë!",
    "Hiqe",
    "nga ne",
    "dënimin!",
    "Vërtet ne (jemi)",
    "besimtarë."
  ],
  "13": [
    "Prej nga",
    "atyre",
    "përkujtimi",
    "ndërkohë që tashmë",
    "u erdhi atyre",
    "një i dërguar",
    "i qartë?"
  ],
  "14": [
    "Pastaj",
    "ata u zmbrapsën",
    "prej tij",
    "dhe thanë",
    "\"Një i mësuar",
    "i çmendur!\""
  ],
  "15": [
    "Vërtet Ne (jemi)",
    "largues",
    "të dënimit",
    "pak",
    "(por) vërtet ju (jeni)",
    "kthyes."
  ],
  "16": [
    "Ditën kur",
    "Ne do të rrëmbejmë",
    "me rrëmbimin",
    "më të madh.",
    "Vërtet Ne (jemi)",
    "hakmarrës."
  ],
  "17": [
    "Dhe njëmend",
    "Ne e sprovuam",
    "para tyre",
    "popullin",
    "e faraonit",
    "dhe u erdhi atyre",
    "një i dërguar",
    "fisnik."
  ],
  "18": [
    "Që",
    "\"Dorëzoni",
    "tek unë",
    "robërit",
    "e Allahut!",
    "Vërtet unë (jam)",
    "për ju",
    "një i dërguar",
    "besnik."
  ],
  "19": [
    "Dhe që të",
    "mos",
    "lartësoheni",
    "mbi",
    "Allahun.",
    "Vërtet unë",
    "jua sjell",
    "një argument",
    "të qartë."
  ],
  "20": [
    "Dhe vërtet unë",
    "strehohem",
    "tek Zoti im",
    "dhe Zoti juaj",
    "që të",
    "më gurëzoni."
  ],
  "21": [
    "E nëse",
    "nuk",
    "(më) besoni",
    "mua",
    "atëherë veçohuni nga unë!\""
  ],
  "22": [
    "Atëherë ai e thirri",
    "Zotin e tij",
    "se",
    "këta (janë)",
    "një popull",
    "kriminel."
  ],
  "23": [
    "Prandaj ec",
    "me robërit e Mi",
    "natën!",
    "Vërtet ju (do të jeni)",
    "të përndjekur."
  ],
  "24": [
    "Dhe lëre",
    "detin",
    "të qetë!",
    "Vërtet ata (janë)",
    "një ushtri",
    "të përmbytur."
  ],
  "25": [
    "Sa shumë",
    "kanë lënë",
    "nga",
    "kopshte",
    "e kroje!"
  ],
  "26": [
    "Edhe ara",
    "dhe vend",
    "të këndshëm."
  ],
  "27": [
    "Dhe begati",
    "ata ishin",
    "në të",
    "shijues."
  ],
  "28": [
    "Ashtu",
    "dhe Ne ua lamë në trashëgim atë",
    "një populli",
    "tjetër."
  ],
  "29": [
    "E nuk",
    "qanë",
    "për ta",
    "qielli",
    "dhe toka",
    "dhe nuk",
    "ata ishin",
    "të afatizuar."
  ],
  "30": [
    "Dhe sigurisht",
    "Ne i shpëtuam",
    "bijtë",
    "e Israilit",
    "prej",
    "dënimit",
    "poshtërues."
  ],
  "31": [
    "Prej",
    "faraonit.",
    "Vërtet ai",
    "ishte",
    "mendjemadh",
    "prej",
    "tepruesve."
  ],
  "32": [
    "Dhe njëmend",
    "Ne i përzgjodhëm ata",
    "me",
    "dije",
    "ndër",
    "botët."
  ],
  "33": [
    "Dhe Ne u dhamë atyre",
    "prej",
    "provave",
    "çfarë (kishte)",
    "në të",
    "një sprovë",
    "të qartë."
  ],
  "34": [
    "Vërtet",
    "këta",
    "sigurisht do të thonë."
  ],
  "35": [
    "\"Nuk",
    "ajo (është)",
    "veçse",
    "vdekja jonë",
    "e parë",
    "dhe nuk",
    "ne (do të jemi)",
    "të ringjallur\"."
  ],
  "36": [
    "Pra sillni",
    "baballarët tanë",
    "nëse",
    "ju jeni",
    "të sinqertë!"
  ],
  "37": [
    "A ata (janë)",
    "më të mirë",
    "apo",
    "populli",
    "i Tubasë",
    "dhe ata të cilët (ishin)",
    "nga",
    "para tyre?",
    "Ne i shkatërruam ata",
    "(se) vërtet ata",
    "ishin",
    "kriminelë."
  ],
  "38": [
    "Dhe nuk",
    "Ne i krijuam",
    "qiejt",
    "dhe tokën",
    "dhe çfarë (është)",
    "mes atyre dyjave",
    "për të luajtur."
  ],
  "39": [
    "Nuk",
    "Ne i krijuam ato dyja",
    "përveçse",
    "me vërtetësi",
    "mirëpo",
    "shumica e tyre",
    "nuk",
    "dinë."
  ],
  "40": [
    "Vërtet",
    "dita",
    "vendimtare",
    "(është) caktimi i tyre",
    "të gjithëve."
  ],
  "41": [
    "Ditën kur",
    "nuk",
    "bën dobi",
    "dosti",
    "për",
    "dostin",
    "asgjë",
    "dhe nuk",
    "ata",
    "(do të) ndihmohen."
  ],
  "42": [
    "Përveçse",
    "atë që",
    "e mëshiron",
    "Allahu.",
    "Vërtet Ai",
    "Ai (është)",
    "i Pamposhturi",
    "Mëshiruesi."
  ],
  "43": [
    "Vërtet",
    "pema",
    "e Zekumit."
  ],
  "44": [
    "(Do të jetë) ushqimi",
    "i gjynahqarit."
  ],
  "45": [
    "Si katran",
    "vlon",
    "në",
    "barqe."
  ],
  "46": [
    "Sikurse vlimi",
    "i ujit të vluar."
  ],
  "47": [
    "Merreni atë",
    "e hidheni",
    "tek",
    "mesi",
    "i zjarrit flakërues."
  ],
  "48": [
    "Pastaj",
    "derdhni",
    "mbi",
    "kokën e tij",
    "prej",
    "dënimit",
    "të ujit të vluar."
  ],
  "49": [
    "Shijoje!",
    "Vërtet ti",
    "ti je",
    "i forti",
    "fisniku."
  ],
  "50": [
    "Vërtet",
    "kjo (është)",
    "ajo çfarë",
    "ju ishit",
    "në të",
    "dyshonit."
  ],
  "51": [
    "Vërtet",
    "të druajturit",
    "(do të jenë) në",
    "një vend",
    "të sigurt."
  ],
  "52": [
    "Në",
    "xhenete",
    "e kroje."
  ],
  "53": [
    "Ata do të veshin (rroba)",
    "nga",
    "mëndafshi",
    "dhe kadifeja",
    "përballë njëri-tjetrit."
  ],
  "54": [
    "Kështu",
    "dhe Ne do t'i martojmë ata",
    "me hyri",
    "symëdha."
  ],
  "55": [
    "Ata do të kërkojnë",
    "në të",
    "çdo",
    "pemë",
    "duke qenë të sigurtë."
  ],
  "56": [
    "Nuk",
    "ata do të shijojnë",
    "në të",
    "vdekjen",
    "përveçse",
    "vdekjen",
    "e parë",
    "dhe Ai do t'i mbrojë ata",
    "(nga) dënimi",
    "i zjarrit flakërues."
  ],
  "57": [
    "Mirësi",
    "nga",
    "Zoti yt.",
    "Ajo",
    "ajo (është)",
    "fitorja",
    "madhështore."
  ],
  "58": [
    "Vetëm",
    "Ne e lehtësuam atë",
    "me gjuhën tënde",
    "ashtu që ata",
    "të përkujtojnë."
  ],
  "59": [
    "Pra prit ti",
    "(se) vërtet ata",
    "(janë) pritës."
  ]
}