{
  "1": [
    "Ta, Sin, Mim."
  ],
  "2": [
    "Ato (janë)",
    "ajetet",
    "e Librit",
    "të qartë."
  ],
  "3": [
    "Ne po të recitojmë",
    "ty",
    "nga",
    "lajmi i vërtetë",
    "(për) Musain",
    "dhe faraonin",
    "me vërtetësi",
    "për një popull",
    "(që) besojnë."
  ],
  "4": [
    "Vërtet",
    "faraoni",
    "u lartësua",
    "në",
    "tokë",
    "dhe i bëri",
    "njerëzit e saj",
    "grupe-grupe",
    "e shtypte",
    "një grup",
    "prej tyre",
    "i masakronte",
    "bijtë e tyre",
    "dhe i linte të jetojnë",
    "gratë e tyre.",
    "Vërtet ai",
    "ishte",
    "prej",
    "shkatërruesve."
  ],
  "5": [
    "E Ne duam",
    "që të",
    "bëjmë privilegje",
    "për",
    "ata të cilët",
    "u shtypën",
    "në",
    "tokë",
    "dhe t'i bëjmë ata",
    "udhëheqës",
    "dhe t'i bëjmë ata",
    "trashëgues."
  ],
  "6": [
    "Dhe t'u japim pushtet",
    "atyre",
    "në",
    "tokë",
    "dhe t'ia shfaqim",
    "faraonit",
    "dhe Hamanit",
    "dhe ushtrive të tyre",
    "prej tyre",
    "atë çfarë",
    "ata ishin",
    "duke u ruajtur."
  ],
  "7": [
    "Dhe Ne shpallëm",
    "tek",
    "nëna",
    "e Musait",
    "që",
    "\"Jepi gji atij!\"",
    "E kur",
    "frikësohesh",
    "për të",
    "atëherë hidhe atë",
    "në",
    "lumë",
    "dhe mos",
    "u frikëso",
    "dhe mos",
    "u pikëllo!",
    "Vërtet Ne",
    "do ta kthejmë atë",
    "tek ti",
    "dhe do ta bëjmë atë",
    "prej",
    "të dërguarve."
  ],
  "8": [
    "Dhe e kapi atë",
    "familja",
    "e faraonit",
    "që ai të jetë",
    "për ta",
    "armik",
    "dhe siklet.",
    "Vërtet",
    "faraoni",
    "dhe Hamani",
    "dhe ushtritë e atyre dyve",
    "ishin",
    "gabimtarë."
  ],
  "9": [
    "Dhe tha",
    "gruaja",
    "e faraonit",
    "\"Loçka",
    "e zemrës",
    "për mua",
    "dhe për ty.",
    "Mos",
    "e vrisni atë!",
    "Mbase",
    "të",
    "na sjellë dobi ne",
    "apo",
    "e marrim atë",
    "si fëmijë\"",
    "ndërkohë që ata",
    "nuk",
    "ndjenin."
  ],
  "10": [
    "Dhe u bë",
    "zemra",
    "e nënës",
    "së Musait",
    "e zbrazët.",
    "Sa",
    "ishte gati",
    "ta shfaqte",
    "atë",
    "sikur të mos",
    "ta",
    "lidhnim",
    "tek",
    "zemra e saj",
    "që ajo të jetë",
    "prej",
    "besimtarëve."
  ],
  "11": [
    "Dhe ajo i tha",
    "motrës së tij",
    "\"Gjurmoje atë!\"",
    "Dhe ajo e vështronte",
    "atë",
    "prej",
    "një ane",
    "ndërkohë që ata",
    "nuk",
    "hetonin."
  ],
  "12": [
    "Dhe Ne ia ndaluam",
    "atij",
    "mëndeshat",
    "nga",
    "përpara",
    "e ajo tha",
    "\"A",
    "t'ju dëftoj",
    "për",
    "anëtarët",
    "e një familjeje",
    "(që) do të kujdesen për të",
    "për ju",
    "dhe ata (do të jenë)",
    "për të",
    "këshillues?\""
  ],
  "13": [
    "E Ne e kthyem atë",
    "tek",
    "nëna e tij",
    "ashtu që",
    "t'i kënaqet",
    "syri i saj",
    "dhe të mos",
    "pikëllohet",
    "dhe që të dijë",
    "se vërtet",
    "premtimi",
    "i Allahut",
    "(është) i vërtetë",
    "mirëpo",
    "shumica e tyre",
    "nuk",
    "dinë."
  ],
  "14": [
    "Dhe pasi që",
    "ai arriti",
    "pjekurinë e tij",
    "dhe u rrit",
    "Ne i dhamë atij",
    "urtësi",
    "dhe dituri.",
    "Dhe kështu",
    "Ne i shpërblejmë",
    "bamirësit."
  ],
  "15": [
    "Dhe ai hyri",
    "(në) qytet",
    "në",
    "një kohë",
    "gafleti",
    "nga",
    "banorët e tij",
    "dhe gjeti",
    "në të",
    "dy burra",
    "(që) ziheshin mes vete",
    "ky (ishte)",
    "nga",
    "krahu i tij",
    "kurse ky",
    "nga",
    "armiqtë e tij.",
    "Atëherë kërkoi ndihmë prej tij",
    "ai i cili (ishte)",
    "nga",
    "krahu i tij",
    "ndaj",
    "atij i cili (ishte)",
    "nga",
    "armiqtë e tij.",
    "Sakaq e grushtoi atë",
    "Musai",
    "dhe e la të vdekur",
    "atë.",
    "Ai tha",
    "\"Kjo (është)",
    "prej",
    "punës",
    "së shejtanit.",
    "Vërtet ai (është)",
    "armik",
    "devijues",
    "i qartë\"."
  ],
  "16": [
    "Ai tha",
    "\"Zoti im!",
    "Vërtet unë",
    "i bëra zullum",
    "vetes sime",
    "prandaj më fal",
    "mua!\"",
    "Dhe Ai ia fali",
    "atij.",
    "Vërtet Ai",
    "Ai (është)",
    "Falësi",
    "i Mëshirshmi."
  ],
  "17": [
    "Ai tha",
    "\"Zoti im!",
    "Për shkak të asaj çfarë",
    "Ti më begatove",
    "mua",
    "nuk do të",
    "jem",
    "përkrahës",
    "për kriminelët\"."
  ],
  "18": [
    "Dhe ai agoi",
    "në",
    "qytet",
    "i frikësuar",
    "rrinte në pritë.",
    "Kur sakaq",
    "ai i cili",
    "i kërkoi ndihmë atij",
    "dje",
    "e thërriste për ndihmë atë.",
    "I tha",
    "atij",
    "Musai",
    "\"Vërtet ti (qenke)",
    "njëmend një ngatërrestar",
    "i njohur!\""
  ],
  "19": [
    "E pasi",
    "që",
    "Ai deshi",
    "të",
    "sulmojë",
    "atë që",
    "ai (ishte)",
    "armik",
    "i atyre dyve",
    "tha",
    "\"O Musa",
    "a dëshiron",
    "të",
    "më mbysësh mua",
    "ashtu siç",
    "e mbyte",
    "një njeri",
    "dje?",
    "Nuk",
    "ti dëshiron",
    "përveçse",
    "të",
    "jesh",
    "mizor",
    "në",
    "tokë",
    "dhe nuk",
    "ti dëshiron",
    "të",
    "jesh",
    "prej",
    "përmirësuesve\"."
  ],
  "20": [
    "Dhe erdhi",
    "një njeri",
    "nga",
    "skaji",
    "i qytetit",
    "duke vrapuar",
    "tha",
    "\"O Musa!",
    "Vërtet",
    "paria",
    "po këshillohen",
    "rreth teje",
    "që të të mbysin ty",
    "prandaj dil!",
    "Vërtet unë (jam)",
    "për ty",
    "prej",
    "këshilluesve\"."
  ],
  "21": [
    "Dhe ai doli",
    "prej tij",
    "i frikësuar",
    "rrinte në pritë.",
    "Tha",
    "\"Zoti im!",
    "Më shpëto",
    "prej",
    "popullit",
    "zullumqar!\""
  ],
  "22": [
    "Dhe pasi që",
    "ai u drejtua",
    "në kahun e",
    "Medjenit",
    "tha",
    "\"Mbase",
    "Zoti im",
    "të",
    "më udhëzojë mua",
    "(në) të drejtën",
    "rrugë\"."
  ],
  "23": [
    "Dhe pasi që",
    "arriti",
    "(tek) uji",
    "i Medjenit",
    "gjeti",
    "pranë tij",
    "një grup",
    "prej",
    "njerëzve",
    "u jepnin ujë (bagëtive)",
    "dhe gjeti",
    "nga",
    "larg tyre",
    "dy gra",
    "i largonin (bagëtitë).",
    "Ai tha",
    "\"Çfarë (është)",
    "puna e ju dyjave?\"",
    "Ato dyja thanë",
    "\"Nuk",
    "ne u japim ujë (bagëtive)",
    "derisa",
    "të largohen",
    "barinjtë",
    "kurse babai ynë (është)",
    "një plak",
    "i moshuar\"."
  ],
  "24": [
    "E ai u dha ujë (bagëtive)",
    "të atyre dyjave",
    "pastaj",
    "u largua",
    "drejt",
    "hijes",
    "e tha",
    "\"Zoti im!",
    "Vërtet unë (jam)",
    "për atë çfarë",
    "Ti zbret",
    "tek unë",
    "nga",
    "mirësia",
    "nevojtar\"."
  ],
  "25": [
    "Dhe erdhi tek ai",
    "njëra prej atyre dyjave",
    "ecte",
    "me",
    "mirësjellje.",
    "Ajo tha",
    "\"Vërtet",
    "babai im",
    "të fton ty",
    "që të të kompensojë ty",
    "shpërblimin",
    "(për) atë çfarë",
    "ti i dhe ujë (bagëtive)",
    "për ne.",
    "E pasi që",
    "erdhi tek ai",
    "dhe ia rrëfeu",
    "atij",
    "ngjarjet",
    "ai tha",
    "\"Mos",
    "u frikëso!",
    "Ke shpëtuar",
    "prej",
    "popullit",
    "zullumqar!\""
  ],
  "26": [
    "Tha",
    "njëra prej atyre dyjave",
    "\"O babush!",
    "Merre për argat atë!",
    "Vërtet",
    "më i miri",
    "që",
    "ti e merr për argat",
    "(është) i forti",
    "besniku\"."
  ],
  "27": [
    "Ai tha",
    "\"Vërtet unë",
    "dëshiroj",
    "të",
    "të martoj ty",
    "(me) njërën",
    "(nga) dy bijat e mia",
    "këto dyja",
    "(ashtu) që",
    "të",
    "më shërbesh mua",
    "tetë",
    "vjet",
    "e nëse",
    "ti i plotëson",
    "dhjetë (vjet)",
    "atëherë (është) nga",
    "ana jote",
    "dhe nuk",
    "dëshiroj",
    "të",
    "të rëndoj",
    "ty.",
    "Do t'më gjesh mua",
    "nëse",
    "do",
    "Allahu",
    "prej",
    "punëmirëve!\""
  ],
  "28": [
    "Ai tha",
    "\"Ajo (është)",
    "mes meje",
    "dhe mes teje.",
    "Cilëndo prej",
    "dy afateve",
    "(që) unë e zbatoj",
    "nuk (ka)",
    "dënim",
    "për mua",
    "e Allahu (është)",
    "për",
    "atë çfarë",
    "ne themi",
    "garantues\"."
  ],
  "29": [
    "Pasi që",
    "zbatoi",
    "Musai",
    "afatin",
    "dhe eci",
    "me familjen e tij",
    "ndjeu",
    "nga",
    "ana",
    "e Turit",
    "një zjarr.",
    "Ai i tha",
    "familjes së tij",
    "\"Qëndroni!",
    "Vërtet unë",
    "ndjeva",
    "një zjarr",
    "mbase unë",
    "(do) t'ju sjell ju",
    "prej tij",
    "një haber",
    "ose",
    "një gacë",
    "prej",
    "zjarrit",
    "ashtu që ju",
    "të ndizni\"."
  ],
  "30": [
    "Pasi që",
    "erdhi tek ai",
    "ai u thirr",
    "nga",
    "ana",
    "e luginës",
    "e djathtë",
    "në",
    "vendin",
    "e bekuar",
    "prej",
    "pemës",
    "që",
    "\"O Musa!",
    "Vërtet Unë",
    "Unë (jam)",
    "Allahu",
    "Zoti",
    "i botëve\"."
  ],
  "31": [
    "Dhe se",
    "hidhe",
    "shkopin tënd!",
    "Pasi që",
    "ai e pa atë",
    "duke lëvizur",
    "si të ishte ai",
    "një gjarpër",
    "u zmbraps",
    "duke ia mbathur me të katra",
    "dhe nuk",
    "vështroi prapa.",
    "\"O Musa!",
    "Hajde",
    "dhe mos",
    "u frikëso!",
    "Vërtet ti (je)",
    "prej",
    "të sigurtëve\"."
  ],
  "32": [
    "Fute",
    "dorën tënde",
    "në",
    "sqetullën tënde",
    "ajo do të dalë",
    "e bardhë",
    "pa",
    "asnjë",
    "të metë",
    "dhe shtrëngoje",
    "tek ti",
    "krahun tënd",
    "prej",
    "frikës!",
    "Pra ato dyja (janë)",
    "dy prova",
    "prej",
    "Zotit tënd",
    "tek",
    "faraoni",
    "dhe paria e tij.",
    "Vërtet ata",
    "janë",
    "një popull",
    "të prishur."
  ],
  "33": [
    "Ai tha",
    "\"Zoti im!",
    "Vërtet unë",
    "vrava",
    "prej tyre",
    "një person",
    "prandaj frikësohem",
    "se",
    "ata do të më vrasin mua\"."
  ],
  "34": [
    "Ndërsa vëllai im",
    "Haruni",
    "ai (është)",
    "më gojëtar",
    "se unë",
    "(në) gjuhë",
    "prandaj dërgoje atë",
    "me mua",
    "si ndihmës",
    "që t'më vërtetojë mua.",
    "Vërtet unë",
    "frikësohem",
    "se",
    "ata do të më përgënjeshtrojnë mua."
  ],
  "35": [
    "Ai tha",
    "\"Ne do ta forcojmë",
    "krahun tënd",
    "me vëllain tënd",
    "dhe do të caktojmë",
    "për ju dy",
    "një argument",
    "ashtu që nuk",
    "ata do të arrijnë",
    "tek ju dy.",
    "Me provat Tona",
    "ju dy",
    "dhe ata të cilët",
    "ju pasojnë ju dy",
    "(do të jeni) ngadhënjimtarët\"."
  ],
  "36": [
    "Pasi që",
    "(ua) solli atyre",
    "Musai",
    "provat Tona",
    "të qarta",
    "ata thanë",
    "\"Nuk (është)",
    "kjo",
    "veçse",
    "një magji",
    "e trilluar",
    "dhe nuk",
    "kemi dëgjuar",
    "për këtë",
    "nga",
    "baballarët tanë",
    "të parë\"."
  ],
  "37": [
    "Dhe tha",
    "Musai",
    "\"Zoti im",
    "di më mirë",
    "atë që",
    "e solli",
    "udhëzimin",
    "nga",
    "ana e Tij",
    "dhe atë që",
    "do të jetë",
    "për të",
    "përfundimi",
    "i shtëpisë.",
    "Vërtet",
    "nuk",
    "do të shpëtojnë",
    "zullumqarët."
  ],
  "38": [
    "Dhe tha",
    "faraoni",
    "\"O ju",
    "pari!",
    "Nuk",
    "unë di",
    "për ju",
    "asnjë",
    "zot",
    "pos meje",
    "prandaj m'i ndez (tullat)",
    "mua",
    "o Haman",
    "nga",
    "dheu",
    "dhe ndërto",
    "për mua",
    "një kullë",
    "mbase unë",
    "do të ngjitem",
    "tek",
    "Zoti",
    "i Musait",
    "e vërtet unë",
    "njëmend e konsideroj atë",
    "prej",
    "gënjeshtarëve\"."
  ],
  "39": [
    "Dhe shfaqi mendjemadhësi",
    "ai",
    "dhe ushtritë e tij",
    "në",
    "tokë",
    "pa",
    "të drejtë",
    "dhe ata pandehën",
    "se ata",
    "tek ne",
    "nuk",
    "do të kthehen."
  ],
  "40": [
    "Prandaj Ne e mbërthyem atë",
    "dhe ushtritë e tij",
    "dhe i hodhëm",
    "në",
    "det.",
    "Pra shiko",
    "si",
    "ishte",
    "përfundimi",
    "i zullumqarëve!"
  ],
  "41": [
    "Dhe Ne i bëmë ata",
    "prijës",
    "(që) thërrasin",
    "tek",
    "zjarri",
    "e ditën",
    "e Kijametit",
    "nuk",
    "do të ndihmohen."
  ],
  "42": [
    "Dhe Ne i përcollëm ata",
    "në",
    "këtë",
    "botë",
    "(me) mallkim",
    "ndërsa ditën",
    "e Kijametit",
    "ata (do të jenë)",
    "prej",
    "të shëmtuarve."
  ],
  "43": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Musait",
    "Librin",
    "nga",
    "pasi",
    "që",
    "Ne i shkatërruam",
    "brezat",
    "e parë",
    "shndritje",
    "për njerëzit",
    "dhe udhëzim",
    "dhe mëshirë",
    "ashtu që ata",
    "të përkujtojnë."
  ],
  "44": [
    "Dhe nuk",
    "ti ishe",
    "në anën",
    "perëndimore",
    "kur",
    "Ne caktuam",
    "për",
    "Musain",
    "urdhrin",
    "dhe nuk",
    "ti ishe",
    "prej",
    "dëshmitarëve."
  ],
  "45": [
    "Mirëpo Ne",
    "krijuam",
    "breza",
    "e u zgjat",
    "atyre",
    "koha.",
    "Dhe nuk",
    "ti ishe",
    "banor",
    "në mesin e",
    "banorëve",
    "të Medjenit",
    "t'u recitosh",
    "atyre",
    "ajetet Tona",
    "mirëpo Ne",
    "ishim",
    "dërgues."
  ],
  "46": [
    "Dhe nuk",
    "ti ishe",
    "në anën",
    "e Turit",
    "kur",
    "Ne thirrëm",
    "por",
    "si mëshirë",
    "nga",
    "Zoti yt",
    "që ta paralajmërosh",
    "një popull",
    "nuk",
    "u erdhi atyre",
    "asnjë",
    "paralajmërues",
    "nga",
    "para teje",
    "ashtu që ata",
    "të përkujtojnë."
  ],
  "47": [
    "E sikur të mos",
    "t'i",
    "godiste ata",
    "një goditje",
    "për atë çfarë",
    "paraprinë",
    "duart e tyre",
    "e të thonë",
    "\"Zoti ynë!",
    "Përse nuk",
    "dërgove",
    "tek ne",
    "një të dërguar",
    "ashtu që ne t'i pasonim",
    "provat Tua",
    "dhe të bëheshim",
    "prej",
    "besimtarëve!\""
  ],
  "48": [
    "Pasi që",
    "u erdhi atyre",
    "e vërteta",
    "nga",
    "ana Jonë",
    "ata thanë",
    "\"Përse nuk",
    "iu dha atij",
    "sikurse",
    "ajo çfarë",
