    "besuan",
    "dhe nuk është ajo",
    "dëmtuese e tyre",
    "asgjë",
    "përveç",
    "me lejen",
    "e Allahut",
    "dhe tek",
    "Allahu",
    "le të mbështeten",
    "besimtarët."
  ],
  "11": [
    "O ju",
    "të cilët",
    "besuan",
    "kur",
    "thuhet",
    "për ju",
    "\"Zgjerohuni",
    "në",
    "ndeja!\"",
    "atëherë zgjeroni",
    "zgjeron",
    "Allahu",
    "për ju.",
    "E kur",
    "thuhet",
    "\"Ngrituni!\"",
    "atëherë ngrituni",
    "(do t'i) ngrisë",
    "Allahu",
    "ata të cilët",
    "besuan",
    "prej jush",
