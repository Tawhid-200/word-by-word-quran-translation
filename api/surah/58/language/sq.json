    "dhe jua pranoi pendimin",
    "Allahu",
    "juve",
    "atëherë përsoseni",
    "namazin",
    "dhe jepni",
    "zeqatin",
    "dhe bindjuni",
    "Allahut",
    "dhe të dërguarit të Tij",
    "e Allahu (është)",
    "i Gjithëinformuar",
    "për atë çfarë",
    "ju punoni."
  ],
  "14": [
    "A nuk",
    "ti sheh",
    "tek",
    "ata të cilët",
    "e miqësuan",
    "një popull",
    "u zemërua",
    "Allahu",
    "ndaj tyre",
    "nuk (janë)",
    "ata",
    "prej jush",
    "dhe as",
    "prej tyre",
    "dhe ata betohen",
    "me",
    "gënjeshtër",
    "duke qenë ata",
    "dinë."
  ],
  "15": [
    "Përgatiti",
    "Allahu",
    "për ta",
    "një dënim",
    "të ashpër",
    "vërtet ata",
    "sa keq është",
    "ajo çfarë",
    "ata ishin",
    "punonin."
  ],
  "16": [
    "Ata morën",
    "hetimet e tyre",
