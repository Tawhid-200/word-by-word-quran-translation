  ],
  "12": [
    "O ju",
    "të cilët",
    "besuan",
    "kur",
    "të bisedoni me zë të ulur",
    "me të dërguarin",
    "atëherë jepni",
    "mes",
    "para",
    "bisedës suaj",
    "lëmoshë.",
    "Ajo (është)",
    "më e mirë",
    "për ju",
    "dhe më e pastër",
    "e nëse",
    "nuk",
    "gjeni",
    "atëherë vërtet",
    "Allahu",
    "(është) Falës i madh",
