  "8": [
    "A nuk",
    "ti sheh",
    "tek",
    "ata të cilët",
    "u ndaluan",
    "nga",
    "biseda e fshehtë",
    "pastaj",
    "kthehen",
    "tek ajo çfarë",
    "u ndaluan",
    "prej saj",
    "dhe pëshpërisin mes vete",
    "për mëkat",
    "dhe armiqësi",
    "dhe mosbindje",
    "të të dërguarit",
    "dhe kur",
    "ata vijnë tek ti",
    "të përshëndesin ty",
    "me atë çfarë",
    "nuk",
    "të përshëndet ty",
    "me të",
    "Allahu",
    "dhe ata thonë",
    "në",
    "veten e tyre",
    "\"Përse nuk",
    "na dënon ne",
    "Allahu",
    "për atë çfarë",
    "ne themi?\"",
    "(Është) i mjaftueshëm për ta",
    "xhehenemi",
    "ata do të hidhen në të",
    "e sa i keq (është)",
    "përfundimi!"
  ],
  "9": [
    "O ju",
    "të cilët",
    "besuan",
    "kur",
    "ju pëshpërisni mes vete",
    "atëherë mos",
    "pëshpërisni",
    "për mëkat",
    "dhe armiqësi",
    "dhe mosbindje",
    "të të dërguarit",
    "por pëshpërisni",
    "për bamirësi",
    "dhe devotshmëri",
    "dhe druajuni",
    "Allahut",
    "i Cili",
    "tek Ai",
    "ju (do të) tuboheni."
  ],
  "10": [
    "Vetëm (është)",
    "biseda e fshehtë",
    "nga",
    "shejtani",
