{
  "1": [
    "Sigurisht",
    "ka dëgjuar",
    "Allahu",
    "fjalën",
    "e asaj e cila",
    "diskutonte me ty",
    "lidhur me",
    "burrin e saj",
    "dhe ankohej",
    "tek",
    "Allahu",
    "e Allahu",
    "dëgjon",
    "bisedën e juve dyve",
    "vërtet",
    "Allahu",
    "(është) Gjithëdëgjues",
    "Gjithëshikues."
  ],
  "2": [
    "Ata të cilët",
    "i shkurorëzojnë (duke i krahasuar gratë e tyre me shpinën e nënave të tyre)",
    "prej jush",
    "nga",
    "gratë e tyre",
    "nuk (janë)",
    "ato",
    "nënat e tyre",
    "nuk (janë)",
    "nënat e tyre",
    "përveç",
    "ato të cilat",
    "i kanë lindur ata",
    "dhe vërtet ata",
    "thonë",
    "neveri",
    "nga",
    "fjalët",
    "dhe shpifje",
    "dhe vërtet",
    "Allahu",
    "(është) Fshirës (i gjynaheve)",
    "Falës i madh."
  ],
  "3": [
    "Dhe ata të cilët",
    "i krahasojnë me shpinat e nënave",
    "nga",
    "gratë e tyre",
    "pastaj",
    "kthehen",
    "tek ajo çfarë",
    "thanë",
    "atëherë (është obligim) lirimi",
    "i qafës (një robi)",
    "nga",
    "para",
    "se",
    "ta prekin njëri-tjetrin.",
    "Këto (janë)",
    "(çfarë) ju këshilloheni",
    "me të",
    "e Allahu",
    "për atë çfarë",
    "ju punoni",
    "(është) i Gjithëinformuar."
  ],
  "4": [
    "E kush",
    "nuk",
    "gjen",
    "atëherë (është obligim) agjërimi",
    "i dy muajve",
    "të njëpasnjëshëm",
    "nga",
    "para",
    "se",
    "ta prekin njërin-tjetrin.",
    "E kush",
    "nuk",
    "mundet",
    "atëherë (kompensimi është) ushqyerja",
    "e gjashtëdhjetë",
    "nevojtarëve.",
    "Kjo (është)",
    "që ta besoni",
    "Allahun",
    "dhe të dërguarin e Tij",
    "dhe ato (janë)",
    "kufijtë",
    "e Allahut",
    "e për mohuesit (ka)",
    "dënim",
    "të dhembshëm."
  ],
  "5": [
    "Vërtet",
    "ata të cilët",
    "i kundërvihen",
    "Allahut",
    "dhe të dërguarit të Tij",
    "janë të mposhtur",
    "ashtu siç",
    "u mposhtën",
    "ata të cilët",
    "nga",
    "para tyre.",
    "E sigurisht",
    "Ne zbritëm",
    "prova",
    "të qarta",
    "e për mohuesit (ka)",
    "ndëshkim",
    "poshtërues."
  ],
  "6": [
    "Ditën kur",
    "do t'i ringjall ata",
    "Allahu",
    "të gjithë",
    "e t'i njoftojë ata",
    "për atë çfarë",
    "ata punuan",
    "e regjistroi atë",
    "Allahu",
    "e ata e harruan atë",
    "e Allahu (është)",
    "për",
    "çdo",
    "gjë",
    "Dëshmues."
  ],
  "7": [
    "A nuk",
    "ti sheh",
    "se",
    "Allahu",
    "di",
    "atë çfarë",
    "(është) në",
    "qiejt",
    "dhe atë çfarë",
    "(është) në",
    "tokë.",
    "Nuk",
    "është",
    "asnjë",
    "bisedë e fshehtë",
    "e treve",
    "përveçse",
    "Ai (është)",
