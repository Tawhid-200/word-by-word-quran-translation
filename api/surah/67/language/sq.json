{
  "1": [
    "I Madhëruar (është)",
    "Ai i Cili",
    "në Dorën e Tij (është)",
    "sundimi",
    "dhe Ai",
    "mbi",
    "çdo",
    "gjë",
    "(është) i Plotfuqishëm."
  ],
  "2": [
    "Ai i Cili",
    "krijoi",
    "vdekjen",
    "dhe jetën",
    "për t'ju sprovuar",
    "cili prej jush",
    "(bën) më të bukurën",
    "vepër",
    "dhe Ai (është)",
    "i Pamposhtur",
    "Falës."
  ],
  "3": [
    "Ai i Cili",
    "krijoi",
    "shtatë",
    "qiej",
    "palë-palë.",
    "Nuk",
    "ti sheh",
    "në",
    "krijimin",
    "e të Gjithëmëshirshmit",
    "asnjë",
    "kontrast.",
    "Prandaj ktheje",
    "shikimin",
    "a",
    "po sheh",
    "ndonjë",
    "çarje?"
  ],
  "4": [
    "Pastaj",
    "ktheje",
    "shikimin",
    "herë pas here",
    "(do të) kthehet",
    "tek ti",
    "shikimi",
    "i përulur",
    "duke qenë ai",
    "i molisur."
  ],
  "5": [
    "Dhe sigurisht",
    "Ne e stolisëm",
    "qiellin",
    "e kësaj bote",
    "me fenerë",
    "dhe i bëmë ata",
    "gjuajtës",
    "kundër djajve",
    "dhe përgatitëm",
    "për ta",
    "dënimin",
    "e zjarrit ndezës."
  ],
  "6": [
    "E për ata të cilët",
    "mohuan",
    "Zotin e tyre",
    "(është) dënimi",
    "i xhehenemit",
    "e sa i keq (është)",
    "destinimi!"
  ],
  "7": [
    "Kur",
    "(të) hidhen",
    "në të",
    "(do t'ia) dëgjojnë",
    "atij",
    "një ushtimë",
    "ndërkohë që ai",
    "vlon."
  ],
  "8": [
    "(Është) gati",
    "(të) copëtohet",
    "nga",
    "mllefi.",
    "Sa herë që",
    "hidhet",
    "në të",
    "një turmë",
    "i pyesin ata",
    "rojet e tij",
    "\"A nuk",
    "erdhi tek ju",
    "ndonjë paralajmërues?\""
  ],
  "9": [
    "Ata (do të) thonë",
    "\"Po",
    "sigurisht",
    "erdhi tek ne",
    "një paralajmërues",
    "por ne përgënjeshtruam",
    "dhe thamë",
    "\"Nuk",
    "ka zbritur",
    "Allahu",
    "asnjë",
    "gjë",
    "nuk",
    "ju (jeni)",
    "përveçse",
    "në",
    "një humbje",
    "të madhe\"."
  ],
  "10": [
    "Dhe thanë",
    "\"Sikur",
    "(të) ishim",
    "(që) dëgjonim",
    "apo",
    "rroknim",
    "nuk",
    "(do të) ishim",
    "në mesin e",
    "banorëve",
    "të zjarrit ndezës\"."
  ],
  "11": [
    "Prandaj ata e pranuan",
    "mëkatin e tyre",
    "e qofshin larg",
    "banorët",
    "e zjarrit ndezës!"
  ],
  "12": [
    "Vërtet",
    "ata të cilët",
    "i përulen me frikë",
    "Zotit të tyre",
    "në fshehtësi",
    "për ta",
    "(ka) falje",
    "dhe shpërblim",
    "të madh."
  ],
  "13": [
    "Dhe fshiheni",
    "fjalën tuan",
    "apo",
    "publikojeni",
    "atë!",
    "Vërtet Ai (është)",
    "i Gjithëdijshëm",
    "për atë çfarë posedojnë",
    "gjokset."
  ],
  "14": [
    "A nuk",
    "e di",
    "Ai i Cili",
    "krijoi",
    "dhe Ai (është)",
    "Më i Buti",
    "dhe i Mirënjohuri."
  ],
  "15": [
    "Ai (është)",
    "i Cili",
    "bëri",
    "për ju",
    "tokën",
    "të përshtatshme",
    "prandaj ecni",
    "nëpër",
    "pjesët e saj",
    "dhe hani",
    "nga",
    "furnizimi i Tij",
    "dhe tek Ai (është)",
    "ringjallja."
  ],
  "16": [
    "A u siguruat",
    "prej Atij që",
    "(është) në",
    "qiell",
    "që të",
    "fundosë",
    "me ju",
    "tokën",
    "ndërkohë që",
    "ajo",
    "dridhet."
  ],
  "17": [
    "Apo vallë",
    "u siguruat",
    "prej Atij që",
    "(është) në",
    "qiell",
    "që të",
    "dërgojë",
    "mbi ju",
    "një stuhi",
    "e ju pastaj do të kuptoni",
    "si (është)",
    "paralajmërimi im!"
  ],
  "18": [
    "E sigurisht",
    "kanë përgënjeshtruar",
    "ata të cilët (ishin)",
    "nga",
    "para tyre",
    "e si",
    "ishte",
    "ndëshkimi im?"
  ],
  "19": [
    "A nuk",
    "shikojnë",
    "tek",
    "shpendët",
    "mbi ta",
    "krahëhapur",
    "dhe i palosin?",
    "Nuk",
    "i mban ata",
    "askush përveç",
    "të Gjithëmëshirshmit.",
    "Vërtet Ai",
    "për çdo",
    "gjë",
    "(është) Gjithëshikues."
  ],
  "20": [
    "A ka ndonjë sikur",
    "kjo",
    "e cila",
    "ajo (është)",
    "një ushtri",
    "për ju",
    "ju ndihmon ju",
    "nga",
    "pos",
    "të Gjithëmëshirshmit?",
    "Nuk (janë)",
    "mohuesit",
    "përveçse",
    "në",
    "një mashtrim."
  ],
  "21": [
    "A ka ndonjë sikur",
    "Ky",
    "i Cili",
    "ju ushqen ju",
    "nëse",
    "Ai e ndërpret",
    "furnizimin e Tij?",
    "Jo, por",
    "ata janë futur",
    "në",
    "arrogancë",
    "dhe neveri."
  ],
  "22": [
    "Vallë ai i cili",
    "ecën",
    "i përmbysur",
    "me",
    "fytyrën e tij",
    "(është) më i udhëzuar",
    "apo ai i cili",
    "ecën",
    "pingul",
    "në",
    "një rrugë",
    "të drejtë?"
  ],
  "23": [
    "Thuaj",
    "\"Ai (është)",
    "i Cili",
    "ju krijoi ju",
    "dhe bëri",
    "për ju",
    "dëgjimin",
    "dhe shikimet",
    "dhe zemrat.",
    "Sa pak",
    "[është ajo çfarë]",
    "ju falënderoni\"."
  ],
  "24": [
    "Thuaj",
    "\"Ai (është)",
    "i Cili",
    "ju krijoi e ju shumoi",
    "në",
    "tokë",
    "dhe tek Ai",
    "(do të) tuboheni\"."
  ],
  "25": [
    "Dhe ata thonë",
    "\"Kur (është)",
    "ky",
    "premtim",
    "nëse",
    "jeni",
    "të sinqertë?\""
  ],
  "26": [
    "Thuaj",
    "\"Vetëm",
    "dija",
    "(është) tek",
    "Allahu",
    "dhe vetëm",
    "unë (jam)",
    "një paralajmërues",
    "i qartë\"."
  ],
  "27": [
    "E pasi që",
    "ata e panë atë (xhehenemin)",
    "duke u afruar",
    "u shëmtuan",
    "fytyrat",
    "e atyre të cilët",
    "mohuan",
    "dhe u thuhet",
    "\"Kjo (është)",
    "ajo e cila",
    "ju ishit",
    "atë",
    "duke e kërkuar\"."
  ],
  "28": [
    "Thuaj",
    "\"Ç'mendoni vallë",
    "nëse",
    "më shkatërron mua",
    "Allahu",
    "dhe ata që janë",
    "me mua",
    "apo",
    "na mëshiron ne",
    "atëherë kush",
    "(do t'i) mbrojë",
    "mohuesit",
    "nga",
    "një dënim",
    "i dhembshëm?\""
  ],
  "29": [
    "Thuaj",
    "\"Ai (është)",
    "i Gjithëmëshirshmi",
    "ne besojmë",
    "në Të",
    "dhe tek Ai",
    "mbështetemi",
    "e ju do ta dini",
    "kush",
    "(është) ai",
    "në",
    "një devijim",
    "të qartë\"."
  ],
  "30": [
    "Thuaj",
    "\"Ç'mendoni vallë",
    "nëse",
    "shteret",
    "uji juaj",
    "në thellësi",
    "atëherë kush",
    "do t'ju sjell ju",
    "ujë",
    "burimor?\""
  ]
}