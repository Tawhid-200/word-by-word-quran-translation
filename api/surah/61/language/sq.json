{
  "1": [
    "E lartësoi",
    "Allahun",
    "ajo çfarë (është)",
    "në",
    "qiej",
    "dhe ajo çfarë (është)",
    "në",
    "tokë",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ],
  "2": [
    "O ju",
    "të cilët",
    "besuat",
    "përse",
    "ju thuani",
    "atë çfarë",
    "nuk",
    "ju bëni."
  ],
  "3": [
    "Është e madhe",
    "neveri",
    "tek",
    "Allahu",
    "të",
    "ju thoni",
    "atë çfarë",
    "nuk",
    "ju bëni."
  ],
  "4": [
    "Vërtet",
    "Allahu",
    "i do",
    "ata të cilët",
    "luftojnë",
    "në",
    "rrugën e Tij",
    "të rreshtuar",
    "si të ishin ata",
    "një ndërtesë",
    "e fortifikuar."
  ],
  "5": [
    "Dhe kur",
    "(i) tha",
    "Musai",
    "popullit të tij",
    "\"O populli im",
    "përse",
    "ju më lëndoni mua",
    "e sigurisht",
    "ju e dini",
    "se vërtet unë (jam)",
    "i dërguari",
    "i Allahut",
    "tek ju.",
    "Pasi që",
    "ata u larguan (nga e vërteta)",
    "i largoi",
    "Allahu",
    "zemrat e tyre",
    "e Allahu",
    "nuk",
    "e udhëzon",
    "popullin",
    "e prishur\"."
  ],
  "6": [
    "Dhe kur",
    "tha",
    "Isai",
    "biri",
    "i Merjemes",
    "\"O bijtë",
    "e Israilit",
    "vërtet unë (jam)",
    "i dërguari",
    "i Allahut",
    "tek ju",
    "vërtetues",
    "i asaj çfarë (është)",
    "mes",
    "duarve rë mia",
    "nga",
    "Tevrati",
    "dhe përgëzues",
    "për një të dërguar",
    "(i cili) do të vijë",
    "prej",
    "pas meje",
    "emri i tij (është)",
    "Ahmed\".",
    "Pasi që",
    "ai u solli atyre",
    "prova të qarta",
    "ata thanë",
    "\"Kjo (është)",
    "një magji",
    "e qartë!\""
  ],
  "7": [
    "E kush (është)",
    "më i padrejtë",
    "se ai që",
    "shpif",
    "kundër",
    "Allahut",
    "gënjeshtrën",
    "dhe ai",
    "ftohet",
    "tek",
    "Islami?!",
    "E Allahu",
    "nuk",
    "e udhëzon",
    "popullin",
    "zullumqar."
  ],
  "8": [
    "Ata dëshirojnë",
    "ta shuajnë",
    "dritën",
    "e Allahut",
    "me gojët e tyre",
    "por Allahu",
    "(është) Plotësues",
    "i dritës së Tij",
    "ndonëse (këtë)",
    "e urrejnë",
    "mohuesit."
  ],
  "9": [
    "Ai (është)",
    "i Cili",
    "e dërgoi",
    "të dërguarin e Tij",
    "me udhëzim",
    "dhe fenë",
    "e vërtetë",
    "për ta shpërfaqur atë",
    "mbi",
    "fenë",
    "secilën",
    "ndonëse (këtë)",
    "e urrejnë",
    "idhujtarët."
  ],
  "10": [
    "O ju",
    "të cilët",
    "besuat",
    "a",
    "t'ju dëftoj ju",
    "për",
    "një tregti",
    "(e cila) do t'ju shpëtojë ju",
    "nga",
    "një dënim",
    "i dhembshëm?"
  ],
  "11": [
    "(Të) besoni",
    "Allahun",
    "dhe të dërguarin e Tij",
    "dhe të luftoni",
    "në",
    "rrugën",
    "e Allahut",
    "me pasuritë tuaja",
    "dhe veten tuaj.",
    "Ajo (është)",
    "më e mirë",
    "për ju",
    "nëse",
    "jeni",
    "ju dini."
  ],
  "12": [
    "Ai do t'ju falë",
    "ju",
    "mëkatet tuaja",
    "dhe do t'ju fusë ju",
    "(në) xhenete",
    "rrjedhin",
    "nga",
    "poshtë tyre",
    "lumenjtë",
    "dhe vendbanime",
    "të këndshme",
    "në",
    "xhenetet",
    "e Adnit.",
    "Ajo (është)",
    "fitorja",
    "madhështore."
  ],
  "13": [
    "Dhe tjetra",
    "ju e doni atë",
    "ndihmë",
    "prej",
    "Allahut",
    "dhe çlirim",
    "i afërt",
    "dhe përgëzoji",
    "besimtarët!"
  ],
  "14": [
    "O ju",
    "të cilët",
    "besuat",
    "jini",
    "ndihmësit",
    "e Allahut",
    "ashtu siç",
    "(u) tha",
    "Isai",
    "biri",
    "I Merjemes",
    "dishepujve",
    "\"Kush (janë)",
    "ndihmësit e mi",
    "tek",
    "Allahu?\"",
    "Thanë",
    "dishepujt",
    "\"Ne (jemi)",
    "ndihmësit",
    "e Allahut!\"",
    "Atëherë besoi",
    "një grup",
    "nga",
    "bijtë",
    "e Israilit",
    "dhe mohoi",
    "një grup.",
    "Kështu Ne i përkrahëm",
    "ata të cilët",
    "besuan",
    "ndaj",
    "armiqve të tyre",
    "dhe ashtu ata u bënë",
    "ngadhënjyes."
  ]
}