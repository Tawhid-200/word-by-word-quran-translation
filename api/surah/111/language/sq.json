{
  "1": [
    "Shkatërruar qofshin",
    "dy duart",
    "e Ebu",
    "Lehebit",
    "e ai tashmë është shkatërruar"
  ],
  "2": [
    "Nuk",
    "i bëri dobi",
    "atij",
    "pasuria e tij",
    "dhe ajo çfarë",
    "ai fitoi"
  ],
  "3": [
    "do të hidhet",
    "në zjarr",
    "që ka",
    "flakë"
  ],
  "4": [
    "e edhe gruaja e tij",
    "bartëse",
    "e dunjve"
  ],
  "5": [
    "në",
    "qafën e saj",
    "(ajo ka) një litar",
    "nga",
    "fibra e palmës"
  ]
}