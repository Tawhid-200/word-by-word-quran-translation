{
  "1": [
    "Krisma (e kijametit)"
  ],
  "2": [
    "Ç'(është)",
    "krisma (e kijametit)?"
  ],
  "3": [
    "Dhe çka",
    "di ti",
    "ç'(është)",
    "krisma (e kijametit)?"
  ],
  "4": [
    "Atë ditë",
    "(do të) jenë",
    "njerëzit",
    "si flutura",
    "të shpërndara"
  ],
  "5": [
    "Dhe (do të) jenë",
    "kodrat",
    "si leshi",
    "i shprishur"
  ],
  "6": [
    "E sa i përket",
    "atij që",
    "rëndohen",
    "peshoret e tij"
  ],
  "7": [
    "Atëherë ai",
    "(do të jetë) në",
    "një jetë",
    "të kënaqshme"
  ],
  "8": [
    "E sa i përket",
    "atij që",
    "lehtësohen",
    "peshoret e tij"
  ],
  "9": [
    "Atëherë vendi i tij",
    "(do të jetë) një Gropë"
  ],
  "10": [
    "E çfarë",
    "di ti",
    "ç'(është)",
    "ajo?"
  ],
  "11": [
    "Një zjarr",
    "fort i nxehtë"
  ]
