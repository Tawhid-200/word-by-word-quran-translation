{
  "1": [
    "Është i bekuar",
    "Ai i Cili",
    "zbriti",
    "Kriterin dallues",
    "tek",
    "robi i Tij",
    "që të jetë",
    "për botët",
    "paralajmërues."
  ],
  "2": [
    "Ai i Cili",
    "Atij (i takon)",
    "sundimi",
    "i qiejve",
    "dhe i tokës",
    "dhe nuk",
    "Ai mori",
    "fëmijë",
    "dhe nuk",
    "është",
    "për Të",
    "(asnjë) shoqërues",
    "në",
    "sundim",
    "dhe Ai krijoi",
    "çdo",
    "gjë",
    "dhe e përcaktoi atë",
    "me përcaktim."
  ],
  "3": [
    "Dhe ata morën",
    "nga",
    "përveç Tij",
    "zota",
    "nuk",
    "ata krijojnë",
    "asgjë",
    "ndërkohë që ata",
    "krijohen",
    "dhe nuk",
    "ata posedojnë",
    "për veten e tyre",
    "(asnjë) dëm",
    "dhe as",
    "(asnjë) dobi",
    "dhe nuk",
    "ata posedojnë",
    "vdekje",
    "dhe as",
    "jetë",
    "dhe as",
    "ringjallje."
  ],
  "4": [
    "Dhe thanë",
    "ata të cilët",
    "mohuan",
    "\"Nuk (është)",
    "ky (Kur'ani)",
    "përveçse",
    "(një) rrenë",
    "ai e trilloi atë",
    "dhe e ndihmuan atë",
    "për të",
    "njerëz",
    "të tjerë\".",
    "Sigurisht",
    "ata sollën",
    "zullum",
    "dhe shpifje."
  ],
  "5": [
    "Dhe ata thanë",
    "\"Mite",
    "të të parëve",
    "ai i shkroi ato",
    "e ato",
    "(i) diktohen",
    "atij",
    "në mëngjes",
    "dhe në mbrëmje\"."
  ],
  "6": [
    "Thuaj",
    "\"E zbriti atë",
    "Ai i Cili",
    "di",
    "të fshehtën",
    "në",
    "qiejt",
    "dhe tokë.",
    "Vërtet Ai",
    "është",
    "Falës i madh",
    "Mëshirues."
  ],
  "7": [
    "Dhe ata thanë",
    "\"Ç'është puna",
    "(me) këtë",
    "të dërguar",
    "ai ha",
    "ushqim",
    "dhe ecën",
    "nëpër",
    "tregje?",
    "Përse nuk",
    "(iu) zbrit",
    "atij",
    "(një) engjëll",
    "e ai të jetë",
    "me të",
    "paralajmërues?\""
  ],
  "8": [
    "Apo",
    "t'i hidhej",
    "atij",
    "(një) thesar",
    "apo",
    "të jetë",
    "për të",
    "(një) kopsht",
    "ai të hajë",
    "prej tij.",
    "Dhe thonë",
    "zullumqarët",
    "\"Nuk",
    "ju pasoni",
    "përveçse",
    "një njeri",
    "të magjepsur!\""
  ],
  "9": [
    "Shiko",
    "si",
    "ata (të) sjellin",
    "ty",
    "shembujt",
    "ashtu që ata devijuan",
    "dhe nuk",
    "ata mund (të gjejnë)",
    "rrugë."
  ],
  "10": [
    "Bekuar qoftë",
    "Ai i Cili",
    "nëse",
    "Ai dëshiron",
    "Ai cakton",
    "për ty",
    "më mirë",
    "nga",
    "ajo",
    "kopshte",
    "rrjedhin",
    "nga",
    "poshtë tyre",
    "lumenjtë",
    "dhe Ai cakton",
    "për ty",
    "pallate."
  ],
  "11": [
    "Por jo!",
    "Ata përgënjeshtruan",
    "Orën (kijametin)",
    "dhe Ne i përgatitëm",
    "atij që",
    "përgënjeshtron",
    "Orën (kijametin)",
    "(një) zjarr flakërues."
  ],
  "12": [
    "Kur",
    "ai i sheh ata",
    "nga",
    "(një) vend",
    "i largët",
    "ata dëgjojnë",
    "të tij",
    "(një) tërbim",
    "dhe gërhitje."
  ],
  "13": [
    "E kur",
    "ata të hidhen",
    "prej tij",
    "(në) një vend",
    "të ngushtë",
    "duarlidhur",
    "ata do të thërrasin",
    "atje",
    "shkatërrim."
  ],
  "14": [
    "\"Mos",
    "thërrisni",
    "sot",
    "shkatërrim",
    "një",
    "por thërrisni",
    "shkatërrim",
    "të shumtë!\""
  ],
  "15": [
    "Thuaj",
    "\"A ai (është)",
    "më i mirë",
    "apo",
    "xheneti",
    "i përhershmërisë",
    "i cili",
    "(u) është premtuar",
    "të druajturve?",
    "Ai do të jetë",
    "për ta",
    "shpërblim",
    "dhe destinim."
  ],
  "16": [
    "Për ta",
    "në të",
    "(ka) çfarë",
    "ata dëshirojnë",
    "(janë) të gjithmonshëm.",
    "Ai është",
    "për",
    "Zotin tënd",
    "një premtim",
    "i kërkuar\"."
  ],
  "17": [
    "E ditën kur",
    "Ai do t'i tubojë ata",
    "dhe atë çfarë",
    "ata adhurojnë",
    "nga",
    "përveç",
    "Allahut",
    "e Ai do të thotë",
    "\"A ju",
    "i devijuat",
    "robërit e Mi",
    "këta",
    "apo",
    "ata",
    "e devijuan",
    "rrugën?\""
  ],
  "18": [
    "Ata do të thonë",
    "\"Lartësuar qofsh Ti!",
    "Nuk",
    "është",
    "nevojitet",
    "për ne",
    "që të",
    "marrim",
    "nga",
    "përveç Teje",
    "asnjë",
    "ndihmës",
    "mirëpo",
    "Ti i kënaqe ata",
    "dhe baballarët e tyre",
    "derisa",
    "ata e harruan",
    "përkujtimin",
    "dhe u bënë",
    "një popull",
    "i shkatërruar\"."
  ],
  "19": [
    "Sigurisht",
    "ata ju përgënjeshtruan ju",
    "rreth asaj çfarë",
    "ju thoni",
    "prandaj nuk",
    "ju keni mundësi",
    "(për) shmangie",
    "e as",
    "(për) ndihmë.",
    "E kushdo që",
    "bën zullum",
    "prej jush",
    "Ne do të bëjmë të shijojë",
    "një dënim",
    "të madh."
  ],
  "20": [
    "Dhe nuk",
    "Ne dërguam",
    "para teje",
    "prej",
    "të dërguarve",
    "përveçse",
    "vërtet ata",
    "hanin",
    "ushqim",
    "dhe ecnin",
    "nëpër",
    "tregje.",
    "Dhe Ne bëmë",
    "disa prej jush",
    "për disa të tjerë",
    "(një) sprovë",
    "a do të duroni?",
    "Dhe është",
    "Zoti yt",
    "Gjithëshikues."
  ],
  "21": [
    "Dhe thanë",
    "ata të cilët",
    "nuk",
    "shpresojnë",
    "takimin Tonë",
    "\"Përse nuk",
    "u zbritën",
    "tek ne",
    "engjëjt",
    "apo",
    "ne ta shohim",
    "Zotin tonë?\"",
    "Njëmend",
    "ata shprehën kryeneçësi",
    "në",
    "vetët e tyre",
    "dhe u rebeluan",
    "(me) një rebelim",
    "të madh."
  ],
  "22": [
    "Ditën kur",
    "ata (do t'i) shohin",
    "engjëjt",
    "nuk (ka)",
    "myzhde",
    "atë ditë",
    "për kriminelët",
    "dhe ata do të thonë",
    "\"Pengesë",
    "e penguar!\""
  ],
  "23": [
    "Dhe Ne do të drejtohemi",
    "tek",
    "ajo çfarë",
    "ata punuan",
    "nga",
    "puna",
    "e do ta bëjmë atë",
    "hi",
    "të shpërndarë."
  ],
  "24": [
    "Banorët",
    "e xhenetit",
    "atë ditë",
    "(kanë) më të mirin",
    "vendbanim",
    "dhe më të këndshmen",
    "pushimore."
  ],
  "25": [
    "Dhe ditën kur",
    "do të çahet",
    "qielli",
    "me retë",
    "dhe do të zbresin",
    "engjëjt",
    "me zbritje graduale."
  ],
  "26": [
    "Sundimi",
    "atë ditë",
    "i vërtetë",
    "i përket të Gjithëmëshirshmit.",
    "E ajo do të jetë",
    "një ditë",
    "për",
    "mohuesit",
    "e vështirë."
  ],
  "27": [
    "E ditën kur",
    "(do të) kafshojë",
    "zullumqari",
    "mbi",
    "dy duart e tij",
    "thotë",
    "\"Kuku për mua!",
    "Të kisha marrë",
    "me",
    "të dërguarin",
    "një rrugë!\""
  ],
  "28": [
    "Vaj halli për mua",
    "sikur unë",
    "të mos",
    "e merrja",
    "filanin",
    "për dost!"
  ],
  "29": [
    "Sigurisht",
    "ai më devijoi mua",
    "nga",
    "përkujtimi",
    "pasi",
    "atëherë",
    "më kishte ardhur.",
    "Dhe është",
    "shejtani",
    "për njeriun",
    "tradhtar."
  ],
  "30": [
    "Dhe tha",
    "i dërguari",
    "\"O Zoti im",
    "vërtet",
    "populli im",
    "e morën",
    "këtë",
    "Kur'an",
    "të braktisur\"."
  ],
  "31": [
    "Dhe kështu",
    "Ne bëmë",
    "për secilin",
    "pejgamber",
    "një armik",
    "prej",
    "kriminelëve",
    "dhe mjafton",
    "Zoti yt",
    "Udhëzues",
    "dhe Përkrahës."
  ],
  "32": [
    "Dhe thanë",
    "ata të cilët",
    "mohuan",
    "\"Përse të mos",
    "(i) zbriste",
    "atij",
    "Kur'ani",
    "si tërësi",
    "e vetme?\"",
    "Kështu",
    "që Ne ta përforcojmë",
    "me të",
    "zemrën tënde",
    "dhe Ne e recituam atë",
    "me recitim (të ngadalshëm)."
  ],
  "33": [
    "Dhe nuk",
    "ata të sjellin ty",
    "një shembull",
    "përveçse",
    "Ne ta sjellim ty",
    "të vërtetën",
    "dhe më të bukurin",
    "shpjegim."
  ],
  "34": [
    "Ata të cilët",
    "do të tubohen",
    "me",
    "fytyrat e tyre",
    "tek",
    "xhehenemi.",
    "Të tillëve (u përket)",
    "më i keqi",
    "vend",
    "dhe (janë) më të humburit",
    "(në) rrugë."
  ],
  "35": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Musait",
    "Librin",
    "dhe Ne e bëmë",
    "me të",
    "vëllain e tij",
    "Harunin",
    "ndihmës."
  ],
  "36": [
    "E (u) thamë",
    "\"Shkoni ju dy",
    "tek",
    "populli",
    "të cilët",
    "përgënjeshtruan",
    "dëshmitë tona\"",
    "e Ne i shkatërruam ata",
    "(me) shkatërrim (të plotë)."
  ],
  "37": [
    "Edhe popullin",
    "e Nuhut",
    "pasi që",
    "ata i përgënjeshtruan",
    "të dërguarit",
    "Ne i përmbytëm ata",
    "dhe i bëmë ata",
    "për njerëzit",
    "(një) dëshmi",
    "edhe përgatitëm",
    "për zullumqarët",
    "(një) dënim",
    "të dhembshëm."
  ],
  "38": [
    "Edhe Adin",
    "dhe Themudin",
    "dhe banorët",
    "e Resit",
    "dhe breza",
    "mes",
    "asaj",
    "të shumtë."
  ],
  "39": [
    "Dhe secilit",
    "Ne i sollëm",
    "atij",
    "shembujt",
    "dhe secilin",
    "e shkatërruam",
    "(me) shkatërrim."
  ],
  "40": [
    "Dhe sigurisht",
    "ata erdhën",
    "pranë",
    "vendbanimit",
    "i cili",
    "ishte mbërthyer",
    "(nga) shiu",
    "i ligësisë.",
    "A nuk",
    "ishin",
    "duke e parë atë?",
    "Jo, por",
    "ata ishin",
    "nuk",
    "shpresonin",
    "(në) ringjallje."
  ],
  "41": [
    "E kur",
    "ata të shohin ty",
    "nuk",
    "ata të konsiderojnë ty",
    "përveçse",
    "(me) tallje",
    "\"A ky (është)",
    "ai i cili",
    "e dërgoi",
    "Allahu",
    "të dërguar?\""
  ],
  "42": [
    "[Por]",
    "pothuaj",
    "ai do t'na largonte ne",
    "nga",
    "zotat tanë",
    "sikur mos",
    "të",
    "duronim",
    "për ta.",
    "E do të",
    "dinë",
    "kur",
    "e shohin",
    "dënimin",
    "kush (është)",
    "më i humburi",
    "(nga) rruga."
  ],
  "43": [
    "A e ke parë",
    "atë që",
    "e mori",
    "zotin e tij",
    "nepsin e tij",
    "vallë, a ti",
    "do të jesh",
    "për të",
    "mbrojtës?"
  ],
  "44": [
    "Apo",
    "mendo",
    "se",
    "shumica e tyre",
    "dëgjojnë",
    "apo",
    "logjikojnë?",
    "Nuk",
    "ata (janë)",
    "përveçse",
    "sikurse bagëtitë",
    "madje",
    "ata (janë)",
    "më të humbur",
    "(nga) rruga."
  ],
  "45": [
    "A nuk",
    "ti sheh",
    "tek",
    "Zoti yt",
    "si",
    "e zgjati",
    "hijen",
    "e sikur",
    "Ai të dëshironte",
    "do ta bënte atë",
    "statike.",
    "Pastaj",
    "Ne e bëmë",
    "diellin",
    "për të",
    "dëftues."
  ],
  "46": [
    "Pastaj",
    "Ne e kapim atë",
    "tek Ne",
    "(me) një kapje",
    "të lehtë."
  ],
  "47": [
    "Dhe Ai (është)",
    "i Cili",
    "bëri",
    "për ju",
    "natën",
    "mbulim",
    "e gjumin",
    "pushim",
    "dhe e bëri",
    "ditën",
    "gjallërim."
  ],
  "48": [
    "Dhe Ai (është)",
    "i Cili",
    "dërgoi",
    "erërat",
    "(si) myzhde",
    "mes",
    "pranë",
    "mëshirës së Tij.",
    "Dhe Ne zbritëm",
    "prej",
    "qiellit",
    "ujë",
    "të pastër."
  ],
  "49": [
    "Që Ne ta ngjallim",
    "me të",
    "një vend",
    "të vdekur",
    "dhe t'u japim të pinë",
    "prej asaj çfarë",
    "Ne krijuam",
    "bagëti",
    "e njerëz",
    "të shumtë."
  ],
  "50": [
    "E sigurisht",
    "Ne e kemi shpërndarë atë",
    "mes tyre",
    "që ata të përkujtojnë",
    "por refuzuan",
    "shumica",
    "e njerëzve",
    "përveçse",
    "(duke shfaqur) mohim."
  ],
  "51": [
    "E sikur",
    "Ne të donim",
    "Ne do të dërgonim",
    "në",
    "çdo",
    "vendbanim",
    "një paralajmërues."
  ],
  "52": [
    "Prandaj mos",
    "iu bind",
    "mohuesve",
    "dhe përballoji ata",
    "me të (Kur'an)",
    "(me) një përballim",
    "të madh."
  ],
  "53": [
    "Dhe Ai (ështël",
    "i Cili",
    "i ka lëshuar",
    "dy detet",
    "ky (është)",
    "i pijshëm",
    "i shijshëm",
    "kurse ky (është)",
    "i kripur",
    "i hidhur",
    "dhe Ai bëri",
    "mes atyre dyve",
    "një mur",
    "dhe pengesë",
    "të penguar."
  ],
  "54": [
    "Dhe Ai (është)",
    "i Cili",
    "krijoi",
    "nga",
    "uji",
    "qenien njerëzore",
    "dhe bëri për të",
    "prejardhje",
    "dhe lidhje martesore",
    "dhe është",
    "Zoti yt",
    "i Plotfuqishëm."
  ],
  "55": [
    "Dhe ata adhurojnë",
    "nga",
    "përveç",
    "Allahut",
    "atë çfarë",
    "nuk",
    "u sjell dobi atyre",
    "dhe as",
    "(u) sjell dëm atyre",
    "dhe është",
    "mohuesi",
    "kundër",
    "Zotit të tij",
    "përkrahës."
  ],
  "56": [
    "Dhe nuk",
    "Ne të dërguam ty",
    "përveçse",
    "përgëzues",
    "dhe paralajmërues."
  ],
  "57": [
    "Thuaj",
    "\"Nuk",
    "unë ju kërkoj ju",
    "për të",
    "asnjë",
    "shpërblim",
    "përveçse",
    "kushdo që",
    "dëshiron",
    "të",
    "marrë",
    "tek",
    "Zoti i tij",
    "një rrugë\"."
  ],
  "58": [
    "Dhe mbështetu",
    "tek",
    "i Gjalli",
    "i Cili",
    "nuk",
    "vdes",
    "dhe lartësoje",
    "me falënderimin ndaj Tij",
    "dhe mjafton",
    "(që) Ai",
    "për mëkatet",
    "e robërve të Tij",
    "(është) i Gjithëinformuar."
  ],
  "59": [
    "Ai i Cili",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "dhe çfarë është",
    "mes atyre dyve",
    "për",
    "gjashtë",
    "ditë",
    "pastaj",
    "qëndroi",
    "mbi",
    "Arsh.",
    "Ai (është) i Gjithëmëshirshmi.",
    "Prandaj pyet",
    "për Të",
    "një ekspert!"
  ],
  "60": [
    "E kur",
    "(u) thuhet",
    "atyre",
    "\"Bëni sexhde",
    "të Gjithëmëshirshmit!\"",
    "ata thonë",
    "\"E çfarë (është)",
    "i Gjithëmëshirshmi?",
    "A ne t'i bëjmë sexhde",
    "asaj çfarë",
    "ti na urdhëron ne?\"",
    "Dhe ua shtoi atyre",
    "largimin."
  ],
  "61": [
    "I bekuar qoftë",
    "Ai i Cili",
    "vendosi",
    "në",
    "qiell",
    "yje",
    "dhe vendosi",
    "në të",
    "fener",
    "dhe hënë",
    "të ndritshme."
  ],
  "62": [
    "Dhe Ai (është)",
    "i Cili",
    "bëri",
    "natën",
    "dhe ditën",
    "zëvendësuese",
    "për atë që",
    "dëshiron",
    "të",
    "përkujtojë",
    "apo",
    "dëshiron",
    "falënderim."
  ],
  "63": [
    "Dhe robërit",
    "e të Gjithëmëshirshmit",
    "(janë) ata të cilët",
    "ecin",
    "mbi",
    "tokë",
    "të qetë",
    "dhe kur",
    "u drejtohen me fjalë atyre",
    "të padijshmit",
    "ata thonë",
    "\"Paqe!\""
  ],
  "64": [
    "Dhe ata të cilët",
    "e kalojnë natën",
    "për Zotin e tyre",
    "në sexhde",
    "dhe në këmbë."
  ],
  "65": [
    "Dhe ata të cilët",
    "thonë",
    "\"Zoti ynë",
    "largoje",
    "nga ne",
    "dënimin",
    "e xhehenemit!",
    "Vërtet",
    "dënimi i tij",
    "është",
    "i pashkëputur."
  ],
  "66": [
    "Vërtet ai",
    "është i keq",
    "vendqëndrim",
    "dhe vendpushim."
  ],
  "67": [
    "Dhe ata të cilët",
    "kur",
    "shpenzojnë",
    "nuk",
    "teprojnë",
    "dhe nuk",
    "bëjnë koprraci",
    "dhe janë",
    "mes",
    "asaj",
    "të matur."
  ],
  "68": [
    "Dhe ata të cilët",
    "nuk",
    "lusin",
    "krahas",
    "Allahut",
    "një zot",
    "tjetër",
    "dhe nuk",
    "mbysin",
    "njeriun",
    "të cilin",
    "e ka ndaluar",
    "Allahu",
    "përveçse",
    "me hak",
    "dhe nuk",
    "bëjnë kurvëri.",
    "E kushdo që",
    "bën",
    "atë",
    "do të gjejë",
    "ndëshkim."
  ],
  "69": [
    "Do t'i dyfishohet",
    "atij",
    "dënimi",
    "ditën",
    "e Kijametit",
    "dhe do të mbetet përgjithmonë",
    "në të",
    "i nënçmuar."
  ],
  "70": [
    "Përveç",
    "atij që",
    "pendohet",
    "dhe beson",
    "dhe punon",
    "punë",
    "të mirë",
    "e të tillëve",
    "do t'ua zëvendësojë",
    "Allahu",
    "të këqijat e tyre",
    "me të mira",
    "dhe është",
    "Allahu",
    "Falës i madh",
    "Mëshirues."
  ],
  "71": [
    "E kushdo që",
    "pendohet",
    "dhe punon",
    "të mira",
    "atëherë vërtet ai",
    "kthehet",
    "tek",
    "Allahu",
    "(me) kthim."
  ],
  "72": [
    "Dhe ata të cilët",
    "nuk",
    "dëshmojnë",
    "trilimin",
    "dhe kur",
    "kalojnë",
    "pranë llomotitjes",
    "kalojnë",
    "të dinjitetshëm."
  ],
  "73": [
    "Dhe ata të cilët",
    "kur",
    "përkujtohen",
    "me ajetet",
    "e Zotit të tyre",
    "nuk",
    "bijnë",
    "mbi to",
    "të shurdhër",
    "dhe të verbër."
  ],
  "74": [
    "Dhe ata të cilët",
    "thonë",
    "\"Zoti ynë",
    "(na) dhuro",
    "ne",
    "prej",
    "grave tona",
    "dhe pasardhësve tonë",
    "kënaqësinë",
    "e syve",
    "dhe na bëj ne",
    "për të druajturit",
    "prijës\"."
  ],
  "75": [
    "Të tillët",
    "(do të) shpërblehen",
    "me dhomë të lartë",
    "për atë se",
    "duruan",
    "dhe do të priten",
    "në të",
    "me përshëndetje",
    "dhe paqe."
  ],
  "76": [
    "Të gjithmonshëm",
    "në të.",
    "Sa i mirë është ai",
    "vendqëndrim",
    "dhe pushimore."
  ],
  "77": [
    "Thuaj",
    "\"Nuk",
    "do të kujdeset",
    "për ju",
    "Zoti im",
    "po të mos (ishte)",
    "lutja juaj.",
    "Por sigurisht",
    "ju përgënjeshtruat",
    "prandaj do të",
    "jetë",
    "(ndëshkim) i parevokueshëm."
  ]
}