    "të urdhëron ty",
    "që",
    "ne ta lëmë",
    "atë çfarë",
    "adhurojnë",
    "baballarët tanë",
    "apo",
    "që",
    "ne të bëjmë",
    "në",
    "pasuritë tona",
    "atë çfarë",
    "ne dëshirojmë?",
    "Vërtet ti",
    "sigurisht ti (je)",
    "i matur",
    "i pjekur!\""
  ],
  "88": [
    "Ai tha",
    "\"O populli im!",
    "Ç'mendoni ju",
    "nëse",
    "unë jam",
    "në",
    "një provë",
    "nga",
    "Zoti im",
    "dhe Ai më furnizoi mua",
    "nga Ai",
    "(me) një furnizim",
    "të mirë",
    "dhe nuk",
    "unë dëshiroj",
    "që",
    "unë t'ju kundërshtoj ju",
    "tek ajo",
    "e cila",
    "unë ju ndaloj ju",
    "prej saj.",
    "Nuk",
    "unë dëshiroj",
    "përveç",
    "përmirësimin",
    "aq sa",
    "unë mundem",
    "dhe nuk",
    "(është) suksesi im",
    "përveç",
    "me Allahun",
    "tek Ai",
    "unë mbështetem",
    "dhe tek Ai",
    "unë kthehem\"."
  ],
  "89": [
    "\"Dhe o populli im",
    "mos",
    "t'ju nxisë ju assesi",
    "revolta ndaj meje",
    "që",
    "t'ju godasë ju",
    "sikurse",
    "ajo çfarë",
    "e goditi",
    "popullin",
    "e Nuhut",
    "apo",
    "popullin",
    "e Hudit",
    "apo",
    "popullin",
    "e Salihut",
    "dhe nuk (është)",
    "populli",
    "i Lutit",
    "prej jush",
    "i largët."
  ],
  "90": [
    "Dhe kërkoni falje",
    "Zotit tuaj",
    "pastaj",
    "pendohuni",
    "tek Ai",
    "vërtet",
    "Zoti im",
    "(është) Mëshirues",
    "shumë i dashur."
  ],
  "91": [
    "Ata thanë",
    "\"O Shuajb",
    "nuk",
    "kuptojmë",
    "shumicën",
    "e asaj çfarë",
    "ti thua",
    "dhe ne vërtet",
    "të shohim ty",
    "në mesin tonë",
    "të dobët",
    "e sikur të mos (ishte)",
    "rrethi yt",
    "sigurisht do të të gurëzonim ty",
    "dhe nuk",
    "ti (je)",
    "për ne",
    "i çmuar\"."
  ],
  "92": [
    "Ai tha",
    "\"O populli im",
    "a rrethi im",
    "(është) më i çmuar",
    "tek ju",
    "sesa",
    "Allahu",
    "e ju e keni marrë atë",
    "mbrapa jush",
    "(pas) shpine?",
    "Vërtet",
    "Zoti im",
    "për atë çfarë",
    "ju punoni",
    "(është) Gjithëpërfshirës."
  ],
  "93": [
    "Dhe o populli im",
