    "dhe ata u gdhinë",
    "në",
    "vendbanimet e tyre",
    "(si) kufoma."
  ],
  "68": [
    "Sikur të",
    "mos",
    "ekzistonin",
    "në to.",
    "Vini re!",
    "Vërtet",
    "Themudi",
    "mohuan",
    "Zotin e tyre.",
    "Vini re!",
    "Larg qoftë",
    "Themudi!"
  ],
  "69": [
    "Dhe sigurisht",
    "(i) erdhën",
    "të dërguarit Tanë",
    "Ibrahimit",
    "me myzhde",
    "dhe i thanë",
    "\"Paqe!\"",
    "Ai tha",
    "\"Paqe\"",
    "Dhe nuk",
    "qëndroi",
    "[që të]",
    "solli",
    "një viç",
    "të pjekur."
  ],
  "70": [
    "E kur",
    "pa",
    "duart e tyre",
    "nuk",
    "arrinin",
    "tek ai",
    "ndjeu mosbesim tek ta",
    "dhe ndjeu",
    "prej tyre",
    "frikë.",
    "Ata thanë",
    "\"Mos",
    "u frikëso",
    "vërtet ne",
    "jemi dërguar",
    "tek",
    "populli",
    "i Lutit\"."
  ],
  "71": [
    "E gruaja e tij",
    "qëndronte në këmbë",
