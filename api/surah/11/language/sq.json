    "tashmë",
    "ka ardhur",
    "urdhri",
    "i Zotit tënd",
    "e vërtet atyre",
    "do t'u vijë atyre",
    "një dënim",
    "i pa",
    "kthyeshëm."
  ],
  "77": [
    "Pasi që",
    "(i) erdhën",
    "të dërguarit Tanë",
    "Lutit",
    "ai u shqetësua",
    "për shkak të tyre",
    "dhe u ngushtua",
    "për shkak të tyre",
    "rëndë",
    "e tha",
    "\"Kjo (është)",
    "një ditë",
    "e trishtueshme!\""
  ],
  "78": [
    "Dhe erdhën tek ai",
