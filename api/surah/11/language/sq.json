    "urdhri",
    "i Allahut?",
    "Mëshira",
    "e Allahut",
    "dhe bekimi i Tij",
    "qofshin për ju",
    "oj familje",
    "e shtëpisë!",
    "Vërtet Ai (është)",
    "Meritori i të gjitha falënderimeve",
    "i Lavdishëm\"."
  ],
  "74": [
    "Pasi që",
    "iku",
    "nga",
    "Ibrahimi",
    "frika",
    "dhe i erdhi atij",
    "përgëzimi",
    "ai filloi dialogun me ne",
    "rreth",
    "popullit",
    "të Lutit."
  ],
  "75": [
    "Vërtet",
    "Ibrahimi",
    "(ishte) shumë i durueshëm",
    "shumë i përulur me lutje",
    "dhe i kthyeshëm (tek Allahu)."
  ],
  "76": [
    "\"O Ibrahim",
