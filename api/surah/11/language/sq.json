{
  "1": [
    "Elif, Lam, Ra.",
    "Libër",
    "janë përsosur",
    "ajetet e tij",
    "pastaj",
    "janë shkoqitur",
    "nga",
    "ana e",
    "të Urtit",
    "të Gjithëinformuarit."
  ],
  "2": [
    "Që të mos",
    "adhuroni",
    "përveç",
    "Allahun",
    "vërtet unë (jam)",
    "për ju",
    "prej Tij",
    "paralajmërues",
    "dhe përgëzues."
  ],
  "3": [
    "Dhe që",
    "kërkoni falje",
    "Zotit tuaj",
    "pastaj",
    "pendohuni",
    "tek Ai",
    "Ai jua mundëson ju",
    "një përjetim",
    "të bukur",
    "deri",
    "(në) një afat",
    "të caktuar",
    "dhe i jep",
    "secilit",
    "bartës",
    "të mirësisë",
    "mirësinë e tij",
    "e nëse",
    "ata zmbrapsen",
    "atëherë vërtet unë",
    "frikësohem",
    "për ju",
    "(nga) dënimi",
    "i një dite",
    "të madhe."
  ],
  "4": [
    "Tek",
    "Allahu",
    "(është vendkthimi juaj)",
    "dhe AI (është)",
    "mbi",
    "çdo",
    "gjë",
    "i Plotfuqishëm."
  ],
  "5": [
    "Vini re!",
    "Vërtet ata",
    "mbështjellin",
    "gjokset e tyre",
    "që të fshehin",
    "prej Tij.",
    "Vini re!",
    "Kur",
    "ata mbulohen",
    "me rrobat e tyre",
    "Ai e di",
    "atë çfarë",
    "ata fshehin",
    "dhe atë çfarë",
    "ata shpalosin.",
    "Vërtet Ai (është)",
    "i Gjithëdijshëm",
    "për atë çfarë mbajnë",
    "gjokset."
  ],
  "6": [
    "Dhe nuk (ka)",
    "asnjë",
    "gjallesë",
    "në",
    "tokë",
    "përveç",
    "tek",
    "Allahu",
    "(është) furnizimi i saj",
    "dhe AI e di",
    "vendbanimin e saj",
    "dhe vendstrehimin e saj.",
    "Të gjitha (janë)",
    "në",
    "një libër",
    "të qartë."
  ],
  "7": [
    "Dhe Ai (është)",
    "i Cili",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "për",
    "gjashtë",
    "ditë",
    "dhe ishte",
    "Froni i Tij",
    "mbi",
    "ujë",
    "që t'ju sprovojë",
    "cili nga ju",
    "(është) më i miri",
    "(me) punë.",
    "E nëse",
    "ti thua",
    "\"Vërtet ju",
    "(do të jeni) të ringjallur",
    "nga",
    "pas",
    "vdekjes",
    "sigurisht do të thonë",
    "ata të cilët",
    "mohuan",
    "nuk (është)",
    "kjo",
    "përveç",
    "një magji",
    "e qartë\"."
  ],
  "8": [
    "E nëse",
    "Ne ua shtyjmë",
    "atyre",
    "dënimin",
    "deri (në)",
    "një periudhë",
    "të caktuar",
    "sigurisht do të thonë",
    "\"Çka",
    "e pengon atë?\"",
    "Vini re!",
    "Ditën kur",
    "(do) t'u vijë atyre",
    "ai nuk",
    "(është) i zmbrapsur",
    "prej tyre",
    "dhe i përfshin",
    "ata",
    "ajo çfarë",
    "ata ishin",
    "me të",
    "talleshin."
  ],
  "9": [
    "E nëse",
    "Ne bëjmë të shijojë",
    "njeriu",
    "prej Nesh",
    "një mëshirë",
    "pastaj",
    "Ne e largojmë atë",
    "prej tij",
    "vërtet ai",
    "(është) shumë i dëshpëruar",
    "mohues i madh."
  ],
  "10": [
    "E nëse",
    "Ne bëjmë që ai të shijojë",
    "begati",
    "pas",
    "vështirësisë",
    "(që) e ka prekur atë",
    "sigurisht ai do të thotë",
    "\"U larguan",
    "të këqijat",
    "nga unë!\"",
    "Vërtet ai",
    "(është) i shfrenuar",
    "mburravec i madh"
  ],
  "11": [
    "Përveç",
    "atyre të cilët",
    "duruan",
    "dhe punuan",
    "vepra të mira",
    "të tillët",
    "për ta (ka)",
    "falje",
    "dhe shpërblim",
    "të madh."
  ],
  "12": [
    "Mbase ti",
    "(je) lënës",
    "i një pjese",
    "të asaj çfarë",
    "shpallet",
    "tek ti",
    "dhe shtrëngon",
    "për të",
    "gjoksin tënd",
    "që",
    "ata thonë",
    "\"Përse të mos",
    "zbriste",
    "tek ai",
    "një thesar",
    "apo",
    "të vinte",
    "me të",
    "një engjëll?\"",
    "Vetëm",
    "ti (je)",
    "paralajmërues",
    "e Allahu",
    "mbi",
    "çdo",
    "gjë",
    "(është) mbikëqyrës."
  ],
  "13": [
    "Apo vallë",
    "ata thonë",
    "\"Ai e trilloi atë\".",
    "Thuaj",
    "\"Atëherë, sillni",
    "dhjetë",
    "sure",
    "sikurse ai",
    "të trilluara",
    "dhe thirrni",
    "këdo që",
    "ju mundeni",
    "nga",
    "pos",
    "Allahut",
    "nëse",
    "jeni",
    "të sinqertë\"."
  ],
  "14": [
    "E nëse nuk",
    "(ju) përgjigjen",
    "ju",
    "atëherë dini",
    "se vërtet",
    "ai është zbritur",
    "me dijen",
    "e Allahut",
    "dhe se",
    "nuk (ka)",
    "zot",
    "përveç",
    "Tij.",
    "A jeni, pra",
    "ju",
    "muslimanë?"
  ],
  "15": [
    "Kushdo që",
    "është",
    "dëshiron",
    "jetën",
    "e kësaj bote",
    "dhe stolinë e saj",
    "Ne ua përmbushim",
    "atyre",
    "veprat e tyre",
    "në të",
    "dhe atyre",
    "në të",
    "nuk",
    "do t'u pakësohet (asgjë)."
  ],
  "16": [
    "Të tillët (janë)",
    "ata të cilët",
    "nuk (ka)",
    "për ta",
    "në",
    "botën tjetër",
    "përveç",
    "zjarrit",
    "dhe shkatërrohet",
    "ajo çfarë",
    "ata vepruan",
    "në të",
    "dhe dështon",
    "ajo çfarë",
    "ata ishin",
    "punonin."
  ],
  "17": [
    "Vallë, ai i cili",
    "është",
    "(i mbështetur) në",
    "argument",
    "nga",
    "Zoti i tij",
    "dhe e pason atë",
    "një dëshmi",
    "prej tij",
    "dhe prej",
    "para tij",
    "libri",
    "i Musait",
    "udhëzues",
    "dhe mëshirë.",
    "Të tillët",
    "besojnë",
    "në të.",
    "E kushdo që",
    "mohon",
    "në të",
    "nga",
    "grupet",
    "atëherë zjarri",
    "(është) premtimi i tij.",
    "Prandaj mos",
    "ji",
    "në",
    "dyshim",
    "prej tij.",
    "Vërtet ai",
    "(është) e vërteta",
    "nga",
    "Zoti yt",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "besojnë."
  ],
  "18": [
    "E kush (është)",
    "më i padrejtë",
    "sesa ai që",
    "trilloi",
    "për",
    "Allahun",
    "gënjeshtër?",
    "Të tillët",
    "paraqiten",
    "para",
    "Zotit të tyre",
    "dhe thonë",
    "dëshmitarët",
    "\"Këta (janë)",
    "ata të cilët",
    "gënjyen",
    "ndaj",
    "Zotit të tyre.",
    "pra",
    "mallkimi",
    "i Allahut",
    "(qoftë) ndaj",
    "zullumqarëve!\""
  ],
  "19": [
    "Ata të cilët",
    "pengojnë",
    "nga",
    "rruga",
    "e Allahut",
    "dhe e dëshirojnë atë",
    "të shtrembëruar",
    "ndërkohë që ata",
    "në botën tjetër",
    "ata (janë)",
    "mohues."
  ],
  "20": [
    "Të tillët",
    "nuk",
    "janë",
    "të shpëtuar (nga dënimi)",
    "në",
    "tokë",
    "dhe nuk",
    "është",
    "për ta",
    "nga",
    "pos",
    "Allahut",
    "asnjë",
    "mbrojtës.",
    "Dyfishohet",
    "për ta",
    "dënimi.",
    "Nuk",
    "ata ishin",
    "kanë mundësi",
    "dëgjimin",
    "dhe nuk",
    "ata ishin",
    "shikojnë."
  ],
  "21": [
    "Të tillët (janë)",
    "ata të cilët",
    "e dështuan",
    "vetveten e tyre",
    "dhe humbi",
    "prej tyre",
    "ajo çfarë",
    "ata ishin",
    "duke trilluar."
  ],
  "22": [
    "Nuk (ka)",
    "dyshim",
    "se vërtet ata",
    "në",
    "botën tjetër",
    "ata (janë)",
    "më të dështuarit."
  ],
  "23": [
    "Vërtet",
    "ata të cilët",
    "besuan",
    "dhe vepruan",
    "veprat e mira",
    "dhe u nënshtruan",
    "ndaj",
    "Zotit të tyre",
    "të tillët (janë)",
    "banorët",
    "e xhenetit",
    "ata",
    "në të",
    "(janë) të gjithmonshëm."
  ],
  "24": [
    "Shembulli",
    "i dy grupeve",
    "(është) sikurse i verbëri",
    "dhe i shurdhëri",
    "dhe shikuesi",
    "dhe dëgjuesi.",
    "A",
    "barazohen",
    "si shembull?",
    "A nuk",
    "përkujtoheni?!"
  ],
  "25": [
    "E sigurisht",
    "Ne dërguam",
    "Nuhun",
    "tek",
    "populli i tij",
    "(e ai tha) vërtet unë (jam)",
    "për ju",
    "një paralajmërues",
    "i qartë."
  ],
  "26": [
    "Që të",
    "mos",
    "adhuroni",
    "përveç",
    "Allahut.",
    "Vërtet unë",
    "(i) frikësohem",
    "për ju",
    "një dënimi",
    "të një dite",
    "të dhembshme."
  ],
  "27": [
    "E tha",
    "paria",
    "ata të cilët",
    "mohuan",
    "nga",
    "populli i tij",
    "\"Nuk",
    "ne të shohim ty",
    "përveç",
    "një njeri",
    "sikurse ne",
    "dhe nuk",
    "ne të shohim ty",
    "(të) pasojnë ty",
    "përveç",
    "ata të cilët",
    "ata (janë)",
    "më të poshtëruarit nga ne",
    "të lehtë",
    "nga mendja",
    "dhe nuk",
    "ne shohim",
    "për ju",
    "ndaj nesh",
    "ndonjë",
    "vlerë",
    "përkundrazi",
    "ne ju konsiderojmë ju",
    "gënjeshtarë\"."
  ],
  "28": [
    "Ai tha",
    "\"O populli im",
    "si mendoni",
    "nëse",
    "unë jam",
    "(i mbështetur) në",
    "një argument",
    "nga",
    "Zoti im",
    "dhe Ai më dha mua",
    "mëshirë",
    "nga",
    "ana e Tij",
    "e ato u mbuluan",
    "juve",
    "a Ne do t'jua detyrojmë juve ato",
    "ndërkohë që ju (jeni)",
    "ndaj tyre",
    "urrejtës."
  ],
  "29": [
    "Dhe o populli im",
    "nuk",
    "unë ju kërkoj ju",
    "për të",
    "pasuri.",
    "Nuk (është)",
    "shpërblimi im",
    "përveç",
    "tek",
    "Allahu",
    "dhe nuk",
    "unë (jam)",
    "largues",
    "i atyre të cilët",
    "besuan.",
    "Vërtet ata (janë)",
    "takues",
    "të Zotit të tyre",
    "mirëpo unë",
    "ju shoh ju",
    "një popull",
    "(që) nuk dini."
  ],
  "30": [
    "Dhe o populli im",
    "kush",
    "(më) ndihmon mua",
    "nga",
    "Allahu",
    "nëse",
    "unë i largoj ata?",
    "A nuk",
    "përkujtoheni?!"
  ],
  "31": [
    "Dhe nuk",
    "unë (ju) them",
    "juve",
    "tek unë (janë)",
    "depot",
    "e Allahut",
    "dhe as",
    "unë di",
    "fshehtësinë",
    "dhe as",
    "unë them",
    "vërtet unë (jam)",
    "një engjëll",
    "dhe as",
    "unë (u) them",
    "atyre të cilët",
    "i nënçmojnë",
    "sytë tuaj",
    "nuk",
    "do t'ju dhurojë atyre",
    "Allahu",
    "mirësi.",
    "Allahu",
    "(është) më i dijshëm",
    "për atë çfarë",
    "(është) në",
    "vetveten e tyre.",
    "Vërtet unë",
    "atëherë",
    "(do të isha) nga",
    "zullumqarët\"."
  ],
  "32": [
    "Ata thanë",
    "\"O Nuh",
    "tashmë",
    "ti debatove me ne",
    "dhe e zgjate",
    "debatin tonë",
    "prandaj na e sjell",
    "atë çfarë",
    "na e premton",
    "nëse",
    "ti je",
    "nga",
    "të sinqertët\"."
  ],
  "33": [
    "Ai tha",
    "\"Vetëm",
    "jua sjell ju",
    "atë",
    "Allahu",
    "nëse",
    "Ai dëshiron",
    "dhe nuk",
    "ju jeni",
    "pengues\"."
  ],
  "34": [
    "Dhe nuk",
    "ju bën dobi ju",
    "këshilla ime",
    "nëse",
    "unë dëshiroj",
    "që",
    "(t'ju) këshilloj",
    "ju",
    "nëse",
    "është",
    "Allahu",
    "dëshiron",
    "që",
    "(t'ju) lë në humbje.",
    "Ai (është)",
    "Zoti juaj",
    "dhe tek Ai",
    "ju ktheheni."
  ],
  "35": [
    "Vallë, a",
    "ata thonë",
    "\"Ai e trilloi atë?\"",
    "Thuaj",
    "\"Nëse",
    "unë e trillova atë",
    "atëherë mbi mua (bie)",
    "krimi im",
    "dhe unë (jam)",
    "i pafajshëm",
    "nga ajo çfarë",
    "ju bëni krime\"."
  ],
  "36": [
    "Dhe u shpall",
    "tek",
    "Nuhu",
    "se vërtet",
    "nuk",
    "do të besojë",
    "nga",
    "populli yt",
    "përveç",
    "ai që",
    "tashmë",
    "ka besuar",
    "prandaj mos",
    "u brengos",
    "për atë çfarë",
    "ata ishin",
    "duke bërë."
  ],
  "37": [
    "Dhe ndërtoje",
    "anijen",
    "nën vëzhgimin Tonë",
    "dhe shpalljen Tonë",
    "dhe mos",
    "më flit Mua",
    "për",
    "ata të cilët",
    "bënë zullum",
    "vërtet ata (janë)",
    "të përmbytur."
  ],
  "38": [
    "Dhe ai ndërtonte",
    "anijen",
    "dhe saherë që",
    "kalonte",
    "para tij",
    "një pari",
    "nga",
    "populli i tij",
    "talleshin",
    "me të.",
    "Ai thoshte",
    "\"Nëse",
    "ju talleni",
    "me ne",
    "edhe ne vërtet",
    "do të tallemi",
    "me ju",
    "ashtu siç",
    "ju talleni\"."
  ],
  "39": [
    "Atëherë do të",
    "dini",
    "kujt",
    "do t'i vijë atij",
    "një dënim",
    "(që) do ta poshtërojë atë",
    "dhe do ta përfshijë",
    "atë",
    "një dënim",
    "i përhershëm."
  ],
  "40": [
    "Derisa",
    "kur",
    "erdhi",
    "urdhëri ynë",
    "dhe gufoi",
    "furra",
    "Ne (i) thamë",
    "\"Ngarko",
    "në të",
    "nga",
    "secila (krijesë)",
    "çifte",
    "dy",
    "dhe familjen tënde",
    "përveç",
    "atë që",
    "ka paraprirë",
    "për të",
    "fjala",
    "dhe atë që",
    "ka besuar",
    "e nuk",
    "ka besuar",
    "me të",
    "përveç",
    "një pakicë\"."
  ],
  "41": [
    "Dhe ai tha",
    "\"Hipni",
    "në të",
    "me emrin",
    "e Allahut",
    "(me) lundrimin e saj",
    "dhe ndaljen e saj.",
    "Vërtet",
    "Zoti im",
    "(është) Falës i madh",
    "Mëshirues."
  ],
  "42": [
    "Dhe ajo",
    "lundronte",
    "me ta",
    "në",
    "dallgë",
    "sikur kodrat",
    "dhe thirri",
    "Nuhu",
    "birin e tij",
    "dhe ai ishte",
    "në",
    "një vend të veçuar",
    "\"Biri im",
    "hip",
    "me ne",
    "dhe mos",
    "ji",
    "me",
    "mohuesit!\""
  ],
  "43": [
    "Ai tha",
    "\"Do të ngjitem",
    "në",
    "një kodër",
    "do t'më mbrojë mua",
    "nga",
    "uji\".",
    "Ai (i) tha",
    "\"Nuk (ka)",
    "të mbrojtur",
    "sot",
    "nga",
    "dënimi",
    "i Allahut",
    "përveç",
    "atij që",
    "Ai e mëshiroi\".",
    "Dhe (i) ndau",
    "mes tyre",
    "dallga",
    "dhe ai ishte",
    "nga",
    "të përmbyturit."
  ],
  "44": [
    "Dhe u tha",
    "\"Oj tokë",
    "gëlltite",
    "ujin tënd",
    "dhe o qiell",
    "ndërpreje (shiun)\"",
    "dhe u tërhoq",
    "uji",
    "dhe përfundoi",
    "çështja",
    "dhe ajo u ndal",
    "në",
    "(kodrën) Xhudij",
    "dhe u tha",
    "\"Larg qoftë",
    "populli",
    "zullumqarë!\""
  ],
  "45": [
    "Dhe (e) thirri",
    "Nuhu",
    "Zotin e tij",
    "dhe tha",
    "\"Zoti im",
    "vërtet",
    "biri im",
    "(është) nga",
    "familja ime",
    "dhe vërtet",
    "premtimi Yt",
    "(është) i vërtetë",
    "dhe Ti (je)",
    "më i drejti",
    "i gjykuesve\"."
  ],
  "46": [
    "Ai (i) tha",
    "\"O Nuh",
    "vërtet ai",
    "nuk është",
    "nga",
    "familja jote",
    "vërtet ai (është)",
    "një punë",
    "jo",
    "e mirë",
    "prandaj mos",
    "më kërko Mua",
    "atë çfarë",
    "nuk është",
    "për ty",
    "me të",
    "dije.",
    "Vërtet Unë",
    "të këshilloj ty",
    "që",
    "të (mos) jesh",
    "nga",
    "të paditurit\"."
  ],
  "47": [
    "Ai tha",
    "\"Zoti im",
    "vërtet unë",
    "strehohem",
    "tek Ti",
    "që",
    "të kërkoj prej Teje",
    "atë çfarë",
    "nuk është",
    "për mua",
    "me të",
    "dije",
    "dhe nëse nuk",
    "Ti (më) fal",
    "mua",
    "dhe (më) mëshiron mua",
    "do të jem",
    "nga",
    "të dështuarit\"."
  ],
  "48": [
    "U tha",
    "\"O Nuh",
    "zbarko",
    "paqësisht",
    "nga Ne",
    "dhe me bekime",
    "për ty",
    "dhe për",
    "popuj",
    "nga ata të cilët",
    "(janë) me ty",
    "dhe popuj",
    "Ne do t'u japim përjetim",
    "pastaj",
    "do t'i prek ata",
    "nga Ne",
    "një dënim",
    "i dhembshëm."
  ],
  "49": [
    "Ato (janë)",
    "nga",
    "njoftimet",
    "e fshehtësisë",
    "Ne t'i shpallim ato",
    "ty.",
    "Nuk",
    "ti ishe",
    "i dije ato",
    "ti",
    "dhe as",
    "populli yt",
    "nga",
    "para",
    "kësaj.",
    "Prandaj duro",
    "vërtet",
    "përfundimi",
    "(është) për të druajturit."
  ],
  "50": [
    "Dhe (dërguam) tek",
    "Adi",
    "vëllain e tyre",
    "Hudin.",
    "Ai tha",
    "\"O populli im",
    "adhuroni",
    "Allahun",
    "nuk (ka)",
    "për ju",
    "asnjë",
    "zot",
    "pos Tij.",
    "Nuk",
    "ju (jeni)",
    "përveç",
    "të hutuar me trillime\"."
  ],
  "51": [
    "\"O populli im",
    "nuk",
    "unë kërkoj nga ju",
    "për të",
    "shpërblim.",
    "Nuk (është)",
    "shpërblimi im",
    "përveç",
    "nga",
    "Ai i Cili",
    "më krijoi mua",
    "vallë, a nuk",
    "perceptoni?!\""
  ],
  "52": [
    "\"Dhe o populli im",
    "kërkoni falje",
    "Zotit tuaj",
    "pastaj",
    "pendohuni",
    "tek Ai",
    "Ai do të dërgojë",
    "nga qielli",
    "për ju",
    "shi të bollshëm",
    "dhe do t'ju shtojë",
    "fuqi",
    "krahas",
    "fuqisë tuaj",
    "e të mos",
    "refuzoni",
    "si kriminelë\"."
  ],
  "53": [
    "Ata thanë",
    "\"O Hud",
    "nuk",
    "ti na solle ne",
    "ndonjë argument",
    "dhe nuk",
    "ne",
    "(jemi) lënës",
    "të zotave tanë",
    "për",
    "fjalën tënde",
    "dhe nuk",
    "ne (jemi)",
    "në ty",
    "besimtarë\"."
  ],
  "54": [
    "Nuk",
    "ne themi",
    "përveç",
    "të kanë goditur ty",
    "disa",
    "zotat tanë",
    "me të keqe.",
    "Ai tha",
    "\"Vërtet unë",
    "marr për dëshmitar",
    "Allahun",
    "dhe dëshmoni",
    "se vërtet unë",
    "(jam) i distancuar",
    "nga ajo çfarë",
    "ju shoqëroni (gjatë adhurimit)\"."
  ],
  "55": [
    "Nga",
    "përveç Tij",
    "prandaj bëni dredhi kundër meje",
    "të gjithë",
    "pastaj",
    "mos",
    "më afatizoni!"
  ],
  "56": [
    "Vërtet unë",
    "jam mbështetur",
    "tek",
    "Allahu",
    "Zoti im",
    "dhe Zoti juaj.",
    "Nuk (ka)",
    "asnjë",
    "gjallesë",
    "përveç",
    "Ai (është)",
    "mbërthyes",
    "i ballukes së saj.",
    "Vërtet",
    "Zoti im (është)",
    "në",
    "Udhën",
    "e Drejtë."
  ],
  "57": [
    "E nëse",
    "ata zmbrapsen",
    "tashmë",
    "unë ju kumtova ju",
    "atë çfarë",
    "unë u dërgova",
    "me të",
    "tek ju",
    "dhe do të zëvendësojë",
    "Zoti im",
    "një popull",
    "tjetër pos jush",
    "dhe nuk",
    "do t'i bëni dëm",
    "(në) asgjë.",
    "Vërtet",
    "Zoti im",
    "mbi",
    "çdo",
    "gjë",
    "(është) ruajtës."
  ],
  "58": [
    "E pasi që",
    "erdhi",
    "urdhri Ynë",
    "Ne e shpëtuam",
    "Hudin",
    "dhe ata të cilët",
    "besuan",
    "me të",
    "me mëshirë",
    "nga Ne",
    "dhe i shpëtuam ata",
    "nga",
    "një dënim",
    "i rëndë."
  ],
  "59": [
    "Dhe ai (ishte)",
    "Adi.",
    "Ata mohuan",
    "argumentet",
    "e Zotit të tyre",
    "dhe kundërshtuan",
    "të dërguarit e Tij",
    "dhe ndoqën",
    "urdhrin",
    "e secilit",
    "mizor",
    "kokëfortë."
  ],
  "60": [
    "Dhe ata u përcollën",
    "në",
    "këtë",
    "botë",
    "(me) mallkim",
    "dhe ditën",
    "e kijametit.",
    "Vini re!",
    "Vërtet",
    "Adi",
    "mohuan",
    "Zotin e tyre.",
    "Vini re!",
    "Qoftë shkatërruar",
    "Adi",
    "populli",
    "i Hudit!"
  ],
  "61": [
    "Dhe tek",
    "Themudi",
    "vëllain e tyre",
    "Salihun.",
    "Ai tha",
    "\"O populli im",
    "adhuroni",
    "Allahun",
    "nuk (ka)",
    "për ju",
    "asnjë",
    "zot",
    "pos Tij",
    "Ai",
    "ju krijoi ju",
    "nga",
    "toka",
    "dhe ju bëri banues",
    "në të",
    "prandaj kërkojini falje Atij",
    "pastaj",
    "pendohuni",
    "tek Ai.",
    "Vërtet",
    "Zoti im",
    "(është) i afërt",
    "përgjigjës (i lutjeve)\"."
  ],
  "62": [
    "Ata thanë",
    "\"O Salih",
    "sigurisht",
    "ti ishe",
    "në mesin tonë",
    "shpresëdhënës",
    "para",
    "kësaj.",
    "A na ndalon ne",
    "që të",
    "adhurojmë",
    "atë çfarë",
    "adhurojnë",
    "baballarët tanë?!",
    "Dhe vërtet ne (jemi)",
    "në",
    "një dilemë",
    "nga ajo çfarë",
    "ti na fton ne",
    "tek ajo",
    "të dyshimtë\"."
  ],
  "63": [
    "Ai tha",
    "\"O populli im",
    "më thuani",
    "nëse",
    "unë jam",
    "në",
    "argument",
    "nga",
    "Zoti im",
    "dhe Ai më ka dhënë mua",
    "prej Tij",
    "mëshirë",
    "pra, kush",
    "do të më ndihmojë mua",
    "nga",
    "Allahu",
    "nëse",
    "unë e kundërshtoj Atë?",
    "Dhe nuk",
    "ju më shtoni mua",
    "pos",
    "humbjes\"."
  ],
  "64": [
    "\"Dhe o populli im",
    "kjo (është)",
    "deveja",
    "e Allahut",
    "për ju (është)",
    "një argument.",
    "Pra lëreni atë",
    "të hajë",
    "në",
    "tokën",
    "e Allahut",
    "dhe mos",
    "e prekni atë",
    "me ndonjë të keqe",
    "e t'ju përfshijë ju",
    "një dënim",
    "i afërt\"."
  ],
  "65": [
    "Por ata e therën atë",
    "e ai (Salihu) tha",
    "\"Kënaquni",
    "në",
    "vendbanimin tuaj",
    "tri",
    "ditë.",
    "Ky (është)",
    "një premtim",
    "jo",
    "i rrejshëm\"."
  ],
  "66": [
    "E kur",
    "erdhi",
    "urdhri Ynë",
    "Ne e shpëtuam",
    "Salihun",
    "dhe ata të cilët",
    "besuan",
    "me të",
    "me mëshirën",
    "nga Ne",
    "dhe nga",
    "poshtërimi",
    "i asaj dite.",
    "Vërtet",
    "Zoti yt",
    "Ai (është)",
    "i Forti",
    "i Pamposhturi."
  ],
  "67": [
    "E i mbërtheu",
