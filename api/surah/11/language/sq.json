    "tek ai",
    "ndërkohë që nga",
    "përpara",
    "ata ishin",
    "duke bërë",
    "vepra të këqija.",
    "Ai tha",
    "\"O populli im",
    "këto (janë)",
    "bijat e mia",
    "ato (janë)",
    "më të pastra",
    "për ju",
    "prandaj druajuni",
    "Allahut",
    "dhe mos",
    "(më) turpëroni mua",
    "në mesin e",
    "mysafirëve të mi.",
    "A nuk (ka)",
    "prej jush",
    "ndonjë burrë",
    "i pjekur?\""
  ],
  "79": [
    "Ata thanë",
    "\"Sigurisht",
    "ti ke ditur",
    "nuk",
    "ne kemi",
    "në",
    "bijat tua",
    "asnjë",
    "të drejtë",
    "dhe vërtet ti",
    "me siguri e di",
    "çka",
    "ne dëshirojmë!\""
  ],
  "80": [
    "Ai tha",
    "\"Sikur",
    "të",
    "kisha",
    "karshi jush",
    "ndonjë forcë",
    "apo",
    "të strehohem",
    "tek",
    "ndonjë shtyllë",
    "e fortë!\""
  ],
  "81": [
    "Ata thanë",
    "\"O Lut",
    "vërtet ne (jemi)",
    "të dërguarit",
    "e Zotit tënd.",
    "Nuk do të",
    "ata arrijnë",
    "tek ti.",
    "E ti nisu",
    "me familjen tënde",
    "një një pjesë",
    "nga",
    "nata",
    "dhe mos",
    "të kthehet mbrapa",
    "prej jush",
    "askush",
    "përveç",
    "gruas tënde.",
    "Vërtet",
    "do ta godasë atë",
    "ajo çfarë",
    "do t'i godasë ata.",
    "Vërtet",
    "afati i tyre",
    "(është) mëngjesi",
    "a nuk është",
    "mëngjesi",
    "afër?!\""
  ],
  "82": [
    "E kur",
    "erdhi",
    "urdhri Ynë",
    "Ne e bëmë",
    "të lartën e tyre",
    "të poshtmen e tyre",
    "dhe reshëm",
    "mbi ta",
    "gurë",
    "nga",
    "balta e gurëzuar",
    "palë-palë."
  ],
  "83": [
    "të shënuar",
    "tek",
    "Zoti yt",
    "e nuk (është)",
    "ai (vend)",
    "nga",
    "zullumqarët",
    "larg."
  ],
  "84": [
    "Dhe tek",
    "Medjeni",
    "vëllain e tyre",
    "Shuajbin.",
    "Ai ka thënë",
    "\"O populli im",
    "adhuroni",
    "Allahun",
    "nuk (ka)",
    "për ju",
    "[asnjë]",
    "zot",
    "përveç Tij",
    "dhe mos",
    "pakësoni",
    "matjen",
    "dhe peshimin",
