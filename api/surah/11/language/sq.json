    "e Ne e përgëzuam",
    "me Is'hakun",
    "e nga",
    "pas",
    "Is'hakut",
    "Jakubin."
  ],
  "72": [
    "Ajo tha",
    "\"E mjera unë",
    "a do të lind",
    "ndërkohë që unë (jam)",
    "një plakë",
    "kurse ky",
    "burri im",
    "(është) një plak?!",
    "Vërtet",
    "kjo (është)",
    "një gjë",
    "e çuditshme\"."
  ],
  "73": [
    "Ata thanë",
    "\"A po çuditesh",
