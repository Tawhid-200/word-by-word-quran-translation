{
  "1": [
    "Elif Lam Mim."
  ],
  "2": [
    "Ato (janë)",
    "ajetet",
    "e Librit",
    "të urtë."
  ],
  "3": [
    "Udhëzim",
    "dhe mëshirë",
    "për bamirësit."
  ],
  "4": [
    "Ata të cilët",
    "e përsosin",
    "namazin",
    "dhe japin",
    "zeqatin",
    "dhe ata",
    "në jetën e fundit",
    "ata",
    "janë të bindur."
  ],
  "5": [
    "Të tillët (janë)",
    "mbi",
    "një udhëzim",
    "prej",
    "Zotit të tyre",
    "dhe të tillët",
    "ata (janë)",
    "fituesit."
  ],
  "6": [
    "Dhe prej",
    "njerëzve",
    "(ka) dikush që",
    "blen",
    "argëtim",
    "të bisedës",
    "që të devijojë",
    "nga",
    "rruga",
    "e Allahut",
    "pa",
    "dije",
    "dhe t'i marrë ato",
    "si tallje.",
    "Të tillët",
    "do të kenë",
    "dënim",
    "përbuzës."
  ],
  "7": [
    "E kur",
    "(i) recitohen",
    "atij",
    "ajetet Tona",
    "ai largohet",
    "me mendjemadhësi",
    "sikur",
    "nuk",
    "i dëgjoi ato",
    "sikur (të ishte)",
    "në",
    "veshët e tij",
    "shurdhim",
    "kështu që përgëzoje atë",
    "me një dënim",
    "të dhembshëm."
  ],
  "8": [
    "Vërtet",
    "ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "ata do të kenë",
    "xhenete",
    "të begatisë."
  ],
  "9": [
    "Të gjithmonshëm",
    "në to.",
    "Premtimi",
    "i Allahut",
    "i vërtetë",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ],
  "10": [
    "Ai krijoi",
    "qiejt",
    "pa",
    "shtylla",
    "ju i shihni ato",
    "dhe hodhi",
    "në",
    "tokë",
    "kodra",
    "që (të mos)",
    "(ju) tundin",
    "ju",
    "dhe Ai shpërndau",
    "në të",
    "nga",
    "çdo",
    "gjallesë.",
    "Dhe Ne zbritëm",
    "prej",
    "qiellit",
    "ujë",
    "edhe bëmë që të mbijë",
    "në të",
    "prej",
    "çdo",
    "lloji",
    "fisnik."
  ],
  "11": [
    "Ky (është)",
    "krijimi",
    "i Allahut",
    "pa më tregoni",
    "çfarë",
    "krijuan",
    "ata të cilët",
    "nga",
    "pos Tij?",
    "Ama",
    "zullumqarët",
    "(janë) në",
    "devijim",
    "të qartë."
  ],
  "12": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Llukmanit",
    "urtësi",
    "që",
    "\"Falënderoje",
    "Allahun!",
    "Dhe kushdo që",
    "falënderon",
    "atëherë vetëm",
    "e falënderon",
    "veten e tij",
    "e kushdo që",
    "mohon",
    "po vërtet",
    "Allahu (është)",
    "i Vetëmjaftueshëm",
    "i Lavdëruar."
  ],
  "13": [
    "Dhe kur",
    "(i) tha",
    "Llukmani",
    "birit të tij",
    "ndërkohë që ai",
    "e këshillonte atë",
    "\"O vogëlushi im!",
    "Mos",
    "shoqëro zota",
    "krahas Allahut!",
    "Vërtet",
    "idhujtaria",
    "(është) njëmend një zullum",
    "i madh\"."
  ],
  "14": [
    "Dhe Ne e porositëm",
    "njeriun",
    "për prindërit e tij.",
    "E barti atë",
    "nëna e tij",
    "vështirësi",
    "pas",
    "vështirësie",
    "e ndërprerja e tij nga gjiri",
    "(është) në",
    "dy vjet",
    "që",
    "\"(Më) falëndero",
    "Mua",
    "dhe dy prindërit e tu",
    "tek Unë",
    "(është) kthimi!\""
  ],
  "15": [
    "E nëse",
    "ata dy të nxisin ty",
    "që",
    "të",
    "(më) shoqërosh zota",
    "Mua",
    "çfarë",
    "nuk",
    "ke ti",
    "për të",
    "asnjë njohuri",
    "pa mos",
    "iu bind atyre dyve!",
    "Dhe shoqëroji ata dy",
    "në",
    "këtë botë",
    "me të mira",
    "dhe ndiq",
    "rrugën",
    "e atij që",
    "u kthye",
    "tek Unë!",
    "Pastaj",
    "tek Unë",
    "(është) kthimi juaj",
    "e Unë do t'ju njoftoj ju",
    "për atë çfarë",
    "ju ishit",
    "duke punuar."
  ],
  "16": [
    "\"O vogëlushi im!",
    "Vërtet ajo",
    "nëse",
    "është",
    "(sa) pesha",
    "e një kokrre",
    "nga",
    "sinapi",
    "e ajo të jetë",
    "brenda",
    "një shkëmbi",
    "apo",
    "në",
    "qiej",
    "apo",
    "në",
    "tokë",
    "do ta sjell",
    "atë",
    "Allahu.",
    "Vërtet",
    "Allahu",
    "(është) i Butë",
    "i Gjithëinformuar."
  ],
  "17": [
    "O vogëlushi im!",
    "Përsose",
    "namazin",
    "dhe urdhëro",
    "të mirën",
    "dhe ndalo",
    "nga",
    "e keqja",
    "dhe duro",
    "në",
    "atë çfarë",
    "të godet ty!",
    "Vërtet",
    "ajo (është)",
    "nga",
    "vendosmëria",
    "e çështjeve."
  ],
  "18": [
    "Dhe mos",
    "(ua) shfaq",
    "faqen tënde",
    "njerëzve",
    "dhe mos",
    "ec",
    "në",
    "tokë",
    "me fodullëk!",
    "Vërtet",
    "Allahu",
    "nuk",
    "e do",
    "secilin",
    "kryelartë",
    "mburravec."
  ],
  "19": [
    "Dhe ji i matur",
    "gjatë",
    "ecjes tënde",
    "dhe ule",
    "prej",
    "zërit tënd!",
    "Vërtet",
    "më i urrejturi",
    "i zërave",
    "(është) njëmend zëri",
    "i gomarëve."
  ],
  "20": [
    "A nuk",
    "ju shihni",
    "se",
    "Allahu",
    "nënshtroi",
    "për ju",
    "çfarë (është)",
    "në",
    "qiej",
    "dhe çfarë (është)",
    "në",
    "tokë",
    "dhe derdhi",
    "mbi ju",
    "begatitë e Tij",
    "të jashtme",
    "e të brendshme?!",
    "E prej",
    "njerëzve",
    "(është edhe) ai që",
    "debaton",
    "rreth",
    "Allahut",
    "pa",
    "dije",
    "e as",
    "udhëzim",
    "dhe as",
    "libër",
    "ndriçues."
  ],
  "21": [
    "E kur",
    "(u) thuhet",
    "atyre",
    "\"Ndiqni",
    "atë çfarë",
    "zbriti",
    "Allahu!\"",
    "Ata thonë",
    "\"Përkundrazi",
    "ne do të ndjekim",
    "atë çfarë",
    "i gjetëm",
    "në të",
    "baballarët tanë!\"",
    "Po edhe sikur",
    "ishte",
    "shejtani",
    "duke i ftuar ata",
    "tek",
    "dënimi",
    "i zjarrit flakërues?!"
  ],
  "22": [
    "E kushdo që",
    "e dorëzon",
    "fytyrën e tij",
    "tek",
    "Allahu",
    "duke qenë ai",
    "bamirës",
    "sigurisht",
    "ai është kapur fuqishëm",
    "për lidhjen",
    "më të besueshme.",
    "E tek",
    "Allahu",
    "(është) përfundimi",
    "i çështjeve."
  ],
  "23": [
    "E kushdo që",
    "mohon",
    "pa mos",
    "të të pikëllojë ty",
    "mohimi i tij.",
    "Tek Ne",
    "(është) kthimi i tyre",
    "e Ne do t'i njoftojmë ata",
    "për atë çfarë",
    "ata punuan.",
    "Vërtet",
    "Allahu",
    "(është) i Gjithëdijshëm",
    "për atë që posedojnë",
    "gjokset."
  ],
  "24": [
    "Ne u japim një komoditet atyre",
    "përkohësisht",
    "pastaj",
    "i detyrojmë ata",
    "drejt",
    "një dënimi",
    "të vrazhdë."
  ],
  "25": [
    "E nëse",
    "ti i pyet ata",
    "\"Kush",
    "krijoi",
    "qiejt",
    "dhe tokën?\"",
    "ata do të thonë sigurisht",
    "\"Allahu!\"",
    "Thuaj",
    "\"Falënderimi (i përket)",
    "Allahut!\"",
    "ama",
    "shumica e tyre",
    "nuk",
    "dinë."
  ],
  "26": [
    "Allahut (i përket)",
    "ajo çfarë (është)",
    "në",
    "qiej",
    "dhe tokë.",
    "Vërtet",
    "Allahu",
    "Ai (është)",
    "i Vetëmjaftueshmi",
    "i Lavdishmi."
  ],
  "27": [
    "E sikur",
    "vetëm (të ishin)",
    "në",
    "tokë",
    "prej",
    "drunjve",
    "lapsa",
    "edhe deti",
    "ta shtrinin atë",
    "nga",
    "pas tij",
    "shtatë",
    "dete",
    "nuk",
    "do të soseshin",
    "fjalët",
    "e Allahut.",
    "Vërtet",
    "Allahu",
    "(është) i Pamposhtur",
    "i Urtë."
  ],
  "28": [
    "Nuk (është)",
    "krijimi juaj",
    "e as",
    "ringjallja juaj",
    "veçse",
    "si i personit",
    "një.",
    "Vërtet",
    "Allahu",
    "(është) Gjithëdëgjues",
    "Gjithëshikues."
  ],
  "29": [
    "A nuk",
    "ti sheh",
    "se",
    "Allahu",
    "e fut",
    "natën",
    "brenda",
    "ditës",
    "dhe e fut",
    "ditën",
    "brenda",
    "natës",
    "dhe Ai nënshtroi",
    "diellin",
    "dhe hënën.",
    "Secili",
    "lëviz",
    "drejt",
    "një afati",
    "të caktuar",
    "dhe se",
    "Allahu",
    "për atë çfarë",
    "ju punoni",
    "(është) i Gjithëinformuar."
  ],
  "30": [
    "Ashtu",
    "se",
    "Allahu",
    "Ai (është)",
    "e Vërteta",
    "dhe se",
    "ajo çfarë",
    "ata luten",
    "nga",
    "pos Tij",
    "(është) kotësia",
    "dhe se",
    "Allahu",
    "Ai (është)",
    "i Lartësuari",
    "Madhështori."
  ],
  "31": [
    "A nuk",
    "ti sheh",
    "se",
    "anijet",
    "lundrojnë",
    "në",
    "det",
    "me begatinë",
    "e Allahut",
    "që t'ju shfaq ju",
    "prej",
    "provave të Tij.",
    "Vërtet",
    "në",
    "të",
    "(ka) njëmend prova",
    "për secilin",
    "durimtar",
    "shumë mirënjohës."
  ],
  "32": [
    "E kur",
    "t'i mbulojnë ata",
    "dallgët",
    "si retë",
    "ata i luten",
    "Allahut",
    "duke ia shprehur sinqerisht",
    "Atij",
    "fenë",
    "e pasi që",
    "Ai i shpëton ata",
    "drejt",
    "tokës",
    "atëherë prej tyre (ka)",
    "(ndonjë) i matur",
    "ndërsa nuk",
    "mohon",
    "provat Tona",
    "përveçse",
    "secili",
    "tradhtar",
    "bukëshkalë."
  ],
  "33": [
    "O ju",
    "njerëz!",
    "Druajuni",
    "Zotit tuaj",
    "dhe frikësojuni",
    "një dite",
    "nuk",
    "do të kompensojë",
    "asnjë prind",
    "për",
    "fëmijën e tij",
    "dhe as",
    "asnjë i lindur",
    "ai (do të jetë)",
    "kompensues",
    "për",
    "prindin e tij",
    "diçka.",
    "Vërtet",
    "premtimi",
    "i Allahut",
    "(është) i vërtetë",
    "prandaj mos",
    "t'ju mashtrojë ju",
    "jeta",
    "e kësaj bote",
    "dhe mos",
    "t'ju mashtrojë ju",
    "ndaj Allahut",
    "djalli."
  ],
  "34": [
    "Vërtet",
    "Allahu",
    "tek Ai (është)",
    "dija",
    "e çastit të Kijametit",
    "dhe Ai zbret",
    "shiun",
    "dhe Ai di",
    "çfarë (është)",
    "në",
    "mitra",
    "dhe nuk",
    "e di",
    "asnjë person",
    "çfarë",
    "do të fitojë",
    "nesër",
    "dhe nuk",
    "e di",
    "asnjë person",
    "në cilën",
    "tokë",
    "do të vdesë.",
    "Vërtet",
    "Allahu",
    "(është) i Gjithëdijshëm",
    "i Gjithëinformuar."
  ]
}