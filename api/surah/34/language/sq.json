    "për Allahun",
    "dy nga dy",
    "dhe tek e tek",
    "pastaj",
    "të mendoni.",
    "Nuk (ka)",
    "shoku juaj",
    "asnjë",
    "çmenduri.",
    "Nuk (është)",
    "ai",
    "përveçse",
    "një paralajmërues",
    "për ju",
    "në mes të",
    "pranë",
    "një dënimi",
    "të ashpër\"."
  ],
  "47": [
    "Thuaj",
    "\"Nuk",
    "unë ju kërkoj ju",
    "asnjë",
    "shpërblim",
    "se ai (është)",
    "për ju.",
    "Nuk (është)",
    "shpërblimi im",
    "veçse",
    "tek",
    "Allahu",
    "dhe Ai (është)",
    "mbi",
    "çdo",
    "gjë",
    "Dëshmues\"."
  ],
  "48": [
    "Thuaj",
    "\"Vërtet",
    "Zoti im",
    "e sjell",
    "të vërtetën",
    "Njohësi",
    "i së padukshmes\"."
  ],
  "49": [
    "Thuaj",
    "\"Erdhi",
    "e vërteta",
    "dhe nuk",
    "zë fill",
    "e kota",
    "dhe nuk",
    "përsëritet\"."
  ],
  "50": [
    "Thuaj",
    "\"Nëse",
    "unë devijoj",
    "atëherë vetëm",
    "unë devijoj",
    "për",
    "veten time",
    "e nëse",
    "unë udhëzohem",
    "atëherë me atë çfarë",
    "më shpall",
    "mua",
    "Zoti im.",
    "Vërtet Ai (është)",
    "Gjithëdëgjues",
    "i Afërt\"."
  ],
  "51": [
    "E sikur",
    "të shihje",
    "kur",
    "ata tmerrohen",
    "e nuk (ka)",
    "ikje",
    "dhe ata mbërthehen",
    "prej",
    "një vendi",
    "të afërt."
  ],
  "52": [
    "Dhe ata thonë",
    "\"Ne besuam",
    "në të\".",
    "E prej nga",
    "atyre",
    "arritja",
    "prej",
    "një vendi",
    "të largët?"
  ],
  "53": [
    "E sigurisht",
    "ata e mohuan",
    "atë",
    "nga",
    "përpara",
    "dhe hamendësojnë",
    "të padukshmen",
    "prej",
    "një vendi",
    "të largët."
  ],
  "54": [
    "Dhe vendoset një pengesë",
    "mes tyre",
    "dhe mes",
    "asaj çfarë",
    "ata pëlqejnë",
    "sikurse",
    "është vepruar",
    "me sojet e tyre",
    "nga",
    "përpara.",
    "Vërtet",
    "ata ishin",
    "në",
    "një dyshim",
    "të trazuar."
  ]
