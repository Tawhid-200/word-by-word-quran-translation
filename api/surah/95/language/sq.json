{
  "1": [
    "Për fikun",
    "dhe ullirin"
  ],
  "2": [
    "Dhe kodrën",
    "Sina"
  ],
  "3": [
    "Dhe këtë",
    "vend",
    "të sigurt"
  ],
  "4": [
    "Vërtet",
    "Ne e krijuam",
    "njeriun",
    "në",
    "më të bukurën",
    "strukturë"
  ],
  "5": [
    "Pastaj",
    "e kthyem atë",
    "(në) më të ultin",
    "e të ultëve"
  ],
  "6": [
    "Përveç",
    "atyre të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "për ta",
    "(është) një shpërblim",
    "i pa-",
    "shkëputur"
  ],
  "7": [
    "Pra, çka",
    "(të shtyn ty) të përgënjeshtrosh",
    "pas (kësaj)",
    "(në) ditën e Gjykimit?"
  ],
  "8": [
    "A nuk është",
    "Allahu",
    "më i drejti",
    "i Gjykatësve?"
  ]
}