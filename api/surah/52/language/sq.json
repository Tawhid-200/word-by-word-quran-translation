{
  "1": [
    "Pasha Turin!"
  ],
  "2": [
    "Dhe Librin",
    "e shkruar."
  ],
  "3": [
    "Në",
    "pergamenë",
    "të shpalosur."
  ],
  "4": [
    "Dhe Shtëpinë",
    "e vizituar."
  ],
  "5": [
    "Dhe kulmin (qiellin)",
    "e ngritur."
  ],
  "6": [
    "Dhe detin",
    "e mbushur."
  ],
  "7": [
    "Vërtet",
    "dënimi",
    "i Zotit tënd",
    "(është) sigurisht i ndodhur."
  ],
  "8": [
    "Nuk (ka)",
    "për të",
    "asnjë",
    "pengues."
  ],
  "9": [
    "Ditën kur",
    "tronditet",
    "qielli",
    "(me) tronditje."
  ],
  "10": [
    "Dhe lëvizin",
    "kodrat",
    "(me) lëvizje."
  ],
  "11": [
    "Atëherë mjerë",
    "atë ditë",
    "për përgënjeshtruesit."
  ],
  "12": [
    "Të cilët",
    "ata (janë)",
    "në",
    "zhytje",
    "luajnë."
  ],
  "13": [
    "Ditën kur",
    "ata shtyhen",
    "drejt",
    "zjarrit",
    "të xhehenemit",
    "(me) shtyrje."
  ],
  "14": [
    "Ky (është)",
    "zjarri",
    "i cili",
    "ju ishit",
    "në të",
    "përgënjeshtronit."
  ],
  "15": [
    "Vallë a magji (është)",
    "kjo",
    "apo",
    "ju",
    "nuk",
    "shihni?"
  ],
  "16": [
    "Ndizuni në të!",
    "E duroni",
    "apo",
    "mos",
    "duroni",
    "njësoj (është)",
    "për ju.",
    "Vetëm",
    "ju (do të) shpërbleheni",
    "(me) atë çfarë",
    "ju ishit",
    "punonit."
  ],
  "17": [
    "Vërtet",
    "të druajturit",
    "(do të jenë) në",
    "xhenete",
    "dhe begati."
  ],
  "18": [
    "Të kënaqur",
    "me atë çfarë",
    "u dhuroi atyre",
    "Zoti i tyre",
    "dhe i ruajti ata",
    "Zoti i tyre",
    "(nga) dënimi",
    "i zjarrit flakërues."
  ],
  "19": [
    "\"Hani",
    "dhe pini",
    "ju bëftë mirë",
    "për atë çfarë",
    "ju ishit",
    "punonit\"."
  ],
  "20": [
    "Të mbështetur",
    "mbi",
    "divane",
    "të renditura",
    "dhe Ne do t'i martojmë ata",
    "me hyri",
    "symëdha."
  ],
  "21": [
    "E ata të cilët",
    "besuan",
    "dhe i ndoqën ata",
    "pasardhësit e tyre",
    "me besim",
    "Ne do t'i bashkojmë",
    "ata",
    "me pasardhësit e tyre",
    "dhe nuk",
    "Ne do t'u pakësojmë atyre",
    "nga",
    "puna e tyre",
    "asnjë",
    "gjë.",
    "Secili",
    "njeri",
    "me atë çfarë",
    "fitoi",
    "(është) peng."
  ],
  "22": [
    "Dhe Ne do t'ua shtojmë atyre",
    "pemë",
    "dhe mish",
    "prej asaj çfarë",
    "ata pëlqejnë."
  ],
  "23": [
    "Ata do t'ia zgjasin njëri-tjetrit",
    "në të",
    "gota të mbushura",
    "nuk (ka)",
    "pallavra",
    "në të",
    "dhe as",
    "gjynah."
  ],
  "24": [
    "Dhe (do të) sillen",
    "rreth tyre",
    "djelmosha",
    "për ta",
    "sikur ata (të ishin)",
    "margaritar",
    "i fshehur."
  ],
  "25": [
    "Dhe afrohen",
    "disa prej tyre",
    "pranë",
    "disa (të tjerëve)",
    "e pyesin njëri-tjetrin."
  ],
  "26": [
    "Ata do të thonë",
    "\"Vërtet ne",
    "ishim",
    "përpara",
    "në",
    "familjen tonë",
    "të frikësuar."
  ],
  "27": [
    "E na favorizoi",
    "Allahu",
    "ne",
    "dhe Ai na ruajti",
    "(nga) dënimi",
    "i erës përcëlluese."
  ],
  "28": [
    "Vërtet ne",
    "ishim",
    "nga",
    "përpara",
    "e lusnim Atë",
    "vërtet Ai (është)",
    "(pikërisht) Ai",
    "Bamirësi",
    "Mëshiruesi\"."
  ],
  "29": [
    "Prandaj përkujto",
    "se nuk",
    "ti (je)",
    "me begatinë",
    "e Zotit tënd",
    "falltar",
    "dhe as",
    "i çmendur."
  ],
  "30": [
    "Vallë",
    "ata thonë",
    "\"Një poet",
    "ne presim",
    "për të",
    "fatkeqësinë",
    "e vdekjes\"."
  ],
  "31": [
    "Thuaj",
    "\"Pritni",
    "se vërtet unë (jam)",
    "me ju",
    "nga",
    "pritësit!\""
  ],
  "32": [
    "Vallë",
    "i urdhërojnë ata",
    "ëndrrat e tyre",
    "për këtë",
    "apo",
    "ata (janë)",
    "një popull",
    "që kalojnë çdo kufi."
  ],
  "33": [
    "Vallë",
    "ata thonë",
    "\"Ai e trilloi atë!\"",
    "Jo por",
    "nuk",
    "ata besojnë."
  ],
  "34": [
    "Prandaj le të sjellin",
    "një fjalim",
    "të ngjashëm me të",
    "nëse",
    "ata janë",
    "të sinqertë."
  ],
  "35": [
    "Vallë",
    "ata u krijuan",
    "nga",
    "asnjë",
    "gjë",
    "apo",
    "ata (janë)",
    "krijuesit?"
  ],
  "36": [
    "Vallë",
    "ata krijuan",
    "qiejt",
    "dhe tokën?",
    "Jo por",
    "nuk",
    "janë të bindur."
  ],
  "37": [
    "Vallë",
    "tek ta (janë)",
    "thesaret",
    "e Zotit tënd",
    "apo",
    "ata (janë)",
    "mbizotërues?"
  ],
  "38": [
    "Vallë",
    "ata kanë",
    "shkallë",
    "dëgjojnë",
    "në to?",
    "Pra, le të sjellë",
    "dëgjuesi i tyre",
    "një fakt",
    "të qartë!"
  ],
  "39": [
    "Vallë",
    "për Të (janë)",
    "vajzat",
    "kurse për ju",
    "djemtë?"
  ],
  "40": [
    "Vallë",
    "ti u kërkon atyre",
    "shpërblim",
    "e ata",
    "nga",
    "tagri",
    "(janë) të ngarkuar?"
  ],
  "41": [
    "Vallë",
    "tek ta (është)",
    "e fshehta",
    "e ata",
    "shkruajnë?"
  ],
  "42": [
    "Vallë",
    "ata dëshirojnë",
    "kurth?",
    "Po ata të cilët",
    "mohuan",
    "ata (janë)",
    "të rënë në kurth."
  ],
  "43": [
    "Vallë",
    "ata kanë",
    "zot",
    "pos",
    "Allahut?!",
    "Lartësuar qoftë",
    "Allahu",
    "nga ajo çfarë",
    "ata i shoqërojnë (në adhurim)."
  ],
  "44": [
    "Edhe po të",
    "ata shohin",
    "një copë",
    "nga",
    "qielli",
    "të rrëzuar",
    "ata do të thoshin",
    "\"Re",
    "të dendura\"."
  ],
  "45": [
    "Po lëri ata",
    "derisa",
    "ata të përballen",
    "(me) ditën e tyre",
    "në të cilën",
    "në të",
    "ata (do të) zalisen."
  ],
  "46": [
    "Ditën kur",
    "nuk",
    "u bën dobi",
    "atyre",
    "kurthi i tyre",
    "asgjë",
    "dhe nuk",
    "ata",
    "(do të) ndihmohen."
  ],
  "47": [
    "E vërtet",
    "për ata të cilët",
    "bënë zullum",
    "(do të ketë) dënim",
    "para",
    "atij (Kijametit)",
    "mirëpo",
    "shumica e tyre",
    "nuk",
    "dinë."
  ],
  "48": [
    "Dhe duro",
    "për gjykimin",
    "e Zotit tënd",
    "se vërtet ti (je)",
    "nën vëzhgimin Tonë",
    "dhe lartësoje",
    "me falënderim",
    "Zotin tënd",
    "kur",
    "të ngrihesh!"
  ],
  "49": [
    "Edhe gjatë",
    "natës",
    "lartësoje Atë",
    "dhe (pas) largimit",
    "të yjeve!"
  ]
}