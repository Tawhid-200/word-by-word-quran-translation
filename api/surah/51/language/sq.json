{
  "1": [
    "Pasha erërat",
    "me shpërndarje (të pluhurit)."
  ],
  "2": [
    "Dhe në retë",
    "(me) bartje të rëndë."
  ],
  "3": [
    "Dhe në anijet lundruese",
    "lehtë."
  ],
  "4": [
    "Dhe në engjëjt që shpërndajnë",
    "urdhrin."
  ],
  "5": [
    "Vërtet ajo çfarë",
    "ju premtohet",
    "(është) e sigurt."
  ],
  "6": [
    "Dhe vërtet",
    "kompensimi",
    "gjithsesi (është) i ndodhur."
  ],
  "7": [
    "Pasha qiellin",
    "që ka",
    "(shumë) gërshetime."
  ],
  "8": [
    "Vërtet ju (jeni)",
    "në",
    "një frazë",
    "kontradiktore."
  ],
  "9": [
    "zmbrapset",
    "prej tij (Kur'anit)",
    "ai që",
    "është zmbrapsur."
  ],
  "10": [
    "Mallkuar qofshin",
    "gënjeshtarët!"
  ],
  "11": [
    "Të cilët",
    "ata (janë)",
    "në",
    "zhytje",
    "të shkujdesur."
  ],
  "12": [
    "Ata pyesin",
    "\"Kur (është)",
    "dita",
    "e kompensimit?\""
  ],
  "13": [
    "Ditën kur",
    "ata",
    "mbi",
    "zjarr",
    "(do të) digjen."
  ],
  "14": [
    "\"Shijoni",
    "djegien tuaj!\"",
    "Kjo (është)",
    "ajo e cila",
    "ju ishit",
    "atë",
    "ju e shpejtonit."
  ],
  "15": [
    "Vërtet",
    "të druajturit",
    "(do të jenë) në",
    "xhenete",
    "dhe burime."
  ],
  "16": [
    "Duke marrë",
    "atë çfarë",
    "u dhuroi atyre",
    "Zoti i tyre.",
    "Vërtet ata",
    "ishin",
    "para",
    "asaj",
    "punëmirë."
  ],
  "17": [
    "Ata ishin",
    "pak",
    "nga",
    "nata",
    "që",
    "flenin."
  ],
  "18": [
    "Dhe në kohët e fundit të natës",
    "ata",
    "kërkonin falje."
  ],
  "19": [
    "Dhe në",
    "pasuritë e tyre",
    "(ka) të drejtë",
    "për lypësin",
    "dhe të privuarin."
  ],
  "20": [
    "Dhe në",
    "tokë",
    "(ka) dëshmi",
    "për të bindurit."
  ],
  "21": [
    "Dhe në",
    "veten tuaj",
    "vallë a nuk",
    "shihni?!"
  ],
  "22": [
    "Dhe në",
    "qiell",
    "(është) furnizimi juaj",
    "dhe ajo çfarë",
    "ju premtohet."
  ],
  "23": [
    "Pra pasha Zotin",
    "e qiellit",
    "dhe tokës",
    "vërtet ajo (është)",
    "e vërtetë",
    "sikurse",
    "ajo çfarë",
    "që ju",
    "flisni."
  ],
  "24": [
    "A",
    "të erdhi ty",
    "rrëfimi",
    "i mysafirëve",
    "të Ibrahimit",
    "të nderuar?"
  ],
  "25": [
    "Kur",
    "ata hynë",
    "tek ai",
    "e i thanë",
    "\"Paqe!\"",
    "Ai tha",
    "\"Paqe",
    "(o) njerëz",
    "të panjohur!\""
  ],
  "26": [
    "Dhe iku pa u vënë re",
    "tek",
    "familja e tij",
    "dhe solli",
    "një viç",
    "të majmë."
  ],
  "27": [
    "Dhe ua ofroi atë",
    "atyre",
    "tha",
    "\"A nuk",
    "po hani?\""
  ],
  "28": [
    "Dhe ai ndjeu",
    "prej tyre",
    "një frikë",
    "ata thanë",
    "\"Mos",
    "ki frikë!\"",
    "Dhe e përgëzuan atë",
    "me një djalosh",
    "të ditur."
  ],
  "29": [
    "Dhe u afrua",
    "gruaja e tij",
    "me",
    "një bërtimë",
    "dhe e goditi",
    "fytyrën e saj",
    "dhe tha",
    "\"(Unë jam) një plakë",
    "shterpë\"."
  ],
  "30": [
    "Ata thanë",
    "\"Kështu",
    "tha",
    "Zoti yt",
    "vërtet Ai",
    "Ai (është)",
    "i Urti",
    "i Gjithëdijshmi."
  ],
  "31": [
    "Ai tha",
    "\"Po çka (është)",
    "puna juaj",
    "o ju",
    "të dërguar?\""
  ],
  "32": [
    "Ata thanë",
    "\"Vërtet ne",
    "u dërguam",
    "tek",
    "një popull",
    "kriminelë\"."
  ],
  "33": [
    "Që të dërgojmë",
    "mbi ta",
    "gurë",
    "nga",
    "një baltë e pjekur."
  ],
  "34": [
    "Të regjistruar",
    "tek",
    "Zoti yt",
    "për tepruesit."
  ],
  "35": [
    "Atëherë i nxorëm",
    "atë që",
    "ishte",
    "në të",
    "prej",
    "besimtarëve."
  ],
  "36": [
    "Po nuk",
    "gjetëm",
    "në të",
    "pos",
    "një shtëpi",
    "prej",
    "muslimanëve."
  ],
  "37": [
    "Dhe Ne lamë",
    "në të",
    "një shenjë",
    "për ata të cilët",
    "(i) frikësohen",
    "dënimit",
    "të dhembshëm."
  ],
  "38": [
    "Edhe në (rrëfimin)",
    "e Musait",
    "kur",
    "Ne e dërguam atë",
    "te",
    "faraoni",
    "me një fakt",
    "të qartë."
  ],
  "39": [
    "E ai u zmbraps",
    "me elitën e tij",
    "dhe tha",
    "\"Magjistar",
    "apo",
    "i marrë!\""
  ],
  "40": [
    "E Ne e mbërthyem atë",
    "dhe ushtritë e tij",
    "e i hodhëm",
    "në",
    "det",
    "dhe ai (ishte)",
    "i qortuar."
  ],
  "41": [
    "Edhe në (rrëfimin)",
    "e Adit",
    "kur",
    "Ne dërguam",
    "mbi ta",
    "erën",
    "shkretuese."
  ],
  "42": [
    "Nuk",
    "ajo lë",
    "asnjë",
    "gjë",
    "(i) vjen",
    "asaj",
    "përveçse",
    "e bën atë",
    "si kalbësira."
  ],
  "43": [
    "Edhe në (rrëfimin)",
    "e Themudit",
    "kur",
    "(u) tha",
    "atyre",
    "\"Kënaquni",
    "deri (në)",
    "një kohë\"."
  ],
  "44": [
    "E ata u rebeluan",
    "nga",
    "urdhri",
    "i Zotit të tyre",
    "prandaj i mbërtheu ata",
    "britma",
    "ndërkohë që ata",
    "shikonin."
  ],
  "45": [
    "Dhe nuk",
    "ata mundën (të bënin)",
    "asnjë",
    "ngritje",
    "dhe nuk",
    "ishin",
    "fituesit."
  ],
  "46": [
    "Dhe popullin",
    "e Nuhut",
    "nga",
    "përpara.",
    "Vërtet ata",
    "ishin",
    "një popull",
    "të prishur."
  ],
  "47": [
    "Kurse qiellin",
    "Ne e ndërtuam atë",
    "me fuqi",
    "dhe vërtet Ne (jemi)",
    "zgjerues."
  ],
  "48": [
    "Dhe tokën",
    "Ne e shtruam atë",
    "e sa të mirë (që janë)",
    "Shtruesit!"
  ],
  "49": [
    "Dhe nga",
    "çdo",
    "gjë",
    "Ne krijuam",
    "dy lloje",
    "ashtu që ju",
    "të përkujtoni."
  ],
  "50": [
    "Prandaj ikni",
    "tek",
    "Allahu!",
    "Vërtet unë (jam)",
    "për ju",
    "prej Tij",
    "një paralajmërues",
    "i qartë."
  ],
  "51": [
    "Dhe mos",
    "vendosni",
    "me",
    "Allahun",
    "zot",
    "tjetër!",
    "Vërtet unë (jam)",
    "për ju",
    "prej Tij",
    "një paralajmërues",
    "i qartë."
  ],
  "52": [
    "Kështu",
    "nuk",
    "(u) vinte",
    "atyre të cilët (ishin)",
    "nga",
    "përpara tyre",
    "asnjë",
    "i dërguar",
    "përveçse",
    "thoshin",
    "\"Magjistar",
    "apo",
    "i marrë!\""
  ],
  "53": [
    "A e porositën ata njëri-tjetrin",
    "me të?",
    "Jo por",
    "ata (janë)",
    "një popull",
    "që kalojnë çdo kufi."
  ],
  "54": [
    "Prandaj largohu",
    "nga ta",
    "se nuk",
    "ti (je)",
    "i qortuar."
  ],
  "55": [
    "Dhe përkujto",
    "se vërtet",
    "përkujtimi",
    "(u) bën dobi",
    "besimtarëve."
  ],
  "56": [
    "Dhe nuk",
    "Unë krijova",
    "xhinët",
    "dhe njerëzit",
    "përveçse",
    "që t'Më adhurojnë Mua."
  ],
  "57": [
    "Nuk",
    "unë dëshiroj",
    "prej tyre",
    "asnjë",
    "furnizim",
    "dhe nuk",
    "unë dëshiroj",
    "që",
    "ata t'më ushqejnë mua."
  ],
  "58": [
    "Vërtet",
    "Allahu",
    "Ai (është)",
    "Furnizuesi",
    "Poseduesi",
    "i fuqisë",
    "i Forti."
  ],
  "59": [
    "E vërtet",
    "për ata të cilët",
    "bënë zullum",
    "(do të ketë) hise (nga dënimi)",
    "sikurse",
    "hiseja",
    "e shokëve të tyre",
    "prandaj të mos",
    "nxitojnë."
  ],
  "60": [
    "E mjerë",
    "për ata të cilët",
    "mohuan",
    "nga",
    "dita e tyre",
    "e cila",
    "ju premtohet!"
  ]
}