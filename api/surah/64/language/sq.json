{
  "1": [
    "E lartëson",
    "Allahun",
    "ajo çfarë",
    "(është) në",
    "qiej",
    "dhe ajo çfarë",
    "(është) në",
    "tokë.",
    "Për Të (është)",
    "sundimi",
    "dhe për Të (është)",
    "falënderimi",
    "dhe Ai",
    "për",
    "çdo",
    "gjë",
    "(është) i Plotfuqishëm."
  ],
  "2": [
    "Ai (është)",
    "i Cili",
    "ju krijoi ju",
    "e nga ju (ka)",
    "mohues",
    "dhe nga ju (ka)",
    "besimtar",
    "dhe Allahu",
    "për atë çfarë",
    "ju punoni",
    "(është) Gjithëshikues."
  ],
  "3": [
    "Ai krijoi",
    "qiejt",
    "dhe tokën",
    "me hak",
    "dhe ju formësoi ju",
    "e i përsosi",
    "format tuaja",
    "dhe tek Ai",
    "(është) përfundimi."
  ],
  "4": [
    "Ai e di",
    "atë çfarë",
    "(është) në",
    "qiejt",
    "dhe tokë",
    "dhe Ai e di",
    "atë çfarë",
    "ju fshehni",
    "dhe atë çfarë",
    "ju shfaqni",
    "dhe Allahu",
    "(është) i Gjithëdijshëm",
    "për ato çfarë posedojnë",
    "kraharorët."
  ],
  "5": [
    "A nuk",
    "ju erdhi ju",
    "lajmi",
    "i atyre të cilët",
    "mohuan",
    "nga",
    "përpara",
    "prandaj shijuan",
    "mynxyrën",
    "e punës së tyre",
    "dhe ata kanë",
    "një dënim",
    "të dhembshëm."
  ],
  "6": [
    "Kjo",
    "për shkak se",
    "[në vazhdimësi]",
    "u vinin atyre",
    "të dërguarit e tyre",
    "me prova të qarta",
    "e ata thonin",
    "\"A njerëz (të thjeshtë)",
    "t'na udhëzojnë në?!\"",
    "Kështu, ata mohuan",
    "dhe u zmbrapsën",
    "dhe është i pavarur",
    "Allahu",
    "dhe Allahu",
    "(është) i Pasur",
    "i Falënderuar."
  ],
  "7": [
    "Pandehin",
    "ata të cilët",
    "mohuan",
    "se",
    "nuk do të",
    "ringjallen",
    "thuaj",
    "\"Gjithsesi",
    "pasha Zotin tim",
    "sigurisht ju do të ringjalleni",
    "pastaj",
    "sigurisht do të njoftoheni",
    "për atë çfarë",
    "keni punuar",
    "e ajo",
    "për",
    "Allahun",
    "(është) e lehtë\"."
  ],
  "8": [
    "Prandaj besoni",
    "Allahun",
    "dhe të dërguarin e Tij",
    "dhe dritën",
    "të cilën",
    "Ne zbritëm",
    "e Allahu",
    "për atë çfarë",
    "ju punoni",
    "(është) i Mirëinformuar."
  ],
  "9": [
    "Ditën kur",
    "Ai ju tubon ju",
    "për ditën",
    "e Tubimit",
    "ajo (është)",
    "Dita",
    "e Mashtrimit (të ndërsjellë)",
    "e kushdo që",
    "beson",
    "Allahun",
    "dhe punon",
    "vepra të mira",
    "Ai do t'ia shlyejë",
    "atij",
    "të këqijat e tij",
    "dhe do ta fusë atë",
    "(në) xhenete",
    "rrjedhin",
    "nga",
    "poshtë tyre",
    "lumenjtë",
    "(ata janë) të gjithmonshëm",
    "në to",
    "pafundësisht",
    "ajo (është)",
    "fitorja",
    "madhështore."
  ],
  "10": [
    "E ata të cilët",
    "mohuan",
    "dhe përgënjeshtruan",
    "argumentet Tona",
    "ata (janë)",
    "shoqëruesit",
    "e zjarrit",
    "të gjithmonshëm",
    "në të",
    "e sa i keq (është)",
    "përfundimi!"
  ],
  "11": [
    "Nuk",
    "godet",
    "asnjë",
    "goditje",
    "përveçse",
    "me lejen",
    "e Allahut",
    "e kushdo që",
    "beson",
    "Allahun",
    "Ai udhëzon",
    "zemrën e tij",
    "e Allahu",
    "për çdo",
    "gjë",
    "(është) i Gjithëdijshëm."
  ],
  "12": [
    "Dhe bindjuni",
    "Allahut",
    "dhe bindjuni",
    "të dërguarit!",
    "E nëse",
    "ju zmbrapseni",
    "atëherë vetëm",
    "detyrë",
    "e të dërguarit Tonë",
    "(është) kumtimi",
    "i qartë."
  ],
  "13": [
    "Allahu!",
    "Nuk (ka)",
    "zot",
    "përveç",
    "Tij",
    "dhe tek",
    "Allahu",
    "le të mbështeten",
    "besimtarët!"
  ],
  "14": [
    "O ju",
    "të cilët",
    "besuat",
    "vërtet",
    "(disa) nga",
    "bashkëshortet tuaja",
    "dhe fëmijët tuaj",
    "(janë) armiq",
    "për ju",
    "prandaj ruajuni prej tyre!",
    "E nëse",
    "ju fshini gjurmët",
    "dhe toleroni",
    "dhe falni",
    "atëherë vërtet",
    "Allahu",
    "(është) Falës",
    "Mëshirues."
  ],
  "15": [
    "Vetëm",
    "pasuritë tuaja",
    "dhe fëmijët tuaj",
    "(janë) një sprovë",
    "e Allahu",
    "tek Ai",
    "(ka) shpërblim",
    "të madh."
  ],
  "16": [
    "Prandaj druajuni",
    "Allahut",
    "aq sa",
    "ju mundeni",
    "dhe dëgjoni",
    "dhe binduni",
    "dhe shpenzoni",
    "(për) të mirën",
    "e vetes suaj!",
    "E kushdo që",
    "(i) druhet",
    "lakmisë",
    "së vetes së tij",
    "atëherë të tillët",
    "ata",
    "(janë) të shpëtuarit."
  ],
  "17": [
    "Nëse",
    "ju i huazoni",
    "Allahut",
    "një hua",
    "të mirë",
    "Ai do ta shumëfishojë atë",
    "për ju",
    "dhe do t'ju falë",
    "ju",
    "e Allahu",
    "(është) shumë Mirënjohës",
    "shumë i Matur."
  ],
  "18": [
    "Njohës",
    "i së fshehtës",
    "dhe i së dukshmes",
    "i Pamposhturi",
    "i Urti."
  ]
}