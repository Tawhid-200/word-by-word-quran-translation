    "tokë",
    "e të jenë",
    "për ta",
    "zemra",
    "(që) perceptojnë",
    "me to",
    "apo",
    "veshë",
    "(që) dëgjojnë",
    "me to?",
    "Po vërtet",
    "nuk",
    "verbohen",
    "shikimet",
    "por",
    "verbohen",
    "zemrat",
    "të cilat",
    "(janë) në",
    "kraharorët."
  ],
  "47": [
    "Dhe ata kërkojnë nga ti të shpejtosh",
    "dënimin",
    "por nuk do të",
    "thyejë",
    "Allahu",
    "premtimin e Tij.",
    "Dhe vërtet",
    "një ditë",
    "tek",
    "Zoti yt",
    "(është) sa një mijë",
    "vjet",
    "nga ajo çfarë",
    "ju llogarisni."
  ],
  "48": [
    "E sa shumë",
    "nga",
    "vendbanime",
    "Unë u dhashë afat",
    "atyre",
    "duke qenë ato",
    "zullumqare",
    "pastaj",
    "Unë i mbërtheva ato",
    "dhe tek Unë",
    "(është) destinimi."
  ],
  "49": [
    "Thuaj",
    "\"O ju",
    "njerëz!",
    "Vetëm",
    "unë (jam)",
    "për ju",
    "një paralajmërues",
    "i qartë\"."
  ],
  "50": [
    "E ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "për ta (ka)",
    "falje",
    "dhe furnizim",
    "bujar."
  ],
  "51": [
    "E ata të cilët",
    "përpiqen",
    "kundër",
    "ajeteve Tona",
    "si mposhtës (të tyre)",
    "të tillët",
    "(janë) banorët",
    "e zjarrit të ferrit."
  ],
  "52": [
    "Dhe nuk",
    "Ne dërguam",
    "nga",
    "para teje",
    "asnjë",
    "të dërguar",
    "dhe as",
    "pejgamber",
    "veçse",
    "kur",
    "ai recitonte",
    "hidhte",
    "djalli",
    "në",
    "recitimin e tij.",
    "Por asgjëson",
    "Allahu",
    "atë çfarë",
    "hidhte",
    "djalli",
    "pastaj",
    "i përforcon",
    "Allahu",
    "ajetet e Tij.",
    "E Allahu",
    "(është) i Gjithëdijshëm",
    "i Urtë."
  ],
  "53": [
    "Që Ai të bëjë",
    "atë çfarë",
    "hedh",
    "djalli",
    "një sprovë",
    "për ata të cilët",
    "në",
    "zemrat e tyre",
    "(ka) sëmundje",
    "dhe (janë) të ngurta",
    "zemrat e tyre.",
    "E vërtet",
    "zullumqarët",
    "(janë) sigurisht në",
    "një përçarje",
    "të largët."
  ],
  "54": [
    "Dhe që të dinë",
    "ata të cilëve",
    "u është dhënë",
    "dija",
    "se ai (Kur'ani)",
    "(është) e vërteta",
    "nga",
    "Zoti yt",
    "që ata të besojnë",
    "në të",
    "e t'i nënshtrohen",
    "atij",
    "zemrat e tyre.",
    "E vërtet",
    "Allahu",
    "(është) sigurisht Udhëzues",
    "i atyre të cilët",
    "besuan",
    "tek",
    "një rrugë",
    "e drejtë."
  ],
  "55": [
    "Dhe nuk",
    "do të pushojnë",
    "ata të cilët",
    "mohuan",
    "(të jenë) në",
    "një dyshim",
    "për të",
    "derisa",
    "t'u vijë atyre",
    "Ora (e Kijametit)",
    "papritmas",
    "apo",
    "t'u vijë atyre",
    "një dënim",
    "i një dite",
    "shterpe."
  ],
  "56": [
    "Pushteti",
    "atë ditë",
    "(do të jetë) për Allahun",
    "Ai do të gjykojë",
    "mes tyre.",
    "E ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "(do të jenë) në",
    "kopshtet",
    "e begatisë."
  ],
  "57": [
    "E ata të cilët",
    "mohuan",
    "dhe përgënjeshtruan",
    "ajetet Tona",
    "atëherë të tillët",
    "për ta",
    "(do të jetë) një dënim",
    "poshtërues."
  ],
  "58": [
    "E ata të cilët",
    "emigruan",
    "në",
    "rrugën",
    "e Allahut",
    "pastaj",
    "u vranë",
    "apo",
    "vdiqën",
    "sigurisht do t'i furnizojë ata",
    "Allahu",
    "(me) një furnizim",
    "të mirë.",
    "E vërtet",
    "Allahu",
    "sigurisht Ai",
    "(është) më i miri",
    "i Furnizuesve."
  ],
  "59": [
    "Sigurisht Ai do t'i fusë ata",
    "(në) një hyrje",
    "ata do të jenë të kënaqur (me) të.",
    "E vërtet",
    "Allahu",
    "(është) sigurisht i Gjithëdijshëm",
    "shumë i Matur."
  ],
  "60": [
    "Kjo (është pra).",
    "E kushdo që",
    "shpaguan",
    "me aq",
    "sa",
    "ai u dëmtua",
    "me të",
    "pastaj",
    "i është bërë padrejtësi",
    "për të",
    "sigurisht do ta ndihmojë atë",
    "Allahu.",
    "Vërtet",
    "Allahu",
    "(është) sigurisht Fshirës (i gjynaheve)",
    "Falës i madh."
  ],
  "61": [
    "Kjo",
    "ngase",
    "Allahu",
    "ndërfut",
    "natën",
    "brenda",
    "ditës",
    "dhe ndërfut",
    "ditën",
    "brenda",
    "natës",
    "dhe se vërtet",
    "Allahu",
    "(është) Gjithëdëgjues",
    "Gjithëshikues."
  ],
  "62": [
    "Kjo",
    "ngase",
    "Allahu",
    "Ai (është)",
    "e Vërteta",
    "dhe se",
    "ajo çfarë",
    "ata lusin",
    "nga",
    "pos Tij",
    "ajo (është)",
    "e pavërteta",
    "dhe se vërtet",
    "Allahu",
    "Ai (është)",
    "i Larti",
    "i Madhi."
  ],
  "63": [
    "A nuk",
    "ti sheh",
    "se",
    "Allahu",
    "zbriti",
    "nga",
    "qielli",
    "ujë",
    "pastaj bëhet",
    "toka",
    "e blertë?",
    "Vërtet",
    "Allahu",
    "(është) i Butë",
    "i Gjithëinformuar."
  ],
  "64": [
    "Për Të (është)",
    "çfarëdo që",
    "(gjendet) në",
    "qiej",
    "dhe çfarëdo që",
    "(gjendet) në",
    "tokë.",
    "E vërtet",
    "Allahu",
    "sigurisht Ai",
    "(është) i Pasuri",
    "i Lavdëruari."
  ],
  "65": [
    "A nuk",
    "ti sheh",
    "se",
    "Allahu",
    "ka nënshtruar",
    "për ju",
    "atë çfarë",
    "(është) në",
    "tokë",
    "edhe anijet",
    "(që) lundrojnë",
    "në",
    "det",
    "me urdhrin e Tij?",
    "Dhe Ai e mban",
    "qiellin",
    "që të (mos)",
    "bie",
    "mbi",
    "tokë",
    "përveçse",
    "me lejen e Tij.",
    "Vërtet",
    "Allahu",
    "me njerëzit",
    "(është) shumë i Dhembshur",
    "Mëshirues."
  ],
  "66": [
    "Dhe Ai (është)",
    "i Cili",
    "ju dha jetë juve",
    "pastaj",
    "Ai bën që ju të vdisni",
    "pastaj",
    "Ai do t'ju ringjallë ju.",
    "Vërtet",
    "njeriu",
    "(është) sigurisht mosmirënjohës i madh."
  ],
  "67": [
    "Për çdo",
    "popull",
    "Ne kemi përcaktuar",
    "ritual(e)",
    "ata",
    "(janë) ndjekës të tij.",
    "Prandaj mos",
    "ata të polemizojnë kurrsesi me ty",
    "për",
    "çështjen",
    "por ftoji",
    "tek",
    "Zoti yt!",
    "Vërtet ti",
    "(je) sigurisht mbi",
    "një udhëzim",
    "të drejtë."
  ],
  "68": [
    "E nëse",
    "ata diskutojnë me ty",
    "atëherë thuaj",
    "\"Allahu",
    "e di më mirë",
    "atë çfarë",
    "ju punoni."
  ],
  "69": [
    "Allahu",
    "do të gjykojë",
    "mes jush",
    "ditën",
    "e Kijametit",
    "për atë çfarë",
    "ju ishit",
    "për të",
    "e kundërshtonit njëri-tjetrin\"."
  ],
  "70": [
    "A nuk",
    "ti e di",
    "se",
    "Allahu",
    "e di",
    "atë çfarë",
    "(është) në",
    "qiell",
    "dhe tokë?",
    "Vërtet",
    "ajo",
    "(është) në",
    "një Regjistër.",
    "Vërtet",
    "ajo",
    "për",
    "Allahun",
    "(është) e lehtë."
  ],
  "71": [
    "Dhe ata adhurojnë",
    "nga",
    "pos",
    "Allahut",
    "atë çfarë",
    "nuk",
    "Ai zbriti",
    "për të",
    "(asnjë) provë",
    "dhe atë çfarë",
    "nuk",
    "ata kanë",
    "për të",
    "(asnjë) dije.",
    "Dhe nuk",
    "(ka) për zullumqarët",
    "asnjë",
    "ndihmës."
  ],
  "72": [
    "E kur",
    "(u) recitohen",
    "atyre",
    "ajetet Tona",
    "të qarta",
    "ti do të njohësh",
    "në",
    "fytyrat",
    "e atyre të cilët",
    "mohuan",
    "marazin.",
    "Ata janë gati",
    "t'u vërsulen",
    "atyre të cilët",
    "ata i recitojnë",
    "atyre",
    "ajetet Tona.",
    "Thuaj",
    "\"A t'ju njoftoj ju",
    "për një më të keqe",
    "se",
    "ajo?",
    "Zjarri",
    "(Ua) premtoi atë",
    "Allahu",
    "atyre të cilët",
    "mohuan",
    "dhe sa i keq",
    "(është) destinimi!\""
  ],
  "73": [
    "O ju",
    "njerëz!",
    "Është sjellë",
    "një shembull",
    "prandaj dëgjojeni",
    "atë!",
    "Vërtet",
    "ata të cilët",
    "ju i lusni",
    "nga",
    "pos",
    "Allahut",
    "kurrë nuk do të",
    "krijojnë",
    "një mizë",
    "edhe sikur",
    "ata të mblidheshin",
    "për të.",
    "E nëse",
    "ua rrëmben atyre",
    "miza",
    "diçka",
    "nuk",
    "ata mund ta rikthejnë atë",
    "prej saj.",
    "I dobët është",
    "kërkuesi",
    "dhe i kërkuari."
  ],
  "74": [
    "Nuk",
    "ata e vlerësojnë",
    "Allahun",
    "(me) të duhurën",
    "vlerë të Tij.",
    "Vërtet",
    "Allahu",
    "(është) sigurisht i Fortë",
    "i Pamposhtur."
  ],
  "75": [
    "Allahu",
    "zgjedh",
    "nga",
    "engjëjt",
    "të dërguar",
    "dhe nga",
    "njerëzit.",
    "Vërtet",
    "Allahu",
    "(është) Gjithëdëgjues",
    "Gjithëshikues."
  ],
  "76": [
    "Ai e di",
    "çfarë",
    "(është) para",
    "tyre",
    "dhe çfarë",
    "(është) mbrapa tyre.",
    "Dhe tek",
    "Allahu",
    "kthehen",
    "(të gjitha) çështjet."
  ],
  "77": [
    "O ju",
    "të cilët",
    "besuan",
    "përkuluni",
    "dhe përuluni",
    "dhe adhuroni",
    "Zotin tuaj",
    "dhe bëni",
    "mirësi",
    "ashtu që ju",
    "të arrini sukses."
  ],
  "78": [
    "Dhe flijoni",
    "për (kauzën)",
    "e Allahut",
    "(me) të duhurin",
    "flijim ndaj Tij.",
    "Ai",
    "ju zgjodhi ju",
    "dhe nuk",
    "vendosi",
    "mbi ju",
    "në",
    "fe",
    "asnjë",
    "ngarkesë.",
    "(Në) fenë",
    "e babait tuaj",
    "Ibrahimit.",
    "Ai",
    "ju emërtoi ju",
    "muslimanë",
    "nga",
    "përpara",
    "edhe në",
    "këtë (Kur'an)",
    "që të jetë",
    "i dërguari",
    "dëshmitar",
    "mbi ju",
    "dhe ju të jeni",
    "dëshmitarë",
    "mbi",
    "njerëzit.",
    "Prandaj përsoseni",
    "namazin",
    "dhe jepni",
    "zeqatin",
    "dhe kapuni fuqishëm",
    "për Allahun!",
    "Ai (është)",
    "Mbrojtësi juaj",
    "e sa i mirë (është)",
    "Mbrojtësi",
    "dhe sa i mirë është",
    "Ndihmësi!"
  ]
}