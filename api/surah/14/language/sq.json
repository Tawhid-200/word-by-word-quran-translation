{
  "1": [
    "Elif, Lam, Ra.",
    "Libër",
    "Ne e zbritëm atë",
    "tek ti",
    "që t'i nxjerrësh",
    "njerëzit",
    "nga",
    "errësirat",
    "tek",
    "drita",
    "me lejen",
    "e Zotit të tyre",
    "për tek",
    "rruga",
    "e të Pamposhturit",
    "të Falënderuarit."
  ],
  "2": [
    "Allahut",
    "i Cili",
    "për Të (është)",
    "ajo çfarë (ka)",
    "në",
    "qiej",
    "dhe ajo çfarë (ka)",
    "në",
    "tokë.",
    "E mjerë",
    "për mohuesit",
    "nga",
    "një dënim",
    "i ashpër."
  ],
  "3": [
    "Ata të cilët",
    "e parapëlqejnë",
    "jetën",
    "e kësaj bote",
    "ndaj",
    "jetës së fundit",
    "dhe pengojnë",
    "nga",
    "rruga",
    "e Allahut",
    "dhe e kërkojnë atë",
    "të shtrembëruar.",
    "Të tillët (janë)",
    "në",
    "një humbje",
    "të largët."
  ],
  "4": [
    "Dhe nuk",
    "Ne dërguam",
    "asnjë",
    "të dërguar",
    "përveçse",
    "në.gjuhën",
    "e popullit të tij",
    "që t'u sqarojë",
    "atyre",
    "ashtu që e lë në humbje",
    "Allahu",
    "atë që",
    "Ai dëshiron",
    "dhe e udhëzon",
    "atë që",
    "Ai dëshiron",
    "dhe Ai (është)",
    "i Pamposhturi",
    "i Urti."
  ],
  "5": [
    "Dhe sigurisht",
    "Ne e dërguam",
    "Musain",
    "me dlshmitë Tona",
    "që (duke i thënë)",
    "\"Nxire",
    "popullin tënd",
    "nga",
    "errësirat",
    "tek",
    "drita",
    "dhe kujtoja atyre",
    "ditët",
    "e Allahut!\"",
    "Vërtet",
    "në",
    "këtë",
    "(ka) dëshmi",
    "për secilin",
    "shumë durimtar",
    "shumë falënderues."
  ],
  "6": [
    "Dhe kur",
    "(i) tha",
    "Musai",
    "popullit të tij",
    "\"Kujtoni",
    "begatinë",
    "e Allahut",
    "ndaj jush",
    "kur",
    "Ai ju shpëtoi ju",
    "nga",
    "familja",
    "e faraonit",
    "ata ju maltretonin ju",
    "(me) ligësinë",
    "e dënimit",
    "dhe i thernin",
    "djemtë tuaj",
    "dhe i lënin të gjalla",
    "gratë tuaja",
    "e në",
    "ato",
    "(ka) sprovë",
    "nga",
    "Zoti juaj",
    "e madhe\"."
  ],
  "7": [
    "Dhe kur",
    "njoftoi",
    "Zoti juaj",
    "\"Nëse",
    "ju falënderoni",
    "sigurisht Unë do t'jua shtoj ju",
    "e nëse",
    "ju mohoni",
    "vërtet",
    "dënimi Im (është)",
    "i ashpër\"."
  ],
  "8": [
    "Dhe tha",
    "Musai",
    "\"Nëse",
    "mohoni",
    "ju",
    "dhe kushdo që (është)",
    "në",
    "tokë",
    "të gjithë",
    "atëherë vërtet",
    "Allahu",
    "(është) i Pasur",
    "i Falënderuar."
  ],
  "9": [
    "A nuk",
    "ju erdhi ju",
    "lajmi",
    "i atyre të cilët (ishin)",
    "nga",
    "para jush",
    "i popullit",
    "të Nuhut",
    "Adit",
    "dhe Themudit",
    "dhe i atyre të cilët (ishin)",
    "nga",
    "pas tyre",
    "nuk",
    "i di ata",
    "përveçse",
    "Allahu.",
    "(U) erdhën atyre",
    "të dërguarit e tyre",
    "me prova të qarta",
    "por ata i kthyen",
    "duart e tyre",
    "në",
    "gojët e tyre",
    "dhe thanë",
    "\"Vërtet ne",
    "mohuam",
    "atë çfarë",
    "ju u dërguat",
    "me të",
    "dhe vërtet ne (jemi)",
    "në",
    "një dyshim",
    "nga ajo çfarë",
    "ju na ftoni ne",
    "tek ajo",
    "të thellë\"."
  ],
  "10": [
    "Thanë",
    "të dërguarit e tyre",
    "\"A në",
    "Allahun",
    "(ka) dyshim",
    "Shpikësi",
    "i qiejve",
    "dhe i tokës",
    "Ai ju fton ju",
    "që t'ju falë",
    "ju",
    "prej",
    "gjynaheve tuaja",
    "dhe t'jua shtyejë",
    "deri te",
    "një afat",
    "i përcaktuar\".",
    "Ata thanë",
    "\"Nuk",
    "ju (jeni)",
    "përveçse",
    "njerëz",
    "sikur ne",
    "ju dëshironi",
    "që",
    "t'na pengoni ne",
    "nga ajo çfarë",
    "ishin",
    "adhuronin",
    "baballarët tanë",
    "prandaj na sjell ne",
    "një argument",
    "të qartë!\""
  ],
  "11": [
    "(U) thanë",
    "atyre",
    "të dërguarit e tyre",
    "\"Nuk",
    "ne (jemi)",
    "përveçse",
    "njerëz",
    "sikurse ju",
    "mirëpo",
    "Allahu",
    "jep mirësi",
    "për",
    "atë që",
    "Ai dëshiron",
    "prej",
    "robërve të Tij",
    "dhe nuk",
    "është",
    "për ne",
    "që",
    "ne t'jua sjellim",
    "(ndonjë) argument",
    "përveçse",
    "me lejen",
    "e Allahut",
    "dhe tek",
    "Allahu",
    "le të mbështeten",
    "besimtarët\"."
  ],
  "12": [
    "E ç'(është)",
    "me ne",
    "që të mos",
    "mbështetemi",
    "tek",
    "Allahu",
    "kur tashmë",
    "Ai na udhëzoi ne",
    "(tek) rrugët tona",
    "dhe gjithsesi ne do të durojmë",
    "në",
    "atë çfarë",
    "ju na lënduat ne",
    "dhe tek",
    "Allahu",
    "le të mbështeten",
    "mbështetësit."
  ],
  "13": [
    "Dhe (u) thanë",
    "ata të cilët",
    "mohuan",
    "të dërguarve të tyre",
    "\"Gjithsesi ne so t'ju dëbojmë ju",
    "nga",
    "toka jonë",
    "apo",
    "doemos do të ktheheni",
    "në",
    "fenë tonë\".",
    "Atëherë i shpalli",
    "atyre",
    "Zoti i tyre",
    "\"Gjithsesi Ne do t'i shkatërrojmë",
    "zullumqarët."
  ],
  "14": [
    "Dhe gjithsesi Ne do t'ju bëjmë ju banorë",
    "të tokës",
    "nga",
    "pas tyre.",
    "Kjo (është)",
    "për secilin që",
    "(i) frikësohet",
    "qëndrimit para Meje",
    "dhe (i) frikësohet",
    "kërcënimit Tim\"."
  ],
  "15": [
    "Dhe ata (të dërguarit) kërkuan ndihmë",
    "dhe dështoi",
    "secili",
    "diktator",
    "kokëfortë."
  ],
  "16": [
    "Nga",
    "para tij",
    "(është) xhehenemi",
    "dhe atij i jepet",
    "prej",
    "një uji",
    "të qelbur."
  ],
  "17": [
    "Ai do të përpiqet ta gëlltisë atë",
    "por nuk",
    "mund",
    "ta kapërdijë atë",
    "dhe do t'i vijë",
    "vdekja",
    "nga",
    "çdo",
    "vend",
    "por nuk (do të jetë)",
    "ai",
    "i vdekur.",
    "E nga",
    "para tij",
    "(është) një dënim",
    "i tmerrshëm."
  ],
  "18": [
    "Shembulli",
    "i atyre të cilët",
    "mohuan",
    "Zotin e tyre",
    "veprat e tyre (janë)",
    "si një hi",
    "e shpërndan",
    "atë",
    "era",
    "në",
    "një ditë",
    "të stuhishme.",
    "Nuk",
    "ata mund të mbajnë",
    "prej asaj çfarë",
    "kanë fituar",
    "[për]",
    "asgjë.",
    "Ajo (është)",
    "(pikërisht) ajo",
    "humbja",
    "e largët."
  ],
  "19": [
    "A nuk",
    "ti sheh",
    "se vërtet",
    "Allahu",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "me të vërtetën.",
    "Nëse",
    "Ai dëshiron",
    "ju largon ju",
    "dhe sjell",
    "krijesë",
    "të re."
  ],
  "20": [
    "Dhe nuk (është)",
    "ajo",
    "për",
    "Allahun",
    "e vështirë."
  ],
  "21": [
    "Dhe ata do të paraqiten",
    "para Allahut",
    "të gjithë",
    "e do t'u thonë",
    "të dobëtit",
    "atyre të cilët",
    "ishin mendjemëdhenj",
    "\"Vërtet ne",
    "ishim",
    "ndaj jush",
    "pasues",
    "prandaj a (mund të jeni)",
    "ju",
    "largues",
    "nga ne",
    "prej",
    "dënimit",
    "të Allahut",
    "ndonjë",
    "gjë?\"",
    "Ata do të thonë",
    "\"Sikur",
    "t'na udhëzonte",
    "Allahu",
    "ne do t'ju udhëzonim ju.",
    "Njësoj (është)",
    "për ne",
    "a u mërzitëm",
    "apo",
    "duruam",
    "nuk (ka)",
    "për ne",
    "asnjë",
    "rrugëdalje\"."
  ],
  "22": [
    "Dhe do të thotë",
    "shejtani",
    "pasi të",
    "përfundojë",
    "çështja",
    "\"Vërtet",
    "Allahu",
    "ju premtoi ju",
    "premtimin",
    "e vërtetë",
    "dhe unë ju premtova ju",
    "e ju mashtrova.",
    "Dhe nuk",
    "ishte",
    "për mua",
    "ndaj jush",
    "asnjë",
    "fuqi",
    "përveç",
    "që",
    "unë ju ftova ju",
    "e ju m'u përgjigjët",
    "mua",
    "prandaj mos",
    "më qortoni mua",
    "por qortojeni",
    "veten tuaj!",
    "Nuk",
    "unë (jam)",
    "shpëtuesi juaj",
    "dhe nuk",
    "ju (jeni)",
    "shpëtuesit e mi.",
    "Vërtet unë",
    "mohova",
    "atë çfarë",
    "ju më shoqëruat mua",
    "nga",
    "përpara.",
    "Vërtet",
    "zullumqarët",
    "ata kanë",
    "dënim",
    "të dhembshëm\"."
  ],
  "23": [
    "Dhe u futën",
    "ata të cilët",
    "besuan",
    "punuan",
    "vepra të mira",
    "(në) xhenete",
    "rrjedhin",
    "nga",
    "poshtë tyre",
    "lumenjtë",
    "(ata janë) të gjithmonshëm",
    "në to",
    "me lejen",
    "e Zotit të tyre.",
    "Përshëndetja e tyre (është)",
    "në to",
    "\"paqe\"."
  ],
  "24": [
    "A nuk",
    "ti sheh",
    "si",
    "solli",
    "Allahu",
    "shembull",
    "një fjalë",
    "të mirë",
    "si një pemë",
    "e mirë",
    "rrënja e saj (është)",
    "e qëndrueshme",
    "kurse dega e saj",
    "në",
    "qiell."
  ],
  "25": [
    "Ajo jep",
    "frutat e saj",
    "(në) çdo",
    "kohë",
    "me lejen",
    "e Zotit të saj",
    "dhe sjell",
    "Allahu",
    "shembujt",
    "për njerëzit",
    "ashtu që ata",
    "(të) përkujtojnë."
  ],
  "26": [
    "Ndërsa shembulli",
    "i një fjale",
    "të keqe",
    "(është) si një pemë",
    "e keqe",
    "ajo është shkulur",
    "nga",
    "përmbi",
    "toka",
    "nuk (ka)",
    "për të",
    "asnjë",
    "qëndrueshmëri."
  ],
  "27": [
    "I forcon",
    "Allahu",
    "ata të cilët",
    "besuan",
    "me fjalën",
    "e qëndrueshme",
    "në",
    "jetën",
    "e kësaj bote",
    "dhe në",
    "jetën e fundit",
    "dhe i lë në humbje",
    "Allahu",
    "zullumqarët",
    "dhe bën",
    "Allahu",
    "çfarë",
    "Ai dëshiron."
  ],
  "28": [
    "A nuk",
    "ti sheh",
    "tek",
    "ata të cilët",
    "e zëvendësuan",
    "begatinë",
    "e Allahut",
    "(me) mohim",
    "dhe e shpien",
    "popullin e tyre",
    "(tek) vendbanimi",
    "i shkatërrimit."
  ],
  "29": [
    "(Në) xhehenem",
    "(do të) ndizen në të",
    "dhe sa i keq (është)",
    "qëndrimi."
  ],
  "30": [
    "Dhe ata i bënë",
    "Allahut",
    "ortakë",
    "që t'i devijojnë (të tjerët)",
    "nga",
    "rruga e Tij.",
    "Thuaj",
    "\"Kënaquni",
    "e vërtet",
    "fundi juaj (është)",
    "tek",
    "zjarri!\""
  ],
  "31": [
    "Thuaju",
    "robërve të Mi",
    "të cilët",
    "besuan",
    "\"Le ta përsosin",
    "namazin",
    "dhe le të shpenzojnë",
    "prej asaj çfarë",
    "Ne i furnizuam ata",
    "fshehtazi",
    "dhe haptazi",
    "nga",
    "para",
    "se",
    "të vijë",
    "një ditë",
    "nuk (ka)",
    "shitje",
    "në të",
    "dhe as",
    "miqësi\"."
  ],
  "32": [
    "Allahu (është)",
    "Ai i Cili",
    "krijoi",
    "qiejt",
    "dhe tokën",
    "dhe zbriti",
    "nga",
    "qielli",
    "ujë",
    "e nxori",
    "me të",
    "nga",
    "frutat",
    "furnizim",
    "për ju",
    "dhe nënshtroi",
    "për ju",
    "anijet",
    "që ato të lundrojnë",
    "në",
    "det",
    "me urdhrin e Tij",
    "dhe nënshtroi",
    "për ju",
    "lumenjtë."
  ],
  "33": [
    "Dhe nënshtroi",
    "për ju",
    "diellin",
    "dhe hënën",
    "ato dy (lëvizin) në vazhdimësi",
    "dhe nënshtroi",
    "për ju",
    "natën",
    "dhe ditën."
  ],
  "34": [
    "Dhe Ai ju dha ju",
    "nga",
    "gjithçka",
    "çfarë",
    "ju i kërkuat Atij",
    "e nëse",
    "ju e numëroni",
    "begatinë",
    "e Allahut",
    "nuk (mund)",
    "ta përfshini atë.",
    "Vërtet",
    "njeriu",
    "(është) shumë i padrejtë",
    "shumë mosmirënjohës."
  ],
  "35": [
    "Dhe kur",
    "tha",
    "Ibrahimi",
    "\"Zoti im",
    "bëje",
    "këtë",
    "vend",
    "të sigurt",
    "dhe më largo mua",
    "dhe bijtë e mi",
    "që",
    "t'i adhurojmë",
    "idhujt."
  ],
  "36": [
    "Zoti im",
    "vërtet ata",
    "i devijojnë",
    "shumë",
    "nga",
    "njerëzit",
    "prandaj kushdo që",
    "më pason mua",
    "atëherë vërtet Ai (është)",
    "prej meje",
    "e kushdo që",
    "më kundërshton mua",
    "atëherë vërtet Ti (je)",
    "Falës i madh",
    "Mëshirues."
  ],
  "37": [
    "Zoti ynë",
    "vërtet unë",
    "i vendosa të banojnë",
    "prej",
    "pasardhësve të mi",
    "në një luginë",
    "nuk",
    "ajo posedon",
    "të mbjella",
    "tek",
    "Shtëpia Jote",
    "e Shenjtë.",
    "Zoti ynë",
    "që ata ta përsosin",
    "namazin",
    "prandaj bëji",
    "zemra",
    "prej",
    "njerëzve",
    "(të) përmallohen",
    "për ta",
    "dhe furnizoji ata",
    "nga",
    "frutat",
    "ashtu që ata",
    "(të) falënderojnë."
  ],
  "38": [
    "Zoti ynë",
    "vërtet Ti",
    "di",
    "atë çfarë",
    "ne fshehim",
    "dhe atë çfarë",
    "ne shpalosim",
    "dhe nuk",
    "fshihet",
    "për",
    "Allahun",
    "asnjë",
    "gjë",
    "në",
    "tokë",
    "dhe as",
    "në",
    "qiell."
  ],
  "39": [
    "Falënderimi (është)",
    "për Allahun",
    "i Cili",
    "(më) dhuroi",
    "mua",
    "megjithë",
    "pleqërinë",
    "Ismailin",
    "dhe Is'hakun.",
    "Vërtet",
    "Zoti im (është)",
    "Dëgjues",
    "i lutjes."
  ],
  "40": [
    "Zoti im",
    "më bëj mua",
    "falës të përsosur",
    "të namazit",
    "dhe nga",
    "pasardhësit e mi.",
    "Zoti ynë",
    "dhe pranoje",
    "lutjen time!"
  ],
  "41": [
    "Zoti ynë",
    "(më) fal",
    "mua",
    "dhe prindërit e mi",
    "edhe besimtarët",
    "ditën kur",
    "(të) ngrihet",
    "llogaria\"."
  ],
  "42": [
    "Dhe mos",
    "mendo kurrsesi",
    "Allahun",
    "të shkujdesur",
    "për atë çfarë",
    "punojnë",
    "zullumqarët!",
    "Vetëm",
    "ai ua shtyen (afatin) atyre",
    "për një ditë",
    "shtangen",
    "në të",
    "shikimet."
  ],
  "43": [
    "(Ata do të jenë) duke vrapuar",
    "duke ngritur",
    "kokat e tyre",
    "nuk",
    "kthehet",
    "tek ta",
    "vështrimi i tyre",
    "kurse zemrat e tyre (janë)",
    "bosh."
  ],
  "44": [
    "Dhe paralajmëroji",
    "njerëzit",
    "ditën kur",
    "(do t'u) vijë atyre",
    "dënimi",
    "e (të) thonë",
    "ata të cilët",
    "bënë zullum",
    "\"Zoti ynë",
    "na shtyej (kohën)",
    "deri tek",
    "një afat",
    "i afërt",
    "(që) ne t'i përgjjgjemi",
    "thirrjes Tënde",
    "dhe t'i pasojmë",
    "të dërguarit!\"",
    "Vallë a nuk",
    "ju ishit",
    "u betuat",
    "nga",
    "përpara",
    "(se) nuk (ka)",
    "për ju",
    "asnjë",
    "largim?"
  ],
  "45": [
    "Dhe ju banuat",
    "në",
    "vendbanime",
    "të atyre të cilët",
    "i bënë zullum",
    "vetes së tyre",
    "dhe u sqarua",
    "për ju",
    "(se) si",
    "Ne vepruam",
    "me ta",
    "dhe sollëm",
    "për ju",
    "shembuj."
  ],
  "46": [
    "Dhe sigurisht",
    "ata përgatitën",
    "kurthin e tyre",
    "porse tek",
    "Allahu",
    "(është) kurthi i tyre",
    "edhe nëse",
    "është",
    "kurthi i tyre",
    "që të shkulen",
    "prej tij",
    "kodrat."
  ],
  "47": [
    "Kështu që mos",
    "mendo kurrsesi",
    "(se) Allahu",
    "(është) shkelës",
    "i premtimit të Tij",
    "të dërguarve të Tij.",
    "Vërtet",
    "Allahu (është)",
    "i Pamposhtur",
    "Zotërues",
    "i hakmarrjes."
  ],
  "48": [
    "Ditën kur",
    "(do të) ndërrohet",
    "toka",
    "(me) një tjetër",
    "tokë",
    "edhe qiejt",
    "dhe ata do të paraqiten",
    "para Allahut",
    "të Vetmit",
    "Ngadhënjyesit."
  ],
  "49": [
    "Dhe ti i sheh",
    "kriminelët",
    "atë ditë",
    "të lidhur",
    "në",
    "pranga."
  ],
  "50": [
    "Rrobat e tyre (do të jenë)",
    "nga",
    "katrani",
    "dhe (do t'i) mbulojë",
    "fytyrat e tyre",
    "zjarri."
  ],
  "51": [
    "Që ta shpërblejë",
    "Allahu",
    "çdo",
    "frymë",
    "(për) atë çfarë",
    "ajo ka fituar.",
    "Vërtet",
    "Allahu",
    "(është) i shpejtë",
    "(në) llogari."
  ],
  "52": [
    "Ky (Kur'an)",
    "(është) një kumtesë",
    "për njerëzit",
    "dhe që ata të paralajmërohen",
    "me të",
    "dhe që të dinë",
    "se vetëm",
    "Ai (është)",
    "Zot",
    "Një",
    "dhe që të përkujtojnë",
    "poseduesit",
    "e mendjeve."
  ]
}