{
  "1": [
    "Kaf.",
    "Pasha Kur'anin",
    "e lavdishëm!"
  ],
  "2": [
    "Jo por",
    "ata u çuditën",
    "që",
    "u erdhi atyre",
    "një paralajmërues",
    "prej tyre",
    "dhe thanë",
    "mohuesit",
    "\"Kjo (është)",
    "një gjë",
    "e çuditshme!\""
  ],
  "3": [
    "A kur",
    "të vdesim",
    "dhe të jemi",
    "dhé?",
    "Ai (është)",
    "një kthim",
    "i largët."
  ],
  "4": [
    "Sigurisht",
    "Ne dimë",
    "çfarë",
    "pakëson",
    "toka",
    "prej tyre",
    "dhe tek Ne (është)",
    "një Libër",
    "ruajtës."
  ],
  "5": [
    "Jo por",
    "ata përgënjeshtruan",
    "të vërtetën",
    "pasi që",
    "u erdhi atyre",
    "e ata (janë)",
    "në",
    "një gjendje",
    "të përzier."
  ],
  "6": [
    "Vallë a nuk",
    "ata shohin",
    "tek",
    "qielli",
    "mbi ta",
    "si",
    "Ne e ndërtuam atë",
    "dhe e stolisëm atë",
    "dhe nuk",
    "ai ka",
    "asnjë",
    "vrimë."
  ],
  "7": [
    "Edhe tokën",
    "Ne e kemi shtrirë atë",
    "dhe kemi vendosur",
    "në të",
    "kodra",
    "dhe mundësuam që të mbijë",
    "në të",
    "nga",
    "çdo",
    "lloj",
    "mahnitës."
  ],
  "8": [
    "Reflektim",
    "dhe përkujtim",
    "për çdo",
    "rob",
    "kthyes (tek Allahu)."
  ],
  "9": [
    "Dhe Ne zbritëm",
    "nga",
    "qielli",
    "ujë",
    "të bereqetshëm",
    "dhe mundësuam që të mbijnë",
    "në të",
    "kopshte",
    "dhe drithëra",
    "të korrur."
  ],
  "10": [
    "Edhe palmat",
    "të larta",
    "ato kanë",
    "vile",
    "të renditura."
  ],
  "11": [
    "Furnizim",
    "për robërit.",
    "Dhe Ne ngjallim",
    "me të",
    "një vend",
    "të vdekur.",
    "Kështu (është)",
    "dalja (ringjallja)."
  ],
  "12": [
    "Ka përgënjeshtruar",
    "para tyre",
    "populli",
    "i Nuhut",
    "dhe banorët",
    "e Resit",
    "dhe Themudi."
  ],
  "13": [
    "Edhe Adi",
    "dhe faraoni",
    "dhe vëllezërit",
    "e Lutit."
  ],
  "14": [
    "Edhe banorët",
    "e Ejkes",
    "dhe populli",
    "i Tubasë.",
    "Secili",
    "përgënjeshtronte",
    "të dërguarit",
    "prandaj e meritoi",
    "kërcënimin Tim."
  ],
  "15": [
    "Vallë a u lodhëm ne",
    "me krijimin",
    "e parë?!",
    "Jo por",
    "ata (janë)",
    "në",
    "një pështjellim",
    "nga",
    "një krijim",
    "i ri."
  ],
  "16": [
    "E sigurisht",
    "Ne e krijuam",
    "njeriun",
    "dhe Ne dimë",
    "atë çfarë",
    "i pëshpërit",
    "atjj",
    "vetja e tij",
    "dhe Ne (jemi)",
    "më afër",
    "tek ai",
    "se",
    "vena",
    "e qafës."
  ],
  "17": [
    "Kur",
    "e marrin",
    "dy marrësit (engjëjt)",
    "nga",
    "e djathta",
    "dhe nga",
    "e majta",
    "ai rri ulur."
  ],
  "18": [
    "Nuk",
    "ai shqipton",
    "asnjë",
    "fjalë",
    "përveçse",
    "pranë tij",
    "(është) një mbikëqyrës",
    "i gatshëm."
  ],
  "19": [
    "Dhe erdhi",
    "agonia",
    "e vdekjes",
    "me të vërtetën.",
    "Ajo (është)",
    "çfarë",
    "ti ishe",
    "prej saj",
    "duke u shmangur."
  ],
  "20": [
    "Dhe do të fryhet",
    "në",
    "Sur.",
    "Ajo (është)",
    "dita",
    "e kërcënimit."
  ],
  "21": [
    "Dhe do të vijë",
    "secili",
    "person",
    "me të (do të jetë)",
    "një drejtues",
    "dhe një dëshmues."
  ],
  "22": [
    "Sigurisht",
    "ti ishe",
    "në",
    "një shkujdesje",
    "prej",
    "kësaj",
    "e Ne e hoqëm",
    "nga ti",
    "perden tënde",
    "e shikimi yt",
    "sot",
    "(është) i mprehtë."
  ],
  "23": [
    "Dhe do të thotë",
    "shoqëruesi i tij",
    "\"Kjo (është)",
    "ajo çfarë",
    "(kam) pranë meje",
    "e gatshme\"."
  ],
  "24": [
    "Hidheni ju dy",
    "në",
    "xhehenem",
    "secilin",
    "mohues të madh",
    "kokëfortë."
  ],
  "25": [
    "Pengues të madh",
    "të së mirës",
    "shkelës",
    "dyshues."
  ],
  "26": [
    "Ai i cili",
    "bëri",
    "me",
    "Allahun",
    "zot",
    "tjetër",
    "pra hidheni ju dy atë",
    "në",
    "dënimin",
    "e ashpër."
  ],
  "27": [
    "Do të thotë",
    "shoqëruesi i tij (shejtani)",
    "\"Zoti ynë",
    "nuk",
    "unë e nxita atë që t'i kalojë kufijtë",
    "por",
    "ai ishte",
    "në",
    "një devijim",
    "të largët\"."
  ],
  "28": [
    "Do të thotë (Allahu)",
    "\"Mos",
    "u fjalosni",
    "pranë Meje",
    "kur tashmë",
    "Unë ju paralajmërova",
    "ju",
    "për kërcënimin\"."
  ],
  "29": [
    "Nuk",
    "ndryshohet",
    "fjala",
    "tek Unë",
    "dhe nuk",
    "Unë (jam)",
    "zullumqar",
    "ndaj robërve."
  ],
  "30": [
    "Ditën kur",
    "Ne (do t'i) themi",
    "xhehenemit",
    "\"A",
    "u mbushe?\"",
    "E ai (do të) thotë",
    "\"A (ka)",
    "ndonjë",
    "shtesë?\""
  ],
  "31": [
    "Dhe do të afrohet",
    "xheneti",
    "për të druajturit",
    "jo",
    "larg."
  ],
  "32": [
    "Kjo (është)",
    "ajo çfarë",
    "ju premtohet",
    "për secilin",
    "të penduar",
    "të ruajtur."
  ],
  "33": [
    "Kushdo që",
    "i përulet me frikë",
    "të Gjithëmëshirshmit",
    "në fshehtësi",
    "dhe vjen",
    "me zemër",
    "të kthyer (tek Allahu)."
  ],
  "34": [
    "Hyni në të",
    "me paqe!",
    "Kjo (është)",
    "dita",
    "e përhershmërisë."
  ],
  "35": [
    "Ata kanë",
    "çfarëdo që",
    "ata dëshirojnë",
    "në të",
    "kurse te Ne",
    "(ka) shtesë."
  ],
  "36": [
    "E sa e sa",
    "Ne kemi shkatërruar",
    "para tyre",
    "nga",
    "breza",
    "ata (ishin)",
    "më të fortë",
    "se ata",
    "(në) fuqi",
    "e ata vrapuan për strehim",
    "në",
    "vende",
    "a (ka)",
    "ndonjë",
    "rrugëdalje?"
  ],
  "37": [
    "Vërtet",
    "në",
    "këtë",
    "(ka) sigurisht përkujtim",
    "për atë që",
    "është",
    "për të",
    "zemër",
    "apo",
    "hodhi",
    "dëgjimin",
    "duke qenë ai",
    "dëshmues."
  ],
  "38": [
    "Dhe sigurisht",
    "Ne krijuam",
    "qiejt",
    "dhe tokën",
    "dhe atë çfarë",
    "(është) mes atyre dyve",
    "për",
    "gjashtë",
    "ditë",
    "dhe nuk",
    "na preku ne",
    "asnjë",
    "lodhje."
  ],
  "39": [
    "Prandaj duro",
    "lidhur me",
    "atë çfarë",
    "ata thonë",
    "dhe lartësoje",
    "me falënderim",
    "Zotin tënd",
    "para",
    "lindjes",
    "së diellit",
    "dhe para",
    "perëndimit."
  ],
  "40": [
    "Dhe gjatë",
    "natës",
    "lartësoje Atë",
    "edhe pas",
    "rënies në sexhde."
  ],
  "41": [
    "Dhe dëgjo",
    "ditën kur",
    "thërret",
    "thirrësi",
    "nga",
    "një vend",
    "i afërt."
  ],
  "42": [
    "Ditën kur",
    "ata (do të) dëgjojnë",
    "bërtimën",
    "me të vërtetë.",
    "Ajo (është)",
    "dita",
    "e daljes (ringjalljes)."
  ],
  "43": [
    "Vërtet ne",
    "Ne",
    "japim jetë",
    "dhe japim vdekje",
    "dhe tek Ne (është)",
    "kthimi."
  ],
  "44": [
    "Ditën kur",
    "çahet",
    "toka",
    "nga ta",
    "(ata dalin) duke nxituar.",
    "Ai (është)",
    "një tubim",
    "për Ne",
    "i lehtë."
  ],
  "45": [
    "Ne (jemi)",
    "më të ditur",
    "për atë çfarë",
    "ata thonë",
    "dhe nuk",
    "ti (je)",
    "mbi ta",
    "tiran.",
    "Prandaj përkujto",
    "me Kur'an",
    "atë që",
    "(i) frikësohet",
    "kërcënimit Tim."
  ]
}