  ],
  "61": [
    "Allahu (është)",
    "Ai i Cili",
    "bëri",
    "për ju",
    "natën",
    "që të pushoni",
    "në të",
    "dhe ditën",
    "ndriçuese.",
    "Vërtet",
    "Allahu",
    "(është) njëmend Zotërues",
