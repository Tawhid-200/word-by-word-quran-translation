    "format tuaja",
    "edhe Ai ju furnizoi ju",
    "prej",
    "të mirave.",
    "Ai (është)",
    "Allahu",
    "Zoti juaj.",
    "Pra bekuar qoftë",
    "Allahu",
    "Zoti",
    "i botëve!"
  ],
  "65": [
    "Ai (është)",
