{
  "1": [
    "Ha Mim."
  ],
  "2": [
    "Zbritja",
    "e Librit",
    "(është) prej",
    "Allahut",
    "të Pamposhturit",
    "të Gjithëdijshmit."
  ],
  "3": [
    "Falësit",
    "të mëkatit",
    "dhe Pranuesit",
    "të pendimit",
    "të Ashprit",
    "(në) ndëshkim",
    "Zotëruesit",
    "të dhuntisë.",
    "Nuk (ka)",
    "zot",
    "përveç",
    "Ai.",
    "Tek Ai",
    "(është) destinimi."
  ],
  "4": [
    "Nuk",
    "polemizon",
    "rreth",
    "ajeteve",
    "të Allahut",
    "përveç",
    "atyre të cilët",
    "mohuan",
    "prandaj mos",
    "të të mashtrojë ty",
    "qarkullimi i tyre",
    "nëpër",
    "qytete."
  ],
  "5": [
    "Ka përgënjeshtruar",
    "para tyre",
    "populli",
    "i Nuhut",
    "dhe klanet",
    "nga",
    "pas tyre",
    "dhe synoi",
    "secili",
    "popull",
    "të dërguarin e tyre",
    "që ta mbërthejnë atë",
    "dhe ata polemizuan",
    "me të kotën",
    "që ta shtypin",
    "me të",
    "të vërtetën",
    "ama Unë i mbërtheva ata",
    "eh si",
    "ishte",
    "ndëshkimi Im?"
  ],
  "6": [
    "Dhe kështu",
    "u ligjësua",
    "dekreti",
    "i Zotit tënd",
    "ndaj",
    "atyre të cilët",
    "mohuan",
    "se ata (janë)",
    "shoqëruesit",
    "e zjarrit."
  ],
  "7": [
    "Ata të cilët",
    "bartin",
    "Fronin",
    "dhe ata që (janë)",
    "përreth tij",
    "lartësojnë",
    "me falënderim",
    "të Zotit të tyre",
    "dhe besojnë",
    "në Të",
    "edhe kërkojnë falje",
    "për ata të cilët",
    "besuan",
    "\"Zoti ynë!",
    "Ti ke përfshirë",
    "çdo",
    "gjë",
    "(me) mëshirë",
    "dhe dije",
    "prandaj fali",
    "ata të cilët",
    "u penduan",
    "dhe ndoqën",
    "rrugën Tënde",
    "edhe ruaji ata",
    "(prej) dënimit",
    "të zjarrit të xhehenemit."
  ],
  "8": [
    "Zoti ynë!",
    "Dhe futi ata",
    "(në) xhenete",
    "të Adnit",
    "të cilat",
    "Ti ua ke premtuar atyre",
    "dhe secilit që",
    "ishte punëmirë",
    "prej",
    "baballarëve të tyre",
    "dhe grave të tyre",
    "edhe pasardhësve të tyre!",
    "Vërtet Ti",
    "Ti (je)",
    "i Pamposhturi",
    "i Urti."
  ],
  "9": [
    "Dhe ruaji ata",
    "(nga) të këqijat!",
    "E këdo që",
    "Ti e ruan",
    "(nga) të këqijat",
    "atë ditë",
    "sigurisht",
    "Ti e ke mëshiruar atë.",
    "E ajo",
    "ajo (është)",
    "fitorja",
    "e madhe\"."
  ],
  "10": [
    "Vërtet",
    "ata të cilët",
    "mohuan",
    "do të thirren",
    "\"Njëmend hidhërimi",
    "i Allahut",
    "(është) më i madh",
    "se",
    "hidhërimi juaj",
    "(ndaj) vetes suaj",
    "kur",
    "ju ftoheshit",
    "tek",
    "besimi",
    "ama mohonit\"."
  ],
  "11": [
    "Ata do të thonë",
    "\"Zoti ynë!",
    "Ti na bëre të vdesim",
    "dy herë",
    "dhe Ti na ngjalle",
    "dy herë",
    "e ne i pranuam",
    "mëkatet tona",
    "po a (ka)",
    "tek",
    "një dalje",
    "ndonjë",
    "rrugë?\""
  ],
  "12": [
    "\"Ajo (është)",
    "për shkak se",
    "kur",
    "thirrej",
    "Allahu",
    "vetëm Ai",
    "ju mohonit",
    "e nëse",
    "i shoqërohej zot",
    "Atij",
    "ju besonit.",
    "Prandaj gjykimi (I përket)",
    "Allahut",
    "të Lartit",
    "të Madhit\"."
  ],
  "13": [
    "Ai (është)",
    "i Cili",
    "jua shfaq ju",
    "provat e Tij",
    "dhe zbret",
    "për ju",
    "prej",
    "qiellit",
    "furnizim.",
    "Por nuk",
    "përkujton",
    "përveç",
    "atij që",
    "kthehet plotësisht."
  ],
  "14": [
    "Prandaj luteni",
    "Allahun",
    "duke qenë të sinqertë",
    "për Të",
    "(në) fe",
    "edhe sikur",
    "ta urrejnë (këtë)",
    "mohuesit."
  ],
  "15": [
    "I Larti",
    "(në) shkallë",
    "Zotëruesi",
    "i Fronit",
    "Ai hedh",
    "frymëzimin",
    "me",
    "urdhrin e Tij",
    "mbi",
    "atë që",
    "Ai dëshiron",
    "prej",
    "robërve të Tij",
    "që të paralajmërojë",
    "(për) ditën",
    "e takimit."
  ],
  "16": [
    "Ditën kur",
    "ata (do të jenë)",
    "të shfaqur",
    "nuk",
    "fshihet",
    "prej",
    "Allahut",
    "rreth tyre",
    "asgjë.",
    "Kujt i përket",
    "sundimi",
    "sot?",
    "Allahut",
    "Njëshit",
    "Ngadhënjyesit."
  ],
  "17": [
    "Sot",
    "kompensohet",
    "secili",
    "person",
    "me atë çfarë",
    "ka fituar.",
    "Nuk (ka)",
    "zullum",
    "sot.",
    "Vërtet",
    "Allahu",
    "(është) i Shpejtë",
    "(në) llogari."
  ],
  "18": [
    "Dhe paralajmëroji ata",
    "(për) ditën",
    "e afërt",
    "kur",
    "zemrat",
    "tek",
    "grykët",
    "(do të jenë) të papërmbajtura.",
    "Nuk (ka)",
    "për zullumqarët",
    "asnjë",
    "jaran",
    "dhe as",
    "ndërmjetësues",
    "(i cili) dëgjohet."
  ],
  "19": [
    "Ai di",
    "mashtrueset",
    "e syve",
    "dhe atë çfarë",
    "fshehin",
    "gjokset."
  ],
  "20": [
    "Dhe Allahu",
    "gjykon",
    "me vërtetësi.",
    "Kurse ata të cilët",
    "thërrasin",
    "nga",
    "pos Tij",
    "nuk",
    "gjykojnë",
    "me asgjë.",
    "Vërtet",
    "Allahu",
    "Ai (është)",
    "Gjithëdëgjuesi",
    "Gjithëshikuesi."
  ],
  "21": [
    "Vallë a nuk",
    "ata ecin",
    "në",
    "tokë",
    "e të shohin",
    "si",
    "ishte",
    "përfundimi",
    "i atyre të cilët",
    "ishin",
    "nga",
    "para tyre?",
    "Ishin",
    "ata",
    "më të ashpër",
    "se ata",
    "(në) fuqi",
    "dhe (me) gjurmë",
    "në",
    "tokë",
    "por i mbërtheu ata",
    "Allahu",
    "për shkak të mëkateve të tyre",
    "dhe nuk",
    "kishte",
    "për ta",
    "karshi",
    "Allahut",
    "asnjë",
    "mbrojtës."
  ],
  "22": [
    "Ajo (është)",
    "për shkak se atyre",
    "ishin",
    "duke u ardhur atyre",
    "të dërguarit e tyre",
    "me prova të qarta",
    "ama ata mohuan",
    "prandaj i mbërtheu ata",
    "Allahu.",
    "Vërtet ai (është)",
    "i Fortë",
    "(me) të ashpërin",
    "ndëshkim."
  ],
  "23": [
    "Dhe sigurisht",
    "Ne e dërguam",
    "Musain",
    "me provat Tona",
    "dhe një argument",
    "të qartë."
  ],
  "24": [
    "Tek",
    "faraoni",
    "e Hamani",
    "dhe Karuni",
    "por ata thanë",
    "\"Një magjistar",
    "gënjeshtar i madh\"."
  ],
  "25": [
    "Pasi që",
    "ai ua solli atyre",
    "të vërtetën",
    "prej",
    "Nesh",
    "ata thanë",
    "\"Vrisni",
    "bijtë",
    "e atyre të cilët",
    "besuan",
    "me të",
    "dhe lini të jetojnë",
    "gratë e tyre!\"",
    "Por nuk (është)",
    "komploti",
    "i mohuesve",
    "veçse",
    "në",
    "dështim."
  ],
  "26": [
    "Dhe tha",
    "faraoni",
    "\"Më lini mua",
    "ta vras",
    "Musain",
    "e ai le ta thërrasë",
    "Zotin e tij!",
    "Vërtet unë",
    "frikësohem",
    "që",
    "ai do ta ndërrojë",
    "fenë tuaj",
    "apo",
    "të",
    "nxisë",
    "në",
    "tokë",
    "shthurjen\"."
  ],
  "27": [
    "Dhe tha",
    "Musai",
    "\"Vërtet unë",
    "strehohem",
    "tek Zoti im",
    "dhe Zoti juaj",
    "prej",
    "çdo",
    "mendjemadhi",
    "(që) nuk",
    "beson",
    "në ditën",
    "e llogarisë\"."
  ],
  "28": [
    "Dhe tha",
    "një burrë",
    "besimtar",
    "prej",
    "familjes",
    "së faraonit",
    "e fshehte",
    "besimin e tij",
    "\"A po e vrisni",
    "një burrë",
    "që",
    "thotë",
    "\"Zoti im (është)",
    "Allahu\"",
    "ndërkohë që sigurisht",
    "ai ju solli",
    "provat e qarta",
    "prej",
    "Zotit tuaj?",
    "Po nëse",
    "ai është",
    "gënjeshtar",
    "atëherë kundër tij",
    "(është) gënjeshtra e tij",
    "e nëse",
    "ai është",
    "i sinqertë",
    "ju qëllon ju",
    "diçka",
    "të asaj që",
    "ai ju premton ju.",
    "Vërtet",
    "Allahu",
    "nuk",
    "e udhëzon",
    "atë që",
    "ai (është)",
    "teprues",
    "gënjeshtar i madh."
  ],
  "29": [
    "O populli im!",
    "Për ju (është)",
    "mbretëria",
    "sot",
    "mbizotërues",
    "në",
    "tokë.",
    "E kush",
    "do t'na ndihmojë ne",
    "prej",
    "dënimit",
    "të Allahut",
    "nëse",
    "na vjen ne?\"",
    "Tha",
    "faraoni",
    "\"Nuk",
    "unë jua shfaq ju",
    "veçse",
    "atë çfarë",
    "unë shoh",
    "dhe nuk",
    "unë ju udhëzoj ju",
    "veçse",
    "rrugën",
    "e drejtë\"."
  ],
  "30": [
    "Dhe tha",
    "ai i cili",
    "besoi",
    "\"O populli im!",
    "Vërtet unë",
    "frikësohem",
    "për ju",
    "sikurse",
    "dita",
    "e klaneve."
  ],
  "31": [
    "Sikurse",
    "rasti",
    "i popullit",
    "të Nuhut",
    "e Adit",
    "dhe Themudit",
    "edhe atyre të cilët (ishin)",
    "nga",
    "pas tyre.",
    "Dhe nuk",
    "Allahu",
    "dëshiron",
    "zullum",
    "për robërit."
  ],
  "32": [
    "Dhe o populli im!",
    "Vërtet unë",
    "frikësohem",
    "për ju",
    "ditën",
    "e thirrjes."
  ],
  "33": [
    "Ditën kur",
    "ju do të zmbrapseni",
    "duke ia mbathur",
    "nuk (ka)",
    "për ju",
    "prej",
    "Allahut",
    "asnjë",
    "mbrojtës.",
    "E atë që",
    "e lë në devijim",
    "Allahu",
    "atëherë nuk (ka)",
    "për të",
    "asnjë",
    "udhëzues\"."
  ],
  "34": [
    "Dhe sigurisht",
    "erdhi tek ju",
    "Jusufi",
    "nga",
    "përpara",
    "me provat e qarta",
    "por",
    "ju vazhdonit pandërprerë",
    "në",
    "një dyshim",
    "me atë çfarë",
    "ai ju solli ju",
    "atë",
    "deri",
    "kur",
    "ai vdiq",
    "ju thatë",
    "\"Nuk",
    "do të dërgojë",
    "Allahu",
    "nga",
    "pas tij",
    "të dërguar\".",
    "Kështu",
    "e lë në devijim",
    "Allahu",
    "atë që",
    "ai (është)",
    "teprues",
    "dyshues."
  ],
  "35": [
    "Ata të cilët",
    "polemizojnë",
    "rreth",
    "ajeteve",
    "të Allahut",
    "pa",
    "ndonjë argument",
    "(që) u erdhi atyre.",
    "Është e madhe",
    "neveri",
    "tek",
    "Allahu",
    "dhe tek",
    "ata të cilët",
    "besuan.",
    "Kështu",
    "vulos",
    "Allahu",
    "mbi",
    "çdo",
    "zemër",
    "të një mendjemadhi",
    "tirani."
  ],
  "36": [
    "Dhe tha",
    "faraoni",
    "\"O Haman!",
    "Ndërto",
    "për mua",
    "një kullë",
    "mbase unë",
    "i mbërrij",
    "rrugët."
  ],
  "37": [
    "Rrugët",
    "e qiejve",
    "e të vështroj",
    "tek",
    "Zoti",
    "i Musait",
    "dhe vërtet unë",
    "njëmend e konsideroj atë",
    "gënjeshtar\".",
    "Dhe kështu",
    "iu zbukurua",
    "faraonit",
    "ligësia",
    "e punës së tij",
    "dhe ai u shmang",
    "nga",
    "rruga.",
    "Dhe nuk (është)",
    "komploti",
    "i faraonit",
    "veçse",
    "në",
    "shkatërrim."
  ],
  "38": [
    "Dhe tha",
    "ai i cili",
    "besoi",
    "\"O populli im!",
    "Më ndiqni mua",
    "t'ju udhëzoj ju",
    "(në) rrugën",
    "e drejtë."
  ],
  "39": [
    "O populli im!",
    "Vetëm (është)",
    "kjo",
    "jetë",
    "e kësaj bote",
    "një plaçkë",
    "dhe vërtet",
    "jeta e fundit",
    "ajo (është)",
    "shtëpia",
    "e vendqëndrimit."
  ],
  "40": [
    "Kushdo që",
    "punon",
    "një të keqe",
    "nuk",
    "do të kompensohet",
    "veçse",
    "sikurse ajo",
    "e kushdo që",
    "punon",
    "një të mirë",
    "qoftë",
    "mashkull",
    "apo",
    "femër",
    "ndërkohë që ai (është)",
    "besimtar",
    "atëherë të tillët",
    "do të hyjnë",
    "(në) xhenet",
    "do të furnizohen",
    "në të",
    "pa",
    "hesap."
  ],
  "41": [
    "Dhe o populli im!",
    "Ç'(është)",
    "me mua",
    "unë ju ftoj ju",
    "tek",
    "shpëtimi",
    "ndërsa ju më ftoni mua",
    "tek",
    "zjarri?"
  ],
  "42": [
    "Ju më ftoni mua",
    "që ta mohoj",
    "Allahun",
    "dhe t'i shoqëroj zota",
    "Atij",
    "çfarë",
    "nuk ka",
    "për mua",
    "lidhur me të",
    "(asnjë) dije",
    "kurse unë",
    "ju ftoj ju",
    "tek",
    "i Pamposhturi",
    "Falësi."
  ],
  "43": [
    "Nuk (ka)",
    "dyshim",
    "se ajo çfarë",
    "ju më ftoni mua",
    "tek ajo",
    "nuk ka",
    "për të",
    "lutje",
    "në",
    "këtë botë",
    "dhe as",
    "në",
    "të fundit",
    "dhe se",
    "kthimi ynë (është)",
    "tek",
    "Allahu",
    "dhe se",
    "tepruesit",
    "ata (janë)",
    "shoqëruesit",
    "e zjarrit."
  ],
  "44": [
    "Ama ju do ta kujtoni",
    "atë çfarë",
    "unë (ju) them",
    "ju",
    "dhe unë e dorëzoj",
    "çështjen time",
    "tek",
    "Allahu.",
    "Vërtet",
    "Allahu",
    "(është) Gjithëshikues",
    "i robërve\"."
  ],
  "45": [
    "Prandaj e mbrojti atë",
    "Allahu",
    "(nga) të këqijat",
    "e asaj çfarë",
    "ata komplotuan",
    "dhe e përfshiu",
    "familjen",
    "e faraonit",
    "ligësia",
    "e dënimit."
  ],
  "46": [
    "Zjarri",
    "ata do t'i ekspozohen",
    "atij",
    "mëngjes",
    "e mbrëmje.",
    "E ditën kur",
    "të ndodhë",
    "Kijameti",
    "\"Futni",
    "familjen",
    "e faraonit",
    "(në) më të ashpërin",
    "dënim!\""
  ],
  "47": [
    "Dhe kur",
    "ata të grinden",
    "në",
    "zjarr",
    "e t'u thonë",
    "të dobëtit",
    "atyre të cilët",
    "ishin mendjemëdhenj",
    "\"Vërtet ne",
    "ishim",
    "për ju",
    "ndjekës",
    "kështu që a",
    "(jeni) ju",
    "largues",
    "prej nesh",
    "një hise",
    "prej",
    "zjarrit?\""
  ],
  "48": [
    "Do të thonë",
    "ata të cilët",
    "ishin mendjemëdhenj",
    "\"Vërtet ne",
    "të gjithë",
    "(jemi) në të.",
    "Vërtet",
    "Allahu",
    "sigurisht",
    "gjykoi",
    "mes",
    "robërve\"."
  ],
  "49": [
    "Dhe do t'u thonë",
    "ata të cilët (janë)",
    "në",
    "zjarr",
    "rojtarëve",
    "të xhehenemit",
    "\"Thirrni",
    "Zotin tuaj",
    "të lehtësojë",
    "për ne",
    "një ditë",
    "prej",
    "dënimit\"."
  ],
  "50": [
    "Ata do të thonë",
    "\"Vallë a nuk",
    "ishin",
    "duke ju ardhur ju",
    "të dërguarit tuaj",
    "me provat e qarta?\"",
    "Ata do të thonë",
    "\"Gjithsesi!\"",
    "Ata do të thonë",
    "\"Atëherë thirrni!",
    "Por nuk (është)",
    "thirrja",
    "e mohuesve",
    "veçse",
    "në",
    "dështim\"."
  ],
  "51": [
    "Vërtet Ne",
    "sigurisht do t'i ndihmojmë",
    "të dërguarit Tanë",
    "dhe ata të cilët",
    "besuan",
    "në",
    "jetën",
    "e kësaj bote",
    "dhe ditën kur",
    "do të paraqiten",
    "dëshmitë."
  ],
  "52": [
    "Ditën kur",
    "nuk",
    "do t'u bëjë dobi",
    "zullumqarëve",
    "arsyetimi i tyre",
    "dhe për ta (është)",
    "mallkimi",
    "dhe për ta (është)",
    "ligësia",
    "e shtëpisë."
  ],
  "53": [
    "Dhe sigurisht",
    "Ne i dhamë",
    "Musait",
    "udhëzimin",
    "dhe i lamë në trashëgim",
    "bijve",
    "të Israilit",
    "Librin."
  ],
  "54": [
    "Udhëzim",
    "dhe përkujtim",
    "për poseduesit",
    "e mendjeve."
  ],
  "55": [
    "E ti duro!",
    "Vërtet",
    "premtimi",
    "i Allahut",
    "(është) i vërtetë",
    "dhe kërko falje",
    "për mëkatin tënd",
    "dhe lartëso",
    "me falënderim",
    "të Zotit tënd",
    "në mbrëmje",
    "e mëngjes!"
  ],
  "56": [
    "Vërtet",
    "ata të cilët",
    "polemizojnë",
    "rreth",
    "ajeteve",
    "të Allahut",
    "pa",
    "(ndonjë) argument",
    "(që) ju erdhi atyre",
    "nuk (ka)",
    "në",
    "gjokset e tyre",
    "veçse",
    "mendjemadhësi",
    "nuk",
    "ata",
    "e arrijnë atë.",
    "Prandaj kërko strehë",
    "me Allahun!",
    "Vërtet Ai",
    "Ai (është)",
    "Gjithëdëgjuesi",
    "Gjithëshikuesi."
  ],
  "57": [
    "Njëmend krijimi",
    "i qiejve",
    "dhe i tokës",
    "(është) më i madh",
    "se",
    "krijimi",
    "i njerëzve",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "dinë."
  ],
  "58": [
    "Dhe nuk",
