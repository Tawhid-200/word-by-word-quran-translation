    "e Allahut",
    "duke i mohuar."
  ],
  "64": [
    "Allahu (është)",
    "Ai i Cili",
    "bëri",
    "për ju",
    "tokën",
    "vendqëndrim",
    "dhe qiellin",
    "ndërtim",
    "dhe Ai ju formësoi ju",
