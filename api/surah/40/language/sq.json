    "dhe shikuesi",
    "dhe ata të cilët",
    "besuan",
    "dhe punuan",
    "vepra të mira",
    "dhe as",
    "keqbërësi.",
    "Sa pak",
    "që",
    "ju përkujtoni!"
  ],
  "59": [
    "Vërtet",
    "Ora",
    "(është) njëmend ardhëse",
    "nuk (ka)",
    "dyshim",
    "në të",
    "mirëpo",
    "shumica",
    "e njerëzve",
    "nuk",
    "besojnë."
  ],
  "60": [
    "Dhe tha",
